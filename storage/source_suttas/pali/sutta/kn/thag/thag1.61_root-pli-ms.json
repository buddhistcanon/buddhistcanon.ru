{
  "thag1.61:0.1": "Theragāthā",
  "thag1.61:0.2": "Ekakanipāta",
  "thag1.61:0.3": "Sattamavagga",
  "thag1.61:0.4": "1.61. Vappattheragāthā",
  "thag1.61:1.1": "“Passati passo passantaṃ,",
  "thag1.61:1.2": "apassantañca passati;",
  "thag1.61:1.3": "Apassanto apassantaṃ,",
  "thag1.61:1.4": "passantañca na passatī”ti.",
  "thag1.61:1.5": "…",
  "thag1.61:1.6": "Vappo thero …."
}