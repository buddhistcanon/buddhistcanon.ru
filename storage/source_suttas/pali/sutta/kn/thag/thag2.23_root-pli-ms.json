{
  "thag2.23:0.1": "Theragāthā",
  "thag2.23:0.2": "Dukanipāta",
  "thag2.23:0.3": "Tatiyavagga",
  "thag2.23:0.4": "2.23. Sobhitattheragāthā",
  "thag2.23:1.1": "“Satimā paññavā bhikkhu,",
  "thag2.23:1.2": "āraddhabalavīriyo;",
  "thag2.23:1.3": "Pañca kappasatānāhaṃ,",
  "thag2.23:1.4": "ekarattiṃ anussariṃ.",
  "thag2.23:2.1": "Cattāro satipaṭṭhāne,",
  "thag2.23:2.2": "satta aṭṭha ca bhāvayaṃ;",
  "thag2.23:2.3": "Pañca kappasatānāhaṃ,",
  "thag2.23:2.4": "ekarattiṃ anussarin”ti.",
  "thag2.23:2.5": "…",
  "thag2.23:2.6": "Sobhito thero …."
}