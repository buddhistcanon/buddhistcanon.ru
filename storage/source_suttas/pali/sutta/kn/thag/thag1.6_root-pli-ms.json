{
  "thag1.6:0.1": "Theragāthā",
  "thag1.6:0.2": "Ekakanipāta",
  "thag1.6:0.3": "Paṭhamavagga",
  "thag1.6:0.4": "1.6. Sītavaniyattheragāthā",
  "thag1.6:1.1": "“Yo sītavanaṃ upagā bhikkhu,",
  "thag1.6:1.2": "Eko santusito samāhitatto;",
  "thag1.6:1.3": "Vijitāvī apetalomahaṃso,",
  "thag1.6:1.4": "Rakkhaṃ kāyagatāsatiṃ dhitimā”ti.",
  "thag1.6:1.5": "Itthaṃ sudaṃ āyasmā sītavaniyo thero gāthaṃ abhāsitthāti."
}