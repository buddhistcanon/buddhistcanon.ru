{
  "thag1.67:0.1": "Theragāthā",
  "thag1.67:0.2": "Ekakanipāta",
  "thag1.67:0.3": "Sattamavagga",
  "thag1.67:0.4": "1.67. Ekadhammasavanīyattheragāthā",
  "thag1.67:1.1": "“Kilesā jhāpitā mayhaṃ,",
  "thag1.67:1.2": "bhavā sabbe samūhatā;",
  "thag1.67:1.3": "Vikkhīṇo jātisaṃsāro,",
  "thag1.67:1.4": "natthi dāni punabbhavo”ti.",
  "thag1.67:1.5": "…",
  "thag1.67:1.6": "Ekadhammasavanīyo thero …."
}