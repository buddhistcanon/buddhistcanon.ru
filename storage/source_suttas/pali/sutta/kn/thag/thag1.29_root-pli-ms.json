{
  "thag1.29:0.1": "Theragāthā",
  "thag1.29:0.2": "Ekakanipāta",
  "thag1.29:0.3": "Tatiyavagga",
  "thag1.29:0.4": "1.29. Hāritattheragāthā",
  "thag1.29:1.1": "“Samunnamayamattānaṃ,",
  "thag1.29:1.2": "usukārova tejanaṃ;",
  "thag1.29:1.3": "Cittaṃ ujuṃ karitvāna,",
  "thag1.29:1.4": "avijjaṃ bhinda hāritā”ti.",
  "thag1.29:1.5": "…",
  "thag1.29:1.6": "Hārito thero …."
}