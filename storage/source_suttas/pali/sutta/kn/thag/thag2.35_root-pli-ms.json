{
  "thag2.35:0.1": "Theragāthā",
  "thag2.35:0.2": "Dukanipāta",
  "thag2.35:0.3": "Catutthavagga",
  "thag2.35:0.4": "2.35. Sambulakaccānattheragāthā",
  "thag2.35:1.1": "“Devo ca vassati, devo ca gaḷagaḷāyati,",
  "thag2.35:1.2": "Ekako cāhaṃ bherave bile viharāmi;",
  "thag2.35:1.3": "Tassa mayhaṃ ekakassa bherave bile viharato,",
  "thag2.35:1.4": "Natthi bhayaṃ vā chambhitattaṃ vā lomahaṃso vā.",
  "thag2.35:2.1": "Dhammatā mamesā yassa me,",
  "thag2.35:2.2": "Ekakassa bherave bile;",
  "thag2.35:2.3": "Viharato natthi bhayaṃ vā,",
  "thag2.35:2.4": "Chambhitattaṃ vā lomahaṃso vā”ti.",
  "thag2.35:2.5": "…",
  "thag2.35:2.6": "Sambulakaccāno thero …."
}