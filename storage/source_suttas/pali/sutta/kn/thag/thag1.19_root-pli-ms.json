{
  "thag1.19:0.1": "Theragāthā",
  "thag1.19:0.2": "Ekakanipāta",
  "thag1.19:0.3": "Dutiyavagga",
  "thag1.19:0.4": "1.19 Kulattheragāthā",
  "thag1.19:1.1": "“Udakañhi nayanti nettikā,",
  "thag1.19:1.2": "Usukārā namayanti tejanaṃ;",
  "thag1.19:1.3": "Dāruṃ namayanti tacchakā,",
  "thag1.19:1.4": "Attānaṃ damayanti subbatā”ti.",
  "thag1.19:1.5": "…",
  "thag1.19:1.6": "Kulo thero …."
}