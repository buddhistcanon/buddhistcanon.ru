{
  "thag1.78:0.1": "Theragāthā",
  "thag1.78:0.2": "Ekakanipāta",
  "thag1.78:0.3": "Aṭṭhamavagga",
  "thag1.78:0.4": "1.78. Meṇḍasirattheragāthā",
  "thag1.78:1.1": "“Anekajātisaṃsāraṃ,",
  "thag1.78:1.2": "sandhāvissaṃ anibbisaṃ;",
  "thag1.78:1.3": "Tassa me dukkhajātassa,",
  "thag1.78:1.4": "dukkhakkhandho aparaddho”ti.",
  "thag1.78:1.5": "…",
  "thag1.78:1.6": "Meṇḍasiro thero …."
}