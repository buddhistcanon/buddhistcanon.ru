{
  "thag2.33:0.1": "Theragāthā",
  "thag2.33:0.2": "Dukanipāta",
  "thag2.33:0.3": "Catutthavagga",
  "thag2.33:0.4": "2.33 Upavāṇattheragāthā",
  "thag2.33:1.1": "“Arahaṃ sugato loke,",
  "thag2.33:1.2": "vātehābādhiko muni;",
  "thag2.33:1.3": "Sace uṇhodakaṃ atthi,",
  "thag2.33:1.4": "munino dehi brāhmaṇa.",
  "thag2.33:2.1": "Pūjito pūjaneyyānaṃ,",
  "thag2.33:2.2": "sakkareyyāna sakkato;",
  "thag2.33:2.3": "Apacitopaceyyānaṃ,",
  "thag2.33:2.4": "tassa icchāmi hātave”ti.",
  "thag2.33:2.5": "…",
  "thag2.33:2.6": "Upavāṇo thero …."
}