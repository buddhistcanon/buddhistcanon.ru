{
  "thag1.12:0.1": "Theragāthā",
  "thag1.12:0.2": "Ekakanipāta",
  "thag1.12:0.3": "Dutiyavagga",
  "thag1.12:0.4": "1.12. Mahāvacchattheragāthā",
  "thag1.12:1.1": "“Paññābalī sīlavatūpapanno,",
  "thag1.12:1.2": "Samāhito jhānarato satīmā;",
  "thag1.12:1.3": "Yadatthiyaṃ bhojanaṃ bhuñjamāno,",
  "thag1.12:1.4": "Kaṅkhetha kālaṃ idha vītarāgo”ti.",
  "thag1.12:1.5": "…",
  "thag1.12:1.6": "Mahāvaccho thero …."
}