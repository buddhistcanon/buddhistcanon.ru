{
  "thag2.48:0.1": "Theragāthā",
  "thag2.48:0.2": "Dukanipāta",
  "thag2.48:0.3": "Pañcamavagga",
  "thag2.48:0.4": "2.48. Vajjitattheragāthā",
  "thag2.48:1.1": "“Saṃsaraṃ dīghamaddhānaṃ,",
  "thag2.48:1.2": "gatīsu parivattisaṃ;",
  "thag2.48:1.3": "Apassaṃ ariyasaccāni,",
  "thag2.48:1.4": "andhabhūto puthujjano.",
  "thag2.48:2.1": "Tassa me appamattassa,",
  "thag2.48:2.2": "saṃsārā vinaḷīkatā;",
  "thag2.48:2.3": "Sabbā gatī samucchinnā,",
  "thag2.48:2.4": "natthi dāni punabbhavo”ti.",
  "thag2.48:2.5": "…",
  "thag2.48:2.6": "Vajjito thero …."
}