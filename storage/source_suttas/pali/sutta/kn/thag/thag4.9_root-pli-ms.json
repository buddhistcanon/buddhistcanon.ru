{
  "thag4.9:0.1": "Theragāthā",
  "thag4.9:0.2": "Catukkanipāta",
  "thag4.9:0.3": "Paṭhamavagga",
  "thag4.9:0.4": "4.9. Candanattheragāthā",
  "thag4.9:1.1": "“Jātarūpena sañchannā,",
  "thag4.9:1.2": "dāsīgaṇapurakkhatā;",
  "thag4.9:1.3": "Aṅkena puttamādāya,",
  "thag4.9:1.4": "bhariyā maṃ upāgami.",
  "thag4.9:2.1": "Tañca disvāna āyantiṃ,",
  "thag4.9:2.2": "sakaputtassa mātaraṃ;",
  "thag4.9:2.3": "Alaṅkataṃ suvasanaṃ,",
  "thag4.9:2.4": "maccupāsaṃva oḍḍitaṃ.",
  "thag4.9:3.1": "Tato me manasīkāro,",
  "thag4.9:3.2": "yoniso udapajjatha;",
  "thag4.9:3.3": "Ādīnavo pāturahu,",
  "thag4.9:3.4": "nibbidā samatiṭṭhatha.",
  "thag4.9:4.1": "Tato cittaṃ vimucci me,",
  "thag4.9:4.2": "passa dhammasudhammataṃ;",
  "thag4.9:4.3": "Tisso vijjā anuppattā,",
  "thag4.9:4.4": "kataṃ buddhassa sāsanan”ti.",
  "thag4.9:4.5": "…",
  "thag4.9:4.6": "Candano thero …."
}