{
  "thag1.48:0.1": "Theragāthā",
  "thag1.48:0.2": "Ekakanipāta",
  "thag1.48:0.3": "Pañcamavagga",
  "thag1.48:0.4": "1.48. Sañjayattheragāthā",
  "thag1.48:1.1": "“Yato ahaṃ pabbajito,",
  "thag1.48:1.2": "Agārasmānagāriyaṃ;",
  "thag1.48:1.3": "Nābhijānāmi saṅkappaṃ,",
  "thag1.48:1.4": "Anariyaṃ dosasaṃhitan”ti.",
  "thag1.48:1.5": "…",
  "thag1.48:1.6": "Sañjayo thero …."
}