{
  "thag1.63:0.1": "Theragāthā",
  "thag1.63:0.2": "Ekakanipāta",
  "thag1.63:0.3": "Sattamavagga",
  "thag1.63:0.4": "1.63. Pakkhattheragāthā",
  "thag1.63:1.1": "“Cutā patanti patitā,",
  "thag1.63:1.2": "giddhā ca punarāgatā;",
  "thag1.63:1.3": "Kataṃ kiccaṃ rataṃ rammaṃ,",
  "thag1.63:1.4": "sukhenanvāgataṃ sukhan”ti.",
  "thag1.63:1.5": "…",
  "thag1.63:1.6": "Pakkho thero …."
}