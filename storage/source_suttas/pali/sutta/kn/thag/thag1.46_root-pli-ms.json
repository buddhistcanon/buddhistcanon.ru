{
  "thag1.46:0.1": "Theragāthā",
  "thag1.46:0.2": "Ekakanipāta",
  "thag1.46:0.3": "Pañcamavagga",
  "thag1.46:0.4": "1.46. Samiddhittheragāthā",
  "thag1.46:1.1": "“Saddhāyāhaṃ pabbajito,",
  "thag1.46:1.2": "Agārasmānagāriyaṃ;",
  "thag1.46:1.3": "Sati paññā ca me vuḍḍhā,",
  "thag1.46:1.4": "Cittañca susamāhitaṃ;",
  "thag1.46:1.5": "Kāmaṃ karassu rūpāni,",
  "thag1.46:1.6": "Neva maṃ byādhayissasī”ti.",
  "thag1.46:1.7": "…",
  "thag1.46:1.8": "Samiddhitthero …."
}