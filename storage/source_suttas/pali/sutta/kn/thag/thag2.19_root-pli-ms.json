{
  "thag2.19:0.1": "Theragāthā",
  "thag2.19:0.2": "Dukanipāta",
  "thag2.19:0.3": "Dutiyavagga",
  "thag2.19:0.4": "2.19. Nandattheragāthā",
  "thag2.19:1.1": "“Ayoniso manasikārā,",
  "thag2.19:1.2": "maṇḍanaṃ anuyuñjisaṃ;",
  "thag2.19:1.3": "Uddhato capalo cāsiṃ,",
  "thag2.19:1.4": "kāmarāgena aṭṭito.",
  "thag2.19:2.1": "Upāyakusalenāhaṃ,",
  "thag2.19:2.2": "buddhenādiccabandhunā;",
  "thag2.19:2.3": "Yoniso paṭipajjitvā,",
  "thag2.19:2.4": "bhave cittaṃ udabbahin”ti.",
  "thag2.19:2.5": "…",
  "thag2.19:2.6": "Nando thero …."
}