{
  "thag1.107:0.1": "Theragāthā",
  "thag1.107:0.2": "Ekakanipāta",
  "thag1.107:0.3": "Ekādasamavagga",
  "thag1.107:0.4": "1.107. Dhammasavattheragāthā",
  "thag1.107:1.1": "“Pabbajiṃ tulayitvāna,",
  "thag1.107:1.2": "agārasmānagāriyaṃ;",
  "thag1.107:1.3": "Tisso vijjā anuppattā,",
  "thag1.107:1.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.107:1.5": "…",
  "thag1.107:1.6": "Dhammasavo thero …."
}