{
  "thag1.57:0.1": "Theragāthā",
  "thag1.57:0.2": "Ekakanipāta",
  "thag1.57:0.3": "Chaṭṭhavagga",
  "thag1.57:0.4": "1.57. Dutiyakuṭivihārittheragāthā",
  "thag1.57:1.1": "“Ayamāhu purāṇiyā kuṭi,",
  "thag1.57:1.2": "Aññaṃ patthayase navaṃ kuṭiṃ;",
  "thag1.57:1.3": "Āsaṃ kuṭiyā virājaya,",
  "thag1.57:1.4": "Dukkhā bhikkhu puna navā kuṭī”ti.",
  "thag1.57:1.5": "…",
  "thag1.57:1.6": "Dutiyakuṭivihāritthero …."
}