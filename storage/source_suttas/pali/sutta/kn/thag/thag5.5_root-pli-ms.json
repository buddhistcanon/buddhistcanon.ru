{
  "thag5.5:0.1": "Theragāthā",
  "thag5.5:0.2": "Pañcakanipāta",
  "thag5.5:0.3": "Paṭhamavagga",
  "thag5.5:0.4": "5.5. Vaḍḍhattheragāthā",
  "thag5.5:1.1": "“Sādhū hi kira me mātā,",
  "thag5.5:1.2": "Patodaṃ upadaṃsayi;",
  "thag5.5:1.3": "Yassāhaṃ vacanaṃ sutvā,",
  "thag5.5:1.4": "Anusiṭṭho janettiyā;",
  "thag5.5:1.5": "Āraddhavīriyo pahitatto,",
  "thag5.5:1.6": "Patto sambodhimuttamaṃ.",
  "thag5.5:2.1": "Arahā dakkhiṇeyyomhi,",
  "thag5.5:2.2": "tevijjo amataddaso;",
  "thag5.5:2.3": "Jetvā namucino senaṃ,",
  "thag5.5:2.4": "viharāmi anāsavo.",
  "thag5.5:3.1": "Ajjhattañca bahiddhā ca,",
  "thag5.5:3.2": "ye me vijjiṃsu āsavā;",
  "thag5.5:3.3": "Sabbe asesā ucchinnā,",
  "thag5.5:3.4": "na ca uppajjare puna.",
  "thag5.5:4.1": "Visāradā kho bhaginī,",
  "thag5.5:4.2": "etamatthaṃ abhāsayi;",
  "thag5.5:4.3": "‘Apihā nūna mayipi,",
  "thag5.5:4.4": "vanatho te na vijjati’.",
  "thag5.5:5.1": "Pariyantakataṃ dukkhaṃ,",
  "thag5.5:5.2": "antimoyaṃ samussayo;",
  "thag5.5:5.3": "Jātimaraṇasaṃsāro,",
  "thag5.5:5.4": "natthi dāni punabbhavo”ti.",
  "thag5.5:5.5": "…",
  "thag5.5:5.6": "Vaḍḍho thero …."
}