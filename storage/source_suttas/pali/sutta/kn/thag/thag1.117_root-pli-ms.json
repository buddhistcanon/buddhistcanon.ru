{
  "thag1.117:0.1": "Theragāthā",
  "thag1.117:0.2": "Ekakanipāta",
  "thag1.117:0.3": "Dvādasamavagga",
  "thag1.117:0.4": "1.117. Yasattheragāthā",
  "thag1.117:1.1": "“Suvilitto suvasano,",
  "thag1.117:1.2": "sabbābharaṇabhūsito;",
  "thag1.117:1.3": "Tisso vijjā ajjhagamiṃ,",
  "thag1.117:1.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.117:1.5": "…",
  "thag1.117:1.6": "Yaso thero …."
}