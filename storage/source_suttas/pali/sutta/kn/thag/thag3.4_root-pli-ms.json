{
  "thag3.4:0.1": "Theragāthā",
  "thag3.4:0.2": "Tikanipāta",
  "thag3.4:0.3": "Paṭhamavagga",
  "thag3.4:0.4": "3.4. Dhaniyattheragāthā",
  "thag3.4:1.1": "“Sukhañce jīvituṃ icche,",
  "thag3.4:1.2": "sāmaññasmiṃ apekkhavā;",
  "thag3.4:1.3": "Saṃghikaṃ nātimaññeyya,",
  "thag3.4:1.4": "cīvaraṃ pānabhojanaṃ.",
  "thag3.4:2.1": "Sukhañce jīvituṃ icche,",
  "thag3.4:2.2": "sāmaññasmiṃ apekkhavā;",
  "thag3.4:2.3": "Ahi mūsikasobbhaṃva,",
  "thag3.4:2.4": "sevetha sayanāsanaṃ.",
  "thag3.4:3.1": "Sukhañce jīvituṃ icche,",
  "thag3.4:3.2": "sāmaññasmiṃ apekkhavā;",
  "thag3.4:3.3": "Itarītarena tusseyya,",
  "thag3.4:3.4": "ekadhammañca bhāvaye”ti.",
  "thag3.4:3.5": "…",
  "thag3.4:3.6": "Dhaniyo thero …."
}