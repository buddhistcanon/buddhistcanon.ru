{
  "thag1.109:0.1": "Theragāthā",
  "thag1.109:0.2": "Ekakanipāta",
  "thag1.109:0.3": "Ekādasamavagga",
  "thag1.109:0.4": "1.109. Saṃgharakkhitattheragāthā",
  "thag1.109:1.1": "“Na nūnāyaṃ paramahitānukampino,",
  "thag1.109:1.2": "Rahogato anuvigaṇeti sāsanaṃ;",
  "thag1.109:1.3": "Tathāhayaṃ viharati pākatindriyo,",
  "thag1.109:1.4": "Migī yathā taruṇajātikā vane”ti.",
  "thag1.109:1.5": "…",
  "thag1.109:1.6": "Saṃgharakkhito thero …."
}