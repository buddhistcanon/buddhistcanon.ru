{
  "thag1.35:0.1": "Theragāthā",
  "thag1.35:0.2": "Ekakanipāta",
  "thag1.35:0.3": "Catutthavagga",
  "thag1.35:0.4": "1.35. Sāmaññakānittheragāthā",
  "thag1.35:1.1": "“Sukhaṃ sukhattho labhate tadācaraṃ,",
  "thag1.35:1.2": "Kittiñca pappoti yasassa vaḍḍhati;",
  "thag1.35:1.3": "Yo ariyamaṭṭhaṅgikamañjasaṃ ujuṃ,",
  "thag1.35:1.4": "Bhāveti maggaṃ amatassa pattiyā”ti.",
  "thag1.35:1.5": "…",
  "thag1.35:1.6": "Sāmaññakānitthero …."
}