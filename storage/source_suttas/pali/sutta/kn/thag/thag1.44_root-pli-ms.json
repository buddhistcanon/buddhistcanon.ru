{
  "thag1.44:0.1": "Theragāthā",
  "thag1.44:0.2": "Ekakanipāta",
  "thag1.44:0.3": "Pañcamavagga",
  "thag1.44:0.4": "1.44. Sānuttheragāthā",
  "thag1.44:1.1": "“Mataṃ vā amma rodanti,",
  "thag1.44:1.2": "Yo vā jīvaṃ na dissati;",
  "thag1.44:1.3": "Jīvantaṃ maṃ amma passantī,",
  "thag1.44:1.4": "Kasmā maṃ amma rodasī”ti.",
  "thag1.44:1.5": "…",
  "thag1.44:1.6": "Sānutthero …."
}