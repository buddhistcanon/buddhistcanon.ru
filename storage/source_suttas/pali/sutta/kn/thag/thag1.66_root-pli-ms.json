{
  "thag1.66:0.1": "Theragāthā",
  "thag1.66:0.2": "Ekakanipāta",
  "thag1.66:0.3": "Sattamavagga",
  "thag1.66:0.4": "1.66. Meghiyattheragāthā",
  "thag1.66:1.1": "“Anusāsi mahāvīro,",
  "thag1.66:1.2": "sabbadhammāna pāragū;",
  "thag1.66:1.3": "Tassāhaṃ dhammaṃ sutvāna,",
  "thag1.66:1.4": "vihāsiṃ santike sato;",
  "thag1.66:1.5": "Tisso vijjā anuppattā,",
  "thag1.66:1.6": "kataṃ buddhassa sāsanan”ti.",
  "thag1.66:1.7": "…",
  "thag1.66:1.8": "Meghiyo thero …."
}