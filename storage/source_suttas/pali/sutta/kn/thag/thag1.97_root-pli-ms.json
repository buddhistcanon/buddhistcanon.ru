{
  "thag1.97:0.1": "Theragāthā",
  "thag1.97:0.2": "Ekakanipāta",
  "thag1.97:0.3": "Dasamavagga",
  "thag1.97:0.4": "1.97. Tissattheragāthā",
  "thag1.97:1.1": "“Hitvā satapalaṃ kaṃsaṃ,",
  "thag1.97:1.2": "Sovaṇṇaṃ satarājikaṃ;",
  "thag1.97:1.3": "Aggahiṃ mattikāpattaṃ,",
  "thag1.97:1.4": "Idaṃ dutiyābhisecanan”ti.",
  "thag1.97:1.5": "…",
  "thag1.97:1.6": "Tisso thero …."
}