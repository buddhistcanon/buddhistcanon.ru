{
  "thag2.46:0.1": "Theragāthā",
  "thag2.46:0.2": "Dukanipāta",
  "thag2.46:0.3": "Pañcamavagga",
  "thag2.46:0.4": "2.46. Cūḷakattheragāthā",
  "thag2.46:1.1": "“Nadanti morā susikhā supekhuṇā,",
  "thag2.46:1.2": "Sunīlagīvā sumukhā sugajjino;",
  "thag2.46:1.3": "Susaddalā cāpi mahāmahī ayaṃ,",
  "thag2.46:1.4": "Subyāpitambu suvalāhakaṃ nabhaṃ.",
  "thag2.46:2.1": "Sukallarūpo sumanassa jhāyataṃ,",
  "thag2.46:2.2": "Sunikkamo sādhu subuddhasāsane;",
  "thag2.46:2.3": "Susukkasukkaṃ nipuṇaṃ sududdasaṃ,",
  "thag2.46:2.4": "Phusāhi taṃ uttamamaccutaṃ padan”ti.",
  "thag2.46:2.5": "…",
  "thag2.46:2.6": "Cūḷako thero …."
}