{
  "thag1.32:0.1": "Theragāthā",
  "thag1.32:0.2": "Ekakanipāta",
  "thag1.32:0.3": "Catutthavagga",
  "thag1.32:0.4": "1.32. Suppiyattheragāthā",
  "thag1.32:1.1": "“Ajaraṃ jīramānena,",
  "thag1.32:1.2": "tappamānena nibbutiṃ;",
  "thag1.32:1.3": "Nimiyaṃ paramaṃ santiṃ,",
  "thag1.32:1.4": "yogakkhemaṃ anuttaran”ti.",
  "thag1.32:1.5": "…",
  "thag1.32:1.6": "Suppiyo thero …."
}