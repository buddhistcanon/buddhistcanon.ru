{
  "thag1.80:0.1": "Theragāthā",
  "thag1.80:0.2": "Ekakanipāta",
  "thag1.80:0.3": "Aṭṭhamavagga",
  "thag1.80:0.4": "1.80. Uggattheragāthā",
  "thag1.80:1.1": "“Yaṃ mayā pakataṃ kammaṃ,",
  "thag1.80:1.2": "appaṃ vā yadi vā bahuṃ;",
  "thag1.80:1.3": "Sabbametaṃ parikkhīṇaṃ,",
  "thag1.80:1.4": "natthi dāni punabbhavo”ti.",
  "thag1.80:1.5": "…",
  "thag1.80:1.6": "Uggo thero ….",
  "thag1.80:1.7": "Vaggo aṭṭhamo.",
  "thag1.80:2.0": "Tassuddānaṃ",
  "thag1.80:2.1": "Vacchapālo ca yo thero,",
  "thag1.80:2.2": "ātumo māṇavo isi;",
  "thag1.80:2.3": "Suyāmano susārado,",
  "thag1.80:2.4": "thero yo ca piyañjaho;",
  "thag1.80:2.5": "Ārohaputto meṇḍasiro,",
  "thag1.80:2.6": "rakkhito uggasavhayoti."
}