{
  "thag2.31:0.1": "Theragāthā",
  "thag2.31:0.2": "Dukanipāta",
  "thag2.31:0.3": "Catutthavagga",
  "thag2.31:0.4": "2.31. Migasirattheragāthā",
  "thag2.31:1.1": "“Yato ahaṃ pabbajito,",
  "thag2.31:1.2": "sammāsambuddhasāsane;",
  "thag2.31:1.3": "Vimuccamāno uggacchiṃ,",
  "thag2.31:1.4": "kāmadhātuṃ upaccagaṃ.",
  "thag2.31:2.1": "Brahmuno pekkhamānassa,",
  "thag2.31:2.2": "tato cittaṃ vimucci me;",
  "thag2.31:2.3": "Akuppā me vimuttīti,",
  "thag2.31:2.4": "sabbasaṃyojanakkhayā”ti.",
  "thag2.31:2.5": "…",
  "thag2.31:2.6": "Migasiro thero …."
}