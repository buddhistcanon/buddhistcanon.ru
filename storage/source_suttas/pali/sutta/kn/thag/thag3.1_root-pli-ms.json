{
  "thag3.1:0.1": "Theragāthā",
  "thag3.1:0.2": "Tikanipāta",
  "thag3.1:0.3": "Paṭhamavagga",
  "thag3.1:0.4": "3.1 Aṅgaṇikabhāradvājattheragāthā",
  "thag3.1:1.1": "“Ayoni suddhimanvesaṃ,",
  "thag3.1:1.2": "aggiṃ paricariṃ vane;",
  "thag3.1:1.3": "Suddhimaggaṃ ajānanto,",
  "thag3.1:1.4": "akāsiṃ amaraṃ tapaṃ.",
  "thag3.1:2.1": "Taṃ sukhena sukhaṃ laddhaṃ,",
  "thag3.1:2.2": "passa dhammasudhammataṃ;",
  "thag3.1:2.3": "Tisso vijjā anuppattā,",
  "thag3.1:2.4": "kataṃ buddhassa sāsanaṃ.",
  "thag3.1:3.1": "Brahmabandhu pure āsiṃ,",
  "thag3.1:3.2": "idāni khomhi brāhmaṇo;",
  "thag3.1:3.3": "Tevijjo nhātako camhi,",
  "thag3.1:3.4": "sottiyo camhi vedagū”ti.",
  "thag3.1:3.5": "…",
  "thag3.1:3.6": "Aṅgaṇikabhāradvājo thero …."
}