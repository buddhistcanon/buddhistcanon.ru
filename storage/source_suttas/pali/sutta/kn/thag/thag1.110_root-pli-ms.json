{
  "thag1.110:0.1": "Theragāthā",
  "thag1.110:0.2": "Ekakanipāta",
  "thag1.110:0.3": "Ekādasamavagga",
  "thag1.110:0.4": "1.110. Usabhattheragāthā",
  "thag1.110:1.1": "“Nagā nagaggesu susaṃvirūḷhā,",
  "thag1.110:1.2": "Udaggameghena navena sittā;",
  "thag1.110:1.3": "Vivekakāmassa araññasaññino,",
  "thag1.110:1.4": "Janeti bhiyyo usabhassa kalyatan”ti.",
  "thag1.110:1.5": "…",
  "thag1.110:1.6": "Usabho thero ….",
  "thag1.110:1.7": "Vaggo ekādasamo.",
  "thag1.110:2.0": "Tassuddānaṃ",
  "thag1.110:2.1": "Belaṭṭhāniko setuccho,",
  "thag1.110:2.2": "Bandhuro khitako isi;",
  "thag1.110:2.3": "Malitavambho suhemanto,",
  "thag1.110:2.4": "Dhammasavo dhammasavapitā;",
  "thag1.110:2.5": "Saṃgharakkhitatthero ca,",
  "thag1.110:2.6": "Usabho ca mahāmunīti."
}