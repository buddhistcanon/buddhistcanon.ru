{
  "thag1.23:0.1": "Theragāthā",
  "thag1.23:0.2": "Ekakanipāta",
  "thag1.23:0.3": "Tatiyavagga",
  "thag1.23:0.4": "1.23. Gosālattheragāthā",
  "thag1.23:1.1": "“Ahaṃ kho veḷugumbasmiṃ,",
  "thag1.23:1.2": "bhutvāna madhupāyasaṃ;",
  "thag1.23:1.3": "Padakkhiṇaṃ sammasanto,",
  "thag1.23:1.4": "khandhānaṃ udayabbayaṃ;",
  "thag1.23:1.5": "Sānuṃ paṭigamissāmi,",
  "thag1.23:1.6": "vivekamanubrūhayan”ti.",
  "thag1.23:1.7": "…",
  "thag1.23:1.8": "Gosālo thero …."
}