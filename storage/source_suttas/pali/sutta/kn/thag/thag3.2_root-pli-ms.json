{
  "thag3.2:0.1": "Theragāthā",
  "thag3.2:0.2": "Tikanipāta",
  "thag3.2:0.3": "Paṭhamavagga",
  "thag3.2:0.4": "3.2. Paccayattheragāthā",
  "thag3.2:1.1": "“Pañcāhāhaṃ pabbajito,",
  "thag3.2:1.2": "sekho appattamānaso;",
  "thag3.2:1.3": "Vihāraṃ me paviṭṭhassa,",
  "thag3.2:1.4": "cetaso paṇidhī ahu.",
  "thag3.2:2.1": "Nāsissaṃ na pivissāmi,",
  "thag3.2:2.2": "vihārato na nikkhame;",
  "thag3.2:2.3": "Napi passaṃ nipātessaṃ,",
  "thag3.2:2.4": "taṇhāsalle anūhate.",
  "thag3.2:3.1": "Tassa mevaṃ viharato,",
  "thag3.2:3.2": "passa vīriyaparakkamaṃ;",
  "thag3.2:3.3": "Tisso vijjā anuppattā,",
  "thag3.2:3.4": "kataṃ buddhassa sāsanan”ti.",
  "thag3.2:3.5": "…",
  "thag3.2:3.6": "Paccayo thero …."
}