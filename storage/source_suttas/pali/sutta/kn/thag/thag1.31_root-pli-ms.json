{
  "thag1.31:0.1": "Theragāthā",
  "thag1.31:0.2": "Ekakanipāta",
  "thag1.31:0.3": "Catutthavagga",
  "thag1.31:0.4": "1.31 Gahvaratīriyattheragāthā",
  "thag1.31:1.1": "“Phuṭṭho ḍaṃsehi makasehi,",
  "thag1.31:1.2": "araññasmiṃ brahāvane;",
  "thag1.31:1.3": "Nāgo saṃgāmasīseva,",
  "thag1.31:1.4": "sato tatrādhivāsaye”ti.",
  "thag1.31:1.5": "…",
  "thag1.31:1.6": "Gahvaratīriyo thero …."
}