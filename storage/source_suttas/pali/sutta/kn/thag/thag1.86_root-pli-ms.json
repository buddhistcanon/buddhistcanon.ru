{
  "thag1.86:0.1": "Theragāthā",
  "thag1.86:0.2": "Ekakanipāta",
  "thag1.86:0.3": "Navamavagga",
  "thag1.86:0.4": "1.86. Nāgitattheragāthā",
  "thag1.86:1.1": "“Ito bahiddhā puthu aññavādinaṃ,",
  "thag1.86:1.2": "Maggo na nibbānagamo yathā ayaṃ;",
  "thag1.86:1.3": "Itissu saṃghaṃ bhagavānusāsati,",
  "thag1.86:1.4": "Satthā sayaṃ pāṇitaleva dassayan”ti.",
  "thag1.86:1.5": "…",
  "thag1.86:1.6": "Nāgito thero …."
}