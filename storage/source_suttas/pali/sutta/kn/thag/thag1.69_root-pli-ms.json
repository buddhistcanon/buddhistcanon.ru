{
  "thag1.69:0.1": "Theragāthā",
  "thag1.69:0.2": "Ekakanipāta",
  "thag1.69:0.3": "Sattamavagga",
  "thag1.69:0.4": "1.69. Channattheragāthā",
  "thag1.69:1.1": "“Sutvāna dhammaṃ mahato mahārasaṃ,",
  "thag1.69:1.2": "Sabbaññutaññāṇavarena desitaṃ;",
  "thag1.69:1.3": "Maggaṃ papajjiṃ amatassa pattiyā,",
  "thag1.69:1.4": "So yogakkhemassa pathassa kovido”ti.",
  "thag1.69:1.5": "…",
  "thag1.69:1.6": "Channo thero …."
}