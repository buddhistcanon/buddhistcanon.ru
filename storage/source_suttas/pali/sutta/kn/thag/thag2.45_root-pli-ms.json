{
  "thag2.45:0.1": "Theragāthā",
  "thag2.45:0.2": "Dukanipāta",
  "thag2.45:0.3": "Pañcamavagga",
  "thag2.45:0.4": "2.45. Visākhapañcālaputtattheragāthā",
  "thag2.45:1.1": "“Na ukkhipe no ca parikkhipe pare,",
  "thag2.45:1.2": "Na okkhipe pāragataṃ na eraye;",
  "thag2.45:1.3": "Na cattavaṇṇaṃ parisāsu byāhare,",
  "thag2.45:1.4": "Anuddhato sammitabhāṇi subbato.",
  "thag2.45:2.1": "Susukhumanipuṇatthadassinā,",
  "thag2.45:2.2": "Matikusalena nivātavuttinā;",
  "thag2.45:2.3": "Saṃsevitavuddhasīlinā,",
  "thag2.45:2.4": "Nibbānaṃ na hi tena dullabhan”ti.",
  "thag2.45:2.5": "…",
  "thag2.45:2.6": "Visākho pañcālaputto thero …."
}