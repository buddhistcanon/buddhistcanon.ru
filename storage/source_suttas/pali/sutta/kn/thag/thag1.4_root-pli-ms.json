{
  "thag1.4:0.1": "Theragāthā",
  "thag1.4:0.2": "Ekakanipāta",
  "thag1.4:0.3": "Paṭhamavagga",
  "thag1.4:0.4": "1.4. Puṇṇattheragāthā",
  "thag1.4:1.1": "“Sabbhireva samāsetha,",
  "thag1.4:1.2": "paṇḍitehatthadassibhi;",
  "thag1.4:1.3": "Atthaṃ mahantaṃ gambhīraṃ,",
  "thag1.4:1.4": "duddasaṃ nipuṇaṃ aṇuṃ;",
  "thag1.4:1.5": "Dhīrā samadhigacchanti,",
  "thag1.4:1.6": "appamattā vicakkhaṇā”ti.",
  "thag1.4:1.7": "Itthaṃ sudaṃ āyasmā puṇṇo mantāṇiputto thero gāthaṃ abhāsitthāti."
}