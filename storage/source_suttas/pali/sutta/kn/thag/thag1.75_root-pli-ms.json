{
  "thag1.75:0.1": "Theragāthā",
  "thag1.75:0.2": "Ekakanipāta",
  "thag1.75:0.3": "Aṭṭhamavagga",
  "thag1.75:0.4": "1.75. Susāradattheragāthā",
  "thag1.75:1.1": "“Sādhu suvihitāna dassanaṃ,",
  "thag1.75:1.2": "Kaṅkhā chijjati buddhi vaḍḍhati;",
  "thag1.75:1.3": "Bālampi karonti paṇḍitaṃ,",
  "thag1.75:1.4": "Tasmā sādhu sataṃ samāgamo”ti.",
  "thag1.75:1.5": "…",
  "thag1.75:1.6": "Susārado thero …."
}