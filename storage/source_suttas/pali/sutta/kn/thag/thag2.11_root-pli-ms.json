{
  "thag2.11:0.1": "Theragāthā",
  "thag2.11:0.2": "Dukanipāta",
  "thag2.11:0.3": "Dutiyavagga",
  "thag2.11:0.4": "2.11. Mahācundattheragāthā",
  "thag2.11:1.1": "“Sussūsā sutavaddhanī,",
  "thag2.11:1.2": "sutaṃ paññāya vaddhanaṃ;",
  "thag2.11:1.3": "Paññāya atthaṃ jānāti,",
  "thag2.11:1.4": "ñāto attho sukhāvaho.",
  "thag2.11:2.1": "Sevetha pantāni senāsanāni,",
  "thag2.11:2.2": "Careyya saṃyojanavippamokkhaṃ;",
  "thag2.11:2.3": "Sace ratiṃ nādhigaccheyya tattha,",
  "thag2.11:2.4": "Saṃghe vase rakkhitatto satimā”ti.",
  "thag2.11:2.5": "…",
  "thag2.11:2.6": "Mahācundo thero …."
}