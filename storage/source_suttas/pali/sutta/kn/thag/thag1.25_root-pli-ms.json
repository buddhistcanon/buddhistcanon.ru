{
  "thag1.25:0.1": "Theragāthā",
  "thag1.25:0.2": "Ekakanipāta",
  "thag1.25:0.3": "Tatiyavagga",
  "thag1.25:0.4": "1.25. Nandiyattheragāthā",
  "thag1.25:1.1": "“Obhāsajātaṃ phalagaṃ,",
  "thag1.25:1.2": "cittaṃ yassa abhiṇhaso;",
  "thag1.25:1.3": "Tādisaṃ bhikkhumāsajja,",
  "thag1.25:1.4": "kaṇha dukkhaṃ nigacchasī”ti.",
  "thag1.25:1.5": "…",
  "thag1.25:1.6": "Nandiyo thero …."
}