{
  "thag4.1:0.1": "Theragāthā",
  "thag4.1:0.2": "Catukkanipāta",
  "thag4.1:0.3": "Paṭhamavagga",
  "thag4.1:0.4": "4.1. Nāgasamālattheragāthā",
  "thag4.1:1.1": "“Alaṅkatā suvasanā,",
  "thag4.1:1.2": "mālinī candanussadā;",
  "thag4.1:1.3": "Majjhe mahāpathe nārī,",
  "thag4.1:1.4": "tūriye naccati naṭṭakī.",
  "thag4.1:2.1": "Piṇḍikāya paviṭṭhohaṃ,",
  "thag4.1:2.2": "gacchanto naṃ udikkhisaṃ;",
  "thag4.1:2.3": "Alaṅkataṃ suvasanaṃ,",
  "thag4.1:2.4": "maccupāsaṃva oḍḍitaṃ.",
  "thag4.1:3.1": "Tato me manasīkāro,",
  "thag4.1:3.2": "yoniso udapajjatha;",
  "thag4.1:3.3": "Ādīnavo pāturahu,",
  "thag4.1:3.4": "nibbidā samatiṭṭhatha.",
  "thag4.1:4.1": "Tato cittaṃ vimucci me,",
  "thag4.1:4.2": "passa dhammasudhammataṃ;",
  "thag4.1:4.3": "Tisso vijjā anuppattā,",
  "thag4.1:4.4": "kataṃ buddhassa sāsanan”ti.",
  "thag4.1:4.5": "…",
  "thag4.1:4.6": "Nāgasamālo thero …."
}