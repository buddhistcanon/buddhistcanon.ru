{
  "thag1.81:0.1": "Theragāthā",
  "thag1.81:0.2": "Ekakanipāta",
  "thag1.81:0.3": "Navamavagga",
  "thag1.81:0.4": "1.81. Samitiguttattheragāthā",
  "thag1.81:1.1": "“Yaṃ mayā pakataṃ pāpaṃ,",
  "thag1.81:1.2": "pubbe aññāsu jātisu;",
  "thag1.81:1.3": "Idheva taṃ vedanīyaṃ,",
  "thag1.81:1.4": "vatthu aññaṃ na vijjatī”ti.",
  "thag1.81:1.5": "…",
  "thag1.81:1.6": "Samitigutto thero …."
}