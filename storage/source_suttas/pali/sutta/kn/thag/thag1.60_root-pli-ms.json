{
  "thag1.60:0.1": "Theragāthā",
  "thag1.60:0.2": "Ekakanipāta",
  "thag1.60:0.3": "Chaṭṭhavagga",
  "thag1.60:0.4": "1.60. Sīvalittheragāthā",
  "thag1.60:1.1": "“Te me ijjhiṃsu saṅkappā,",
  "thag1.60:1.2": "yadattho pāvisiṃ kuṭiṃ;",
  "thag1.60:1.3": "Vijjāvimuttiṃ paccesaṃ,",
  "thag1.60:1.4": "mānānusayamujjahan”ti.",
  "thag1.60:1.5": "…",
  "thag1.60:1.6": "Sīvalitthero ….",
  "thag1.60:1.7": "Vaggo chaṭṭho.",
  "thag1.60:2.0": "Tassuddānaṃ",
  "thag1.60:2.1": "Godhiko ca subāhu ca,",
  "thag1.60:2.2": "valliyo uttiyo isi;",
  "thag1.60:2.3": "Añjanavaniyo thero,",
  "thag1.60:2.4": "duve kuṭivihārino;",
  "thag1.60:2.5": "Ramaṇīyakuṭiko ca,",
  "thag1.60:2.6": "kosalavhayasīvalīti."
}