{
  "thag2.16:0.1": "Theragāthā",
  "thag2.16:0.2": "Dukanipāta",
  "thag2.16:0.3": "Dutiyavagga",
  "thag2.16:0.4": "2.16. Mahākāḷattheragāthā",
  "thag2.16:1.1": "“Kāḷī itthī brahatī dhaṅkarūpā,",
  "thag2.16:1.2": "Satthiñca bhetvā aparañca satthiṃ;",
  "thag2.16:1.3": "Bāhañca bhetvā aparañca bāhaṃ,",
  "thag2.16:1.4": "Sīsañca bhetvā dadhithālakaṃva;",
  "thag2.16:1.5": "Esā nisinnā abhisandahitvā.",
  "thag2.16:2.1": "Yo ve avidvā upadhiṃ karoti,",
  "thag2.16:2.2": "Punappunaṃ dukkhamupeti mando;",
  "thag2.16:2.3": "Tasmā pajānaṃ upadhiṃ na kayirā,",
  "thag2.16:2.4": "Māhaṃ puna bhinnasiro sayissan”ti.",
  "thag2.16:2.5": "…",
  "thag2.16:2.6": "Mahākāḷo thero …."
}