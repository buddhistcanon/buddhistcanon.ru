{
  "thag1.71:0.1": "Theragāthā",
  "thag1.71:0.2": "Ekakanipāta",
  "thag1.71:0.3": "Aṭṭhamavagga",
  "thag1.71:0.4": "1.71. Vacchapālattheragāthā",
  "thag1.71:1.1": "“Susukhumanipuṇatthadassinā,",
  "thag1.71:1.2": "Matikusalena nivātavuttinā;",
  "thag1.71:1.3": "Saṃsevitavuddhasīlinā,",
  "thag1.71:1.4": "Nibbānaṃ na hi tena dullabhan”ti.",
  "thag1.71:1.5": "…",
  "thag1.71:1.6": "Vacchapālo thero …."
}