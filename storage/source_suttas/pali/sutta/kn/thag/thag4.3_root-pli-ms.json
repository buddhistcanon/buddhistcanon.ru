{
  "thag4.3:0.1": "Theragāthā",
  "thag4.3:0.2": "Catukkanipāta",
  "thag4.3:0.3": "Paṭhamavagga",
  "thag4.3:0.4": "4.3. Sabhiyattheragāthā",
  "thag4.3:1.1": "“Pare ca na vijānanti,",
  "thag4.3:1.2": "mayamettha yamāmase;",
  "thag4.3:1.3": "Ye ca tattha vijānanti,",
  "thag4.3:1.4": "tato sammanti medhagā.",
  "thag4.3:2.1": "Yadā ca avijānantā,",
  "thag4.3:2.2": "iriyantyamarā viya;",
  "thag4.3:2.3": "Vijānanti ca ye dhammaṃ,",
  "thag4.3:2.4": "āturesu anāturā.",
  "thag4.3:3.1": "Yaṃ kiñci sithilaṃ kammaṃ,",
  "thag4.3:3.2": "saṃkiliṭṭhañca yaṃ vataṃ;",
  "thag4.3:3.3": "Saṅkassaraṃ brahmacariyaṃ,",
  "thag4.3:3.4": "na taṃ hoti mahapphalaṃ.",
  "thag4.3:4.1": "Yassa sabrahmacārīsu,",
  "thag4.3:4.2": "gāravo nūpalabbhati;",
  "thag4.3:4.3": "Ārakā hoti saddhammā,",
  "thag4.3:4.4": "nabhaṃ puthaviyā yathā”ti.",
  "thag4.3:4.5": "…",
  "thag4.3:4.6": "Sabhiyo thero …."
}