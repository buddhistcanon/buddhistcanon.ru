{
  "thag1.108:0.1": "Theragāthā",
  "thag1.108:0.2": "Ekakanipāta",
  "thag1.108:0.3": "Ekādasamavagga",
  "thag1.108:0.4": "1.108. Dhammasavapituttheragāthā",
  "thag1.108:1.1": "“Sa vīsavassasatiko,",
  "thag1.108:1.2": "pabbajiṃ anagāriyaṃ;",
  "thag1.108:1.3": "Tisso vijjā anuppattā,",
  "thag1.108:1.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.108:1.5": "…",
  "thag1.108:1.6": "Dhammasavapitu thero …."
}