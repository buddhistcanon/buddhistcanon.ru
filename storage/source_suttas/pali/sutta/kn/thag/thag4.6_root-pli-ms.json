{
  "thag4.6:0.1": "Theragāthā",
  "thag4.6:0.2": "Catukkanipāta",
  "thag4.6:0.3": "Paṭhamavagga",
  "thag4.6:0.4": "4.6. Senakattheragāthā",
  "thag4.6:1.1": "“Svāgataṃ vata me āsi,",
  "thag4.6:1.2": "gayāyaṃ gayaphagguyā;",
  "thag4.6:1.3": "Yaṃ addasāsiṃ sambuddhaṃ,",
  "thag4.6:1.4": "desentaṃ dhammamuttamaṃ.",
  "thag4.6:2.1": "Mahappabhaṃ gaṇācariyaṃ,",
  "thag4.6:2.2": "aggapattaṃ vināyakaṃ;",
  "thag4.6:2.3": "Sadevakassa lokassa,",
  "thag4.6:2.4": "jinaṃ atuladassanaṃ.",
  "thag4.6:3.1": "Mahānāgaṃ mahāvīraṃ,",
  "thag4.6:3.2": "mahājutimanāsavaṃ;",
  "thag4.6:3.3": "Sabbāsavaparikkhīṇaṃ,",
  "thag4.6:3.4": "satthāramakutobhayaṃ.",
  "thag4.6:4.1": "Cirasaṃkiliṭṭhaṃ vata maṃ,",
  "thag4.6:4.2": "diṭṭhisandānabandhitaṃ;",
  "thag4.6:4.3": "Vimocayi so bhagavā,",
  "thag4.6:4.4": "sabbaganthehi senakan”ti.",
  "thag4.6:4.5": "…",
  "thag4.6:4.6": "Senako thero …."
}