{
  "thag1.65:0.1": "Theragāthā",
  "thag1.65:0.2": "Ekakanipāta",
  "thag1.65:0.3": "Sattamavagga",
  "thag1.65:0.4": "1.65 Ukkhepakatavacchattheragāthā",
  "thag1.65:1.1": "“Ukkhepakatavacchassa,",
  "thag1.65:1.2": "Saṅkalitaṃ bahūhi vassehi;",
  "thag1.65:1.3": "Taṃ bhāsati gahaṭṭhānaṃ,",
  "thag1.65:1.4": "Sunisinno uḷārapāmojjo”ti.",
  "thag1.65:1.5": "…",
  "thag1.65:1.6": "Ukkhepakatavaccho thero …."
}