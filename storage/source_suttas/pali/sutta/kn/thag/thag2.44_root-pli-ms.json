{
  "thag2.44:0.1": "Theragāthā",
  "thag2.44:0.2": "Dukanipāta",
  "thag2.44:0.3": "Pañcamavagga",
  "thag2.44:0.4": "2.44. Mogharājattheragāthā",
  "thag2.44:1.1": "“Chavipāpaka cittabhaddaka,",
  "thag2.44:1.2": "Mogharāja satataṃ samāhito;",
  "thag2.44:1.3": "Hemantikasītakālarattiyo,",
  "thag2.44:1.4": "Bhikkhu tvaṃsi kathaṃ karissasi”.",
  "thag2.44:2.1": "“Sampannasassā magadhā,",
  "thag2.44:2.2": "kevalā iti me sutaṃ;",
  "thag2.44:2.3": "Palālacchannako seyyaṃ,",
  "thag2.44:2.4": "yathaññe sukhajīvino”ti.",
  "thag2.44:2.5": "…",
  "thag2.44:2.6": "Mogharājā thero …."
}