{
  "thag1.112:0.1": "Theragāthā",
  "thag1.112:0.2": "Ekakanipāta",
  "thag1.112:0.3": "Dvādasamavagga",
  "thag1.112:0.4": "1.112. Vacchagottattheragāthā",
  "thag1.112:1.1": "“Tevijjohaṃ mahājhāyī,",
  "thag1.112:1.2": "cetosamathakovido;",
  "thag1.112:1.3": "Sadattho me anuppatto,",
  "thag1.112:1.4": "kataṃ buddhassa sāsanan”ti.",
  "thag1.112:1.5": "…",
  "thag1.112:1.6": "Vacchagotto thero …."
}