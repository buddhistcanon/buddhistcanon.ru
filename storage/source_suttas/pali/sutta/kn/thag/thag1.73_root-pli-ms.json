{
  "thag1.73:0.1": "Theragāthā",
  "thag1.73:0.2": "Ekakanipāta",
  "thag1.73:0.3": "Aṭṭhamavagga",
  "thag1.73:0.4": "1.73. Māṇavattheragāthā",
  "thag1.73:1.1": "“Jiṇṇañca disvā dukhitañca byādhitaṃ,",
  "thag1.73:1.2": "Matañca disvā gatamāyusaṅkhayaṃ;",
  "thag1.73:1.3": "Tato ahaṃ nikkhamitūna pabbajiṃ,",
  "thag1.73:1.4": "Pahāya kāmāni manoramānī”ti.",
  "thag1.73:1.5": "…",
  "thag1.73:1.6": "Māṇavo thero …."
}