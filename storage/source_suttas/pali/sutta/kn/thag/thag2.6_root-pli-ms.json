{
  "thag2.6:0.1": "Theragāthā",
  "thag2.6:0.2": "Dukanipāta",
  "thag2.6:0.3": "Paṭhamavagga",
  "thag2.6:0.4": "2.6 Meḷajinattheragāthā",
  "thag2.6:1.1": "“Yadāhaṃ dhammamassosiṃ,",
  "thag2.6:1.2": "bhāsamānassa satthuno;",
  "thag2.6:1.3": "Na kaṅkhamabhijānāmi,",
  "thag2.6:1.4": "sabbaññūaparājite.",
  "thag2.6:2.1": "Satthavāhe mahāvīre,",
  "thag2.6:2.2": "sārathīnaṃ varuttame;",
  "thag2.6:2.3": "Magge paṭipadāyaṃ vā,",
  "thag2.6:2.4": "kaṅkhā mayhaṃ na vijjatī”ti.",
  "thag2.6:2.5": "…",
  "thag2.6:2.6": "Meḷajino thero …."
}