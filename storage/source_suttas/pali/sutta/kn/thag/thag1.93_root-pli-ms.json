{
  "thag1.93:0.1": "Theragāthā",
  "thag1.93:0.2": "Ekakanipāta",
  "thag1.93:0.3": "Dasamavagga",
  "thag1.93:0.4": "1.93. Erakattheragāthā",
  "thag1.93:1.1": "“Dukkhā kāmā eraka,",
  "thag1.93:1.2": "Na sukhā kāmā eraka;",
  "thag1.93:1.3": "Yo kāme kāmayati,",
  "thag1.93:1.4": "Dukkhaṃ so kāmayati eraka;",
  "thag1.93:1.5": "Yo kāme na kāmayati,",
  "thag1.93:1.6": "Dukkhaṃ so na kāmayati erakā”ti.",
  "thag1.93:1.7": "…",
  "thag1.93:1.8": "Erako thero …."
}