{
  "thag1.1:0.1": "Theragāthā",
  "thag1.1:0.2": "Ekakanipāta",
  "thag1.1:0.3": "Paṭhamavagga",
  "thag1.1:0.4": "1.1. Subhūtittheragāthā",
  "thag1.1:1.0.1": "Nidānagāthā",
  "thag1.1:1.0.2": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "thag1.1:1.1": "Sīhānaṃva nadantānaṃ,",
  "thag1.1:1.2": "dāṭhīnaṃ girigabbhare;",
  "thag1.1:1.3": "Suṇātha bhāvitattānaṃ,",
  "thag1.1:1.4": "gāthā atthūpanāyikā.",
  "thag1.1:2.1": "Yathānāmā yathāgottā,",
  "thag1.1:2.2": "yathādhammavihārino;",
  "thag1.1:2.3": "Yathādhimuttā sappaññā,",
  "thag1.1:2.4": "vihariṃsu atanditā.",
  "thag1.1:3.1": "Tattha tattha vipassitvā,",
  "thag1.1:3.2": "phusitvā accutaṃ padaṃ;",
  "thag1.1:3.3": "Katantaṃ paccavekkhantā,",
  "thag1.1:3.4": "imamatthamabhāsisuṃ.",
  "thag1.1:4.0": "Subhūtittheragāthā",
  "thag1.1:4.1": "“Channā me kuṭikā sukhā nivātā,",
  "thag1.1:4.2": "Vassa deva yathāsukhaṃ;",
  "thag1.1:4.3": "Cittaṃ me susamāhitaṃ vimuttaṃ,",
  "thag1.1:4.4": "Ātāpī viharāmi vassa devā”ti.",
  "thag1.1:4.5": "Itthaṃ sudaṃ āyasmā subhūtitthero gāthaṃ abhāsitthāti."
}