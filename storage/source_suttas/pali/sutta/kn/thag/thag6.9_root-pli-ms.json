{
  "thag6.9:0.1": "Theragāthā",
  "thag6.9:0.2": "Chakkanipāta",
  "thag6.9:0.3": "Paṭhamavagga",
  "thag6.9:0.4": "6.9 Purohitaputtajentattheragāthā",
  "thag6.9:1.1": "“Jātimadena mattohaṃ,",
  "thag6.9:1.2": "bhogaissariyena ca;",
  "thag6.9:1.3": "Saṇṭhānavaṇṇarūpena,",
  "thag6.9:1.4": "madamatto acārihaṃ.",
  "thag6.9:2.1": "Nāttano samakaṃ kañci,",
  "thag6.9:2.2": "atirekaṃ ca maññisaṃ;",
  "thag6.9:2.3": "Atimānahato bālo,",
  "thag6.9:2.4": "patthaddho ussitaddhajo.",
  "thag6.9:3.1": "Mātaraṃ pitarañcāpi,",
  "thag6.9:3.2": "aññepi garusammate;",
  "thag6.9:3.3": "Na kañci abhivādesiṃ,",
  "thag6.9:3.4": "mānatthaddho anādaro.",
  "thag6.9:4.1": "Disvā vināyakaṃ aggaṃ,",
  "thag6.9:4.2": "sārathīnaṃ varuttamaṃ;",
  "thag6.9:4.3": "Tapantamiva ādiccaṃ,",
  "thag6.9:4.4": "bhikkhusaṃghapurakkhataṃ.",
  "thag6.9:5.1": "Mānaṃ madañca chaḍḍetvā,",
  "thag6.9:5.2": "vippasannena cetasā;",
  "thag6.9:5.3": "Sirasā abhivādesiṃ,",
  "thag6.9:5.4": "sabbasattānamuttamaṃ.",
  "thag6.9:6.1": "Atimāno ca omāno,",
  "thag6.9:6.2": "pahīnā susamūhatā;",
  "thag6.9:6.3": "Asmimāno samucchinno,",
  "thag6.9:6.4": "sabbe mānavidhā hatā”ti.",
  "thag6.9:6.5": "…",
  "thag6.9:6.6": "Jento purohitaputto thero …."
}