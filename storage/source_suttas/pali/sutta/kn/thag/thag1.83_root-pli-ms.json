{
  "thag1.83:0.1": "Theragāthā",
  "thag1.83:0.2": "Ekakanipāta",
  "thag1.83:0.3": "Navamavagga",
  "thag1.83:0.4": "1.83. Sīhattheragāthā",
  "thag1.83:1.1": "“Sīhappamatto vihara,",
  "thag1.83:1.2": "rattindivamatandito;",
  "thag1.83:1.3": "Bhāvehi kusalaṃ dhammaṃ,",
  "thag1.83:1.4": "jaha sīghaṃ samussayan”ti.",
  "thag1.83:1.5": "…",
  "thag1.83:1.6": "Sīho thero …."
}