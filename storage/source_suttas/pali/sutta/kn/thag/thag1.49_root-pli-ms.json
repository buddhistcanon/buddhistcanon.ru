{
  "thag1.49:0.1": "Theragāthā",
  "thag1.49:0.2": "Ekakanipāta",
  "thag1.49:0.3": "Pañcamavagga",
  "thag1.49:0.4": "1.49. Rāmaṇeyyakattheragāthā",
  "thag1.49:1.1": "“Cihacihābhinadite,",
  "thag1.49:1.2": "sippikābhirutehi ca;",
  "thag1.49:1.3": "Na me taṃ phandati cittaṃ,",
  "thag1.49:1.4": "ekattanirataṃ hi me”ti.",
  "thag1.49:1.5": "…",
  "thag1.49:1.6": "Rāmaṇeyyako thero …."
}