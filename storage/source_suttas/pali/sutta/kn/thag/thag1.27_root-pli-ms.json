{
  "thag1.27:0.1": "Theragāthā",
  "thag1.27:0.2": "Ekakanipāta",
  "thag1.27:0.3": "Tatiyavagga",
  "thag1.27:0.4": "1.27. Lomasakaṅgiyattheragāthā",
  "thag1.27:1.1": "“Dabbaṃ kusaṃ poṭakilaṃ,",
  "thag1.27:1.2": "usīraṃ muñjapabbajaṃ;",
  "thag1.27:1.3": "Urasā panudissāmi,",
  "thag1.27:1.4": "vivekamanubrūhayan”ti.",
  "thag1.27:1.5": "…",
  "thag1.27:1.6": "Lomasakaṅgiyo thero …."
}