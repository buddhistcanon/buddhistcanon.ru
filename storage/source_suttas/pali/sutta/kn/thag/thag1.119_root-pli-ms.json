{
  "thag1.119:0.1": "Theragāthā",
  "thag1.119:0.2": "Ekakanipāta",
  "thag1.119:0.3": "Dvādasamavagga",
  "thag1.119:0.4": "1.119. Vajjiputtattheragāthā",
  "thag1.119:1.1": "“Rukkhamūlagahanaṃ pasakkiya,",
  "thag1.119:1.2": "Nibbānaṃ hadayasmiṃ opiya;",
  "thag1.119:1.3": "Jhāya gotama mā ca pamādo,",
  "thag1.119:1.4": "Kiṃ te biḷibiḷikā karissatī”ti.",
  "thag1.119:1.5": "…",
  "thag1.119:1.6": "Vajjiputto thero …."
}