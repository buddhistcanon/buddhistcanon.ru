{
  "thag2.39:0.1": "Theragāthā",
  "thag2.39:0.2": "Dukanipāta",
  "thag2.39:0.3": "Catutthavagga",
  "thag2.39:0.4": "2.39. Usabhattheragāthā",
  "thag2.39:1.1": "“Ambapallavasaṅkāsaṃ,",
  "thag2.39:1.2": "aṃse katvāna cīvaraṃ;",
  "thag2.39:1.3": "Nisinno hatthigīvāyaṃ,",
  "thag2.39:1.4": "gāmaṃ piṇḍāya pāvisiṃ.",
  "thag2.39:2.1": "Hatthikkhandhato oruyha,",
  "thag2.39:2.2": "saṃvegaṃ alabhiṃ tadā;",
  "thag2.39:2.3": "Sohaṃ ditto tadā santo,",
  "thag2.39:2.4": "patto me āsavakkhayo”ti.",
  "thag2.39:2.5": "…",
  "thag2.39:2.6": "Usabho thero …."
}