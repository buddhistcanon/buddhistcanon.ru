{
  "thag1.85:0.1": "Theragāthā",
  "thag1.85:0.2": "Ekakanipāta",
  "thag1.85:0.3": "Navamavagga",
  "thag1.85:0.4": "1.85. Sunāgattheragāthā",
  "thag1.85:1.1": "“Cittanimittassa kovido,",
  "thag1.85:1.2": "Pavivekarasaṃ vijāniya;",
  "thag1.85:1.3": "Jhāyaṃ nipako patissato,",
  "thag1.85:1.4": "Adhigaccheyya sukhaṃ nirāmisan”ti.",
  "thag1.85:1.5": "…",
  "thag1.85:1.6": "Sunāgo thero …."
}