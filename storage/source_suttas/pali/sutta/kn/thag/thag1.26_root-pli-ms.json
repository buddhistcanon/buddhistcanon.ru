{
  "thag1.26:0.1": "Theragāthā",
  "thag1.26:0.2": "Ekakanipāta",
  "thag1.26:0.3": "Tatiyavagga",
  "thag1.26:0.4": "1.26. Abhayattheragāthā",
  "thag1.26:1.1": "“Sutvā subhāsitaṃ vācaṃ,",
  "thag1.26:1.2": "buddhassādiccabandhuno;",
  "thag1.26:1.3": "Paccabyadhiṃ hi nipuṇaṃ,",
  "thag1.26:1.4": "vālaggaṃ usunā yathā”ti.",
  "thag1.26:1.5": "…",
  "thag1.26:1.6": "Abhayo thero …."
}