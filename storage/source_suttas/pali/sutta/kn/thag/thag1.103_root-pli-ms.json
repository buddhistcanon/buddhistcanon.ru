{
  "thag1.103:0.1": "Theragāthā",
  "thag1.103:0.2": "Ekakanipāta",
  "thag1.103:0.3": "Ekādasamavagga",
  "thag1.103:0.4": "1.103. Bandhurattheragāthā",
  "thag1.103:1.1": "“Nāhaṃ etena atthiko,",
  "thag1.103:1.2": "Sukhito dhammarasena tappito;",
  "thag1.103:1.3": "Pitvā rasaggamuttamaṃ,",
  "thag1.103:1.4": "Na ca kāhāmi visena santhavan”ti.",
  "thag1.103:1.5": "…",
  "thag1.103:1.6": "Bandhuro thero …."
}