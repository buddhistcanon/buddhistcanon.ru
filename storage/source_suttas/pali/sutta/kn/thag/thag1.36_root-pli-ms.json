{
  "thag1.36:0.1": "Theragāthā",
  "thag1.36:0.2": "Ekakanipāta",
  "thag1.36:0.3": "Catutthavagga",
  "thag1.36:0.4": "1.36. Kumāputtattheragāthā",
  "thag1.36:1.1": "“Sādhu sutaṃ sādhu caritakaṃ,",
  "thag1.36:1.2": "Sādhu sadā aniketavihāro;",
  "thag1.36:1.3": "Atthapucchanaṃ padakkhiṇakammaṃ,",
  "thag1.36:1.4": "Etaṃ sāmaññamakiñcanassā”ti.",
  "thag1.36:1.5": "…",
  "thag1.36:1.6": "Kumāputto thero …."
}