{
  "thag1.62:0.1": "Theragāthā",
  "thag1.62:0.2": "Ekakanipāta",
  "thag1.62:0.3": "Sattamavagga",
  "thag1.62:0.4": "1.62. Vajjiputtattheragāthā",
  "thag1.62:1.1": "“Ekakā mayaṃ araññe viharāma,",
  "thag1.62:1.2": "Apaviddhaṃva vanasmiṃ dārukaṃ;",
  "thag1.62:1.3": "Tassa me bahukā pihayanti,",
  "thag1.62:1.4": "Nerayikā viya saggagāminan”ti.",
  "thag1.62:1.5": "…",
  "thag1.62:1.6": "Vajjiputto thero …."
}