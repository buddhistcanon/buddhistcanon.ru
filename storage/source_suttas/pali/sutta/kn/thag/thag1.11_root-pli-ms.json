{
  "thag1.11:0.1": "Theragāthā",
  "thag1.11:0.2": "Ekakanipāta",
  "thag1.11:0.3": "Dutiyavagga",
  "thag1.11:0.4": "1.11. Cūḷavacchattheragāthā",
  "thag1.11:1.1": "“Pāmojjabahulo bhikkhu,",
  "thag1.11:1.2": "dhamme buddhappavedite;",
  "thag1.11:1.3": "Adhigacche padaṃ santaṃ,",
  "thag1.11:1.4": "saṅkhārūpasamaṃ sukhan”ti.",
  "thag1.11:1.5": "…",
  "thag1.11:1.6": "Cūḷavaccho thero …."
}