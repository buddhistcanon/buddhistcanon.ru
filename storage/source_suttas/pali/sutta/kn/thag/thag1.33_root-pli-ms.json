{
  "thag1.33:0.1": "Theragāthā",
  "thag1.33:0.2": "Ekakanipāta",
  "thag1.33:0.3": "Catutthavagga",
  "thag1.33:0.4": "1.33. Sopākattheragāthā",
  "thag1.33:1.1": "“Yathāpi ekaputtasmiṃ,",
  "thag1.33:1.2": "piyasmiṃ kusalī siyā;",
  "thag1.33:1.3": "Evaṃ sabbesu pāṇesu,",
  "thag1.33:1.4": "sabbattha kusalo siyā”ti.",
  "thag1.33:1.5": "…",
  "thag1.33:1.6": "Sopāko thero …."
}