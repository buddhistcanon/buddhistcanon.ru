{
  "thag1.7:0.1": "Theragāthā",
  "thag1.7:0.2": "Ekakanipāta",
  "thag1.7:0.3": "Paṭhamavagga",
  "thag1.7:0.4": "1.7. Bhalliyattheragāthā",
  "thag1.7:1.1": "“Yopānudī maccurājassa senaṃ,",
  "thag1.7:1.2": "Naḷasetuṃva sudubbalaṃ mahogho;",
  "thag1.7:1.3": "Vijitāvī apetabheravo hi,",
  "thag1.7:1.4": "Danto so parinibbuto ṭhitatto”ti.",
  "thag1.7:1.5": "Itthaṃ sudaṃ āyasmā bhalliyo thero gāthaṃ abhāsitthāti."
}