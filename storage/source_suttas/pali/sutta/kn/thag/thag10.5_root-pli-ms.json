{
  "thag10.5:0.1": "Theragāthā",
  "thag10.5:0.2": "Dasakanipāta",
  "thag10.5:0.3": "Paṭhamavagga",
  "thag10.5:0.4": "10.5. Kappattheragāthā",
  "thag10.5:1.1": "“Nānākulamalasampuṇṇo,",
  "thag10.5:1.2": "mahāukkārasambhavo;",
  "thag10.5:1.3": "Candanikaṃva paripakkaṃ,",
  "thag10.5:1.4": "mahāgaṇḍo mahāvaṇo.",
  "thag10.5:2.1": "Pubbaruhirasampuṇṇo,",
  "thag10.5:2.2": "gūthakūpena gāḷhito;",
  "thag10.5:2.3": "Āpopaggharaṇo kāyo,",
  "thag10.5:2.4": "sadā sandati pūtikaṃ.",
  "thag10.5:3.1": "Saṭṭhikaṇḍarasambandho,",
  "thag10.5:3.2": "maṃsalepanalepito;",
  "thag10.5:3.3": "Cammakañcukasannaddho,",
  "thag10.5:3.4": "pūtikāyo niratthako.",
  "thag10.5:4.1": "Aṭṭhisaṅghātaghaṭito,",
  "thag10.5:4.2": "nhārusuttanibandhano;",
  "thag10.5:4.3": "Nekesaṃ saṅgatībhāvā,",
  "thag10.5:4.4": "kappeti iriyāpathaṃ.",
  "thag10.5:5.1": "Dhuvappayāto maraṇāya,",
  "thag10.5:5.2": "maccurājassa santike;",
  "thag10.5:5.3": "Idheva chaḍḍayitvāna,",
  "thag10.5:5.4": "yenakāmaṅgamo naro.",
  "thag10.5:6.1": "Avijjāya nivuto kāyo,",
  "thag10.5:6.2": "catuganthena ganthito;",
  "thag10.5:6.3": "Oghasaṃsīdano kāyo,",
  "thag10.5:6.4": "anusayajālamotthato.",
  "thag10.5:7.1": "Pañcanīvaraṇe yutto,",
  "thag10.5:7.2": "vitakkena samappito;",
  "thag10.5:7.3": "Taṇhāmūlenānugato,",
  "thag10.5:7.4": "mohacchādanachādito.",
  "thag10.5:8.1": "Evāyaṃ vattate kāyo,",
  "thag10.5:8.2": "kammayantena yantito;",
  "thag10.5:8.3": "Sampatti ca vipatyantā,",
  "thag10.5:8.4": "nānābhāvo vipajjati.",
  "thag10.5:9.1": "Yemaṃ kāyaṃ mamāyanti,",
  "thag10.5:9.2": "andhabālā puthujjanā;",
  "thag10.5:9.3": "Vaḍḍhenti kaṭasiṃ ghoraṃ,",
  "thag10.5:9.4": "ādiyanti punabbhavaṃ.",
  "thag10.5:10.1": "Yemaṃ kāyaṃ vivajjenti,",
  "thag10.5:10.2": "Gūthalittaṃva pannagaṃ;",
  "thag10.5:10.3": "Bhavamūlaṃ vamitvāna,",
  "thag10.5:10.4": "Parinibbissantināsavā”ti.",
  "thag10.5:10.5": "…",
  "thag10.5:10.6": "Kappo thero …."
}