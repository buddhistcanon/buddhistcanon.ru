{
  "thag1.99:0.1": "Theragāthā",
  "thag1.99:0.2": "Ekakanipāta",
  "thag1.99:0.3": "Dasamavagga",
  "thag1.99:0.4": "1.99. Uttiyattheragāthā",
  "thag1.99:1.1": "“Saddaṃ sutvā sati muṭṭhā,",
  "thag1.99:1.2": "Piyaṃ nimittaṃ manasikaroto;",
  "thag1.99:1.3": "Sārattacitto vedeti,",
  "thag1.99:1.4": "Tañca ajjhosa tiṭṭhati;",
  "thag1.99:1.5": "Tassa vaḍḍhanti āsavā,",
  "thag1.99:1.6": "Saṃsāraṃ upagāmino”ti.",
  "thag1.99:1.7": "…",
  "thag1.99:1.8": "Uttiyo thero …."
}