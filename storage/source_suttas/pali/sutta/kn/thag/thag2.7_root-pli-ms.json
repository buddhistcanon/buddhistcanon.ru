{
  "thag2.7:0.1": "Theragāthā",
  "thag2.7:0.2": "Dukanipāta",
  "thag2.7:0.3": "Paṭhamavagga",
  "thag2.7:0.4": "2.7. Rādhattheragāthā",
  "thag2.7:1.1": "“Yathā agāraṃ ducchannaṃ,",
  "thag2.7:1.2": "vuṭṭhī samativijjhati;",
  "thag2.7:1.3": "Evaṃ abhāvitaṃ cittaṃ,",
  "thag2.7:1.4": "rāgo samativijjhati.",
  "thag2.7:2.1": "Yathā agāraṃ succhannaṃ,",
  "thag2.7:2.2": "Vuṭṭhī na samativijjhati;",
  "thag2.7:2.3": "Evaṃ subhāvitaṃ cittaṃ,",
  "thag2.7:2.4": "Rāgo na samativijjhatī”ti.",
  "thag2.7:2.5": "…",
  "thag2.7:2.6": "Rādho thero …."
}