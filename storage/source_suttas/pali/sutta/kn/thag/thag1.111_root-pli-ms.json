{
  "thag1.111:0.1": "Theragāthā",
  "thag1.111:0.2": "Ekakanipāta",
  "thag1.111:0.3": "Dvādasamavagga",
  "thag1.111:0.4": "1.111. Jentattheragāthā",
  "thag1.111:1.1": "“Duppabbajjaṃ ve duradhivāsā gehā,",
  "thag1.111:1.2": "Dhammo gambhīro duradhigamā bhogā;",
  "thag1.111:1.3": "Kicchā vutti no itarītareneva,",
  "thag1.111:1.4": "Yuttaṃ cintetuṃ satatamaniccatan”ti.",
  "thag1.111:1.5": "…",
  "thag1.111:1.6": "Jento thero …."
}