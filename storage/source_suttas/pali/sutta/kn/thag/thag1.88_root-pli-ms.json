{
  "thag1.88:0.1": "Theragāthā",
  "thag1.88:0.2": "Ekakanipāta",
  "thag1.88:0.3": "Navamavagga",
  "thag1.88:0.4": "1.88. Ajjunattheragāthā",
  "thag1.88:1.1": "“Asakkhiṃ vata attānaṃ,",
  "thag1.88:1.2": "uddhātuṃ udakā thalaṃ;",
  "thag1.88:1.3": "Vuyhamāno mahogheva,",
  "thag1.88:1.4": "saccāni paṭivijjhahan”ti.",
  "thag1.88:1.5": "…",
  "thag1.88:1.6": "Ajjuno thero …."
}