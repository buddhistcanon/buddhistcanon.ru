{
  "thag3.10:0.1": "Theragāthā",
  "thag3.10:0.2": "Tikanipāta",
  "thag3.10:0.3": "Paṭhamavagga",
  "thag3.10:0.4": "3.10. Sāṭimattiyattheragāthā",
  "thag3.10:1.1": "“Ahu tuyhaṃ pure saddhā,",
  "thag3.10:1.2": "sā te ajja na vijjati;",
  "thag3.10:1.3": "Yaṃ tuyhaṃ tuyhamevetaṃ,",
  "thag3.10:1.4": "natthi duccaritaṃ mama.",
  "thag3.10:2.1": "Aniccā hi calā saddhā,",
  "thag3.10:2.2": "evaṃ diṭṭhā hi sā mayā;",
  "thag3.10:2.3": "Rajjantipi virajjanti,",
  "thag3.10:2.4": "tattha kiṃ jiyyate muni.",
  "thag3.10:3.1": "Paccati munino bhattaṃ,",
  "thag3.10:3.2": "Thokaṃ thokaṃ kule kule;",
  "thag3.10:3.3": "Piṇḍikāya carissāmi,",
  "thag3.10:3.4": "Atthi jaṅghabalaṃ mamā”ti.",
  "thag3.10:3.5": "…",
  "thag3.10:3.6": "Sāṭimattiyo thero …."
}