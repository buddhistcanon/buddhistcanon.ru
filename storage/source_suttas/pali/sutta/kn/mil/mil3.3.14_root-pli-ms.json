{
  "mil3.3.14:0.1": "Milindapañha",
  "mil3.3.14:0.2": "Vicāravagga",
  "mil3.3.14:0.3": "14. Vicāralakkhaṇapañha",
  "mil3.3.14:1.1": "“Bhante nāgasena, kiṃlakkhaṇo vicāro”ti?",
  "mil3.3.14:1.2": "“Anumajjanalakkhaṇo, mahārāja, vicāro”ti.",
  "mil3.3.14:2.1": "“Opammaṃ karohī”ti.",
  "mil3.3.14:2.2": "“Yathā, mahārāja, kaṃsathālaṃ ākoṭitaṃ pacchā anuravati anusandahati, yathā, mahārāja, ākoṭanā, evaṃ vitakko daṭṭhabbo.",
  "mil3.3.14:2.3": "Yathā anuravanā, evaṃ vicāro daṭṭhabbo”ti.",
  "mil3.3.14:3.1": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.14:4.1": "Vicāralakkhaṇapañho cuddasamo.",
  "mil3.3.14:5.1": "Vicāravaggo tatiyo.",
  "mil3.3.14:6.1": "Imasmiṃ vagge cuddasa pañhā."
}