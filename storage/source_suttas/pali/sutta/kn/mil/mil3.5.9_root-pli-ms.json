{
  "mil3.5.9:0.1": "Milindapañha",
  "mil3.5.9:0.2": "Buddhavagga",
  "mil3.5.9:0.3": "9. Uppajjatijānanapañha",
  "mil3.5.9:1.1": "Rājā āha—",
  "mil3.5.9:1.2": "“bhante nāgasena, yo uppajjati, jānāti so ‘uppajjissāmī’”ti?",
  "mil3.5.9:1.3": "“Āma, mahārāja, yo uppajjati jānāti so ‘uppajjissāmī’”ti.",
  "mil3.5.9:2.1": "“Opammaṃ karohī”ti.",
  "mil3.5.9:2.2": "“Yathā, mahārāja, kassako gahapatiko bījāni pathaviyaṃ nikkhipitvā sammā deve vassante jānāti ‘dhaññaṃ nibbattissatī’”ti?",
  "mil3.5.9:2.3": "“Āma, bhante, jāneyyā”ti.",
  "mil3.5.9:2.4": "“Evameva kho, mahārāja, yo uppajjati, jānāti so ‘uppajjissāmī’”ti.",
  "mil3.5.9:3.1": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.9:4.1": "Uppajjatijānanapañho navamo."
}