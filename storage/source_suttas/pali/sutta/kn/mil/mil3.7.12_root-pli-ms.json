{
  "mil3.7.12:0.1": "Milindapañha",
  "mil3.7.12:0.2": "Arūpadhammavavatthānavagga",
  "mil3.7.12:0.3": "12. Samuddapañha",
  "mil3.7.12:1.1": "Rājā āha—",
  "mil3.7.12:1.2": "“bhante nāgasena, ‘samuddo samuddo’ti vuccati, kena kāraṇena udakaṃ ‘samuddo’ti vuccatī”ti?",
  "mil3.7.12:1.3": "Thero āha—",
  "mil3.7.12:1.4": "“yattakaṃ, mahārāja, udakaṃ, tattakaṃ loṇaṃ.",
  "mil3.7.12:1.5": "Yattakaṃ loṇaṃ, tattakaṃ udakaṃ.",
  "mil3.7.12:1.6": "Tasmā ‘samuddo’ti vuccatī”ti.",
  "mil3.7.12:2.1": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.12:3.1": "Samuddapañho dvādasamo."
}