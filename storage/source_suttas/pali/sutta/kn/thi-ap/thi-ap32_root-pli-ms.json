{
  "thi-ap32:0.1": "Therīapadāna",
  "thi-ap32:0.2": "Khattiyāvagga",
  "thi-ap32:0.3": "2 Caturāsītibhikkhunīsahassaapadāna",
  "thi-ap32:1.1": "“Cullāsītisahassāni,",
  "thi-ap32:1.2": "brāhmaññakulasambhavā;",
  "thi-ap32:1.3": "Sukhumālahatthapādā,",
  "thi-ap32:1.4": "pure tuyhaṃ mahāmune.",
  "thi-ap32:2.1": "Vessasuddakule jātā,",
  "thi-ap32:2.2": "devā nāgā ca kinnarā;",
  "thi-ap32:2.3": "Cātuddīpā bahū kaññā,",
  "thi-ap32:2.4": "pure tuyhaṃ mahāmune.",
  "thi-ap32:3.1": "Kāci pabbajitā atthi,",
  "thi-ap32:3.2": "sabbadassāvino bahū;",
  "thi-ap32:3.3": "Devā ca kinnarā nāgā,",
  "thi-ap32:3.4": "phusissanti anāgate.",
  "thi-ap32:4.1": "Anubhotvā yasaṃ sabbaṃ,",
  "thi-ap32:4.2": "Patvāna sabbasampadā;",
  "thi-ap32:4.3": "Tumhaṃ pasādaṃ paṭiladdhā,",
  "thi-ap32:4.4": "Bujjhissanti anāgate.",
  "thi-ap32:5.1": "Amhe brāhmaṇadhītā tu,",
  "thi-ap32:5.2": "brāhmaññakulasambhavā;",
  "thi-ap32:5.3": "Pekkhato no mahāvīra,",
  "thi-ap32:5.4": "pāde vandāma cakkhuma.",
  "thi-ap32:6.1": "Upāhatā bhavā sabbe,",
  "thi-ap32:6.2": "mūlataṇhā samūhatā;",
  "thi-ap32:6.3": "Samucchinnā anusayā,",
  "thi-ap32:6.4": "puññasaṅkhāradālitā.",
  "thi-ap32:7.1": "Samādhigocarā sabbā,",
  "thi-ap32:7.2": "samāpattivasī katā;",
  "thi-ap32:7.3": "Jhānena dhammaratiyā,",
  "thi-ap32:7.4": "viharissāma no sadā.",
  "thi-ap32:8.1": "Bhavanetti avijjā ca,",
  "thi-ap32:8.2": "saṅkhārāpi ca khepitā;",
  "thi-ap32:8.3": "Sududdasaṃ padaṃ gantvā,",
  "thi-ap32:8.4": "anujānātha nāyaka”.",
  "thi-ap32:9.1": "“Upakārā mamaṃ tumhe,",
  "thi-ap32:9.2": "dīgharattaṃ katāvino;",
  "thi-ap32:9.3": "Catunnaṃ saṃsayaṃ chetvā,",
  "thi-ap32:9.4": "sabbā gacchantu nibbutiṃ”.",
  "thi-ap32:10.1": "Vanditvā munino pāde,",
  "thi-ap32:10.2": "katvā iddhivikubbanaṃ;",
  "thi-ap32:10.3": "Kāci dassenti ālokaṃ,",
  "thi-ap32:10.4": "andhakāramathāparā.",
  "thi-ap32:11.1": "Dassenti candasūriye,",
  "thi-ap32:11.2": "sāgarañca samacchakaṃ;",
  "thi-ap32:11.3": "Sineruṃ paribhaṇḍañca,",
  "thi-ap32:11.4": "dassenti pārichattakaṃ.",
  "thi-ap32:12.1": "Tāvatiṃsañca bhavanaṃ,",
  "thi-ap32:12.2": "yāmaṃ dassenti iddhiyā;",
  "thi-ap32:12.3": "Tusitaṃ nimmite deve,",
  "thi-ap32:12.4": "vasavattī mahissare.",
  "thi-ap32:13.1": "Brahmāno kāci dassenti,",
  "thi-ap32:13.2": "caṅkamañca mahārahaṃ;",
  "thi-ap32:13.3": "Brahmavaṇṇañca māpetvā,",
  "thi-ap32:13.4": "dhammaṃ desenti suññataṃ.",
  "thi-ap32:14.1": "Nānāvikubbanaṃ katvā,",
  "thi-ap32:14.2": "iddhiṃ dassiya satthuno;",
  "thi-ap32:14.3": "Dassayiṃsu balaṃ sabbā,",
  "thi-ap32:14.4": "pāde vandiṃsu satthuno.",
  "thi-ap32:15.1": "“Iddhīsu ca vasī homa,",
  "thi-ap32:15.2": "dibbāya sotadhātuyā;",
  "thi-ap32:15.3": "Cetopariyañāṇassa,",
  "thi-ap32:15.4": "vasī homa mahāmune.",
  "thi-ap32:16.1": "Pubbenivāsaṃ jānāma,",
  "thi-ap32:16.2": "dibbacakkhu visodhitaṃ;",
  "thi-ap32:16.3": "Sabbāsavā parikkhīṇā,",
  "thi-ap32:16.4": "natthi dāni punabbhavo.",
  "thi-ap32:17.1": "Atthadhammaniruttīsu,",
  "thi-ap32:17.2": "paṭibhāne tatheva ca;",
  "thi-ap32:17.3": "Ñāṇaṃ amhaṃ mahāvīra,",
  "thi-ap32:17.4": "uppannaṃ tava santike.",
  "thi-ap32:18.1": "Pubbānaṃ lokanāthānaṃ,",
  "thi-ap32:18.2": "saṅgamaṃ no nidassitaṃ;",
  "thi-ap32:18.3": "Adhikāraṃ bahuṃ amhaṃ,",
  "thi-ap32:18.4": "tuyhatthāya mahāmune.",
  "thi-ap32:19.1": "Yaṃ amhehi kataṃ kammaṃ,",
  "thi-ap32:19.2": "kusalaṃ sara taṃ mune;",
  "thi-ap32:19.3": "Tuyhatthāya mahāvīra,",
  "thi-ap32:19.4": "puññānupacitāni no.",
  "thi-ap32:20.1": "Satasahassito kappe,",
  "thi-ap32:20.2": "padumuttaro mahāmuni;",
  "thi-ap32:20.3": "Puraṃ haṃsavatī nāma,",
  "thi-ap32:20.4": "sambuddhassa kulāsayaṃ.",
  "thi-ap32:21.1": "Dvārena haṃsavatiyā,",
  "thi-ap32:21.2": "gaṅgā sandati sabbadā;",
  "thi-ap32:21.3": "Ubbāḷhā nadiyā bhikkhū,",
  "thi-ap32:21.4": "gamanaṃ na labhanti te.",
  "thi-ap32:22.1": "Divasaṃ dve tayo ceva,",
  "thi-ap32:22.2": "sattāhaṃ māsikaṃ tato;",
  "thi-ap32:22.3": "Catumāsampi sampuṇṇaṃ,",
  "thi-ap32:22.4": "gamanaṃ na labhanti te.",
  "thi-ap32:23.1": "Tadā ahu sattasāro,",
  "thi-ap32:23.2": "jaṭilo nāma raṭṭhiko;",
  "thi-ap32:23.3": "Oruddhe bhikkhavo disvā,",
  "thi-ap32:23.4": "setuṃ gaṅgāya kārayi.",
  "thi-ap32:24.1": "Tadā satasahassehi,",
  "thi-ap32:24.2": "setuṃ gaṅgāya kārayi;",
  "thi-ap32:24.3": "Saṃghassa orime tīre,",
  "thi-ap32:24.4": "vihārañca akārayi.",
  "thi-ap32:25.1": "Itthiyo purisā ceva,",
  "thi-ap32:25.2": "uccanīcakulāni ca;",
  "thi-ap32:25.3": "Tassa setuṃ vihārañca,",
  "thi-ap32:25.4": "samabhāgaṃ akaṃsu te.",
  "thi-ap32:26.1": "Amhe aññe ca mānujā,",
  "thi-ap32:26.2": "vippasannena cetasā;",
  "thi-ap32:26.3": "Tassa dhammesu dāyādā,",
  "thi-ap32:26.4": "nagare janapadesu ca.",
  "thi-ap32:27.1": "Itthī pumā kumārā ca,",
  "thi-ap32:27.2": "bahū ceva kumārikā;",
  "thi-ap32:27.3": "Setuno ca vihārassa,",
  "thi-ap32:27.4": "vālukā ākiriṃsu te.",
  "thi-ap32:28.1": "Vīthiṃ sammajjanaṃ katvā,",
  "thi-ap32:28.2": "kadalīpuṇṇaghaṭe dhaje;",
  "thi-ap32:28.3": "Dhūpaṃ cuṇṇañca mālañca,",
  "thi-ap32:28.4": "kāraṃ katvāna satthuno.",
  "thi-ap32:29.1": "Setuvihāre kāretvā,",
  "thi-ap32:29.2": "nimantetvā vināyakaṃ;",
  "thi-ap32:29.3": "Mahādānaṃ daditvāna,",
  "thi-ap32:29.4": "sambodhiṃ abhipatthayiṃ.",
  "thi-ap32:30.1": "Padumuttaro mahāvīro,",
  "thi-ap32:30.2": "tārako sabbapāṇinaṃ;",
  "thi-ap32:30.3": "Anumodanīyaṃkāsi,",
  "thi-ap32:30.4": "jaṭilassa mahāmuni.",
  "thi-ap32:31.1": "‘Satasahassātikkante,",
  "thi-ap32:31.2": "kappo hessati bhaddako;",
  "thi-ap32:31.3": "Bhavābhavenubhotvāna,",
  "thi-ap32:31.4": "pāpuṇissati bodhiyaṃ.",
  "thi-ap32:32.1": "Kāci hatthaparikammaṃ,",
  "thi-ap32:32.2": "katāvī naranāriyo;",
  "thi-ap32:32.3": "Anāgatamhi addhāne,",
  "thi-ap32:32.4": "sabbā hessanti sammukhā’.",
  "thi-ap32:33.1": "Tena kammavipākena,",
  "thi-ap32:33.2": "cetanāpaṇidhīhi ca;",
  "thi-ap32:33.3": "Uppannā devabhavanaṃ,",
  "thi-ap32:33.4": "tuyhaṃ tā paricārikā.",
  "thi-ap32:34.1": "Dibbasukhaṃ asaṅkhiyaṃ,",
  "thi-ap32:34.2": "mānusañca asaṅkhiyaṃ;",
  "thi-ap32:34.3": "Tuyhaṃ te paricārema,",
  "thi-ap32:34.4": "saṃsarimha bhavābhave.",
  "thi-ap32:35.1": "Satasahassito kappe,",
  "thi-ap32:35.2": "sukataṃ kammasampadaṃ;",
  "thi-ap32:35.3": "Sukhumālī manussānaṃ,",
  "thi-ap32:35.4": "atho devapure vare.",
  "thi-ap32:36.1": "Rūpabhogayase ceva,",
  "thi-ap32:36.2": "atho kittiñca sakkataṃ;",
  "thi-ap32:36.3": "Labhāma satataṃ sabbaṃ,",
  "thi-ap32:36.4": "sukataṃ kammasampadaṃ.",
  "thi-ap32:37.1": "Pacchime bhave sampatte,",
  "thi-ap32:37.2": "jātāmha brāhmaṇe kule;",
  "thi-ap32:37.3": "Sukhumālahatthapādā,",
  "thi-ap32:37.4": "sakyaputtanivesane.",
  "thi-ap32:38.1": "Sabbakālampi pathaviṃ,",
  "thi-ap32:38.2": "na passāma na laṅkataṃ;",
  "thi-ap32:38.3": "Cikkhallabhūmimasuciṃ,",
  "thi-ap32:38.4": "na passāma mahāmune.",
  "thi-ap32:39.1": "Agāraṃ vasante amhe,",
  "thi-ap32:39.2": "sakkāraṃ sabbakālikaṃ;",
  "thi-ap32:39.3": "Upanenti sadā sabbaṃ,",
  "thi-ap32:39.4": "pubbakammaphalena no.",
  "thi-ap32:40.1": "Agāraṃ pajahitvāna,",
  "thi-ap32:40.2": "pabbajitvānagāriyaṃ;",
  "thi-ap32:40.3": "Saṃsārapathanitthiṇṇā,",
  "thi-ap32:40.4": "vītarāgā bhavāmase.",
  "thi-ap32:41.1": "Cīvaraṃ piṇḍapātañca,",
  "thi-ap32:41.2": "paccayaṃ sayanāsanaṃ;",
  "thi-ap32:41.3": "Upanenti sadā amhe,",
  "thi-ap32:41.4": "sahassāni tato tato.",
  "thi-ap32:42.1": "Kilesā jhāpitā amhaṃ,",
  "thi-ap32:42.2": "…pe…",
  "thi-ap32:42.3": "viharāma anāsavā.",
  "thi-ap32:43.1": "Svāgataṃ vata no āsi,",
  "thi-ap32:43.2": "…pe…",
  "thi-ap32:43.3": "kataṃ buddhassa sāsanaṃ.",
  "thi-ap32:44.1": "Paṭisambhidā catasso,",
  "thi-ap32:44.2": "…pe…",
  "thi-ap32:44.3": "kataṃ buddhassa sāsanaṃ”.",
  "thi-ap32:45.1": "Itthaṃ sudaṃ caturāsītibrāhmaṇakaññābhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap32:46.1": "Caturāsītibhikkhunīsahassāpadānaṃ dutiyaṃ."
}