{
  "thi-ap7:0.1": "Therīapadāna",
  "thi-ap7:0.2": "Sumedhāvagga",
  "thi-ap7:0.3": "7 Kaṭacchubhikkhādāyikātherīapadāna",
  "thi-ap7:1.1": "“Piṇḍacāraṃ carantassa,",
  "thi-ap7:1.2": "tissanāmassa satthuno;",
  "thi-ap7:1.3": "Kaṭacchubhikkhaṃ paggayha,",
  "thi-ap7:1.4": "buddhaseṭṭhassa dāsahaṃ.",
  "thi-ap7:2.1": "Paṭiggahetvā sambuddho,",
  "thi-ap7:2.2": "tisso lokagganāyako;",
  "thi-ap7:2.3": "Vīthiyā saṇṭhito satthā,",
  "thi-ap7:2.4": "akā me anumodanaṃ.",
  "thi-ap7:3.1": "‘Kaṭacchubhikkhaṃ datvāna,",
  "thi-ap7:3.2": "tāvatiṃsaṃ gamissasi;",
  "thi-ap7:3.3": "Chattiṃsadevarājūnaṃ,",
  "thi-ap7:3.4": "mahesittaṃ karissasi.",
  "thi-ap7:4.1": "Paññāsaṃ cakkavattīnaṃ,",
  "thi-ap7:4.2": "mahesittaṃ karissasi;",
  "thi-ap7:4.3": "Manasā patthitaṃ sabbaṃ,",
  "thi-ap7:4.4": "paṭilacchasi sabbadā.",
  "thi-ap7:5.1": "Sampattiṃ anubhotvāna,",
  "thi-ap7:5.2": "pabbajissasikiñcanā;",
  "thi-ap7:5.3": "Sabbāsave pariññāya,",
  "thi-ap7:5.4": "nibbāyissasināsavā’.",
  "thi-ap7:6.1": "Idaṃ vatvāna sambuddho,",
  "thi-ap7:6.2": "tisso lokagganāyako;",
  "thi-ap7:6.3": "Nabhaṃ abbhuggamī vīro,",
  "thi-ap7:6.4": "haṃsarājāva ambare.",
  "thi-ap7:7.1": "Sudinnaṃ me dānavaraṃ,",
  "thi-ap7:7.2": "suyiṭṭhā yāgasampadā;",
  "thi-ap7:7.3": "Kaṭacchubhikkhaṃ datvāna,",
  "thi-ap7:7.4": "pattāhaṃ acalaṃ padaṃ.",
  "thi-ap7:8.1": "Dvenavute ito kappe,",
  "thi-ap7:8.2": "yaṃ dānamadadiṃ tadā;",
  "thi-ap7:8.3": "Duggatiṃ nābhijānāmi,",
  "thi-ap7:8.4": "bhikkhādānassidaṃ phalaṃ.",
  "thi-ap7:9.1": "Kilesā jhāpitā mayhaṃ,",
  "thi-ap7:9.2": "…pe…",
  "thi-ap7:9.3": "viharāmi anāsavā.",
  "thi-ap7:10.1": "Svāgataṃ vata me āsi,",
  "thi-ap7:10.2": "…pe…",
  "thi-ap7:10.3": "kataṃ buddhassa sāsanaṃ.",
  "thi-ap7:11.1": "Paṭisambhidā catasso,",
  "thi-ap7:11.2": "…pe…",
  "thi-ap7:11.3": "kataṃ buddhassa sāsanaṃ”.",
  "thi-ap7:12.1": "Itthaṃ sudaṃ kaṭacchubhikkhādāyikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap7:13.1": "Kaṭacchubhikkhādāyikātheriyāpadānaṃ sattamaṃ."
}