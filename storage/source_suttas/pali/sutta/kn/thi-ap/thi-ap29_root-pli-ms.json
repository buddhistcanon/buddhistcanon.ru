{
  "thi-ap29:0.1": "Therīapadāna",
  "thi-ap29:0.2": "Kuṇḍalakesīvagga",
  "thi-ap29:0.3": "9 Yasodharāpamukhadasabhikkhunīsahassaapadāna",
  "thi-ap29:1.1": "“Kappe ca satasahasse,",
  "thi-ap29:1.2": "caturo ca asaṅkhiye;",
  "thi-ap29:1.3": "Dīpaṅkaro nāma jino,",
  "thi-ap29:1.4": "uppajji lokanāyako.",
  "thi-ap29:2.1": "Dīpaṅkaro mahāvīro,",
  "thi-ap29:2.2": "viyākāsi vināyako;",
  "thi-ap29:2.3": "Sumedhañca sumittañca,",
  "thi-ap29:2.4": "samānasukhadukkhataṃ.",
  "thi-ap29:3.1": "Sadevakañca passanto,",
  "thi-ap29:3.2": "vicaranto sadevakaṃ;",
  "thi-ap29:3.3": "Tesaṃ pakittane amhe,",
  "thi-ap29:3.4": "upagamma samāgamaṃ.",
  "thi-ap29:4.1": "Amhaṃ sabbapati hohi,",
  "thi-ap29:4.2": "anāgatasamāgame;",
  "thi-ap29:4.3": "Sabbāva tuyhaṃ bhariyā,",
  "thi-ap29:4.4": "manāpā piyavādikā.",
  "thi-ap29:5.1": "Dānaṃ sīlamayaṃ sabbaṃ,",
  "thi-ap29:5.2": "bhāvanā ca subhāvitā;",
  "thi-ap29:5.3": "Dīgharattañca no sabbaṃ,",
  "thi-ap29:5.4": "pariccattaṃ mahāmune.",
  "thi-ap29:6.1": "Gandhaṃ vilepanaṃ mālaṃ,",
  "thi-ap29:6.2": "dīpañca ratanāmayaṃ;",
  "thi-ap29:6.3": "Yaṃ kiñci patthitaṃ sabbaṃ,",
  "thi-ap29:6.4": "pariccattaṃ mahāmuni.",
  "thi-ap29:7.1": "Aññaṃ vāpi kataṃ kammaṃ,",
  "thi-ap29:7.2": "paribhogañca mānusaṃ;",
  "thi-ap29:7.3": "Dīgharattañhi no sabbaṃ,",
  "thi-ap29:7.4": "pariccattaṃ mahāmuni.",
  "thi-ap29:8.1": "Anekajātisaṃsāraṃ,",
  "thi-ap29:8.2": "bahuṃ puññampi no kataṃ;",
  "thi-ap29:8.3": "Issaramanubhotvāna,",
  "thi-ap29:8.4": "saṃsaritvā bhavābhave.",
  "thi-ap29:9.1": "Pacchime bhave sampatte,",
  "thi-ap29:9.2": "sakyaputtanivesane;",
  "thi-ap29:9.3": "Nānākulūpapannāyo,",
  "thi-ap29:9.4": "accharā kāmavaṇṇinī.",
  "thi-ap29:10.1": "Lābhaggena yasaṃ pattā,",
  "thi-ap29:10.2": "pūjitā sabbasakkatā;",
  "thi-ap29:10.3": "Lābhiyo annapānānaṃ,",
  "thi-ap29:10.4": "sadā sammānitā mayaṃ.",
  "thi-ap29:11.1": "Agāraṃ pajahitvāna,",
  "thi-ap29:11.2": "pabbajimhanagāriyaṃ;",
  "thi-ap29:11.3": "Aḍḍhamāse asampatte,",
  "thi-ap29:11.4": "sabbā pattāmha nibbutiṃ.",
  "thi-ap29:12.1": "Lābhiyo annapānānaṃ,",
  "thi-ap29:12.2": "vatthasenāsanāni ca;",
  "thi-ap29:12.3": "Upenti paccayā sabbe,",
  "thi-ap29:12.4": "sadā sakkatapūjitā.",
  "thi-ap29:13.1": "Kilesā jhāpitā amhaṃ,",
  "thi-ap29:13.2": "…pe…",
  "thi-ap29:13.3": "viharāma anāsavā.",
  "thi-ap29:14.1": "Svāgataṃ vata no āsi,",
  "thi-ap29:14.2": "…pe…",
  "thi-ap29:14.3": "kataṃ buddhassa sāsanaṃ.",
  "thi-ap29:15.1": "Paṭisambhidā catasso,",
  "thi-ap29:15.2": "…pe…",
  "thi-ap29:15.3": "kataṃ buddhassa sāsanaṃ”.",
  "thi-ap29:16.1": "Itthaṃ sudaṃ yasodharāpamukhāni dasabhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap29:17.1": "Yasodharāpamukhadasabhikkhunīsahassāpadānaṃ navamaṃ."
}