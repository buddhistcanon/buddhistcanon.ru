{
  "thi-ap4:0.1": "Therīapadāna",
  "thi-ap4:0.2": "Sumedhāvagga",
  "thi-ap4:0.3": "4 Saṅkamanatthātherīapadāna",
  "thi-ap4:1.1": "“Vipassissa bhagavato,",
  "thi-ap4:1.2": "lokajeṭṭhassa tādino;",
  "thi-ap4:1.3": "Rathiyaṃ paṭipannassa,",
  "thi-ap4:1.4": "tārayantassa pāṇino.",
  "thi-ap4:2.1": "Gharato nikkhamitvāna,",
  "thi-ap4:2.2": "avakujjā nipajjahaṃ;",
  "thi-ap4:2.3": "Anukampako lokanātho,",
  "thi-ap4:2.4": "sirasi akkamī mama.",
  "thi-ap4:3.1": "Akkamitvāna sirasi,",
  "thi-ap4:3.2": "agamā lokanāyako;",
  "thi-ap4:3.3": "Tena cittappasādena,",
  "thi-ap4:3.4": "tusitaṃ agamāsahaṃ.",
  "thi-ap4:4.1": "Kilesā jhāpitā mayhaṃ,",
  "thi-ap4:4.2": "…pe…",
  "thi-ap4:4.3": "viharāmi anāsavā.",
  "thi-ap4:5.1": "Svāgataṃ vata me āsi,",
  "thi-ap4:5.2": "…pe…",
  "thi-ap4:5.3": "kataṃ buddhassa sāsanaṃ.",
  "thi-ap4:6.1": "Paṭisambhidā catasso,",
  "thi-ap4:6.2": "…pe…",
  "thi-ap4:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "thi-ap4:7.1": "Itthaṃ sudaṃ saṅkamanatthā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap4:8.1": "Saṅkamanatthātheriyāpadānaṃ catutthaṃ."
}