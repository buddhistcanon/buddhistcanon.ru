{
  "thi-ap12:0.1": "Therīapadāna",
  "thi-ap12:0.2": "Ekūposathikavagga",
  "thi-ap12:0.3": "2 Saḷalapupphikātherīapadāna",
  "thi-ap12:1.1": "“Candabhāgānadītīre,",
  "thi-ap12:1.2": "ahosiṃ kinnarī tadā;",
  "thi-ap12:1.3": "Addasāhaṃ devadevaṃ,",
  "thi-ap12:1.4": "caṅkamantaṃ narāsabhaṃ.",
  "thi-ap12:2.1": "Ocinitvāna saḷalaṃ,",
  "thi-ap12:2.2": "buddhaseṭṭhassadāsahaṃ;",
  "thi-ap12:2.3": "Upasiṅghi mahāvīro,",
  "thi-ap12:2.4": "saḷalaṃ devagandhikaṃ.",
  "thi-ap12:3.1": "Paṭiggahetvā sambuddho,",
  "thi-ap12:3.2": "vipassī lokanāyako;",
  "thi-ap12:3.3": "Upasiṅghi mahāvīro,",
  "thi-ap12:3.4": "pekkhamānāya me tadā.",
  "thi-ap12:4.1": "Añjaliṃ paggahetvāna,",
  "thi-ap12:4.2": "vanditvā dvipaduttamaṃ;",
  "thi-ap12:4.3": "Sakaṃ cittaṃ pasādetvā,",
  "thi-ap12:4.4": "tato pabbatamāruhiṃ.",
  "thi-ap12:5.1": "Ekanavutito kappe,",
  "thi-ap12:5.2": "yaṃ pupphamadadiṃ tadā;",
  "thi-ap12:5.3": "Duggatiṃ nābhijānāmi,",
  "thi-ap12:5.4": "buddhapūjāyidaṃ phalaṃ.",
  "thi-ap12:6.1": "Kilesā jhāpitā mayhaṃ,",
  "thi-ap12:6.2": "…pe…",
  "thi-ap12:6.3": "viharāmi anāsavā.",
  "thi-ap12:7.1": "Svāgataṃ vata me āsi,",
  "thi-ap12:7.2": "…pe…",
  "thi-ap12:7.3": "kataṃ buddhassa sāsanaṃ.",
  "thi-ap12:8.1": "Paṭisambhidā catasso,",
  "thi-ap12:8.2": "…pe…",
  "thi-ap12:8.3": "kataṃ buddhassa sāsanaṃ”.",
  "thi-ap12:9.1": "Itthaṃ sudaṃ saḷalapupphikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap12:10.1": "Saḷalapupphikātheriyāpadānaṃ dutiyaṃ."
}