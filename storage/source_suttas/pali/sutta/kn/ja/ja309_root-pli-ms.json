{
  "ja309:0.1": "Jātaka",
  "ja309:0.2": "Catukkanipāta",
  "ja309:0.3": "Kāliṅgavagga",
  "ja309:0.4": "9. Chavakajātaka",
  "ja309:1.1": "“Sabbamidaṃ carimaṃ kataṃ,",
  "ja309:1.2": "Ubho dhammaṃ na passare;",
  "ja309:1.3": "Ubho pakatiyā cutā,",
  "ja309:1.4": "Yo cāyaṃ mantejjhāpeti;",
  "ja309:1.5": "Yo ca mantaṃ adhīyati”.",
  "ja309:2.1": "“Sālīnaṃ odanaṃ bhuñje,",
  "ja309:2.2": "suciṃ maṃsūpasecanaṃ;",
  "ja309:2.3": "Tasmā etaṃ na sevāmi,",
  "ja309:2.4": "dhammaṃ isīhi sevitaṃ”.",
  "ja309:3.1": "“Paribbaja mahā loko,",
  "ja309:3.2": "pacantaññepi pāṇino;",
  "ja309:3.3": "Mā taṃ adhammo ācarito,",
  "ja309:3.4": "asmā kumbhamivābhidā.",
  "ja309:4.1": "Dhiratthu taṃ yasalābhaṃ,",
  "ja309:4.2": "dhanalābhañca brāhmaṇa;",
  "ja309:4.3": "Yā vutti vinipātena,",
  "ja309:4.4": "adhammacaraṇena vā”ti.",
  "ja309:5.1": "Chavakajātakaṃ navamaṃ."
}