{
  "ja218:0.1": "Jātaka",
  "ja218:0.2": "Dukanipāta",
  "ja218:0.3": "Bīraṇathambhavagga",
  "ja218:0.4": "8. Kūṭavāṇijajātaka",
  "ja218:1.1": "“Saṭhassa sāṭheyyamidaṃ sucintitaṃ,",
  "ja218:1.2": "Paccoḍḍitaṃ paṭikūṭassa kūṭaṃ;",
  "ja218:1.3": "Phālañce khādeyyuṃ mūsikā,",
  "ja218:1.4": "Kasmā kumāraṃ kulalā na hareyyuṃ.",
  "ja218:2.1": "Kūṭassa hi santi kūṭakūṭā,",
  "ja218:2.2": "Bhavati cāpi nikatino nikatyā;",
  "ja218:2.3": "Dehi puttanaṭṭha phālanaṭṭhassa phālaṃ,",
  "ja218:2.4": "Mā te puttamahāsi phālanaṭṭho”ti.",
  "ja218:3.1": "Kūṭavāṇijajātakaṃ aṭṭhamaṃ."
}