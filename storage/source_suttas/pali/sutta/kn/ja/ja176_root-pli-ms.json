{
  "ja176:0.1": "Jātaka",
  "ja176:0.2": "Dukanipāta",
  "ja176:0.3": "Kalyāṇavagga",
  "ja176:0.4": "6. Kaḷāyamuṭṭhijātaka",
  "ja176:1.1": "“Bālo vatāyaṃ dumasākhagocaro,",
  "ja176:1.2": "Paññā janinda nayimassa vijjati;",
  "ja176:1.3": "Kaḷāyamuṭṭhiṃ avakiriya kevalaṃ,",
  "ja176:1.4": "Ekaṃ kaḷāyaṃ patitaṃ gavesati”.",
  "ja176:2.1": "“Evameva mayaṃ rāja,",
  "ja176:2.2": "ye caññe atilobhino;",
  "ja176:2.3": "Appena bahuṃ jiyyāma,",
  "ja176:2.4": "kaḷāyeneva vānaro”ti.",
  "ja176:3.1": "Kaḷāyamuṭṭhijātakaṃ chaṭṭhaṃ."
}