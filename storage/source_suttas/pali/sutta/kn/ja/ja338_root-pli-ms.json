{
  "ja338:0.1": "Jātaka",
  "ja338:0.2": "Catukkanipāta",
  "ja338:0.3": "Kokilavagga",
  "ja338:0.4": "8. Thusajātaka",
  "ja338:1.1": "“Viditaṃ thusaṃ undurānaṃ,",
  "ja338:1.2": "Viditaṃ pana taṇḍulaṃ;",
  "ja338:1.3": "Thusaṃ thusaṃ vivajjetvā,",
  "ja338:1.4": "Taṇḍulaṃ pana khādare.",
  "ja338:2.1": "Yā mantanā araññasmiṃ,",
  "ja338:2.2": "yā ca gāme nikaṇṇikā;",
  "ja338:2.3": "Yañcetaṃ iti cīti ca,",
  "ja338:2.4": "etampi viditaṃ mayā.",
  "ja338:3.1": "Dhammena kira jātassa,",
  "ja338:3.2": "pitā puttassa makkaṭo;",
  "ja338:3.3": "Daharasseva santassa,",
  "ja338:3.4": "dantehi phalamacchidā.",
  "ja338:4.1": "Yametaṃ parisappasi,",
  "ja338:4.2": "ajakāṇova sāsape;",
  "ja338:4.3": "Yopāyaṃ heṭṭhato seti,",
  "ja338:4.4": "etampi viditaṃ mayā”ti.",
  "ja338:5.1": "Thusajātakaṃ aṭṭhamaṃ."
}