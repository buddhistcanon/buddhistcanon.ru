{
  "ja249:0.1": "Jātaka",
  "ja249:0.2": "Dukanipāta",
  "ja249:0.3": "Siṅgālavagga",
  "ja249:0.4": "9. Sālakajātaka",
  "ja249:1.1": "“Ekaputtako bhavissasi,",
  "ja249:1.2": "Tvañca no hessasi issaro kule;",
  "ja249:1.3": "Oroha dumasmā sālaka,",
  "ja249:1.4": "Ehi dāni gharakaṃ vajemase”.",
  "ja249:2.1": "“Nanu maṃ suhadayoti maññasi,",
  "ja249:2.2": "Yañca maṃ hanasi veḷuyaṭṭhiyā;",
  "ja249:2.3": "Pakkambavane ramāmase,",
  "ja249:2.4": "Gaccha tvaṃ gharakaṃ yathāsukhan”ti.",
  "ja249:3.1": "Sālakajātakaṃ navamaṃ."
}