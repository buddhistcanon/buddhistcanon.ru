{
  "ja97:0.1": "Jātaka",
  "ja97:0.2": "Ekakanipāta",
  "ja97:0.3": "Littavagga",
  "ja97:0.4": "7. Nāmasiddhijātaka",
  "ja97:1.1": "“Jīvakañca mataṃ disvā,",
  "ja97:1.2": "dhanapāliñca duggataṃ;",
  "ja97:1.3": "Panthakañca vane mūḷhaṃ,",
  "ja97:1.4": "pāpako punarāgato”ti.",
  "ja97:2.1": "Nāmasiddhijātakaṃ sattamaṃ."
}