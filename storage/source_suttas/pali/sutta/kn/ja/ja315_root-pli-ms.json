{
  "ja315:0.1": "Jātaka",
  "ja315:0.2": "Catukkanipāta",
  "ja315:0.3": "Pucimandavagga",
  "ja315:0.4": "5. Sabbamaṃsalābhajātaka",
  "ja315:1.1": "“Pharusā vata te vācā,",
  "ja315:1.2": "maṃsaṃ yācanako asi;",
  "ja315:1.3": "Kilomasadisī vācā,",
  "ja315:1.4": "kilomaṃ samma dammi te”.",
  "ja315:2.1": "“Aṅgametaṃ manussānaṃ,",
  "ja315:2.2": "bhātā loke pavuccati;",
  "ja315:2.3": "Aṅgassa sadisī vācā,",
  "ja315:2.4": "aṅgaṃ samma dadāmi te”.",
  "ja315:3.1": "“Tātāti putto vadamāno,",
  "ja315:3.2": "kampeti hadayaṃ pitu;",
  "ja315:3.3": "Hadayassa sadisī vācā,",
  "ja315:3.4": "hadayaṃ samma dammi te”.",
  "ja315:4.1": "“Yassa gāme sakhā natthi,",
  "ja315:4.2": "yathāraññaṃ tatheva taṃ;",
  "ja315:4.3": "Sabbassa sadisī vācā,",
  "ja315:4.4": "sabbaṃ samma dadāmi te”ti.",
  "ja315:5.1": "Sabbamaṃsalābhajātakaṃ pañcamaṃ."
}