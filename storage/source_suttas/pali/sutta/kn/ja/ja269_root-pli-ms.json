{
  "ja269:0.1": "Jātaka",
  "ja269:0.2": "Tikanipāta",
  "ja269:0.3": "Padumavagga",
  "ja269:0.4": "9. Sujātajātaka",
  "ja269:1.1": "“Na hi vaṇṇena sampannā,",
  "ja269:1.2": "mañjukā piyadassanā;",
  "ja269:1.3": "Kharavācā piyā hoti,",
  "ja269:1.4": "asmiṃ loke paramhi ca.",
  "ja269:2.1": "Nanu passasimaṃ kāḷiṃ,",
  "ja269:2.2": "dubbaṇṇaṃ tilakāhataṃ;",
  "ja269:2.3": "Kokilaṃ saṇhavācena,",
  "ja269:2.4": "bahūnaṃ pāṇinaṃ piyaṃ.",
  "ja269:3.1": "Tasmā sakhilavācassa,",
  "ja269:3.2": "mantabhāṇī anuddhato;",
  "ja269:3.3": "Atthaṃ dhammañca dīpeti,",
  "ja269:3.4": "madhuraṃ tassa bhāsitan”ti.",
  "ja269:4.1": "Sujātajātakaṃ navamaṃ."
}