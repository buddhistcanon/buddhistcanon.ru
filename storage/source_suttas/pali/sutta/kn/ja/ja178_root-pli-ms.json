{
  "ja178:0.1": "Jātaka",
  "ja178:0.2": "Dukanipāta",
  "ja178:0.3": "Kalyāṇavagga",
  "ja178:0.4": "8. Kacchapajātaka",
  "ja178:1.1": "“Janittaṃ me bhavittaṃ me,",
  "ja178:1.2": "iti paṅke avassayiṃ;",
  "ja178:1.3": "Taṃ maṃ paṅko ajjhabhavi,",
  "ja178:1.4": "yathā dubbalakaṃ tathā;",
  "ja178:1.5": "Taṃ taṃ vadāmi bhaggava,",
  "ja178:1.6": "suṇohi vacanaṃ mama”.",
  "ja178:2.1": "“Gāme vā yadi vā raññe,",
  "ja178:2.2": "sukhaṃ yatrādhigacchati;",
  "ja178:2.3": "Taṃ janittaṃ bhavittañca,",
  "ja178:2.4": "purisassa pajānato;",
  "ja178:2.5": "Yamhi jīve tamhi gacche,",
  "ja178:2.6": "na niketahato siyā”ti.",
  "ja178:3.1": "Kacchapajātakaṃ aṭṭhamaṃ."
}