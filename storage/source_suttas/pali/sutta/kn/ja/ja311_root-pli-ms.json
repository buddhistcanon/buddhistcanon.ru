{
  "ja311:0.1": "Jātaka",
  "ja311:0.2": "Catukkanipāta",
  "ja311:0.3": "Pucimandavagga",
  "ja311:0.4": "1. Pucimandajātaka",
  "ja311:1.1": "“Uṭṭhehi cora kiṃ sesi,",
  "ja311:1.2": "ko attho supanena te;",
  "ja311:1.3": "Mā taṃ gahesuṃ rājāno,",
  "ja311:1.4": "gāme kibbisakārakaṃ”.",
  "ja311:2.1": "“Yannu coraṃ gahessanti,",
  "ja311:2.2": "gāme kibbisakārakaṃ;",
  "ja311:2.3": "Kiṃ tattha pucimandassa,",
  "ja311:2.4": "vane jātassa tiṭṭhato”.",
  "ja311:3.1": "“Na tvaṃ assattha jānāsi,",
  "ja311:3.2": "mama corassa cantaraṃ;",
  "ja311:3.3": "Coraṃ gahetvā rājāno,",
  "ja311:3.4": "gāme kibbisakārakaṃ;",
  "ja311:3.5": "Appenti nimbasūlasmiṃ,",
  "ja311:3.6": "tasmiṃ me saṅkate mano”.",
  "ja311:4.1": "“Saṅkeyya saṅkitabbāni,",
  "ja311:4.2": "rakkheyyānāgataṃ bhayaṃ;",
  "ja311:4.3": "Anāgatabhayā dhīro,",
  "ja311:4.4": "ubho loke avekkhatī”ti.",
  "ja311:5.1": "Pucimandajātakaṃ paṭhamaṃ."
}