{
  "ja267:0.1": "Jātaka",
  "ja267:0.2": "Tikanipāta",
  "ja267:0.3": "Padumavagga",
  "ja267:0.4": "7. Kakkaṭakajātaka",
  "ja267:1.1": "“Siṅgīmigo āyatacakkhunetto,",
  "ja267:1.2": "Aṭṭhittaco vārisayo alomo;",
  "ja267:1.3": "Tenābhibhūto kapaṇaṃ rudāmi,",
  "ja267:1.4": "Mā heva maṃ pāṇasamaṃ jaheyya”.",
  "ja267:2.1": "“Ayya na taṃ jahissāmi,",
  "ja267:2.2": "kuñjaraṃ saṭṭhihāyanaṃ;",
  "ja267:2.3": "Pathabyā cāturantāya,",
  "ja267:2.4": "suppiyo hosi me tuvaṃ”.",
  "ja267:3.1": "“Ye kuḷīrā samuddasmiṃ,",
  "ja267:3.2": "gaṅgāya yamunāya ca;",
  "ja267:3.3": "Tesaṃ tvaṃ vārijo seṭṭho,",
  "ja267:3.4": "muñca rodantiyā patin”ti.",
  "ja267:4.1": "Kakkaṭakajātakaṃ sattamaṃ."
}