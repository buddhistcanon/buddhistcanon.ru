{
  "ja346:0.1": "Jātaka",
  "ja346:0.2": "Catukkanipāta",
  "ja346:0.3": "Cūḷakuṇālavagga",
  "ja346:0.4": "6. Kesavajātaka",
  "ja346:1.1": "“Manussindaṃ jahitvāna,",
  "ja346:1.2": "sabbakāmasamiddhinaṃ;",
  "ja346:1.3": "Kathaṃ nu bhagavā kesī,",
  "ja346:1.4": "kappassa ramati assame”.",
  "ja346:2.1": "“Sādūni ramaṇīyāni,",
  "ja346:2.2": "santi vakkhā manoramā;",
  "ja346:2.3": "Subhāsitāni kappassa,",
  "ja346:2.4": "nārada ramayanti maṃ”.",
  "ja346:3.1": "“Sālīnaṃ odanaṃ bhuñje,",
  "ja346:3.2": "suciṃ maṃsūpasecanaṃ;",
  "ja346:3.3": "Kathaṃ sāmākanīvāraṃ,",
  "ja346:3.4": "aloṇaṃ chādayanti taṃ”.",
  "ja346:4.1": "“Sāduṃ vā yadi vāsāduṃ,",
  "ja346:4.2": "appaṃ vā yadi vā bahuṃ;",
  "ja346:4.3": "Vissattho yattha bhuñjeyya,",
  "ja346:4.4": "vissāsaparamā rasā”ti.",
  "ja346:5.1": "Kesavajātakaṃ chaṭṭhaṃ."
}