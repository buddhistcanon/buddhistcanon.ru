{
  "ja444:0.1": "Jātaka",
  "ja444:0.2": "Dasakanipāta",
  "ja444:0.3": "Catudvāravagga",
  "ja444:0.4": "6. Kaṇhadīpāyanajātaka",
  "ja444:1.1": "“Sattāhamevāhaṃ pasannacitto,",
  "ja444:1.2": "Puññatthiko ācariṃ brahmacariyaṃ;",
  "ja444:1.3": "Athāparaṃ yaṃ caritaṃ mamedaṃ,",
  "ja444:1.4": "Vassāni paññāsa samādhikāni;",
  "ja444:1.5": "Akāmakovāpi ahaṃ carāmi,",
  "ja444:1.6": "Etena saccena suvatthi hotu;",
  "ja444:1.7": "Hataṃ visaṃ jīvatu yaññadatto”.",
  "ja444:2.1": "“Yasmā dānaṃ nābhinandiṃ kadāci,",
  "ja444:2.2": "Disvānahaṃ atithiṃ vāsakāle;",
  "ja444:2.3": "Na cāpi me appiyataṃ aveduṃ,",
  "ja444:2.4": "Bahussutā samaṇabrāhmaṇā ca”.",
  "ja444:2.5": "Akāmakovāpi ahaṃ dadāmi,",
  "ja444:2.6": "Etena saccena suvatthi hotu;",
  "ja444:2.7": "Hataṃ visaṃ jīvatu yaññadatto.",
  "ja444:3.1": "“Āsīviso tāta pahūtatejo,",
  "ja444:3.2": "Yo taṃ adaṃsī sacarā udicca;",
  "ja444:3.3": "Tasmiñca me appiyatāya ajja,",
  "ja444:3.4": "Pitari ca te natthi koci viseso;",
  "ja444:3.5": "Etena saccena suvatthi hotu,",
  "ja444:3.6": "Hataṃ visaṃ jīvatu yaññadatto”.",
  "ja444:4.1": "“Santā dantāyeva paribbajanti,",
  "ja444:4.2": "Aññatra kaṇhā natthākāmarūpā;",
  "ja444:4.3": "Dīpāyana kissa jigucchamāno,",
  "ja444:4.4": "Akāmako carasi brahmacariyaṃ”.",
  "ja444:5.1": "“Saddhāya nikkhamma punaṃ nivatto,",
  "ja444:5.2": "So eḷamūgova bālo vatāyaṃ;",
  "ja444:5.3": "Etassa vādassa jigucchamāno,",
  "ja444:5.4": "Akāmako carāmi brahmacariyaṃ;",
  "ja444:5.5": "Viññuppasatthañca satañca ṭhānaṃ,",
  "ja444:5.6": "Evampahaṃ puññakaro bhavāmi”.",
  "ja444:6.1": "“Samaṇe tuvaṃ brāhmaṇe addhike ca,",
  "ja444:6.2": "Santappayāsi annapānena bhikkhaṃ;",
  "ja444:6.3": "Opānabhūtaṃva gharaṃ tava yidaṃ,",
  "ja444:6.4": "Annena pānena upetarūpaṃ;",
  "ja444:6.5": "Atha kissa vādassa jigucchamāno,",
  "ja444:6.6": "Akāmako dānamimaṃ dadāsi”.",
  "ja444:7.1": "“Pitaro ca me āsuṃ pitāmahā ca,",
  "ja444:7.2": "Saddhā ahuṃ dānapatī vadaññū;",
  "ja444:7.3": "Taṃ kullavattaṃ anuvattamāno,",
  "ja444:7.4": "Māhaṃ kule antimagandhano ahuṃ;",
  "ja444:7.5": "Etassa vādassa jigucchamāno,",
  "ja444:7.6": "Akāmako dānamimaṃ dadāmi”.",
  "ja444:8.1": "“Dahariṃ kumāriṃ asamatthapaññaṃ,",
  "ja444:8.2": "Yaṃ tānayiṃ ñātikulā sugatte;",
  "ja444:8.3": "Na cāpi me appiyataṃ avedi,",
  "ja444:8.4": "Aññatra kāmā paricārayantā;",
  "ja444:8.5": "Atha kena vaṇṇena mayā te bhoti,",
  "ja444:8.6": "Saṃvāsadhammo ahu evarūpo”.",
  "ja444:9.1": "“Ārā dūre nayidha kadāci atthi,",
  "ja444:9.2": "Paramparā nāma kule imasmiṃ;",
  "ja444:9.3": "Taṃ kullavattaṃ anuvattamānā,",
  "ja444:9.4": "Māhaṃ kule antimagandhinī ahuṃ;",
  "ja444:9.5": "Etassa vādassa jigucchamānā,",
  "ja444:9.6": "Akāmikā paddhacarāmhi tuyhaṃ”.",
  "ja444:10.1": "“Maṇḍabya bhāsiṃ yamabhāsaneyyaṃ,",
  "ja444:10.2": "Taṃ khamyataṃ puttakahetu majja;",
  "ja444:10.3": "Puttapemā na idha paratthi kiñci,",
  "ja444:10.4": "So no ayaṃ jīvati yaññadatto”ti.",
  "ja444:11.1": "Kaṇhadīpāyanajātakaṃ chaṭṭhaṃ."
}