{
  "ja273:0.1": "Jātaka",
  "ja273:0.2": "Tikanipāta",
  "ja273:0.3": "Udapānavagga",
  "ja273:0.4": "3. Kacchapajātaka",
  "ja273:1.1": "“Ko nu uddhitabhattova,",
  "ja273:1.2": "pūrahatthova brāhmaṇo;",
  "ja273:1.3": "Kahaṃ nu bhikkhaṃ acari,",
  "ja273:1.4": "kaṃ saddhaṃ upasaṅkami”.",
  "ja273:2.1": "“Ahaṃ kapismi dummedho,",
  "ja273:2.2": "anāmāsāni āmasiṃ;",
  "ja273:2.3": "Tvaṃ maṃ mocaya bhaddaṃ te,",
  "ja273:2.4": "mutto gaccheyya pabbataṃ”.",
  "ja273:3.1": "“Kacchapā kassapā honti,",
  "ja273:3.2": "Koṇḍaññā honti makkaṭā;",
  "ja273:3.3": "Muñca kassapa koṇḍaññaṃ,",
  "ja273:3.4": "Kataṃ methunakaṃ tayā”ti.",
  "ja273:4.1": "Kacchapajātakaṃ tatiyaṃ."
}