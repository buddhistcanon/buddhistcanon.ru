{
  "ja316:0.1": "Jātaka",
  "ja316:0.2": "Catukkanipāta",
  "ja316:0.3": "Pucimandavagga",
  "ja316:0.4": "6. Sasapaṇḍitajātaka",
  "ja316:1.1": "“Satta me rohitā macchā,",
  "ja316:1.2": "udakā thalamubbhatā;",
  "ja316:1.3": "Idaṃ brāhmaṇa me atthi,",
  "ja316:1.4": "etaṃ bhutvā vane vasa”.",
  "ja316:2.1": "“Dussa me khettapālassa,",
  "ja316:2.2": "rattibhattaṃ apābhataṃ;",
  "ja316:2.3": "Maṃsasūlā ca dve godhā,",
  "ja316:2.4": "ekañca dadhivārakaṃ;",
  "ja316:2.5": "Idaṃ brāhmaṇa me atthi,",
  "ja316:2.6": "etaṃ bhutvā vane vasa”.",
  "ja316:3.1": "“Ambapakkaṃ dakaṃ sītaṃ,",
  "ja316:3.2": "Sītacchāyā manoramā;",
  "ja316:3.3": "Idaṃ brāhmaṇa me atthi,",
  "ja316:3.4": "Etaṃ bhutvā vane vasa”.",
  "ja316:4.1": "“Na sasassa tilā atthi,",
  "ja316:4.2": "Na muggā napi taṇḍulā;",
  "ja316:4.3": "Iminā agginā pakkaṃ,",
  "ja316:4.4": "Mamaṃ bhutvā vane vasā”ti.",
  "ja316:5.1": "Sasapaṇḍitajātakaṃ chaṭṭhaṃ."
}