{
  "ja190:0.1": "Jātaka",
  "ja190:0.2": "Dukanipāta",
  "ja190:0.3": "Asadisavagga",
  "ja190:0.4": "10. Sīlānisaṃsajātaka",
  "ja190:1.1": "“Passa saddhāya sīlassa,",
  "ja190:1.2": "cāgassa ca ayaṃ phalaṃ;",
  "ja190:1.3": "Nāgo nāvāya vaṇṇena,",
  "ja190:1.4": "saddhaṃ vahatupāsakaṃ.",
  "ja190:2.1": "Sabbhireva samāsetha,",
  "ja190:2.2": "sabbhi kubbetha santhavaṃ;",
  "ja190:2.3": "Satañhi sannivāsena,",
  "ja190:2.4": "sotthiṃ gacchati nhāpito”ti.",
  "ja190:3.1": "Sīlānisaṃsajātakaṃ dasamaṃ.",
  "ja190:3.2": "Asadisavaggo catuttho.",
  "ja190:4.1": "Tassuddānaṃ",
  "ja190:5.1": "Dhanuggaha kuñjara apparaso,",
  "ja190:5.2": "Giridattamanāvilacittavaraṃ;",
  "ja190:5.3": "Dadhivāhana jambūka sīhanakho,",
  "ja190:5.4": "Haritayava nāgavarena dasāti."
}