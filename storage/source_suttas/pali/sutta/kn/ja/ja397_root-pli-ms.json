{
  "ja397:0.1": "Jātaka",
  "ja397:0.2": "Sattakanipāta",
  "ja397:0.3": "Kukkuvagga",
  "ja397:0.4": "2. Manojajātaka",
  "ja397:1.1": "“Yathā cāpo ninnamati,",
  "ja397:1.2": "jiyā cāpi nikūjati;",
  "ja397:1.3": "Haññate nūna manojo,",
  "ja397:1.4": "migarājā sakhā mama.",
  "ja397:2.1": "Handa dāni vanantāni,",
  "ja397:2.2": "pakkamāmi yathāsukhaṃ;",
  "ja397:2.3": "Netādisā sakhā honti,",
  "ja397:2.4": "labbhā me jīvato sakhā”.",
  "ja397:3.1": "“Na pāpajanasaṃsevī,",
  "ja397:3.2": "accantaṃ sukhamedhati;",
  "ja397:3.3": "Manojaṃ passa semānaṃ,",
  "ja397:3.4": "giriyassānusāsanī”.",
  "ja397:4.1": "“Na pāpasampavaṅkena,",
  "ja397:4.2": "mātā puttena nandati;",
  "ja397:4.3": "Manojaṃ passa semānaṃ,",
  "ja397:4.4": "acchannaṃ samhi lohite”.",
  "ja397:5.1": "“Evamāpajjate poso,",
  "ja397:5.2": "pāpiyo ca nigacchati;",
  "ja397:5.3": "Yo ve hitānaṃ vacanaṃ,",
  "ja397:5.4": "na karoti atthadassinaṃ”.",
  "ja397:6.1": "“Evañca so hoti tato ca pāpiyo,",
  "ja397:6.2": "Yo uttamo adhamajanūpasevī;",
  "ja397:6.3": "Passuttamaṃ adhamajanūpasevitaṃ,",
  "ja397:6.4": "Migādhipaṃ saravaraveganiddhutaṃ”.",
  "ja397:7.1": "“Nihīyati puriso nihīnasevī,",
  "ja397:7.2": "Na ca hāyetha kadāci tulyasevī;",
  "ja397:7.3": "Seṭṭhamupagamaṃ udeti khippaṃ,",
  "ja397:7.4": "Tasmāttanā uttaritaraṃ bhajethā”ti.",
  "ja397:8.1": "Manojajātakaṃ dutiyaṃ."
}