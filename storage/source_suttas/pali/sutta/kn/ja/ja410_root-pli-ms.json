{
  "ja410:0.1": "Jātaka",
  "ja410:0.2": "Sattakanipāta",
  "ja410:0.3": "Gandhāravagga",
  "ja410:0.4": "5. Somadattajātaka",
  "ja410:1.1": "“Yo maṃ pure paccuḍḍeti,",
  "ja410:1.2": "araññe dūramāyato;",
  "ja410:1.3": "So na dissati mātaṅgo,",
  "ja410:1.4": "somadatto kuhiṃ gato.",
  "ja410:2.1": "Ayaṃ vā so mato seti,",
  "ja410:2.2": "allasiṅgaṃva vacchito;",
  "ja410:2.3": "Bhumyā nipatito seti,",
  "ja410:2.4": "amarā vata kuñjaro”.",
  "ja410:3.1": "“Anagāriyupetassa,",
  "ja410:3.2": "vippamuttassa te sato;",
  "ja410:3.3": "Samaṇassa na taṃ sādhu,",
  "ja410:3.4": "yaṃ petamanusocasi”.",
  "ja410:4.1": "“Saṃvāsena have sakka,",
  "ja410:4.2": "manussassa migassa vā;",
  "ja410:4.3": "Hadaye jāyate pemaṃ,",
  "ja410:4.4": "taṃ na sakkā asocituṃ”.",
  "ja410:5.1": "“Mataṃ marissaṃ rodanti,",
  "ja410:5.2": "ye rudanti lapanti ca;",
  "ja410:5.3": "Tasmā tvaṃ isi mā rodi,",
  "ja410:5.4": "roditaṃ moghamāhu santo.",
  "ja410:6.1": "Kanditena have brahme,",
  "ja410:6.2": "mato peto samuṭṭhahe;",
  "ja410:6.3": "Sabbe saṅgamma rodāma,",
  "ja410:6.4": "aññamaññassa ñātake”.",
  "ja410:7.1": "“Ādittaṃ vata maṃ santaṃ,",
  "ja410:7.2": "ghatasittaṃva pāvakaṃ;",
  "ja410:7.3": "Vārinā viya osiñcaṃ,",
  "ja410:7.4": "sabbaṃ nibbāpaye daraṃ.",
  "ja410:8.1": "Abbahī vata me sallaṃ,",
  "ja410:8.2": "yamāsi hadayassitaṃ;",
  "ja410:8.3": "Yo me sokaparetassa,",
  "ja410:8.4": "puttasokaṃ apānudi.",
  "ja410:9.1": "Sohaṃ abbūḷhasallosmi,",
  "ja410:9.2": "vītasoko anāvilo;",
  "ja410:9.3": "Na socāmi na rodāmi,",
  "ja410:9.4": "tava sutvāna vāsavā”ti.",
  "ja410:10.1": "Somadattajātakaṃ pañcamaṃ."
}