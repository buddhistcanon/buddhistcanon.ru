{
  "ja420:0.1": "Jātaka",
  "ja420:0.2": "Aṭṭhakanipāta",
  "ja420:0.3": "Kaccānivagga",
  "ja420:0.4": "4. Sumaṅgalajātaka",
  "ja420:1.1": "“Bhusamhi kuddhoti avekkhiyāna,",
  "ja420:1.2": "Na tāva daṇḍaṃ paṇayeyya issaro;",
  "ja420:1.3": "Aṭṭhānaso appatirūpamattano,",
  "ja420:1.4": "Parassa dukkhāni bhusaṃ udīraye.",
  "ja420:2.1": "Yato ca jāneyya pasādamattano,",
  "ja420:2.2": "Atthaṃ niyuñjeyya parassa dukkaṭaṃ;",
  "ja420:2.3": "Tadāyamatthoti sayaṃ avekkhiya,",
  "ja420:2.4": "Athassa daṇḍaṃ sadisaṃ nivesaye.",
  "ja420:3.1": "Na cāpi jhāpeti paraṃ na attanaṃ,",
  "ja420:3.2": "Amucchito yo nayate nayānayaṃ;",
  "ja420:3.3": "Yo daṇḍadhāro bhavatīdha issaro,",
  "ja420:3.4": "Sa vaṇṇagutto siriyā na dhaṃsati.",
  "ja420:4.1": "Ye khattiyā se anisammakārino,",
  "ja420:4.2": "Paṇenti daṇḍaṃ sahasā pamucchitā;",
  "ja420:4.3": "Avaṇṇasaṃyutā jahanti jīvitaṃ,",
  "ja420:4.4": "Ito vimuttāpi ca yanti duggatiṃ.",
  "ja420:5.1": "Dhamme ca ye ariyappavedite ratā,",
  "ja420:5.2": "Anuttarā te vacasā manasā kammunā ca;",
  "ja420:5.3": "Te santisoraccasamādhisaṇṭhitā,",
  "ja420:5.4": "Vajanti lokaṃ dubhayaṃ tathāvidhā.",
  "ja420:6.1": "Rājāhamasmi narapamadānamissaro,",
  "ja420:6.2": "Sacepi kujjhāmi ṭhapemi attanaṃ;",
  "ja420:6.3": "Nisedhayanto janataṃ tathāvidhaṃ,",
  "ja420:6.4": "Paṇemi daṇḍaṃ anukampa yoniso”.",
  "ja420:7.1": "“Sirī ca lakkhī ca taveva khattiya,",
  "ja420:7.2": "Janādhipa mā vijahi kudācanaṃ;",
  "ja420:7.3": "Akkodhano niccapasannacitto,",
  "ja420:7.4": "Anīgho tuvaṃ vassasatāni pālaya.",
  "ja420:8.1": "Guṇehi etehi upeta khattiya,",
  "ja420:8.2": "Ṭhitamariyavattī suvaco akodhano;",
  "ja420:8.3": "Sukhī anuppīḷa pasāsamediniṃ,",
  "ja420:8.4": "Ito vimuttopi ca yāhi suggatiṃ.",
  "ja420:9.1": "Evaṃ sunītena subhāsitena,",
  "ja420:9.2": "Dhammena ñāyena upāyaso nayaṃ;",
  "ja420:9.3": "Nibbāpaye saṅkhubhitaṃ mahājanaṃ,",
  "ja420:9.4": "Mahāva megho salilena medinin”ti.",
  "ja420:10.1": "Sumaṅgalajātakaṃ catutthaṃ."
}