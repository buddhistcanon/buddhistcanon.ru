{
  "ja193:0.1": "Jātaka",
  "ja193:0.2": "Dukanipāta",
  "ja193:0.3": "Ruhakavagga",
  "ja193:0.4": "3. Cūḷapadumajātaka",
  "ja193:1.1": "“Ayameva sā ahamapi so anañño,",
  "ja193:1.2": "Ayameva so hatthacchinno anañño;",
  "ja193:1.3": "Yamāha ‘komārapatī maman’ti,",
  "ja193:1.4": "Vajjhitthiyo natthi itthīsu saccaṃ.",
  "ja193:2.1": "Imañca jammaṃ musalena hantvā,",
  "ja193:2.2": "Luddaṃ chavaṃ paradārūpaseviṃ;",
  "ja193:2.3": "Imissā ca naṃ pāpapatibbatāya,",
  "ja193:2.4": "Jīvantiyā chindatha kaṇṇanāsan”ti.",
  "ja193:3.1": "Cūḷapadumajātakaṃ tatiyaṃ."
}