{
  "ja195:0.1": "Jātaka",
  "ja195:0.2": "Dukanipāta",
  "ja195:0.3": "Ruhakavagga",
  "ja195:0.4": "5. Pabbatūpattharajātaka",
  "ja195:1.1": "“Pabbatūpatthare ramme,",
  "ja195:1.2": "jātā pokkharaṇī sivā;",
  "ja195:1.3": "Taṃ siṅgālo apāpāyi,",
  "ja195:1.4": "jānaṃ sīhena rakkhitaṃ”.",
  "ja195:2.1": "“Pivanti ce mahārāja,",
  "ja195:2.2": "sāpadāni mahānadiṃ;",
  "ja195:2.3": "Na tena anadī hoti,",
  "ja195:2.4": "khamassu yadi te piyā”ti.",
  "ja195:3.1": "Pabbatūpattharajātakaṃ pañcamaṃ."
}