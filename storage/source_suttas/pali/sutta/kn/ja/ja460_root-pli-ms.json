{
  "ja460:0.1": "Jātaka",
  "ja460:0.2": "Ekādasakanipāta",
  "ja460:0.3": "Mātuposakavagga",
  "ja460:0.4": "6. Yudhañcayajātaka",
  "ja460:1.1": "“Mittāmaccaparibyūḷhaṃ,",
  "ja460:1.2": "ahaṃ vande rathesabhaṃ;",
  "ja460:1.3": "Pabbajissāmahaṃ rāja,",
  "ja460:1.4": "taṃ devo anumaññatu”.",
  "ja460:2.1": "“Sace te ūnaṃ kāmehi,",
  "ja460:2.2": "ahaṃ paripūrayāmi te;",
  "ja460:2.3": "Yo taṃ hiṃsati vāremi,",
  "ja460:2.4": "mā pabbaja yudhañcaya”.",
  "ja460:3.1": "“Na matthi ūnaṃ kāmehi,",
  "ja460:3.2": "hiṃsitā me na vijjati;",
  "ja460:3.3": "Dīpañca kātumicchāmi,",
  "ja460:3.4": "yaṃ jarā nābhikīrati”.",
  "ja460:4.1": "“Putto vā pitaraṃ yāce,",
  "ja460:4.2": "pitā vā puttamorasaṃ;",
  "ja460:4.3": "Negamo taṃ yāce tāta,",
  "ja460:4.4": "mā pabbaja yudhañcaya”.",
  "ja460:5.1": "“Mā maṃ deva nivārehi,",
  "ja460:5.2": "pabbajantaṃ rathesabha;",
  "ja460:5.3": "Māhaṃ kāmehi sammatto,",
  "ja460:5.4": "jarāya vasamanvagū”.",
  "ja460:6.1": "“Ahaṃ taṃ tāta yācāmi,",
  "ja460:6.2": "ahaṃ putta nivāraye;",
  "ja460:6.3": "Ciraṃ taṃ daṭṭhumicchāmi,",
  "ja460:6.4": "mā pabbaja yudhañcaya”.",
  "ja460:7.1": "“Ussāvova tiṇaggamhi,",
  "ja460:7.2": "sūriyuggamanaṃ pati;",
  "ja460:7.3": "Evamāyu manussānaṃ,",
  "ja460:7.4": "mā maṃ amma nivāraya”.",
  "ja460:8.1": "“Taramāno imaṃ yānaṃ,",
  "ja460:8.2": "āropetu rathesabha;",
  "ja460:8.3": "Mā me mātā tarantassa,",
  "ja460:8.4": "antarāyakarā ahu”.",
  "ja460:9.1": "“Abhidhāvatha bhaddante,",
  "ja460:9.2": "suññaṃ hessati rammakaṃ;",
  "ja460:9.3": "Yudhañcayo anuññāto,",
  "ja460:9.4": "sabbadattena rājinā.",
  "ja460:10.1": "Yohu seṭṭho sahassassa,",
  "ja460:10.2": "yuvā kañcanasannibho;",
  "ja460:10.3": "Soyaṃ kumāro pabbajito,",
  "ja460:10.4": "kāsāyavasano balī”.",
  "ja460:11.1": "“Ubho kumārā pabbajitā,",
  "ja460:11.2": "yudhañcayo yudhiṭṭhilo;",
  "ja460:11.3": "Pahāya mātāpitaro,",
  "ja460:11.4": "saṅgaṃ chetvāna maccuno”ti.",
  "ja460:12.1": "Yudhañcayajātakaṃ chaṭṭhaṃ."
}