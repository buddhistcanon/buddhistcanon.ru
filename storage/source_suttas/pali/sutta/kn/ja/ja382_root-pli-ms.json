{
  "ja382:0.1": "Jātaka",
  "ja382:0.2": "Chakkanipāta",
  "ja382:0.3": "Avāriyavagga",
  "ja382:0.4": "7. Sirikāḷakaṇṇijātaka",
  "ja382:1.1": "“Kā nu kāḷena vaṇṇena,",
  "ja382:1.2": "na cāpi piyadassanā;",
  "ja382:1.3": "Kā vā tvaṃ kassa vā dhītā,",
  "ja382:1.4": "kathaṃ jānemu taṃ mayaṃ”.",
  "ja382:2.1": "“Mahārājassahaṃ dhītā,",
  "ja382:2.2": "virūpakkhassa caṇḍiyā;",
  "ja382:2.3": "Ahaṃ kāḷī alakkhikā,",
  "ja382:2.4": "kāḷakaṇṇīti maṃ vidū;",
  "ja382:2.5": "Okāsaṃ yācito dehi,",
  "ja382:2.6": "vasemu tava santike”.",
  "ja382:3.1": "“Kiṃsīle kiṃsamācāre,",
  "ja382:3.2": "Purise nivisase tuvaṃ;",
  "ja382:3.3": "Puṭṭhā me kāḷi akkhāhi,",
  "ja382:3.4": "Kathaṃ jānemu taṃ mayaṃ”.",
  "ja382:4.1": "“Makkhī paḷāsī sārambhī,",
  "ja382:4.2": "issukī maccharī saṭho;",
  "ja382:4.3": "So mayhaṃ puriso kanto,",
  "ja382:4.4": "laddhaṃ yassa vinassati.",
  "ja382:5.1": "Kodhano upanāhī ca,",
  "ja382:5.2": "pisuṇo ca vibhedako;",
  "ja382:5.3": "Kaṇḍakavāco pharuso,",
  "ja382:5.4": "so me kantataro tato.",
  "ja382:6.1": "Ajja suveti puriso,",
  "ja382:6.2": "sadatthaṃ nāvabujjhati;",
  "ja382:6.3": "Ovajjamāno kuppati,",
  "ja382:6.4": "seyyaṃ so atimaññati.",
  "ja382:7.1": "Davappaluddho puriso,",
  "ja382:7.2": "sabbamittehi dhaṃsati;",
  "ja382:7.3": "So mayhaṃ puriso kanto,",
  "ja382:7.4": "tasmiṃ homi anāmayā”.",
  "ja382:8.1": "“Apehi etto tvaṃ kāḷi,",
  "ja382:8.2": "netaṃ amhesu vijjati;",
  "ja382:8.3": "Aññaṃ janapadaṃ gaccha,",
  "ja382:8.4": "nigame rājadhāniyo”.",
  "ja382:9.1": "“Ahampi kho taṃ jānāmi,",
  "ja382:9.2": "netaṃ tumhesu vijjati;",
  "ja382:9.3": "Santi loke alakkhikā,",
  "ja382:9.4": "saṃgharanti bahuṃ dhanaṃ;",
  "ja382:9.5": "Ahaṃ devo ca me bhātā,",
  "ja382:9.6": "ubho naṃ vidhamāmase”.",
  "ja382:10.1": "“Kā nu dibbena vaṇṇena,",
  "ja382:10.2": "pathabyā supatiṭṭhitā;",
  "ja382:10.3": "Kā vā tvaṃ kassa vā dhītā,",
  "ja382:10.4": "kathaṃ jānemu taṃ mayaṃ”.",
  "ja382:11.1": "“Mahārājassahaṃ dhītā,",
  "ja382:11.2": "dhataraṭṭhassa sirīmato;",
  "ja382:11.3": "Ahaṃ sirī ca lakkhī ca,",
  "ja382:11.4": "bhūripaññāti maṃ vidū;",
  "ja382:11.5": "Okāsaṃ yācito dehi,",
  "ja382:11.6": "vasemu tava santike”.",
  "ja382:12.1": "“Kiṃsīle kiṃsamācāre,",
  "ja382:12.2": "Purise nivisase tuvaṃ;",
  "ja382:12.3": "Puṭṭhā me lakkhi akkhāhi,",
  "ja382:12.4": "Kathaṃ jānemu taṃ mayaṃ”.",
  "ja382:13.1": "“Yo cāpi sīte atha vāpi uṇhe,",
  "ja382:13.2": "Vātātape ḍaṃsasarīsape ca;",
  "ja382:13.3": "Khudhaṃ pipāsaṃ abhibhuyya sabbaṃ,",
  "ja382:13.4": "Rattindivaṃ yo satataṃ niyutto.",
  "ja382:14.1": "Kālāgatañca na hāpeti atthaṃ,",
  "ja382:14.2": "So me manāpo nivise ca tamhi;",
  "ja382:14.3": "Akkodhano mittavā cāgavā ca,",
  "ja382:14.4": "Sīlūpapanno asaṭhojubhūto.",
  "ja382:15.1": "Saṅgāhako sakhilo saṇhavāco,",
  "ja382:15.2": "Mahattapattopi nivātavutti;",
  "ja382:15.3": "Tasmiṃhaṃ pose vipulā bhavāmi,",
  "ja382:15.4": "Ūmi samuddassa yathāpi vaṇṇaṃ.",
  "ja382:16.1": "Yo cāpi mitte atha vā amitte,",
  "ja382:16.2": "Seṭṭhe sarikkhe atha vāpi hīne;",
  "ja382:16.3": "Atthaṃ carantaṃ atha vā anatthaṃ,",
  "ja382:16.4": "Āvī raho saṅgahameva vatte.",
  "ja382:17.1": "Vācaṃ na vajjā pharusaṃ kadāci,",
  "ja382:17.2": "Matassa jīvassa ca tassa homi;",
  "ja382:17.3": "Etesaṃ yo aññataraṃ labhitvā,",
  "ja382:17.4": "Kantā sirī majjati appapañño;",
  "ja382:17.5": "Taṃ dittarūpaṃ visamaṃ carantaṃ,",
  "ja382:17.6": "Karīsaṭhānaṃva vivajjayāmi.",
  "ja382:18.1": "Attanā kurute lakkhiṃ,",
  "ja382:18.2": "alakkhiṃ kurutattanā;",
  "ja382:18.3": "Na hi lakkhiṃ alakkhiṃ vā,",
  "ja382:18.4": "añño aññassa kārako”ti.",
  "ja382:19.1": "Sirikāḷakaṇṇijātakaṃ sattamaṃ."
}