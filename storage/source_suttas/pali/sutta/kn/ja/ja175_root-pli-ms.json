{
  "ja175:0.1": "Jātaka",
  "ja175:0.2": "Dukanipāta",
  "ja175:0.3": "Kalyāṇavagga",
  "ja175:0.4": "5. Ādiccupaṭṭhānajātaka",
  "ja175:1.1": "“Sabbesu kira bhūtesu,",
  "ja175:1.2": "santi sīlasamāhitā;",
  "ja175:1.3": "Passa sākhāmigaṃ jammaṃ,",
  "ja175:1.4": "ādiccamupatiṭṭhati”.",
  "ja175:2.1": "“Nāssa sīlaṃ vijānātha,",
  "ja175:2.2": "anaññāya pasaṃsatha;",
  "ja175:2.3": "Aggihuttañca uhannaṃ,",
  "ja175:2.4": "dve ca bhinnā kamaṇḍalū”ti.",
  "ja175:3.1": "Ādiccupaṭṭhānajātakaṃ pañcamaṃ."
}