{
  "ja236:0.1": "Jātaka",
  "ja236:0.2": "Dukanipāta",
  "ja236:0.3": "Upāhanavagga",
  "ja236:0.4": "6. Bakajātaka",
  "ja236:1.1": "“Bhaddako vatayaṃ pakkhī,",
  "ja236:1.2": "dijo kumudasannibho;",
  "ja236:1.3": "Vūpasantehi pakkhehi,",
  "ja236:1.4": "mandamandova jhāyati”.",
  "ja236:2.1": "“Nāssa sīlaṃ vijānātha,",
  "ja236:2.2": "anaññāya pasaṃsatha;",
  "ja236:2.3": "Amhe dijo na pāleti,",
  "ja236:2.4": "tena pakkhī na phandatī”ti.",
  "ja236:3.1": "Bakajātakaṃ chaṭṭhaṃ."
}