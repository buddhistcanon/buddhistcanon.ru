{
  "ja366:0.1": "Jātaka",
  "ja366:0.2": "Pañcakanipāta",
  "ja366:0.3": "Vaṇṇārohavagga",
  "ja366:0.4": "6. Gumbiyajātaka",
  "ja366:1.1": "“Madhuvaṇṇaṃ madhurasaṃ,",
  "ja366:1.2": "madhugandhaṃ visaṃ ahu;",
  "ja366:1.3": "Gumbiyo ghāsamesāno,",
  "ja366:1.4": "araññe odahī visaṃ.",
  "ja366:2.1": "Madhu iti maññamānā,",
  "ja366:2.2": "ye taṃ visamakhādisuṃ;",
  "ja366:2.3": "Tesaṃ taṃ kaṭukaṃ āsi,",
  "ja366:2.4": "maraṇaṃ tenupāgamuṃ.",
  "ja366:3.1": "Ye ca kho paṭisaṅkhāya,",
  "ja366:3.2": "visaṃ taṃ parivajjayuṃ;",
  "ja366:3.3": "Te āturesu sukhitā,",
  "ja366:3.4": "ḍayhamānesu nibbutā.",
  "ja366:4.1": "Evameva manussesu,",
  "ja366:4.2": "visaṃ kāmā samohitā;",
  "ja366:4.3": "Āmisaṃ bandhanañcetaṃ,",
  "ja366:4.4": "maccuveso guhāsayo.",
  "ja366:5.1": "Evameva ime kāme,",
  "ja366:5.2": "āturā paricārike;",
  "ja366:5.3": "Ye sadā parivajjenti,",
  "ja366:5.4": "saṅgaṃ loke upaccagun”ti.",
  "ja366:6.1": "Gumbiyajātakaṃ chaṭṭhaṃ."
}