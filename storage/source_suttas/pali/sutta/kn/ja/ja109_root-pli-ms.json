{
  "ja109:0.1": "Jātaka",
  "ja109:0.2": "Ekakanipāta",
  "ja109:0.3": "Parosatavagga",
  "ja109:0.4": "9. Kuṇḍapūvajātaka",
  "ja109:1.1": "“Yathanno puriso hoti,",
  "ja109:1.2": "tathannā tassa devatā;",
  "ja109:1.3": "Āharetaṃ kuṇḍapūvaṃ,",
  "ja109:1.4": "mā me bhāgaṃ vināsayā”ti.",
  "ja109:2.1": "Kuṇḍapūvajātakaṃ navamaṃ."
}