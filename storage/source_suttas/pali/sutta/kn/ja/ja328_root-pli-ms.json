{
  "ja328:0.1": "Jātaka",
  "ja328:0.2": "Catukkanipāta",
  "ja328:0.3": "Kuṭidūsakavagga",
  "ja328:0.4": "8. Ananusociyajātaka",
  "ja328:1.1": "“Bahūnaṃ vijjatī bhotī,",
  "ja328:1.2": "tehi me kiṃ bhavissati;",
  "ja328:1.3": "Tasmā etaṃ na socāmi,",
  "ja328:1.4": "piyaṃ sammillahāsiniṃ.",
  "ja328:2.1": "Taṃ tañce anusoceyya,",
  "ja328:2.2": "yaṃ yaṃ tassa na vijjati;",
  "ja328:2.3": "Attānamanusoceyya,",
  "ja328:2.4": "sadā maccuvasaṃ pataṃ.",
  "ja328:3.1": "Na heva ṭhitaṃ nāsīnaṃ,",
  "ja328:3.2": "Na sayānaṃ na paddhaguṃ;",
  "ja328:3.3": "Yāva byāti nimisati,",
  "ja328:3.4": "Tatrāpi rasatī vayo.",
  "ja328:4.1": "Tatthattani vatappaddhe,",
  "ja328:4.2": "vinābhāve asaṃsaye;",
  "ja328:4.3": "Bhūtaṃ sesaṃ dayitabbaṃ,",
  "ja328:4.4": "vītaṃ ananusociyan”ti.",
  "ja328:5.1": "Ananusociyajātakaṃ aṭṭhamaṃ."
}