{
  "ja53:0.1": "Jātaka",
  "ja53:0.2": "Ekakanipāta",
  "ja53:0.3": "Āsīsavagga",
  "ja53:0.4": "3. Puṇṇapātijātaka",
  "ja53:1.1": "“Tatheva puṇṇā pātiyo,",
  "ja53:1.2": "aññāyaṃ vattate kathā;",
  "ja53:1.3": "Ākāraṇena jānāmi,",
  "ja53:1.4": "na cāyaṃ bhaddikā surā”ti.",
  "ja53:2.1": "Puṇṇapātijātakaṃ tatiyaṃ."
}