{
  "ja514:0.1": "Jātaka",
  "ja514:0.2": "Tiṃsanipāta",
  "ja514:0.3": "Kiṃchandavagga",
  "ja514:0.4": "4. Chaddantajātaka",
  "ja514:1.1": "“Kiṃ nu socasinuccaṅgi,",
  "ja514:1.2": "paṇḍūsi varavaṇṇini;",
  "ja514:1.3": "Milāyasi visālakkhi,",
  "ja514:1.4": "mālāva parimadditā”.",
  "ja514:2.1": "“Dohaḷo me mahārāja,",
  "ja514:2.2": "supinantenupajjhagā;",
  "ja514:2.3": "Na so sulabharūpova,",
  "ja514:2.4": "yādiso mama dohaḷo”.",
  "ja514:3.1": "“Ye keci mānusā kāmā,",
  "ja514:3.2": "idha lokasmi nandane;",
  "ja514:3.3": "Sabbe te pacurā mayhaṃ,",
  "ja514:3.4": "ahaṃ te dammi dohaḷaṃ”.",
  "ja514:4.1": "“Luddā deva samāyantu,",
  "ja514:4.2": "ye keci vijite tava;",
  "ja514:4.3": "Etesaṃ ahamakkhissaṃ,",
  "ja514:4.4": "yādiso mama dohaḷo”.",
  "ja514:5.1": "“Ime te luddakā devi,",
  "ja514:5.2": "katahatthā visāradā;",
  "ja514:5.3": "Vanaññū ca migaññū ca,",
  "ja514:5.4": "mamatthe cattajīvitā”.",
  "ja514:6.1": "“Luddaputtā nisāmetha,",
  "ja514:6.2": "yāvantettha samāgatā;",
  "ja514:6.3": "Chabbisāṇaṃ gajaṃ setaṃ,",
  "ja514:6.4": "addasaṃ supine ahaṃ;",
  "ja514:6.5": "Tassa dantehi me attho,",
  "ja514:6.6": "alābhe natthi jīvitaṃ”.",
  "ja514:7.1": "“Na no pitūnaṃ na pitāmahānaṃ,",
  "ja514:7.2": "Diṭṭho suto kuñjaro chabbisāṇo;",
  "ja514:7.3": "Yamaddasā supine rājaputtī,",
  "ja514:7.4": "Akkhāhi no yādiso hatthināgo”.",
  "ja514:8.1": "“Disā catasso vidisā catasso,",
  "ja514:8.2": "Uddhaṃ adho dasa disā imāyo;",
  "ja514:8.3": "Katamaṃ disaṃ tiṭṭhati nāgarājā,",
  "ja514:8.4": "Yamaddasā supine chabbisāṇaṃ”.",
  "ja514:9.1": "“Ito ujuṃ uttariyaṃ disāyaṃ,",
  "ja514:9.2": "Atikkamma so sattagirī brahante;",
  "ja514:9.3": "Suvaṇṇapasso nāma girī uḷāro,",
  "ja514:9.4": "Supupphito kimpurisānuciṇṇo.",
  "ja514:10.1": "Āruyha selaṃ bhavanaṃ kinnarānaṃ,",
  "ja514:10.2": "Olokaya pabbatapādamūlaṃ;",
  "ja514:10.3": "Atha dakkhasī meghasamānavaṇṇaṃ,",
  "ja514:10.4": "Nigrodharājaṃ aṭṭhasahassapādaṃ.",
  "ja514:11.1": "Tatthacchatī kuñjaro chabbisāṇo,",
  "ja514:11.2": "Sabbaseto duppasaho parebhi;",
  "ja514:11.3": "Rakkhanti naṃ aṭṭhasahassanāgā,",
  "ja514:11.4": "Īsādantā vātajavappahārino.",
  "ja514:12.1": "Tiṭṭhanti te tumūlaṃ passasantā,",
  "ja514:12.2": "Kuppanti vātassapi eritassa;",
  "ja514:12.3": "Manussabhūtaṃ pana tattha disvā,",
  "ja514:12.4": "Bhasmaṃ kareyyuṃ nāssa rajopi tassa”.",
  "ja514:13.1": "“Bahū hime rājakulamhi santi,",
  "ja514:13.2": "Piḷandhanā jātarūpassa devī;",
  "ja514:13.3": "Muttā maṇī veḷuriyāmayā ca,",
  "ja514:13.4": "Kiṃ kāhasi dantapiḷandhanena;",
  "ja514:13.5": "Māretukāmā kuñjaraṃ chabbisāṇaṃ,",
  "ja514:13.6": "Udāhu ghātessasi luddaputte”.",
  "ja514:14.1": "“Sā issitā dukkhitā casmi ludda,",
  "ja514:14.2": "Uddhañca sussāmi anussarantī;",
  "ja514:14.3": "Karohi me luddaka etamatthaṃ,",
  "ja514:14.4": "Dassāmi te gāmavarāni pañca”.",
  "ja514:15.1": "“Katthacchatī kattha mupeti ṭhānaṃ,",
  "ja514:15.2": "Vīthissa kā nhāna gatassa hoti;",
  "ja514:15.3": "Kathañhi so nhāyati nāgarājā,",
  "ja514:15.4": "Kathaṃ vijānemu gatiṃ gajassa”.",
  "ja514:16.1": "“Tattheva sā pokkharaṇī adūre,",
  "ja514:16.2": "Rammā sutitthā ca mahodikā ca;",
  "ja514:16.3": "Sampupphitā bhamaragaṇānuciṇṇā,",
  "ja514:16.4": "Ettha hi so nhāyati nāgarājā.",
  "ja514:17.1": "Sīsaṃ nahātuppalamālabhārī,",
  "ja514:17.2": "Sabbaseto puṇḍarīkattacaṅgī;",
  "ja514:17.3": "Āmodamāno gacchati sanniketaṃ,",
  "ja514:17.4": "Purakkhatvā mahesiṃ sabbabhaddaṃ”.",
  "ja514:18.1": "Tattheva so uggahetvāna vākyaṃ,",
  "ja514:18.2": "Ādāya tūṇiñca dhanuñca luddo;",
  "ja514:18.3": "Vituriyati sattagirī brahante,",
  "ja514:18.4": "Suvaṇṇapassaṃ nāma giriṃ uḷāraṃ.",
  "ja514:19.1": "Āruyha selaṃ bhavanaṃ kinnarānaṃ,",
  "ja514:19.2": "Olokayī pabbatapādamūlaṃ;",
  "ja514:19.3": "Tatthaddasā meghasamānavaṇṇaṃ,",
  "ja514:19.4": "Nigrodharājaṃ aṭṭhasahassapādaṃ.",
  "ja514:20.1": "Tatthaddasā kuñjaraṃ chabbisāṇaṃ,",
  "ja514:20.2": "Sabbasetaṃ duppasahaṃ parebhi;",
  "ja514:20.3": "Rakkhanti naṃ aṭṭhasahassanāgā,",
  "ja514:20.4": "Īsādantā vātajavappahārino.",
  "ja514:21.1": "Tatthaddasā pokkharaṇiṃ adūre,",
  "ja514:21.2": "Rammaṃ sutitthañca mahodikañca;",
  "ja514:21.3": "Sampupphitaṃ bhamaragaṇānuciṇṇaṃ,",
  "ja514:21.4": "Yattha hi so nhāyati nāgarājā.",
  "ja514:22.1": "Disvāna nāgassa gatiṃ ṭhitiñca,",
  "ja514:22.2": "Vīthissa yā nhānagatassa hoti;",
  "ja514:22.3": "Opātamāgacchi anariyarūpo,",
  "ja514:22.4": "Payojito cittavasānugāya.",
  "ja514:23.1": "Khaṇitvāna kāsuṃ phalakehi chādayi,",
  "ja514:23.2": "Attānamodhāya dhanuñca luddo;",
  "ja514:23.3": "Passāgataṃ puthusallena nāgaṃ,",
  "ja514:23.4": "Samappayī dukkaṭakammakārī.",
  "ja514:24.1": "Viddho ca nāgo koñcamanādi ghoraṃ,",
  "ja514:24.2": "Sabbe ca nāgā ninnaduṃ ghorarūpaṃ;",
  "ja514:24.3": "Tiṇañca kaṭṭhañca raṇaṃ karontā,",
  "ja514:24.4": "Dhāviṃsu te aṭṭha disā samantato.",
  "ja514:25.1": "Vadhissametanti parāmasanto,",
  "ja514:25.2": "Kāsāvamaddakkhi dhajaṃ isīnaṃ;",
  "ja514:25.3": "Dukkhena phuṭṭhassudapādi saññā,",
  "ja514:25.4": "Arahaddhajo sabbhi avajjharūpo.",
  "ja514:26.1": "“Anikkasāvo kāsāvaṃ,",
  "ja514:26.2": "yo vatthaṃ paridahissati;",
  "ja514:26.3": "Apeto damasaccena,",
  "ja514:26.4": "na so kāsāvamarahati.",
  "ja514:27.1": "Yo ca vantakasāvassa,",
  "ja514:27.2": "sīlesu susamāhito;",
  "ja514:27.3": "Upeto damasaccena,",
  "ja514:27.4": "sa ve kāsāvamarahati”.",
  "ja514:28.1": "Samappito puthusallena nāgo,",
  "ja514:28.2": "Aduṭṭhacitto luddakamajjhabhāsi;",
  "ja514:28.3": "“Kimatthayaṃ kissa vā samma hetu,",
  "ja514:28.4": "Mamaṃ vadhī kassa vāyaṃ payogo”.",
  "ja514:29.1": "“Kāsissa rañño mahesī bhadante,",
  "ja514:29.2": "Sā pūjitā rājakule subhaddā;",
  "ja514:29.3": "Taṃ addasā sā ca mamaṃ asaṃsi,",
  "ja514:29.4": "Dantehi atthoti ca maṃ avoca”.",
  "ja514:30.1": "“Bahū hi me dantayugā uḷārā,",
  "ja514:30.2": "Ye me pitūnañca pitāmahānaṃ;",
  "ja514:30.3": "Jānāti sā kodhanā rājaputtī,",
  "ja514:30.4": "Vadhatthikā veramakāsi bālā.",
  "ja514:31.1": "Uṭṭhehi tvaṃ ludda kharaṃ gahetvā,",
  "ja514:31.2": "Dante ime chinda purā marāmi;",
  "ja514:31.3": "Vajjāsi taṃ kodhanaṃ rājaputtiṃ,",
  "ja514:31.4": "‘Nāgo hato handa imassa dantā’”.",
  "ja514:32.1": "Uṭṭhāya so luddo kharaṃ gahetvā,",
  "ja514:32.2": "Chetvāna dantāni gajuttamassa;",
  "ja514:32.3": "Vaggū subhe appaṭime pathabyā,",
  "ja514:32.4": "Ādāya pakkāmi tato hi khippaṃ.",
  "ja514:33.1": "Bhayaṭṭitā nāgavadhena aṭṭā,",
  "ja514:33.2": "Ye te nāgā aṭṭha disā vidhāvuṃ;",
  "ja514:33.3": "Adisvāna posaṃ gajapaccamittaṃ,",
  "ja514:33.4": "Paccāgamuṃ yena so nāgarājā.",
  "ja514:34.1": "Te tattha kanditvā roditvāna nāgā,",
  "ja514:34.2": "Sīse sake paṃsukaṃ okiritvā;",
  "ja514:34.3": "Agamaṃsu te sabbe sakaṃ niketaṃ,",
  "ja514:34.4": "Purakkhatvā mahesiṃ sabbabhaddaṃ.",
  "ja514:35.1": "Ādāya dantāni gajuttamassa,",
  "ja514:35.2": "Vaggū subhe appaṭime pathabyā;",
  "ja514:35.3": "Suvaṇṇarājīhi samantamodare,",
  "ja514:35.4": "So luddako kāsipuraṃ upāgami;",
  "ja514:35.5": "Upanesi so rājakaññāya dante,",
  "ja514:35.6": "Nāgo hato handa imassa dantā.",
  "ja514:36.1": "Disvāna dantāni gajuttamassa,",
  "ja514:36.2": "Bhattuppiyassa purimāya jātiyā;",
  "ja514:36.3": "Tattheva tassā hadayaṃ aphāli,",
  "ja514:36.4": "Teneva sā kālamakāsi bālā.",
  "ja514:37.1": "Sambodhipatto sa mahānubhāvo,",
  "ja514:37.2": "Sitaṃ akāsi parisāya majjhe;",
  "ja514:37.3": "Pucchiṃsu bhikkhū suvimuttacittā,",
  "ja514:37.4": "“Nākāraṇe pātukaronti buddhā.",
  "ja514:38.1": "Yamaddasātha dahariṃ kumāriṃ,",
  "ja514:38.2": "Kāsāyavatthaṃ anagāriyaṃ carantiṃ;",
  "ja514:38.3": "Sā kho tadā rājakaññā ahosi,",
  "ja514:38.4": "Ahaṃ tadā nāgarājā ahosiṃ.",
  "ja514:39.1": "Ādāya dantāni gajuttamassa,",
  "ja514:39.2": "Vaggū subhe appaṭime pathabyā;",
  "ja514:39.3": "Yo luddako kāsipuraṃ upāgami,",
  "ja514:39.4": "So kho tadā devadatto ahosi”.",
  "ja514:40.1": "Anāvasūraṃ cirarattasaṃsitaṃ,",
  "ja514:40.2": "Uccāvacaṃ caritamidaṃ purāṇaṃ;",
  "ja514:40.3": "Vītaddaro vītasoko visallo,",
  "ja514:40.4": "Sayaṃ abhiññāya abhāsi buddho.",
  "ja514:41.1": "“Ahaṃ vo tena kālena,",
  "ja514:41.2": "ahosiṃ tattha bhikkhavo;",
  "ja514:41.3": "Nāgarājā tadā homi,",
  "ja514:41.4": "evaṃ dhāretha jātakan”ti.",
  "ja514:42.1": "Chaddantajātakaṃ catutthaṃ."
}