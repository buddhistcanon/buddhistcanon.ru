{
  "ja51:0.1": "Jātaka",
  "ja51:0.2": "Ekakanipāta",
  "ja51:0.3": "Āsīsavagga",
  "ja51:0.4": "1. Mahāsīlavajātaka",
  "ja51:1.1": "“Āsīsetheva puriso,",
  "ja51:1.2": "na nibbindeyya paṇḍito;",
  "ja51:1.3": "Passāmi vohaṃ attānaṃ,",
  "ja51:1.4": "yathā icchiṃ tathā ahū”ti.",
  "ja51:2.1": "Mahāsīlavajātakaṃ paṭhamaṃ."
}