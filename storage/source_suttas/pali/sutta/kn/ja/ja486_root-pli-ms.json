{
  "ja486:0.1": "Jātaka",
  "ja486:0.2": "Pakiṇṇakanipāta",
  "ja486:0.3": "Sālikedāravagga",
  "ja486:0.4": "3. Mahāukkusajātaka",
  "ja486:1.1": "“Ukkā cilācā bandhanti dīpe,",
  "ja486:1.2": "Pajā mamaṃ khādituṃ patthayanti;",
  "ja486:1.3": "Mittaṃ sahāyañca vadehi senaka,",
  "ja486:1.4": "Ācikkha ñātibyasanaṃ dijānaṃ”.",
  "ja486:2.1": "“Dijo dijānaṃ pavarosi pakkhima,",
  "ja486:2.2": "Ukkusarāja saraṇaṃ taṃ upema;",
  "ja486:2.3": "Pajā mamaṃ khādituṃ patthayanti,",
  "ja486:2.4": "Luddā cilācā bhava me sukhāya”.",
  "ja486:3.1": "“Mittaṃ sahāyañca karonti paṇḍitā,",
  "ja486:3.2": "Kāle akāle sukhamesamānā;",
  "ja486:3.3": "Karomi te senaka etamatthaṃ,",
  "ja486:3.4": "Ariyo hi ariyassa karoti kiccaṃ”.",
  "ja486:4.1": "“Yaṃ hoti kiccaṃ anukampakena,",
  "ja486:4.2": "Ariyassa ariyena kataṃ tayīdaṃ;",
  "ja486:4.3": "Attānurakkhī bhava mā adayhi,",
  "ja486:4.4": "Lacchāma putte tayi jīvamāne”.",
  "ja486:5.1": "“Taveva rakkhāvaraṇaṃ karonto,",
  "ja486:5.2": "Sarīrabhedāpi na santasāmi;",
  "ja486:5.3": "Karonti heke sakhīnaṃ sakhāro,",
  "ja486:5.4": "Pāṇaṃ cajantā satamesa dhammo”.",
  "ja486:6.1": "“Sudukkaraṃ kammamakāsi,",
  "ja486:6.2": "aṇḍajāyaṃ vihaṅgamo;",
  "ja486:6.3": "Atthāya kuraro putte,",
  "ja486:6.4": "aḍḍharatte anāgate”.",
  "ja486:7.1": "“Cutāpi heke khalitā sakammunā,",
  "ja486:7.2": "Mittānukampāya patiṭṭhahanti;",
  "ja486:7.3": "Puttā mamaṭṭā gatimāgatosmi,",
  "ja486:7.4": "Atthaṃ caretho mama vāricara”.",
  "ja486:8.1": "“Dhanena dhaññena ca attanā ca,",
  "ja486:8.2": "Mittaṃ sahāyañca karonti paṇḍitā;",
  "ja486:8.3": "Karomi te senaka etamatthaṃ,",
  "ja486:8.4": "Ariyo hi ariyassa karoti kiccaṃ”.",
  "ja486:9.1": "“Appossukko tāta tuvaṃ nisīda,",
  "ja486:9.2": "Putto pitu carati atthacariyaṃ;",
  "ja486:9.3": "Ahaṃ carissāmi tavetamatthaṃ,",
  "ja486:9.4": "Senassa putte paritāyamāno”.",
  "ja486:10.1": "“Addhā hi tāta satamesa dhammo,",
  "ja486:10.2": "Putto pitu yaṃ care atthacariyaṃ;",
  "ja486:10.3": "Appeva maṃ disvāna pavaḍḍhakāyaṃ,",
  "ja486:10.4": "Senassa puttā na viheṭhayeyyuṃ”.",
  "ja486:11.1": "“Pasū manussā migavīraseṭṭha,",
  "ja486:11.2": "Bhayaṭṭitā seṭṭhamupabbajanti;",
  "ja486:11.3": "Puttā mamaṭṭā gatimāgatosmi,",
  "ja486:11.4": "Tvaṃ nosi rājā bhava me sukhāya”.",
  "ja486:12.1": "“Karomi te senaka etamatthaṃ,",
  "ja486:12.2": "Āyāmi te taṃ disataṃ vadhāya;",
  "ja486:12.3": "Kathañhi viññū pahu sampajāno,",
  "ja486:12.4": "Na vāyame attajanassa guttiyā”.",
  "ja486:13.1": "“Mittañca kayirātha suhadayañca,",
  "ja486:13.2": "Ayirañca kayirātha sukhāgamāya;",
  "ja486:13.3": "Nivatthakocova sarebhihantvā,",
  "ja486:13.4": "Modāma puttehi samaṅgibhūtā.",
  "ja486:14.1": "Sakamittassa kammena,",
  "ja486:14.2": "sahāyassāpalāyino;",
  "ja486:14.3": "Kūjantamupakūjanti,",
  "ja486:14.4": "lomasā hadayaṅgamaṃ.",
  "ja486:15.1": "Mittaṃ sahāyaṃ adhigamma paṇḍito,",
  "ja486:15.2": "So bhuñjatī putta pasuṃ dhanaṃ vā;",
  "ja486:15.3": "Ahañca puttā ca patī ca mayhaṃ,",
  "ja486:15.4": "Mittānukampāya samaṅgibhūtā.",
  "ja486:16.1": "Rājavatā sūravatā ca attho,",
  "ja486:16.2": "Sampannasakhissa bhavanti hete;",
  "ja486:16.3": "So mittavā yasavā uggatatto,",
  "ja486:16.4": "Asmiṃdhaloke modati kāmakāmī.",
  "ja486:17.1": "Karaṇīyāni mittāni,",
  "ja486:17.2": "daliddenāpi senaka;",
  "ja486:17.3": "Passa mittānukampāya,",
  "ja486:17.4": "samaggamhā sañātake.",
  "ja486:18.1": "Sūrena balavantena,",
  "ja486:18.2": "yo mitte kurute dijo;",
  "ja486:18.3": "Evaṃ so sukhito hoti,",
  "ja486:18.4": "yathāhaṃ tvañca senakāti”.",
  "ja486:19.1": "Mahāukkusajātakaṃ tatiyaṃ."
}