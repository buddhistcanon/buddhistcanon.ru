{
  "ja352:0.1": "Jātaka",
  "ja352:0.2": "Pañcakanipāta",
  "ja352:0.3": "Maṇikuṇḍalavagga",
  "ja352:0.4": "2. Sujātajātaka",
  "ja352:1.1": "“Kiṃ nu santaramānova,",
  "ja352:1.2": "lāyitvā haritaṃ tiṇaṃ;",
  "ja352:1.3": "Khāda khādāti lapasi,",
  "ja352:1.4": "gatasattaṃ jaraggavaṃ.",
  "ja352:2.1": "Na hi annena pānena,",
  "ja352:2.2": "mato goṇo samuṭṭhahe;",
  "ja352:2.3": "Tvañca tucchaṃ vilapasi,",
  "ja352:2.4": "yathā taṃ dummatī tathā”.",
  "ja352:3.1": "“Tatheva tiṭṭhati sīsaṃ,",
  "ja352:3.2": "hatthapādā ca vāladhi;",
  "ja352:3.3": "Sotā tatheva tiṭṭhanti,",
  "ja352:3.4": "maññe goṇo samuṭṭhahe.",
  "ja352:4.1": "Nevayyakassa sīsañca,",
  "ja352:4.2": "hatthapādā ca dissare;",
  "ja352:4.3": "Rudaṃ mattikathūpasmiṃ,",
  "ja352:4.4": "nanu tvaññeva dummati”.",
  "ja352:5.1": "“Ādittaṃ vata maṃ santaṃ,",
  "ja352:5.2": "ghatasittaṃva pāvakaṃ;",
  "ja352:5.3": "Vārinā viya osiñcaṃ,",
  "ja352:5.4": "sabbaṃ nibbāpaye daraṃ.",
  "ja352:6.1": "Abbahī vata me sallaṃ,",
  "ja352:6.2": "yamāsi hadayassitaṃ;",
  "ja352:6.3": "Yo me sokaparetassa,",
  "ja352:6.4": "pitusokaṃ apānudi.",
  "ja352:7.1": "Sohaṃ abbūḷhasallosmi,",
  "ja352:7.2": "vītasoko anāvilo;",
  "ja352:7.3": "Na socāmi na rodāmi,",
  "ja352:7.4": "tava sutvāna māṇava.",
  "ja352:8.1": "Evaṃ karonti sappaññā,",
  "ja352:8.2": "ye honti anukampakā;",
  "ja352:8.3": "Vinivattenti sokamhā,",
  "ja352:8.4": "sujāto pitaraṃ yathā”ti.",
  "ja352:9.1": "Sujātajātakaṃ dutiyaṃ."
}