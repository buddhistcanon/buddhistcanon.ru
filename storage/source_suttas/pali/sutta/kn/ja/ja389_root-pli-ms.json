{
  "ja389:0.1": "Jātaka",
  "ja389:0.2": "Chakkanipāta",
  "ja389:0.3": "Kharaputtavagga",
  "ja389:0.4": "4. Suvaṇṇakakkaṭajātaka",
  "ja389:1.1": "“Siṅgīmigo āyatacakkhunetto,",
  "ja389:1.2": "Aṭṭhittaco vārisayo alomo;",
  "ja389:1.3": "Tenābhibhūto kapaṇaṃ rudāmi,",
  "ja389:1.4": "Hare sakhā kissa nu maṃ jahāsi”.",
  "ja389:2.1": "So passasanto mahatā phaṇena,",
  "ja389:2.2": "Bhujaṅgamo kakkaṭamajjhapatto;",
  "ja389:2.3": "Sakhā sakhāraṃ paritāyamāno,",
  "ja389:2.4": "Bhujaṅgamaṃ kakkaṭako gahesi.",
  "ja389:3.1": "“Na vāyasaṃ no pana kaṇhasappaṃ,",
  "ja389:3.2": "Ghāsatthiko kakkaṭako adeyya;",
  "ja389:3.3": "Pucchāmi taṃ āyatacakkhunetta,",
  "ja389:3.4": "Atha kissa hetumha ubho gahītā”.",
  "ja389:4.1": "“Ayaṃ puriso mama atthakāmo,",
  "ja389:4.2": "Yo maṃ gahetvāna dakāya neti;",
  "ja389:4.3": "Tasmiṃ mate dukkhamanappakaṃ me,",
  "ja389:4.4": "Ahañca eso ca ubho na homa.",
  "ja389:5.1": "Mamañca disvāna pavaddhakāyaṃ,",
  "ja389:5.2": "Sabbo jano hiṃsitumeva micche;",
  "ja389:5.3": "Sāduñca thūlañca muduñca maṃsaṃ,",
  "ja389:5.4": "Kākāpi maṃ disvāna viheṭhayeyyuṃ”.",
  "ja389:6.1": "“Sacetassa hetumha ubho gahītā,",
  "ja389:6.2": "Uṭṭhātu poso visamāvamāmi;",
  "ja389:6.3": "Mamañca kākañca pamuñca khippaṃ,",
  "ja389:6.4": "Pure visaṃ gāḷhamupeti maccaṃ”.",
  "ja389:7.1": "“Sappaṃ pamokkhāmi na tāva kākaṃ,",
  "ja389:7.2": "Paṭibandhako hohiti tāva kāko;",
  "ja389:7.3": "Purisañca disvāna sukhiṃ arogaṃ,",
  "ja389:7.4": "Kākaṃ pamokkhāmi yatheva sappaṃ”.",
  "ja389:8.1": "“Kāko tadā devadatto ahosi,",
  "ja389:8.2": "Māro pana kaṇhasappo ahosi;",
  "ja389:8.3": "Ānandabhaddo kakkaṭako ahosi,",
  "ja389:8.4": "Ahaṃ tadā brāhmaṇo homi satthā”ti.",
  "ja389:9.1": "Suvaṇṇakakkaṭajātakaṃ catutthaṃ."
}