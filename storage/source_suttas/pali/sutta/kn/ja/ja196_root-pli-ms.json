{
  "ja196:0.1": "Jātaka",
  "ja196:0.2": "Dukanipāta",
  "ja196:0.3": "Ruhakavagga",
  "ja196:0.4": "6. Valāhakassajātaka",
  "ja196:1.1": "“Ye na kāhanti ovādaṃ,",
  "ja196:1.2": "narā buddhena desitaṃ;",
  "ja196:1.3": "Byasanaṃ te gamissanti,",
  "ja196:1.4": "rakkhasīhiva vāṇijā.",
  "ja196:2.1": "Ye ca kāhanti ovādaṃ,",
  "ja196:2.2": "narā buddhena desitaṃ;",
  "ja196:2.3": "Sotthiṃ pāraṃ gamissanti,",
  "ja196:2.4": "valāheneva vāṇijā”ti.",
  "ja196:3.1": "Valāhakassajātakaṃ chaṭṭhaṃ."
}