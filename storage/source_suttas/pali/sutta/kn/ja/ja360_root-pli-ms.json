{
  "ja360:0.1": "Jātaka",
  "ja360:0.2": "Pañcakanipāta",
  "ja360:0.3": "Maṇikuṇḍalavagga",
  "ja360:0.4": "10. Suyonandījātaka",
  "ja360:1.1": "“Vāti gandho timirānaṃ,",
  "ja360:1.2": "kusamuddo ca ghosavā;",
  "ja360:1.3": "Dūre ito suyonandī,",
  "ja360:1.4": "tamba kāmā tudanti maṃ”.",
  "ja360:2.1": "“Kathaṃ samuddamatari,",
  "ja360:2.2": "Kathaṃ addakkhi sedumaṃ;",
  "ja360:2.3": "Kathaṃ tassā ca tuyhañca,",
  "ja360:2.4": "Ahu saggasamāgamo”.",
  "ja360:3.1": "“Kurukacchā payātānaṃ,",
  "ja360:3.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja360:3.3": "Makarehi abhidā nāvā,",
  "ja360:3.4": "phalakenāhamaplaviṃ.",
  "ja360:4.1": "Sā maṃ saṇhena mudunā,",
  "ja360:4.2": "niccaṃ candanagandhinī;",
  "ja360:4.3": "Aṅgena uddharī bhaddā,",
  "ja360:4.4": "mātā puttaṃva orasaṃ.",
  "ja360:5.1": "Sā maṃ annena pānena,",
  "ja360:5.2": "vatthena sayanena ca;",
  "ja360:5.3": "Attanāpi ca mandakkhī,",
  "ja360:5.4": "evaṃ tamba vijānahī”ti.",
  "ja360:6.1": "Suyonandījātakaṃ dasamaṃ.",
  "ja360:6.2": "Maṇikuṇḍalavaggo paṭhamo.",
  "ja360:7.1": "Tassuddānaṃ",
  "ja360:8.1": "Atha jinavaro haritaṃ tiṇako,",
  "ja360:8.2": "Atha bhinnaplavo uragova ghaṭo;",
  "ja360:8.3": "Dariyā puna kuñjara bhūnahatā,",
  "ja360:8.4": "Migamuttamasaggavarena dasāti."
}