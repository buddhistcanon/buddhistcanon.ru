{
  "ja167:0.1": "Jātaka",
  "ja167:0.2": "Dukanipāta",
  "ja167:0.3": "Santhavavagga",
  "ja167:0.4": "7. Samiddhijātaka",
  "ja167:1.1": "“Abhutvā bhikkhasi bhikkhu,",
  "ja167:1.2": "Na hi bhutvāna bhikkhasi;",
  "ja167:1.3": "Bhutvāna bhikkhu bhikkhassu,",
  "ja167:1.4": "Mā taṃ kālo upaccagā.",
  "ja167:2.1": "Kālaṃ vohaṃ na jānāmi,",
  "ja167:2.2": "channo kālo na dissati;",
  "ja167:2.3": "Tasmā abhutvā bhikkhāmi,",
  "ja167:2.4": "mā maṃ kālo upaccagā”ti.",
  "ja167:3.1": "Samiddhijātakaṃ sattamaṃ."
}