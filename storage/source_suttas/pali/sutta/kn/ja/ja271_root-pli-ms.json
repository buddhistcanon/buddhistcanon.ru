{
  "ja271:0.1": "Jātaka",
  "ja271:0.2": "Tikanipāta",
  "ja271:0.3": "Udapānavagga",
  "ja271:0.4": "1. Udapānadūsakajātaka",
  "ja271:1.1": "“Āraññikassa isino,",
  "ja271:1.2": "cirarattatapassino;",
  "ja271:1.3": "Kicchākataṃ udapānaṃ,",
  "ja271:1.4": "kathaṃ samma avāhayi”.",
  "ja271:2.1": "“Esa dhammo siṅgālānaṃ,",
  "ja271:2.2": "yaṃ pitvā ohadāmase;",
  "ja271:2.3": "Pitupitāmahaṃ dhammo,",
  "ja271:2.4": "na taṃ ujjhātumarahasi”.",
  "ja271:3.1": "“Yesaṃ vo ediso dhammo,",
  "ja271:3.2": "Adhammo pana kīdiso;",
  "ja271:3.3": "Mā vo dhammaṃ adhammaṃ vā,",
  "ja271:3.4": "Addasāma kudācanan”ti.",
  "ja271:4.1": "Udapānadūsakajātakaṃ paṭhamaṃ."
}