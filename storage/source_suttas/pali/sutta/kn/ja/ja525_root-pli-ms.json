{
  "ja525:0.1": "Jātaka",
  "ja525:0.2": "Cattālīsanipāta",
  "ja525:0.3": "Tesakuṇavagga",
  "ja525:0.4": "5 Cūḷasutasomajātaka",
  "ja525:1.1": "“Āmantayāmi nigamaṃ,",
  "ja525:1.2": "mittāmacce parissaje;",
  "ja525:1.3": "Sirasmiṃ palitaṃ jātaṃ,",
  "ja525:1.4": "pabbajjaṃ dāni rocahaṃ”.",
  "ja525:2.1": "“Abhumme kathaṃ nu bhaṇasi,",
  "ja525:2.2": "Sallaṃ me deva urasi kappesi;",
  "ja525:2.3": "Sattasatā te bhariyā,",
  "ja525:2.4": "Kathaṃ nu te tā bhavissanti”.",
  "ja525:3.1": "“Paññāyihinti etā,",
  "ja525:3.2": "Daharā aññampi tā gamissanti;",
  "ja525:3.3": "Saggañcassa patthayāno,",
  "ja525:3.4": "Tena ahaṃ pabbajissāmi”.",
  "ja525:4.1": "“Dulladdhaṃ me āsi sutasoma,",
  "ja525:4.2": "Yassa te homahaṃ mātā;",
  "ja525:4.3": "Yaṃ me vilapantiyā,",
  "ja525:4.4": "Anapekkho pabbajasi deva.",
  "ja525:5.1": "Dulladdhaṃ me āsi sutasoma,",
  "ja525:5.2": "Yaṃ taṃ ahaṃ vijāyissaṃ;",
  "ja525:5.3": "Yaṃ me vilapantiyā,",
  "ja525:5.4": "Anapekkho pabbajasi deva”.",
  "ja525:6.1": "“Ko nāmeso dhammo,",
  "ja525:6.2": "Sutasoma kā ca nāma pabbajjā;",
  "ja525:6.3": "Yaṃ no amhe jiṇṇe,",
  "ja525:6.4": "Anapekkho pabbajasi deva.",
  "ja525:7.1": "Puttāpi tuyhaṃ bahavo,",
  "ja525:7.2": "daharā appattayobbanā;",
  "ja525:7.3": "Mañjū tepi taṃ apassantā,",
  "ja525:7.4": "maññe dukkhaṃ nigacchanti”.",
  "ja525:8.1": "“Puttehi ca me etehi,",
  "ja525:8.2": "Daharehi appattayobbanehi;",
  "ja525:8.3": "Mañjūhi sabbehipi tumhehi,",
  "ja525:8.4": "Cirampi ṭhatvā vināsabhāvo”.",
  "ja525:9.1": "“Chinnaṃ nu tuyhaṃ hadayaṃ,",
  "ja525:9.2": "Adu te karuṇā ca natthi amhesu;",
  "ja525:9.3": "Yaṃ no vikandantiyo,",
  "ja525:9.4": "Anapekkho pabbajasi deva”.",
  "ja525:10.1": "“Na ca mayhaṃ chinnaṃ hadayaṃ,",
  "ja525:10.2": "Atthi karuṇāpi mayhaṃ tumhesu;",
  "ja525:10.3": "Saggañca patthayāno,",
  "ja525:10.4": "Tena ahaṃ pabbajissāmi”.",
  "ja525:11.1": "“Dulladdhaṃ me āsi,",
  "ja525:11.2": "Sutasoma yassa te ahaṃ bhariyā;",
  "ja525:11.3": "Yaṃ me vilapantiyā,",
  "ja525:11.4": "Anapekkho pabbajasi deva.",
  "ja525:12.1": "Dulladdhaṃ me āsi,",
  "ja525:12.2": "Sutasoma yassa te ahaṃ bhariyā;",
  "ja525:12.3": "Yaṃ me kucchipaṭisandhiṃ,",
  "ja525:12.4": "Anapekkho pabbajasi deva.",
  "ja525:13.1": "Paripakko me gabbho,",
  "ja525:13.2": "Kucchigato yāva naṃ vijāyāmi;",
  "ja525:13.3": "Māhaṃ ekā vidhavā,",
  "ja525:13.4": "Pacchā dukkhāni addakkhiṃ”.",
  "ja525:14.1": "“Paripakko te gabbho,",
  "ja525:14.2": "Kucchigato iṅgha tvaṃ vijāyassu;",
  "ja525:14.3": "Puttaṃ anomavaṇṇaṃ,",
  "ja525:14.4": "Taṃ hitvā pabbajissāmi”.",
  "ja525:15.1": "“Mā tvaṃ cande rudi,",
  "ja525:15.2": "Mā soci vanatimiramattakkhi;",
  "ja525:15.3": "Āroha varapāsādaṃ,",
  "ja525:15.4": "Anapekkho ahaṃ gamissāmi”.",
  "ja525:16.1": "“Ko taṃ amma kopesi,",
  "ja525:16.2": "Kiṃ rodasi pekkhasi ca maṃ bāḷhaṃ;",
  "ja525:16.3": "Kaṃ avajjhaṃ ghātemi,",
  "ja525:16.4": "Ñātīnaṃ udikkhamānānaṃ”.",
  "ja525:17.1": "“Na hi so sakkā hantuṃ,",
  "ja525:17.2": "Vijitāvī yo maṃ tāta kopesi;",
  "ja525:17.3": "Pitā te maṃ tāta avaca,",
  "ja525:17.4": "Anapekkho ahaṃ gamissāmi”.",
  "ja525:18.1": "“Yohaṃ pubbe niyyāmi,",
  "ja525:18.2": "Uyyānaṃ mattakuñjare ca yodhemi;",
  "ja525:18.3": "Sutasome pabbajite,",
  "ja525:18.4": "Kathaṃ nu dāni karissāmi”.",
  "ja525:19.1": "“Mātucca me rudantyā,",
  "ja525:19.2": "Jeṭṭhassa ca bhātuno akāmassa;",
  "ja525:19.3": "Hatthepi te gahessaṃ,",
  "ja525:19.4": "Na hi gacchasi no akāmānaṃ”.",
  "ja525:20.1": "“Uṭṭhehi tvaṃ dhāti,",
  "ja525:20.2": "Imaṃ kumāraṃ ramehi aññattha;",
  "ja525:20.3": "Mā me paripanthamakāsi,",
  "ja525:20.4": "Saggaṃ mama patthayānassa”.",
  "ja525:21.1": "“Yaṃnūnimaṃ dadeyyaṃ pabhaṅkaraṃ,",
  "ja525:21.2": "Ko nu me imināttho;",
  "ja525:21.3": "Sutasome pabbajite,",
  "ja525:21.4": "Kiṃ nu menaṃ karissāmi”.",
  "ja525:22.1": "“Koso ca tuyhaṃ vipulo,",
  "ja525:22.2": "Koṭṭhāgārañca tuyhaṃ paripūraṃ;",
  "ja525:22.3": "Pathavī ca tuyhaṃ vijitā,",
  "ja525:22.4": "Ramassu mā pabbaji deva”.",
  "ja525:23.1": "“Koso ca mayhaṃ vipulo,",
  "ja525:23.2": "Koṭṭhāgārañca mayhaṃ paripūraṃ;",
  "ja525:23.3": "Pathavī ca mayhaṃ vijitā,",
  "ja525:23.4": "Taṃ hitvā pabbajissāmi”.",
  "ja525:24.1": "“Mayhampi dhanaṃ pahūtaṃ,",
  "ja525:24.2": "Saṅkhātuṃ nopi deva sakkomi;",
  "ja525:24.3": "Taṃ te dadāmi sabbampi,",
  "ja525:24.4": "Ramassu mā pabbaji deva”.",
  "ja525:25.1": "“Jānāmi dhanaṃ pahūtaṃ,",
  "ja525:25.2": "Kulavaddhana pūjito tayā casmi;",
  "ja525:25.3": "Saggañca patthayāno,",
  "ja525:25.4": "Tena ahaṃ pabbajissāmi.",
  "ja525:26.1": "Ukkaṇṭhitosmi bāḷhaṃ,",
  "ja525:26.2": "Arati maṃ somadatta āvisati;",
  "ja525:26.3": "Bahukāpi me antarāyā,",
  "ja525:26.4": "Ajjevāhaṃ pabbajissāmi”.",
  "ja525:27.1": "“Idañca tuyhaṃ rucitaṃ,",
  "ja525:27.2": "Sutasoma ajjeva dāni tvaṃ pabbaja;",
  "ja525:27.3": "Ahampi pabbajissāmi,",
  "ja525:27.4": "Na ussahe tayā vinā ahaṃ ṭhātuṃ”.",
  "ja525:28.1": "“Na hi sakkā pabbajituṃ,",
  "ja525:28.2": "Nagare na hi paccati janapade ca;",
  "ja525:28.3": "Sutasome pabbajite,",
  "ja525:28.4": "Kathaṃ nu dāni karissāma.",
  "ja525:29.1": "Upanīyatidaṃ maññe,",
  "ja525:29.2": "Parittaṃ udakaṃva caṅkavāramhi;",
  "ja525:29.3": "Evaṃ suparittake jīvite,",
  "ja525:29.4": "Na ca pamajjituṃ kālo.",
  "ja525:30.1": "Upanīyatidaṃ maññe,",
  "ja525:30.2": "Parittaṃ udakaṃva caṅkavāramhi;",
  "ja525:30.3": "Evaṃ suparittake jīvite,",
  "ja525:30.4": "Andhabālā pamajjanti.",
  "ja525:31.1": "Te vaḍḍhayanti nirayaṃ,",
  "ja525:31.2": "Tiracchānayoniñca pettivisayañca;",
  "ja525:31.3": "Taṇhāya bandhanabaddhā,",
  "ja525:31.4": "Vaḍḍhenti asurakāyaṃ”.",
  "ja525:32.1": "“Ūhaññate rajaggaṃ,",
  "ja525:32.2": "Avidūre pubbakamhi ca pāsāde;",
  "ja525:32.3": "Maññe no kesā chinnā,",
  "ja525:32.4": "Yasassino dhammarājassa”.",
  "ja525:33.1": "“Ayamassa pāsādo,",
  "ja525:33.2": "Sovaṇṇapupphamālyavītikiṇṇo;",
  "ja525:33.3": "Yahimanuvicari rājā,",
  "ja525:33.4": "Parikiṇṇo itthāgārehi.",
  "ja525:34.1": "Ayamassa pāsādo,",
  "ja525:34.2": "Sovaṇṇapupphamālyavītikiṇṇo;",
  "ja525:34.3": "Yahimanuvicari rājā,",
  "ja525:34.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:35.1": "Idamassa kūṭāgāraṃ,",
  "ja525:35.2": "Sovaṇṇapupphamālyavītikiṇṇaṃ;",
  "ja525:35.3": "Yahimanuvicari rājā,",
  "ja525:35.4": "Parikiṇṇo itthāgārehi.",
  "ja525:36.1": "Idamassa kūṭāgāraṃ,",
  "ja525:36.2": "Sovaṇṇapupphamālyavītikiṇṇaṃ;",
  "ja525:36.3": "Yahimanuvicari rājā,",
  "ja525:36.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:37.1": "Ayamassa asokavanikā,",
  "ja525:37.2": "Supupphitā sabbakālikā rammā;",
  "ja525:37.3": "Yahimanuvicari rājā,",
  "ja525:37.4": "Parikiṇṇo itthāgārehi.",
  "ja525:38.1": "Ayamassa asokavanikā,",
  "ja525:38.2": "Supupphitā sabbakālikā rammā;",
  "ja525:38.3": "Yahimanuvicari rājā,",
  "ja525:38.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:39.1": "Idamassa uyyānaṃ,",
  "ja525:39.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:39.3": "Yahimanuvicari rājā,",
  "ja525:39.4": "Parikiṇṇo itthāgārehi.",
  "ja525:40.1": "Idamassa uyyānaṃ,",
  "ja525:40.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:40.3": "Yahimanuvicari rājā,",
  "ja525:40.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:41.1": "Idamassa kaṇikāravanaṃ,",
  "ja525:41.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:41.3": "Yahimanuvicari rājā,",
  "ja525:41.4": "Parikiṇṇo itthāgārehi.",
  "ja525:42.1": "Idamassa kaṇikāravanaṃ,",
  "ja525:42.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:42.3": "Yahimanuvicari rājā,",
  "ja525:42.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:43.1": "Idamassa pāṭalivanaṃ,",
  "ja525:43.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:43.3": "Yahimanuvicari rājā,",
  "ja525:43.4": "Parikiṇṇo itthāgārehi.",
  "ja525:44.1": "Idamassa pāṭalivanaṃ,",
  "ja525:44.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:44.3": "Yahimanuvicari rājā,",
  "ja525:44.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:45.1": "Idamassa ambavanaṃ,",
  "ja525:45.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:45.3": "Yahimanuvicari rājā,",
  "ja525:45.4": "Parikiṇṇo itthāgārehi.",
  "ja525:46.1": "Idamassa ambavanaṃ,",
  "ja525:46.2": "Supupphitaṃ sabbakālikaṃ rammaṃ;",
  "ja525:46.3": "Yahimanuvicari rājā,",
  "ja525:46.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:47.1": "Ayamassa pokkharaṇī,",
  "ja525:47.2": "Sañchannā aṇḍajehi vītikiṇṇā;",
  "ja525:47.3": "Yahimanuvicari rājā,",
  "ja525:47.4": "Parikiṇṇo itthāgārehi.",
  "ja525:48.1": "Ayamassa pokkharaṇī,",
  "ja525:48.2": "Sañchannā aṇḍajehi vītikiṇṇā;",
  "ja525:48.3": "Yahimanuvicari rājā,",
  "ja525:48.4": "Parikiṇṇo ñātisaṅghena.",
  "ja525:49.1": "Rājā vo kho pabbajito,",
  "ja525:49.2": "Sutasomo rajjaṃ imaṃ pahatvāna;",
  "ja525:49.3": "Kāsāyavatthavasano,",
  "ja525:49.4": "Nāgova ekako carati”.",
  "ja525:50.1": "“Māssu pubbe ratikīḷitāni,",
  "ja525:50.2": "Hasitāni ca anussarittha;",
  "ja525:50.3": "Mā vo kāmā haniṃsu,",
  "ja525:50.4": "Rammaṃ hi sudassanaṃ nagaraṃ.",
  "ja525:51.1": "Mettacittañca bhāvetha,",
  "ja525:51.2": "Appamāṇaṃ divā ca ratto ca;",
  "ja525:51.3": "Agacchittha devapuraṃ,",
  "ja525:51.4": "Āvāsaṃ puññakamminan”ti. (3219)",
  "ja525:52.1": "Cūḷasutasomajātakaṃ pañcamaṃ.",
  "ja525:53.1": "Cattālīsanipātaṃ niṭṭhitaṃ.",
  "ja525:54.1": "Tassuddānaṃ",
  "ja525:55.1": "Suvapaṇḍitajambukakuṇḍalino,",
  "ja525:55.2": "Varakaññamalambusajātakañca;",
  "ja525:55.3": "Pavaruttamasaṅkhasirīvhayako,",
  "ja525:55.4": "Sutasomaarindhamarājavaro."
}