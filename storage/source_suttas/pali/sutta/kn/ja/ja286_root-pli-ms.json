{
  "ja286:0.1": "Jātaka",
  "ja286:0.2": "Tikanipāta",
  "ja286:0.3": "Abbhantaravagga",
  "ja286:0.4": "6. Sālūkajātaka",
  "ja286:1.1": "“Mā sālūkassa pihayi,",
  "ja286:1.2": "āturannāni bhuñjati;",
  "ja286:1.3": "Appossukko bhusaṃ khāda,",
  "ja286:1.4": "etaṃ dīghāyulakkhaṇaṃ.",
  "ja286:2.1": "Idāni so idhāgantvā,",
  "ja286:2.2": "atithī yuttasevako;",
  "ja286:2.3": "Atha dakkhasi sālūkaṃ,",
  "ja286:2.4": "sayantaṃ musaluttaraṃ”.",
  "ja286:3.1": "Vikantaṃ sūkaraṃ disvā,",
  "ja286:3.2": "sayantaṃ musaluttaraṃ;",
  "ja286:3.3": "Jaraggavā vicintesuṃ,",
  "ja286:3.4": "varamhākaṃ bhusāmivāti.",
  "ja286:4.1": "Sālūkajātakaṃ chaṭṭhaṃ."
}