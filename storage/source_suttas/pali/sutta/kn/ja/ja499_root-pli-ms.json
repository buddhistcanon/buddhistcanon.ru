{
  "ja499:0.1": "Jātaka",
  "ja499:0.2": "Vīsatinipāta",
  "ja499:0.3": "Mātaṅgavagga",
  "ja499:0.4": "3. Sivijātaka",
  "ja499:1.1": "“Dūre apassaṃ therova,",
  "ja499:1.2": "cakkhuṃ yācitumāgato;",
  "ja499:1.3": "Ekanettā bhavissāma,",
  "ja499:1.4": "cakkhuṃ me dehi yācito”.",
  "ja499:2.1": "“Kenānusiṭṭho idha māgatosi,",
  "ja499:2.2": "Vanibbaka cakkhupathāni yācituṃ;",
  "ja499:2.3": "Suduccajaṃ yācasi uttamaṅgaṃ,",
  "ja499:2.4": "Yamāhu nettaṃ purisena duccajaṃ”.",
  "ja499:3.1": "“Yamāhu devesu sujampatīti,",
  "ja499:3.2": "Maghavāti naṃ āhu manussaloke;",
  "ja499:3.3": "Tenānusiṭṭho idha māgatosmi,",
  "ja499:3.4": "Vanibbako cakkhupathāni yācituṃ.",
  "ja499:4.1": "Vanibbato mayha vaniṃ anuttaraṃ,",
  "ja499:4.2": "Dadāhi te cakkhupathāni yācito;",
  "ja499:4.3": "Dadāhi me cakkhupathaṃ anuttaraṃ,",
  "ja499:4.4": "Yamāhu nettaṃ purisena duccajaṃ”.",
  "ja499:5.1": "“Yena atthena āgacchi,",
  "ja499:5.2": "yamatthamabhipatthayaṃ;",
  "ja499:5.3": "Te te ijjhantu saṅkappā,",
  "ja499:5.4": "labha cakkhūni brāhmaṇa.",
  "ja499:6.1": "Ekaṃ te yācamānassa,",
  "ja499:6.2": "Ubhayāni dadāmahaṃ;",
  "ja499:6.3": "Sa cakkhumā gaccha janassa pekkhato,",
  "ja499:6.4": "Yadicchase tvaṃ tadate samijjhatu”.",
  "ja499:7.1": "“Mā no deva adā cakkhuṃ,",
  "ja499:7.2": "mā no sabbe parākari;",
  "ja499:7.3": "Dhanaṃ dehi mahārāja,",
  "ja499:7.4": "muttā veḷuriyā bahū.",
  "ja499:8.1": "Yutte deva rathe dehi,",
  "ja499:8.2": "ājānīye calaṅkate;",
  "ja499:8.3": "Nāge dehi mahārāja,",
  "ja499:8.4": "hemakappanavāsase.",
  "ja499:9.1": "Yathā taṃ sivayo sabbe,",
  "ja499:9.2": "sayoggā sarathā sadā;",
  "ja499:9.3": "Samantā parikireyyuṃ,",
  "ja499:9.4": "evaṃ dehi rathesabha”.",
  "ja499:10.1": "“Yo ve dassanti vatvāna,",
  "ja499:10.2": "adāne kurute mano;",
  "ja499:10.3": "Bhūmyaṃ so patitaṃ pāsaṃ,",
  "ja499:10.4": "gīvāyaṃ paṭimuñcati.",
  "ja499:11.1": "Yo ve dassanti vatvāna,",
  "ja499:11.2": "adāne kurute mano;",
  "ja499:11.3": "Pāpā pāpataro hoti,",
  "ja499:11.4": "sampatto yamasādhanaṃ.",
  "ja499:12.1": "Yañhi yāce tañhi dade,",
  "ja499:12.2": "yaṃ na yāce na taṃ dade;",
  "ja499:12.3": "Svāhaṃ tameva dassāmi,",
  "ja499:12.4": "yaṃ maṃ yācati brāhmaṇo”.",
  "ja499:13.1": "“Āyuṃ nu vaṇṇaṃ nu sukhaṃ balaṃ nu,",
  "ja499:13.2": "Kiṃ patthayāno nu janinda desi;",
  "ja499:13.3": "Kathañhi rājā sivinaṃ anuttaro,",
  "ja499:13.4": "Cakkhūni dajjā paralokahetu”.",
  "ja499:14.1": "“Na vāhametaṃ yasasā dadāmi,",
  "ja499:14.2": "Na puttamicche na dhanaṃ na raṭṭhaṃ;",
  "ja499:14.3": "Satañca dhammo carito purāṇo,",
  "ja499:14.4": "Icceva dāne ramate mano mama”.",
  "ja499:14.5": "()",
  "ja499:15.1": "“Sakhā ca mitto ca mamāsi sīvika,",
  "ja499:15.2": "Susikkhito sādhu karohi me vaco;",
  "ja499:15.3": "Uddharitvā cakkhūni mamaṃ jigīsato,",
  "ja499:15.4": "Hatthesu ṭhapehi vanibbakassa”.",
  "ja499:16.1": "Codito sivirājena,",
  "ja499:16.2": "sīviko vacanaṅkaro;",
  "ja499:16.3": "Rañño cakkhūnuddharitvā,",
  "ja499:16.4": "brāhmaṇassūpanāmayi;",
  "ja499:16.5": "Sacakkhu brāhmaṇo āsi,",
  "ja499:16.6": "andho rājā upāvisi.",
  "ja499:17.1": "Tato so katipāhassa,",
  "ja499:17.2": "uparūḷhesu cakkhusu;",
  "ja499:17.3": "Sūtaṃ āmantayī rājā,",
  "ja499:17.4": "sivīnaṃ raṭṭhavaḍḍhano.",
  "ja499:18.1": "“Yojehi sārathi yānaṃ,",
  "ja499:18.2": "yuttañca paṭivedaya;",
  "ja499:18.3": "Uyyānabhūmiṃ gacchāma,",
  "ja499:18.4": "pokkharañño vanāni ca”.",
  "ja499:19.1": "So ca pokkharaṇītīre,",
  "ja499:19.2": "pallaṅkena upāvisi;",
  "ja499:19.3": "Tassa sakko pāturahu,",
  "ja499:19.4": "devarājā sujampati.",
  "ja499:20.1": "“Sakkohamasmi devindo,",
  "ja499:20.2": "āgatosmi tavantike;",
  "ja499:20.3": "Varaṃ varassu rājīsi,",
  "ja499:20.4": "yaṃ kiñci manasicchasi”.",
  "ja499:21.1": "“Pahūtaṃ me dhanaṃ sakka,",
  "ja499:21.2": "balaṃ koso canappako;",
  "ja499:21.3": "Andhassa me sato dāni,",
  "ja499:21.4": "maraṇaññeva ruccati”.",
  "ja499:22.1": "“Yāni saccāni dvipadinda,",
  "ja499:22.2": "tāni bhāsassu khattiya;",
  "ja499:22.3": "Saccaṃ te bhaṇamānassa,",
  "ja499:22.4": "puna cakkhu bhavissati”.",
  "ja499:23.1": "“Ye maṃ yācitumāyanti,",
  "ja499:23.2": "nānāgottā vanibbakā;",
  "ja499:23.3": "Yopi maṃ yācate tattha,",
  "ja499:23.4": "sopi me manaso piyo;",
  "ja499:23.5": "Etena saccavajjena,",
  "ja499:23.6": "cakkhu me upapajjatha.",
  "ja499:24.1": "Yaṃ maṃ so yācituṃ āgā,",
  "ja499:24.2": "dehi cakkhunti brāhmaṇo;",
  "ja499:24.3": "Tassa cakkhūni pādāsiṃ,",
  "ja499:24.4": "brāhmaṇassa vanibbato.",
  "ja499:25.1": "Bhiyyo maṃ āvisī pīti,",
  "ja499:25.2": "somanassañcanappakaṃ;",
  "ja499:25.3": "Etena saccavajjena,",
  "ja499:25.4": "dutiyaṃ me upapajjatha”.",
  "ja499:26.1": "“Dhammena bhāsitā gāthā,",
  "ja499:26.2": "sivīnaṃ raṭṭhavaḍḍhana;",
  "ja499:26.3": "Etāni tava nettāni,",
  "ja499:26.4": "dibbāni paṭidissare.",
  "ja499:27.1": "Tirokuṭṭaṃ tiroselaṃ,",
  "ja499:27.2": "samatiggayha pabbataṃ;",
  "ja499:27.3": "Samantā yojanasataṃ,",
  "ja499:27.4": "dassanaṃ anubhontu te”.",
  "ja499:28.1": "“Ko nīdha vittaṃ na dadeyya yācito,",
  "ja499:28.2": "Api visiṭṭhaṃ supiyampi attano;",
  "ja499:28.3": "Tadiṅgha sabbe sivayo samāgatā,",
  "ja499:28.4": "Dibbāni nettāni mamajja passatha.",
  "ja499:29.1": "Tirokuṭṭaṃ tiroselaṃ,",
  "ja499:29.2": "samatiggayha pabbataṃ;",
  "ja499:29.3": "Samantā yojanasataṃ,",
  "ja499:29.4": "dassanaṃ anubhonti me.",
  "ja499:30.1": "Na cāgamattā paramatthi kiñci,",
  "ja499:30.2": "Maccānaṃ idha jīvite;",
  "ja499:30.3": "Datvāna mānusaṃ cakkhuṃ,",
  "ja499:30.4": "Laddhaṃ me cakkhuṃ amānusaṃ.",
  "ja499:31.1": "Etampi disvā sivayo,",
  "ja499:31.2": "Detha dānāni bhuñjatha;",
  "ja499:31.3": "Datvā ca bhutvā ca yathānubhāvaṃ,",
  "ja499:31.4": "Aninditā saggamupetha ṭhānan”ti.",
  "ja499:32.1": "Sivijātakaṃ tatiyaṃ."
}