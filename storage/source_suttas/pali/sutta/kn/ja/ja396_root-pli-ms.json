{
  "ja396:0.1": "Jātaka",
  "ja396:0.2": "Sattakanipāta",
  "ja396:0.3": "Kukkuvagga",
  "ja396:0.4": "1. Kukkujātaka",
  "ja396:1.1": "“Diyaḍḍhakukkū udayena kaṇṇikā,",
  "ja396:1.2": "Vidatthiyo aṭṭha parikkhipanti naṃ;",
  "ja396:1.3": "Sā siṃsapā sāramayā apheggukā,",
  "ja396:1.4": "Kuhiṃ ṭhitā upparito na dhaṃsati”.",
  "ja396:2.1": "“Yā tiṃsati sāramayā anujjukā,",
  "ja396:2.2": "Parikiriya gopānasiyo samaṃ ṭhitā;",
  "ja396:2.3": "Tāhi susaṅgahitā balasā pīḷitā,",
  "ja396:2.4": "Samaṃ ṭhitā upparito na dhaṃsati.",
  "ja396:3.1": "Evampi mittehi daḷhehi paṇḍito,",
  "ja396:3.2": "Abhejjarūpehi sucīhi mantibhi;",
  "ja396:3.3": "Susaṅgahīto siriyā na dhaṃsati,",
  "ja396:3.4": "Gopānasī bhāravahāva kaṇṇikā.",
  "ja396:4.1": "Kharattacaṃ bellaṃ yathāpi satthavā,",
  "ja396:4.2": "Anāmasantopi karoti tittakaṃ;",
  "ja396:4.3": "Samāharaṃ sāduṃ karoti patthiva,",
  "ja396:4.4": "Asāduṃ kayirā tanubandhamuddharaṃ.",
  "ja396:5.1": "Evampi gāmanigamesu paṇḍito,",
  "ja396:5.2": "Asāhasaṃ rājadhanāni saṃgharaṃ;",
  "ja396:5.3": "Dhammānuvattī paṭipajjamāno,",
  "ja396:5.4": "Sa phāti kayirā aviheṭhayaṃ paraṃ.",
  "ja396:6.1": "Odātamūlaṃ sucivārisambhavaṃ,",
  "ja396:6.2": "Jātaṃ yathā pokkharaṇīsu ambujaṃ;",
  "ja396:6.3": "Padumaṃ yathā agginikāsiphālimaṃ,",
  "ja396:6.4": "Na kaddamo na rajo na vāri limpati.",
  "ja396:7.1": "Evampi vohārasuciṃ asāhasaṃ,",
  "ja396:7.2": "Visuddhakammantamapetapāpakaṃ;",
  "ja396:7.3": "Na limpati kammakilesa tādiso,",
  "ja396:7.4": "Jātaṃ yathā pokkharaṇīsu ambujan”ti.",
  "ja396:8.1": "Kukkujātakaṃ paṭhamaṃ."
}