{
  "ja532:0.1": "Jātaka",
  "ja532:0.2": "Sattatinipāta",
  "ja532:0.3": "Kusavagga",
  "ja532:0.4": "2. Soṇanandajātaka",
  "ja532:1.1": "“Devatā nusi gandhabbo,",
  "ja532:1.2": "adu sakko purindado;",
  "ja532:1.3": "Manussabhūto iddhimā,",
  "ja532:1.4": "kathaṃ jānemu taṃ mayaṃ”.",
  "ja532:2.1": "“Nāpi devo na gandhabbo,",
  "ja532:2.2": "nāpi sakko purindado;",
  "ja532:2.3": "Manussabhūto iddhimā,",
  "ja532:2.4": "evaṃ jānāhi bhāradha”.",
  "ja532:3.1": "“Katarūpamidaṃ bhoto,",
  "ja532:3.2": "veyyāvaccaṃ anappakaṃ;",
  "ja532:3.3": "Devamhi vassamānamhi,",
  "ja532:3.4": "anovassaṃ bhavaṃ akā.",
  "ja532:4.1": "Tato vātātape ghore,",
  "ja532:4.2": "sītacchāyaṃ bhavaṃ akā;",
  "ja532:4.3": "Tato amittamajjhesu,",
  "ja532:4.4": "saratāṇaṃ bhavaṃ akā.",
  "ja532:5.1": "Tato phītāni raṭṭhāni,",
  "ja532:5.2": "vasino te bhavaṃ akā;",
  "ja532:5.3": "Tato ekasataṃ khatye,",
  "ja532:5.4": "anuyante bhavaṃ akā.",
  "ja532:6.1": "Patītāssu mayaṃ bhoto,",
  "ja532:6.2": "Vada taṃ bhañjamicchasi;",
  "ja532:6.3": "Hatthiyānaṃ assarathaṃ,",
  "ja532:6.4": "Nāriyo ca alaṅkatā;",
  "ja532:6.5": "Nivesanāni rammāni,",
  "ja532:6.6": "Mayaṃ bhoto dadāmase.",
  "ja532:7.1": "Atha vaṅge vā magadhe,",
  "ja532:7.2": "mayaṃ bhoto dadāmase;",
  "ja532:7.3": "Atha vā assakāvantī,",
  "ja532:7.4": "sumanā damma te mayaṃ.",
  "ja532:8.1": "Upaḍḍhaṃ vāpi rajjassa,",
  "ja532:8.2": "mayaṃ bhoto dadāmase;",
  "ja532:8.3": "Sace te attho rajjena,",
  "ja532:8.4": "anusāsa yadicchasi”.",
  "ja532:9.1": "“Na me atthopi rajjena,",
  "ja532:9.2": "nagarena dhanena vā;",
  "ja532:9.3": "Athopi janapadena,",
  "ja532:9.4": "attho mayhaṃ na vijjati.",
  "ja532:10.1": "Bhotova raṭṭhe vijite,",
  "ja532:10.2": "araññe atthi assamo;",
  "ja532:10.3": "Pitā mayhaṃ janettī ca,",
  "ja532:10.4": "ubho sammanti assame.",
  "ja532:11.1": "Tesāhaṃ pubbācariyesu,",
  "ja532:11.2": "Puññaṃ na labhāmi kātave;",
  "ja532:11.3": "Bhavantaṃ ajjhāvaraṃ katvā,",
  "ja532:11.4": "Soṇaṃ yācemu saṃvaraṃ”.",
  "ja532:12.1": "“Karomi te taṃ vacanaṃ,",
  "ja532:12.2": "yaṃ maṃ bhaṇasi brāhmaṇa;",
  "ja532:12.3": "Etañca kho no akkhāhi,",
  "ja532:12.4": "kīvanto hontu yācakā”.",
  "ja532:13.1": "“Parosataṃ jānapadā,",
  "ja532:13.2": "mahāsālā ca brāhmaṇā;",
  "ja532:13.3": "Ime ca khattiyā sabbe,",
  "ja532:13.4": "abhijātā yasassino;",
  "ja532:13.5": "Bhavañca rājā manojo,",
  "ja532:13.6": "alaṃ hessanti yācakā”.",
  "ja532:14.1": "“Hatthī asse ca yojentu,",
  "ja532:14.2": "rathaṃ sannayha sārathi;",
  "ja532:14.3": "Ābandhanāni gaṇhātha,",
  "ja532:14.4": "pādāsussārayaddhaje;",
  "ja532:14.5": "Assamaṃ taṃ gamissāmi,",
  "ja532:14.6": "yattha sammati kosiyo”.",
  "ja532:15.1": "Tato ca rājā pāyāsi,",
  "ja532:15.2": "senāya caturaṅginī;",
  "ja532:15.3": "Agamā assamaṃ rammaṃ,",
  "ja532:15.4": "yattha sammati kosiyo.",
  "ja532:16.1": "“Kassa kādambayo kājo,",
  "ja532:16.2": "vehāsaṃ caturaṅgulaṃ;",
  "ja532:16.3": "Aṃsaṃ asamphusaṃ eti,",
  "ja532:16.4": "udahārāya gacchato”.",
  "ja532:17.1": "“Ahaṃ soṇo mahārāja,",
  "ja532:17.2": "tāpaso sahitabbato;",
  "ja532:17.3": "Bharāmi mātāpitaro,",
  "ja532:17.4": "rattindivamatandito.",
  "ja532:18.1": "Vane phalañca mūlañca,",
  "ja532:18.2": "āharitvā disampati;",
  "ja532:18.3": "Posemi mātāpitaro,",
  "ja532:18.4": "pubbe katamanussaraṃ”.",
  "ja532:19.1": "“Icchāma assamaṃ gantuṃ,",
  "ja532:19.2": "yattha sammati kosiyo;",
  "ja532:19.3": "Maggaṃ no soṇa akkhāhi,",
  "ja532:19.4": "yena gacchemu assamaṃ”.",
  "ja532:20.1": "“Ayaṃ ekapadī rāja,",
  "ja532:20.2": "yenetaṃ meghasannibhaṃ;",
  "ja532:20.3": "Koviḷārehi sañchannaṃ,",
  "ja532:20.4": "ettha sammati kosiyo”.",
  "ja532:21.1": "Idaṃ vatvāna pakkāmi,",
  "ja532:21.2": "taramāno mahāisi;",
  "ja532:21.3": "Vehāse antalikkhasmiṃ,",
  "ja532:21.4": "anusāsitvāna khattiye.",
  "ja532:22.1": "Assamaṃ parimajjitvā,",
  "ja532:22.2": "paññāpetvāna āsanaṃ;",
  "ja532:22.3": "Paṇṇasālaṃ pavisitvā,",
  "ja532:22.4": "pitaraṃ paṭibodhayi.",
  "ja532:23.1": "“Ime āyanti rājāno,",
  "ja532:23.2": "abhijātā yasassino;",
  "ja532:23.3": "Assamā nikkhamitvāna,",
  "ja532:23.4": "nisīda tvaṃ mahāise”.",
  "ja532:24.1": "Tassa taṃ vacanaṃ sutvā,",
  "ja532:24.2": "taramāno mahāisi;",
  "ja532:24.3": "Assamā nikkhamitvāna,",
  "ja532:24.4": "sadvāramhi upāvisi.",
  "ja532:25.1": "Tañca disvāna āyantaṃ,",
  "ja532:25.2": "jalantaṃriva tejasā;",
  "ja532:25.3": "Khatyasaṅghaparibyūḷhaṃ,",
  "ja532:25.4": "kosiyo etadabravi.",
  "ja532:26.1": "Kassa bherī mudiṅgā ca,",
  "ja532:26.2": "saṅkhā paṇavadindimā;",
  "ja532:26.3": "Purato paṭipannāni,",
  "ja532:26.4": "hāsayantā rathesabhaṃ.",
  "ja532:27.1": "Kassa kañcanapaṭṭena,",
  "ja532:27.2": "puthunā vijjuvaṇṇinā;",
  "ja532:27.3": "Yuvā kalāpasannaddho,",
  "ja532:27.4": "ko eti siriyā jalaṃ.",
  "ja532:28.1": "Ukkāmukhapahaṭṭhaṃva,",
  "ja532:28.2": "khadiraṅgārasannibhaṃ;",
  "ja532:28.3": "Mukhañca rucirā bhāti,",
  "ja532:28.4": "ko eti siriyā jalaṃ.",
  "ja532:29.1": "Kassa paggahitaṃ chattaṃ,",
  "ja532:29.2": "sasalākaṃ manoramaṃ;",
  "ja532:29.3": "Ādiccaraṃsāvaraṇaṃ,",
  "ja532:29.4": "ko eti siriyā jalaṃ.",
  "ja532:30.1": "Kassa aṅgaṃ pariggayha,",
  "ja532:30.2": "vāḷabījanimuttamaṃ;",
  "ja532:30.3": "Caranti varapuññassa,",
  "ja532:30.4": "hatthikkhandhena āyato.",
  "ja532:31.1": "Kassa setāni chattāni,",
  "ja532:31.2": "ājānīyā ca vammitā;",
  "ja532:31.3": "Samantā parikīrenti,",
  "ja532:31.4": "ko eti siriyā jalaṃ.",
  "ja532:32.1": "Kassa ekasataṃ khatyā,",
  "ja532:32.2": "anuyantā yasassino;",
  "ja532:32.3": "Samantānupariyanti,",
  "ja532:32.4": "ko eti siriyā jalaṃ.",
  "ja532:33.1": "Hatthi assaratha patti,",
  "ja532:33.2": "senā ca caturaṅginī;",
  "ja532:33.3": "Samantānupariyanti,",
  "ja532:33.4": "ko eti siriyā jalaṃ.",
  "ja532:34.1": "Kassesā mahatī senā,",
  "ja532:34.2": "piṭṭhito anuvattati;",
  "ja532:34.3": "Akkhobhaṇī apariyantā,",
  "ja532:34.4": "sāgarasseva ūmiyo.",
  "ja532:35.1": "Rājābhirājā manojo,",
  "ja532:35.2": "indova jayataṃ pati;",
  "ja532:35.3": "Nandassajjhāvaraṃ eti,",
  "ja532:35.4": "assamaṃ brahmacārinaṃ.",
  "ja532:36.1": "Tassesā mahatī senā,",
  "ja532:36.2": "piṭṭhito anuvattati;",
  "ja532:36.3": "Akkhobhaṇī apariyantā,",
  "ja532:36.4": "sāgarasseva ūmiyo.",
  "ja532:37.1": "Anulittā candanena,",
  "ja532:37.2": "kāsikuttamadhārino;",
  "ja532:37.3": "Sabbe pañjalikā hutvā,",
  "ja532:37.4": "isīnaṃ ajjhupāgamuṃ.",
  "ja532:38.1": "“Kacci nu bhoto kusalaṃ,",
  "ja532:38.2": "kacci bhoto anāmayaṃ;",
  "ja532:38.3": "Kacci uñchena yāpetha,",
  "ja532:38.4": "kacci mūlaphalā bahū.",
  "ja532:39.1": "Kacci ḍaṃsā makasā ca,",
  "ja532:39.2": "appameva sarīsapā;",
  "ja532:39.3": "Vane vāḷamigākiṇṇe,",
  "ja532:39.4": "kacci hiṃsā na vijjati”.",
  "ja532:40.1": "“Kusalañceva no rāja,",
  "ja532:40.2": "atho rāja anāmayaṃ;",
  "ja532:40.3": "Atho uñchena yāpema,",
  "ja532:40.4": "atho mūlaphalā bahū.",
  "ja532:41.1": "Atho ḍaṃsā makasā ca,",
  "ja532:41.2": "appameva sarīsapā;",
  "ja532:41.3": "Vane vāḷamigākiṇṇe,",
  "ja532:41.4": "hiṃsā mayhaṃ na vijjati.",
  "ja532:42.1": "Bahūni vassapūgāni,",
  "ja532:42.2": "assame sammataṃ idha;",
  "ja532:42.3": "Nābhijānāmi uppannaṃ,",
  "ja532:42.4": "ābādhaṃ amanoramaṃ.",
  "ja532:43.1": "Svāgataṃ te mahārāja,",
  "ja532:43.2": "atho te adurāgataṃ;",
  "ja532:43.3": "Issarosi anuppatto,",
  "ja532:43.4": "yaṃ idhatthi pavedaya.",
  "ja532:44.1": "Tindukāni piyālāni,",
  "ja532:44.2": "madhuke kāsumāriyo;",
  "ja532:44.3": "Phalāni khuddakappāni,",
  "ja532:44.4": "bhuñja rāja varaṃ varaṃ.",
  "ja532:45.1": "Idampi pānīyaṃ sītaṃ,",
  "ja532:45.2": "ābhataṃ girigabbharā;",
  "ja532:45.3": "Tato piva mahārāja,",
  "ja532:45.4": "sace tvaṃ abhikaṅkhasi”.",
  "ja532:46.1": "“Paṭiggahitaṃ yaṃ dinnaṃ,",
  "ja532:46.2": "sabbassa agghiyaṃ kataṃ;",
  "ja532:46.3": "Nandassāpi nisāmetha,",
  "ja532:46.4": "vacanaṃ so pavakkhati.",
  "ja532:47.1": "Ajjhāvaramhā nandassa,",
  "ja532:47.2": "bhoto santikamāgatā;",
  "ja532:47.3": "Suṇātu bhavaṃ vacanaṃ,",
  "ja532:47.4": "nandassa parisāya ca”.",
  "ja532:48.1": "“Parosataṃ jānapadā,",
  "ja532:48.2": "mahāsālā ca brāhmaṇā;",
  "ja532:48.3": "Ime ca khattiyā sabbe,",
  "ja532:48.4": "abhijātā yasassino;",
  "ja532:48.5": "Bhavañca rājā manojo,",
  "ja532:48.6": "anumaññantu me vaco.",
  "ja532:49.1": "Ye ca santi samītāro,",
  "ja532:49.2": "yakkhāni idha massame;",
  "ja532:49.3": "Araññe bhūtabhabyāni,",
  "ja532:49.4": "suṇantu vacanaṃ mama.",
  "ja532:50.1": "Namo katvāna bhūtānaṃ,",
  "ja532:50.2": "isiṃ vakkhāmi subbataṃ;",
  "ja532:50.3": "So tyāhaṃ dakkhiṇā bāhu,",
  "ja532:50.4": "tava kosiya sammato.",
  "ja532:51.1": "Pitaraṃ me janettiñca,",
  "ja532:51.2": "bhattukāmassa me sato;",
  "ja532:51.3": "Vīra puññamidaṃ ṭhānaṃ,",
  "ja532:51.4": "mā maṃ kosiya vāraya.",
  "ja532:52.1": "Sabbhi hetaṃ upaññātaṃ,",
  "ja532:52.2": "mametaṃ upanissaja;",
  "ja532:52.3": "Uṭṭhānapāricariyāya,",
  "ja532:52.4": "dīgharattaṃ tayā kataṃ;",
  "ja532:52.5": "Mātāpitūsu puññāni,",
  "ja532:52.6": "mama lokadado bhava.",
  "ja532:53.1": "Tatheva santi manujā,",
  "ja532:53.2": "dhamme dhammapadaṃ vidū;",
  "ja532:53.3": "Maggo saggassa lokassa,",
  "ja532:53.4": "yathā jānāsi tvaṃ ise.",
  "ja532:54.1": "Uṭṭhānapāricariyāya,",
  "ja532:54.2": "mātāpitusukhāvahaṃ;",
  "ja532:54.3": "Taṃ maṃ puññā nivāreti,",
  "ja532:54.4": "ariyamaggāvaro naro”.",
  "ja532:55.1": "“Suṇantu bhonto vacanaṃ,",
  "ja532:55.2": "bhāturajjhāvarā mama;",
  "ja532:55.3": "Kulavaṃsaṃ mahārāja,",
  "ja532:55.4": "porāṇaṃ parihāpayaṃ;",
  "ja532:55.5": "Adhammacārī jeṭṭhesu,",
  "ja532:55.6": "nirayaṃ sopapajjati.",
  "ja532:56.1": "Ye ca dhammassa kusalā,",
  "ja532:56.2": "porāṇassa disampati;",
  "ja532:56.3": "Cārittena ca sampannā,",
  "ja532:56.4": "na te gacchanti duggatiṃ.",
  "ja532:57.1": "Mātā pitā ca bhātā ca,",
  "ja532:57.2": "bhaginī ñātibandhavā;",
  "ja532:57.3": "Sabbe jeṭṭhassa te bhārā,",
  "ja532:57.4": "evaṃ jānāhi bhāradha.",
  "ja532:58.1": "Ādiyitvā garuṃ bhāraṃ,",
  "ja532:58.2": "nāviko viya ussahe;",
  "ja532:58.3": "Dhammañca nappamajjāmi,",
  "ja532:58.4": "jeṭṭho casmi rathesabha”.",
  "ja532:59.1": "“Adhigamā tame ñāṇaṃ,",
  "ja532:59.2": "Jālaṃva jātavedato;",
  "ja532:59.3": "Evameva no bhavaṃ dhammaṃ,",
  "ja532:59.4": "Kosiyo pavidaṃsayi.",
  "ja532:60.1": "Yathā udayamādicco,",
  "ja532:60.2": "Vāsudevo pabhaṅkaro;",
  "ja532:60.3": "Pāṇīnaṃ pavidaṃseti,",
  "ja532:60.4": "Rūpaṃ kalyāṇapāpakaṃ;",
  "ja532:60.5": "Evameva no bhavaṃ dhammaṃ,",
  "ja532:60.6": "Kosiyo pavidaṃsayi”.",
  "ja532:61.1": "“Evaṃ me yācamānassa,",
  "ja532:61.2": "añjaliṃ nāvabujjhatha;",
  "ja532:61.3": "Tava paddhacaro hessaṃ,",
  "ja532:61.4": "vuṭṭhito paricārako”.",
  "ja532:62.1": "“Addhā nanda vijānāsi,",
  "ja532:62.2": "Saddhammaṃ sabbhi desitaṃ;",
  "ja532:62.3": "Ariyo ariyasamācāro,",
  "ja532:62.4": "Bāḷhaṃ tvaṃ mama ruccasi.",
  "ja532:63.1": "Bhavantaṃ vadāmi bhotiñca,",
  "ja532:63.2": "suṇātha vacanaṃ mama;",
  "ja532:63.3": "Nāyaṃ bhāro bhāramato,",
  "ja532:63.4": "ahu mayhaṃ kudācanaṃ.",
  "ja532:64.1": "Taṃ maṃ upaṭṭhitaṃ santaṃ,",
  "ja532:64.2": "mātāpitusukhāvahaṃ;",
  "ja532:64.3": "Nando ajjhāvaraṃ katvā,",
  "ja532:64.4": "upaṭṭhānāya yācati.",
  "ja532:65.1": "Yo ve icchati kāmena,",
  "ja532:65.2": "santānaṃ brahmacārinaṃ;",
  "ja532:65.3": "Nandaṃ vo varatha eko,",
  "ja532:65.4": "kaṃ nando upatiṭṭhatu”.",
  "ja532:66.1": "“Tayā tāta anuññātā,",
  "ja532:66.2": "soṇa taṃ nissitā mayaṃ;",
  "ja532:66.3": "Upaghātuṃ labhe nandaṃ,",
  "ja532:66.4": "muddhani brahmacārinaṃ.",
  "ja532:67.1": "Assatthasseva taruṇaṃ,",
  "ja532:67.2": "pavāḷaṃ māluteritaṃ;",
  "ja532:67.3": "Cirassaṃ nandaṃ disvāna,",
  "ja532:67.4": "hadayaṃ me pavedhati.",
  "ja532:68.1": "Yadā suttāpi supine,",
  "ja532:68.2": "nandaṃ passāmi āgataṃ;",
  "ja532:68.3": "Udaggā sumanā homi,",
  "ja532:68.4": "nando no āgato ayaṃ.",
  "ja532:69.1": "Yadā ca paṭibujjhitvā,",
  "ja532:69.2": "nandaṃ passāmi nāgataṃ;",
  "ja532:69.3": "Bhiyyo āvisatī soko,",
  "ja532:69.4": "domanassañcanappakaṃ.",
  "ja532:70.1": "Sāhaṃ ajja cirassampi,",
  "ja532:70.2": "nandaṃ passāmi āgataṃ;",
  "ja532:70.3": "Bhattucca mayhañca piyo,",
  "ja532:70.4": "nando no pāvisī gharaṃ.",
  "ja532:71.1": "Pitupi nando suppiyo,",
  "ja532:71.2": "Yaṃ nando nappavase gharā;",
  "ja532:71.3": "Labhatū tāta nando taṃ,",
  "ja532:71.4": "Maṃ nando upatiṭṭhatu”.",
  "ja532:72.1": "“Anukampikā patiṭṭhā ca,",
  "ja532:72.2": "pubbe rasadadī ca no;",
  "ja532:72.3": "Maggo saggassa lokassa,",
  "ja532:72.4": "mātā taṃ varate ise.",
  "ja532:73.1": "Pubbe rasadadī gottī,",
  "ja532:73.2": "mātā puññūpasaṃhitā;",
  "ja532:73.3": "Maggo saggassa lokassa,",
  "ja532:73.4": "mātā taṃ varate ise”.",
  "ja532:74.1": "“Ākaṅkhamānā puttaphalaṃ,",
  "ja532:74.2": "devatāya namassati;",
  "ja532:74.3": "Nakkhattāni ca pucchati,",
  "ja532:74.4": "utusaṃvaccharāni ca.",
  "ja532:75.1": "Tassā utumhi nhātāya,",
  "ja532:75.2": "hoti gabbhassa vokkamo;",
  "ja532:75.3": "Tena dohaḷinī hoti,",
  "ja532:75.4": "suhadā tena vuccati.",
  "ja532:76.1": "Saṃvaccharaṃ vā ūnaṃ vā,",
  "ja532:76.2": "pariharitvā vijāyati;",
  "ja532:76.3": "Tena sā janayantīti,",
  "ja532:76.4": "janetti tena vuccati.",
  "ja532:77.1": "Thanakhīrena gītena,",
  "ja532:77.2": "aṅgapāvuraṇena ca;",
  "ja532:77.3": "Rodantaṃ puttaṃ toseti,",
  "ja532:77.4": "tosentī tena vuccati.",
  "ja532:78.1": "Tato vātātape ghore,",
  "ja532:78.2": "mamaṃ katvā udikkhati;",
  "ja532:78.3": "Dārakaṃ appajānantaṃ,",
  "ja532:78.4": "posentī tena vuccati.",
  "ja532:79.1": "Yañca mātudhanaṃ hoti,",
  "ja532:79.2": "yañca hoti pituddhanaṃ;",
  "ja532:79.3": "Ubhayampetassa gopeti,",
  "ja532:79.4": "api puttassa no siyā.",
  "ja532:80.1": "Evaṃ putta aduṃ putta,",
  "ja532:80.2": "iti mātā vihaññati;",
  "ja532:80.3": "Pamattaṃ paradāresu,",
  "ja532:80.4": "nisīthe pattayobbane;",
  "ja532:80.5": "Sāyaṃ puttaṃ anāyantaṃ,",
  "ja532:80.6": "iti mātā vihaññati.",
  "ja532:81.1": "Evaṃ kicchā bhato poso,",
  "ja532:81.2": "mātu aparicārako;",
  "ja532:81.3": "Mātari micchā caritvāna,",
  "ja532:81.4": "nirayaṃ sopapajjati.",
  "ja532:82.1": "Evaṃ kicchā bhato poso,",
  "ja532:82.2": "pitu aparicārako;",
  "ja532:82.3": "Pitari micchā caritvāna,",
  "ja532:82.4": "nirayaṃ sopapajjati.",
  "ja532:83.1": "Dhanāpi dhanakāmānaṃ,",
  "ja532:83.2": "Nassati iti me sutaṃ;",
  "ja532:83.3": "Mātaraṃ aparicaritvāna,",
  "ja532:83.4": "Kicchaṃ vā so nigacchati.",
  "ja532:84.1": "Dhanāpi dhanakāmānaṃ,",
  "ja532:84.2": "Nassati iti me sutaṃ;",
  "ja532:84.3": "Pitaraṃ aparicaritvāna,",
  "ja532:84.4": "Kicchaṃ vā so nigacchati.",
  "ja532:85.1": "Ānando ca pamodo ca,",
  "ja532:85.2": "sadā hasitakīḷitaṃ;",
  "ja532:85.3": "Mātaraṃ paricaritvāna,",
  "ja532:85.4": "labbhametaṃ vijānato.",
  "ja532:86.1": "Ānando ca pamodo ca,",
  "ja532:86.2": "sadā hasitakīḷitaṃ;",
  "ja532:86.3": "Pitaraṃ paricaritvāna,",
  "ja532:86.4": "labbhametaṃ vijānato.",
  "ja532:87.1": "Dānañca piyavācā ca,",
  "ja532:87.2": "atthacariyā ca yā idha;",
  "ja532:87.3": "Samānattatā ca dhammesu,",
  "ja532:87.4": "tattha tattha yathārahaṃ;",
  "ja532:87.5": "Ete kho saṅgahā loke,",
  "ja532:87.6": "rathassāṇīva yāyato.",
  "ja532:88.1": "Ete ca saṅgahā nāssu,",
  "ja532:88.2": "na mātā puttakāraṇā;",
  "ja532:88.3": "Labhetha mānaṃ pūjaṃ vā,",
  "ja532:88.4": "pitā vā puttakāraṇā.",
  "ja532:89.1": "Yasmā ca saṅgahā ete,",
  "ja532:89.2": "sammapekkhanti paṇḍitā;",
  "ja532:89.3": "Tasmā mahattaṃ papponti,",
  "ja532:89.4": "pāsaṃsā ca bhavanti te.",
  "ja532:90.1": "Brahmāti mātāpitaro,",
  "ja532:90.2": "pubbācariyāti vuccare;",
  "ja532:90.3": "Āhuneyyā ca puttānaṃ,",
  "ja532:90.4": "pajāya anukampakā.",
  "ja532:91.1": "Tasmā hi ne namasseyya,",
  "ja532:91.2": "sakkareyya ca paṇḍito;",
  "ja532:91.3": "Annena atho pānena,",
  "ja532:91.4": "vatthena sayanena ca;",
  "ja532:91.5": "Ucchādanena nhāpanena,",
  "ja532:91.6": "pādānaṃ dhovanena ca.",
  "ja532:92.1": "Tāya naṃ pāricariyāya,",
  "ja532:92.2": "Mātāpitūsu paṇḍitā;",
  "ja532:92.3": "Idheva naṃ pasaṃsanti,",
  "ja532:92.4": "Pecca sagge pamodatī”ti. (3718)",
  "ja532:93.1": "Soṇanandajātakaṃ dutiyaṃ.",
  "ja532:94.1": "Sattatinipātaṃ niṭṭhitaṃ.",
  "ja532:95.1": "Tassuddānaṃ",
  "ja532:96.1": "Atha sattatimamhi nipātavare,",
  "ja532:96.2": "Sabhāvantu kusāvatirājavaro;",
  "ja532:96.3": "Atha soṇasunandavaro ca puna,",
  "ja532:96.4": "Abhivāsitasattatimamhi suteti."
}