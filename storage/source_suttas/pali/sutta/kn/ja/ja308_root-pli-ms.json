{
  "ja308:0.1": "Jātaka",
  "ja308:0.2": "Catukkanipāta",
  "ja308:0.3": "Kāliṅgavagga",
  "ja308:0.4": "8. Sakuṇajātaka",
  "ja308:1.1": "“Akaramhasa te kiccaṃ,",
  "ja308:1.2": "yaṃ balaṃ ahuvamhase;",
  "ja308:1.3": "Migarāja namo tyatthu,",
  "ja308:1.4": "api kiñci labhāmase”.",
  "ja308:2.1": "“Mama lohitabhakkhassa,",
  "ja308:2.2": "niccaṃ luddāni kubbato;",
  "ja308:2.3": "Dantantaragato santo,",
  "ja308:2.4": "taṃ bahuṃ yampi jīvasi”.",
  "ja308:3.1": "“Akataññumakattāraṃ,",
  "ja308:3.2": "katassa appaṭikārakaṃ;",
  "ja308:3.3": "Yasmiṃ kataññutā natthi,",
  "ja308:3.4": "niratthā tassa sevanā.",
  "ja308:4.1": "Yassa sammukhaciṇṇena,",
  "ja308:4.2": "Mittadhammo na labbhati;",
  "ja308:4.3": "Anusūyamanakkosaṃ,",
  "ja308:4.4": "Saṇikaṃ tamhā apakkame”ti.",
  "ja308:5.1": "Sakuṇajātakaṃ aṭṭhamaṃ."
}