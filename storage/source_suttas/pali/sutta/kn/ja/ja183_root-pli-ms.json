{
  "ja183:0.1": "Jātaka",
  "ja183:0.2": "Dukanipāta",
  "ja183:0.3": "Asadisavagga",
  "ja183:0.4": "3. Vālodakajātaka",
  "ja183:1.1": "“Vālodakaṃ apparasaṃ nihīnaṃ,",
  "ja183:1.2": "Pitvā mado jāyati gadrabhānaṃ;",
  "ja183:1.3": "Imañca pitvāna rasaṃ paṇītaṃ,",
  "ja183:1.4": "Mado na sañjāyati sindhavānaṃ”.",
  "ja183:2.1": "“Appaṃ pivitvāna nihīnajacco,",
  "ja183:2.2": "So majjatī tena janinda puṭṭho;",
  "ja183:2.3": "Dhorayhasīlī ca kulamhi jāto,",
  "ja183:2.4": "Na majjatī aggarasaṃ pivitvā”ti.",
  "ja183:3.1": "Vālodakajātakaṃ tatiyaṃ."
}