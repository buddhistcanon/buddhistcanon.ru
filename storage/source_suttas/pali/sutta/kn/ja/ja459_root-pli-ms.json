{
  "ja459:0.1": "Jātaka",
  "ja459:0.2": "Ekādasakanipāta",
  "ja459:0.3": "Mātuposakavagga",
  "ja459:0.4": "5. Pānīyajātaka",
  "ja459:1.1": "“Mitto mittassa pānīyaṃ,",
  "ja459:1.2": "adinnaṃ paribhuñjisaṃ;",
  "ja459:1.3": "Tena pacchā vijigucchiṃ,",
  "ja459:1.4": "taṃ pāpaṃ pakataṃ mayā;",
  "ja459:1.5": "Mā puna akaraṃ pāpaṃ,",
  "ja459:1.6": "tasmā pabbajito ahaṃ.",
  "ja459:2.1": "Paradārañca disvāna,",
  "ja459:2.2": "chando me udapajjatha;",
  "ja459:2.3": "Tena pacchā vijigucchiṃ,",
  "ja459:2.4": "taṃ pāpaṃ pakataṃ mayā;",
  "ja459:2.5": "Mā puna akaraṃ pāpaṃ,",
  "ja459:2.6": "tasmā pabbajito ahaṃ.",
  "ja459:3.1": "Pitaraṃ me mahārāja,",
  "ja459:3.2": "corā agaṇhu kānane;",
  "ja459:3.3": "Tesāhaṃ pucchito jānaṃ,",
  "ja459:3.4": "aññathā naṃ viyākariṃ.",
  "ja459:4.1": "Tena pacchā vijigucchiṃ,",
  "ja459:4.2": "taṃ pāpaṃ pakataṃ mayā;",
  "ja459:4.3": "Mā puna akaraṃ pāpaṃ,",
  "ja459:4.4": "tasmā pabbajito ahaṃ.",
  "ja459:5.1": "Pāṇātipātamakaruṃ,",
  "ja459:5.2": "somayāge upaṭṭhite;",
  "ja459:5.3": "Tesāhaṃ samanuññāsiṃ.",
  "ja459:6.1": "Tena pacchā vijigucchiṃ,",
  "ja459:6.2": "taṃ pāpaṃ pakataṃ mayā;",
  "ja459:6.3": "Mā puna akaraṃ pāpaṃ,",
  "ja459:6.4": "tasmā pabbajito ahaṃ.",
  "ja459:7.1": "Surāmerayamādhukā,",
  "ja459:7.2": "ye janā paṭhamāsu no;",
  "ja459:7.3": "Bahūnaṃ te anatthāya,",
  "ja459:7.4": "majjapānamakappayuṃ;",
  "ja459:7.5": "Tesāhaṃ samanuññāsiṃ.",
  "ja459:8.1": "Tena pacchā vijigucchiṃ,",
  "ja459:8.2": "taṃ pāpaṃ pakataṃ mayā;",
  "ja459:8.3": "Mā puna akaraṃ pāpaṃ,",
  "ja459:8.4": "tasmā pabbajito ahaṃ”.",
  "ja459:9.1": "“Dhiratthu subahū kāme,",
  "ja459:9.2": "duggandhe bahukaṇṭake;",
  "ja459:9.3": "Ye ahaṃ paṭisevanto,",
  "ja459:9.4": "nālabhiṃ tādisaṃ sukhaṃ”.",
  "ja459:10.1": "“Mahassādā sukhā kāmā,",
  "ja459:10.2": "Natthi kāmā paraṃ sukhaṃ;",
  "ja459:10.3": "Ye kāme paṭisevanti,",
  "ja459:10.4": "Saggaṃ te upapajjare”.",
  "ja459:11.1": "“Appassādā dukhā kāmā,",
  "ja459:11.2": "natthi kāmā paraṃ dukhaṃ;",
  "ja459:11.3": "Ye kāme paṭisevanti,",
  "ja459:11.4": "nirayaṃ te upapajjare.",
  "ja459:12.1": "Asī yathā sunisito,",
  "ja459:12.2": "nettiṃsova supāyiko;",
  "ja459:12.3": "Sattīva urasi khittā,",
  "ja459:12.4": "kāmā dukkhatarā tato.",
  "ja459:13.1": "Aṅgārānaṃva jalitaṃ,",
  "ja459:13.2": "kāsuṃ sādhikaporisaṃ;",
  "ja459:13.3": "Phālaṃva divasantattaṃ,",
  "ja459:13.4": "kāmā dukkhatarā tato.",
  "ja459:14.1": "Visaṃ yathā halāhalaṃ,",
  "ja459:14.2": "telaṃ pakkuthitaṃ yathā;",
  "ja459:14.3": "Tambaloha vilīnaṃva,",
  "ja459:14.4": "kāmā dukkhatarā tato”ti.",
  "ja459:15.1": "Pānīyajātakaṃ pañcamaṃ."
}