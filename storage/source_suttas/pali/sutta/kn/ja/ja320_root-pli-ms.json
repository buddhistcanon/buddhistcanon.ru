{
  "ja320:0.1": "Jātaka",
  "ja320:0.2": "Catukkanipāta",
  "ja320:0.3": "Pucimandavagga",
  "ja320:0.4": "10. Succajajātaka",
  "ja320:1.1": "“Succajaṃ vata naccaji,",
  "ja320:1.2": "vācāya adadaṃ giriṃ;",
  "ja320:1.3": "Kiṃ hitassa cajantassa,",
  "ja320:1.4": "vācāya adada pabbataṃ”.",
  "ja320:2.1": "“Yañhi kayirā tañhi vade,",
  "ja320:2.2": "yaṃ na kayirā na taṃ vade;",
  "ja320:2.3": "Akarontaṃ bhāsamānaṃ,",
  "ja320:2.4": "parijānanti paṇḍitā”.",
  "ja320:3.1": "“Rājaputta namo tyatthu,",
  "ja320:3.2": "sacce dhamme ṭhito casi;",
  "ja320:3.3": "Yassa te byasanaṃ patto,",
  "ja320:3.4": "saccasmiṃ ramate mano”.",
  "ja320:4.1": "“Yā daliddī daliddassa,",
  "ja320:4.2": "aḍḍhā aḍḍhassa kittima;",
  "ja320:4.3": "Sā hissa paramā bhariyā,",
  "ja320:4.4": "sahiraññassa itthiyo”ti.",
  "ja320:5.1": "Succajajātakaṃ dasamaṃ.",
  "ja320:5.2": "Pucimandavaggo dutiyo.",
  "ja320:6.1": "Tassuddānaṃ",
  "ja320:7.1": "Atha cora sakassapa khantīvaro,",
  "ja320:7.2": "Dujjīvitatā ca varā pharusā;",
  "ja320:7.3": "Atha sasa matañca vasanta sukhaṃ,",
  "ja320:7.4": "Succajaṃvatanaccajinā ca dasāti."
}