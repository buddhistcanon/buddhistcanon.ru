{
  "ja82:0.1": "Jātaka",
  "ja82:0.2": "Ekakanipāta",
  "ja82:0.3": "Apāyimhavagga",
  "ja82:0.4": "2. Mittavindakajātaka",
  "ja82:1.1": "“Atikkamma ramaṇakaṃ,",
  "ja82:1.2": "sadāmattañca dūbhakaṃ;",
  "ja82:1.3": "Svāsi pāsāṇamāsīno,",
  "ja82:1.4": "yasmā jīvaṃ na mokkhasī”ti.",
  "ja82:2.1": "Mittavindakajātakaṃ dutiyaṃ."
}