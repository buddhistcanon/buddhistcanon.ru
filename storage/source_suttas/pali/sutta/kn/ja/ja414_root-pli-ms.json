{
  "ja414:0.1": "Jātaka",
  "ja414:0.2": "Sattakanipāta",
  "ja414:0.3": "Gandhāravagga",
  "ja414:0.4": "9. Jāgarajātaka",
  "ja414:1.1": "“Kodha jāgarataṃ sutto,",
  "ja414:1.2": "kodha suttesu jāgaro;",
  "ja414:1.3": "Ko mametaṃ vijānāti,",
  "ja414:1.4": "ko taṃ paṭibhaṇāti me”.",
  "ja414:2.1": "“Ahaṃ jāgarataṃ sutto,",
  "ja414:2.2": "ahaṃ suttesu jāgaro;",
  "ja414:2.3": "Ahametaṃ vijānāmi,",
  "ja414:2.4": "ahaṃ paṭibhaṇāmi te”.",
  "ja414:3.1": "“Kathaṃ jāgarataṃ sutto,",
  "ja414:3.2": "kathaṃ suttesu jāgaro;",
  "ja414:3.3": "Kathaṃ etaṃ vijānāsi,",
  "ja414:3.4": "kathaṃ paṭibhaṇāsi me”.",
  "ja414:4.1": "“Ye dhammaṃ nappajānanti,",
  "ja414:4.2": "saṃyamoti damoti ca;",
  "ja414:4.3": "Tesu suppamānesu,",
  "ja414:4.4": "ahaṃ jaggāmi devate.",
  "ja414:5.1": "Yesaṃ rāgo ca doso ca,",
  "ja414:5.2": "avijjā ca virājitā;",
  "ja414:5.3": "Tesu jāgaramānesu,",
  "ja414:5.4": "ahaṃ suttosmi devate.",
  "ja414:6.1": "Evaṃ jāgarataṃ sutto,",
  "ja414:6.2": "evaṃ suttesu jāgaro;",
  "ja414:6.3": "Evametaṃ vijānāmi,",
  "ja414:6.4": "evaṃ paṭibhaṇāmi te”.",
  "ja414:7.1": "“Sādhu jāgarataṃ sutto,",
  "ja414:7.2": "sādhu suttesu jāgaro;",
  "ja414:7.3": "Sādhumetaṃ vijānāsi,",
  "ja414:7.4": "sādhu paṭibhaṇāsi me”ti.",
  "ja414:8.1": "Jāgarajātakaṃ navamaṃ."
}