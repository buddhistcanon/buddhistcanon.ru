{
  "ja483:0.1": "Jātaka",
  "ja483:0.2": "Terasakanipāta",
  "ja483:0.3": "Ambavagga",
  "ja483:0.4": "10. Sarabhamigajātaka",
  "ja483:1.1": "“Āsīsetheva puriso,",
  "ja483:1.2": "na nibbindeyya paṇḍito;",
  "ja483:1.3": "Passāmi vohaṃ attānaṃ,",
  "ja483:1.4": "yathā icchiṃ tathā ahu.",
  "ja483:2.1": "Āsīsetheva puriso,",
  "ja483:2.2": "na nibbindeyya paṇḍito;",
  "ja483:2.3": "Passāmi vohaṃ attānaṃ,",
  "ja483:2.4": "udakā thalamubbhataṃ.",
  "ja483:3.1": "Vāyametheva puriso,",
  "ja483:3.2": "na nibbindeyya paṇḍito;",
  "ja483:3.3": "Passāmi vohaṃ attānaṃ,",
  "ja483:3.4": "yathā icchiṃ tathā ahu.",
  "ja483:4.1": "Vāyametheva puriso,",
  "ja483:4.2": "na nibbindeyya paṇḍito;",
  "ja483:4.3": "Passāmi vohaṃ attānaṃ,",
  "ja483:4.4": "udakā thalamubbhataṃ.",
  "ja483:5.1": "Dukkhūpanītopi naro sapañño,",
  "ja483:5.2": "Āsaṃ na chindeyya sukhāgamāya;",
  "ja483:5.3": "Bahū hi phassā ahitā hitā ca,",
  "ja483:5.4": "Avitakkitā maccamupabbajanti.",
  "ja483:6.1": "Acintitampi bhavati,",
  "ja483:6.2": "cintitampi vinassati;",
  "ja483:6.3": "Na hi cintāmayā bhogā,",
  "ja483:6.4": "itthiyā purisassa vā”.",
  "ja483:7.1": "“Sarabhaṃ giriduggasmiṃ,",
  "ja483:7.2": "yaṃ tvaṃ anusarī pure;",
  "ja483:7.3": "Alīnacittassa tuvaṃ,",
  "ja483:7.4": "vikkantamanujīvasi.",
  "ja483:8.1": "Yo taṃ viduggā narakā samuddhari,",
  "ja483:8.2": "Silāya yoggaṃ sarabho karitvā;",
  "ja483:8.3": "Dukkhūpanītaṃ maccumukhā pamocayi,",
  "ja483:8.4": "Alīnacittaṃ ta migaṃ vadesi”.",
  "ja483:9.1": "“Kiṃ tvaṃ nu tattheva tadā ahosi,",
  "ja483:9.2": "Udāhu te koci naṃ etadakkhā;",
  "ja483:9.3": "Vivaṭṭacchaddo nusi sabbadassī,",
  "ja483:9.4": "Ñāṇaṃ nu te brāhmaṇa bhiṃsarūpaṃ”.",
  "ja483:10.1": "“Na cevahaṃ tattha tadā ahosiṃ,",
  "ja483:10.2": "Na cāpi me koci naṃ etadakkhā;",
  "ja483:10.3": "Gāthāpadānañca subhāsitānaṃ,",
  "ja483:10.4": "Atthaṃ tadānenti janinda dhīrā”.",
  "ja483:11.1": "“Ādāya pattiṃ paraviriyaghātiṃ,",
  "ja483:11.2": "Cāpe saraṃ kiṃ vicikicchase tuvaṃ;",
  "ja483:11.3": "Nunno saro sarabhaṃ hantu khippaṃ,",
  "ja483:11.4": "Annañhi etaṃ varapañña rañño”.",
  "ja483:12.1": "“Addhā pajānāmi ahampi etaṃ,",
  "ja483:12.2": "Annaṃ migo brāhmaṇa khattiyassa;",
  "ja483:12.3": "Pubbe katañca apacāyamāno,",
  "ja483:12.4": "Tasmā migaṃ sarabhaṃ no hanāmi”.",
  "ja483:13.1": "“Neso migo mahārāja,",
  "ja483:13.2": "asureso disampati;",
  "ja483:13.3": "Etaṃ hantvā manussinda,",
  "ja483:13.4": "bhavassu amarādhipo.",
  "ja483:14.1": "Sace ca rājā vicikicchase tuvaṃ,",
  "ja483:14.2": "Hantuṃ migaṃ sarabhaṃ sahāyakaṃ;",
  "ja483:14.3": "Saputtadāro naravīraseṭṭha,",
  "ja483:14.4": "Gantā tuvaṃ vetaraṇiṃ yamassa”.",
  "ja483:15.1": "“Kāmaṃ ahaṃ jānapadā ca sabbe,",
  "ja483:15.2": "Puttā ca dārā ca sahāyasaṅghā;",
  "ja483:15.3": "Gacchemu taṃ vetaraṇiṃ yamassa,",
  "ja483:15.4": "Na tveva hañño mama pāṇado yo.",
  "ja483:16.1": "Ayaṃ migo kicchagatassa mayhaṃ,",
  "ja483:16.2": "Ekassa kattā vivanasmi ghore;",
  "ja483:16.3": "Taṃ tādisaṃ pubbakiccaṃ saranto,",
  "ja483:16.4": "Jānaṃ mahābrahme kathaṃ haneyyaṃ”.",
  "ja483:17.1": "“Mittābhirādhī cirameva jīva,",
  "ja483:17.2": "Rajjaṃ imaṃ dhammaguṇe pasāsa;",
  "ja483:17.3": "Nārīgaṇehi paricāriyanto,",
  "ja483:17.4": "Modassu raṭṭhe tidiveva vāsavo.",
  "ja483:18.1": "Akkodhano niccapasannacitto,",
  "ja483:18.2": "Sabbātithī yācayogo bhavitvā;",
  "ja483:18.3": "Datvā ca bhutvā ca yathānubhāvaṃ,",
  "ja483:18.4": "Anindito saggamupehi ṭhānan”ti. (1930)",
  "ja483:19.1": "Sarabhamigajātakaṃ dasamaṃ.",
  "ja483:20.1": "Terasakanipātaṃ niṭṭhitaṃ.",
  "ja483:21.1": "Tassuddānaṃ",
  "ja483:22.1": "Varaamba kuṭhāri sahaṃsavaro,",
  "ja483:22.2": "Atharaññasmiṃ dūtakapañcamako;",
  "ja483:22.3": "Atha bodhi akitti sutakkarinā,",
  "ja483:22.4": "Atha rurumigenaparo sarabhoti."
}