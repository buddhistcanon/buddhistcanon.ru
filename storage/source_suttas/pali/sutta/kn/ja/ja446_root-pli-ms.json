{
  "ja446:0.1": "Jātaka",
  "ja446:0.2": "Dasakanipāta",
  "ja446:0.3": "Catudvāravagga",
  "ja446:0.4": "8. Takkalajātaka",
  "ja446:1.1": "“Na takkalā santi na āluvāni,",
  "ja446:1.2": "Na biḷāliyo na kaḷambāni tāta;",
  "ja446:1.3": "Eko araññamhi susānamajjhe,",
  "ja446:1.4": "Kimatthiko tāta khaṇāsi kāsuṃ”.",
  "ja446:2.1": "“Pitāmaho tāta sudubbalo te,",
  "ja446:2.2": "Anekabyādhīhi dukhena phuṭṭho;",
  "ja446:2.3": "Tamajjahaṃ nikhaṇissāmi sobbhe,",
  "ja446:2.4": "Na hissa taṃ jīvitaṃ rocayāmi”.",
  "ja446:3.1": "“Saṅkappametaṃ paṭiladdha pāpakaṃ,",
  "ja446:3.2": "Accāhitaṃ kamma karosi luddaṃ;",
  "ja446:3.3": "Mayāpi tāta paṭilacchase tuvaṃ,",
  "ja446:3.4": "Etādisaṃ kamma jarūpanīto;",
  "ja446:3.5": "Taṃ kullavattaṃ anuvattamāno,",
  "ja446:3.6": "Ahampi taṃ nikhaṇissāmi sobbhe”.",
  "ja446:4.1": "“Pharusāhi vācāhi pakubbamāno,",
  "ja446:4.2": "Āsajja maṃ tvaṃ vadase kumāra;",
  "ja446:4.3": "Putto mamaṃ orasako samāno,",
  "ja446:4.4": "Ahitānukampī mama tvaṃsi putta”.",
  "ja446:5.1": "“Na tāhaṃ tāta ahitānukampī,",
  "ja446:5.2": "Hitānukampī te ahampi tāta;",
  "ja446:5.3": "Pāpañca taṃ kamma pakubbamānaṃ,",
  "ja446:5.4": "Arahāmi no vārayituṃ tato”.",
  "ja446:6.1": "“Yo mātaraṃ vā pitaraṃ saviṭṭha,",
  "ja446:6.2": "Adūsake hiṃsati pāpadhammo;",
  "ja446:6.3": "Kāyassa bhedā abhisamparāyaṃ,",
  "ja446:6.4": "Asaṃsayaṃ so nirayaṃ upeti.",
  "ja446:7.1": "Yo mātaraṃ vā pitaraṃ saviṭṭha,",
  "ja446:7.2": "Annena pānena upaṭṭhahāti;",
  "ja446:7.3": "Kāyassa bhedā abhisamparāyaṃ,",
  "ja446:7.4": "Asaṃsayaṃ so sugatiṃ upeti.",
  "ja446:8.1": "Na me tvaṃ putta ahitānukampī,",
  "ja446:8.2": "Hitānukampī me tvaṃsi putta;",
  "ja446:8.3": "Ahañca taṃ mātarā vuccamāno,",
  "ja446:8.4": "Etādisaṃ kamma karomi luddaṃ”.",
  "ja446:9.1": "“Yā te sā bhariyā anariyarūpā,",
  "ja446:9.2": "Mātā mamesā sakiyā janetti;",
  "ja446:9.3": "Niddhāpaye tañca sakā agārā,",
  "ja446:9.4": "Aññampi te sā dukhamāvaheyya.",
  "ja446:10.1": "Yā te sā bhariyā anariyarūpā,",
  "ja446:10.2": "Mātā mamesā sakiyā janetti;",
  "ja446:10.3": "Dantā kareṇūva vasūpanītā,",
  "ja446:10.4": "Sā pāpadhammā punarāvajātū”ti.",
  "ja446:11.1": "Takkalajātakaṃ aṭṭhamaṃ."
}