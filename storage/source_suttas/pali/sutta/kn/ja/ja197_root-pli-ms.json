{
  "ja197:0.1": "Jātaka",
  "ja197:0.2": "Dukanipāta",
  "ja197:0.3": "Ruhakavagga",
  "ja197:0.4": "7. Mittāmittajātaka",
  "ja197:1.1": "“Na naṃ umhayate disvā,",
  "ja197:1.2": "na ca naṃ paṭinandati;",
  "ja197:1.3": "Cakkhūni cassa na dadāti,",
  "ja197:1.4": "paṭilomañca vattati.",
  "ja197:2.1": "Ete bhavanti ākārā,",
  "ja197:2.2": "amittasmiṃ patiṭṭhitā;",
  "ja197:2.3": "Yehi amittaṃ jāneyya,",
  "ja197:2.4": "disvā sutvā ca paṇḍito”ti.",
  "ja197:3.1": "Mittāmittajātakaṃ sattamaṃ."
}