{
  "ja210:0.1": "Jātaka",
  "ja210:0.2": "Dukanipāta",
  "ja210:0.3": "Nataṃdaḷhavagga",
  "ja210:0.4": "10. Kandagalakajātaka",
  "ja210:1.1": "“Ambho ko nāma yaṃ rukkho,",
  "ja210:1.2": "sinnapatto sakaṇṭako;",
  "ja210:1.3": "Yattha ekappahārena,",
  "ja210:1.4": "uttamaṅgaṃ vibhijjitaṃ”.",
  "ja210:2.1": "“Acāri vatāyaṃ vitudaṃ vanāni,",
  "ja210:2.2": "Kaṭṭhaṅgarukkhesu asārakesu;",
  "ja210:2.3": "Athāsadā khadiraṃ jātasāraṃ,",
  "ja210:2.4": "Yatthabbhidā garuḷo uttamaṅgan”ti.",
  "ja210:3.1": "Kandagalakajātakaṃ dasamaṃ.",
  "ja210:3.2": "Nataṃdaḷhavaggo chaṭṭho.",
  "ja210:4.1": "Tassuddānaṃ",
  "ja210:5.1": "Daḷhabandhana haṃsavaro ca puna,",
  "ja210:5.2": "Virūpakkha saviṭṭhaka macchavaro;",
  "ja210:5.3": "Sakuruṅga saassaka ambavaro,",
  "ja210:5.4": "Puna kukkuṭako garuḷena dasāti."
}