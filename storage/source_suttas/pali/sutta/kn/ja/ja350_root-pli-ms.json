{
  "ja350:0.1": "Jātaka",
  "ja350:0.2": "Catukkanipāta",
  "ja350:0.3": "Cūḷakuṇālavagga",
  "ja350:0.4": "10. Devatāpañhajātaka",
  "ja350:1.1": "“Hanti hatthehi pādehi,",
  "ja350:1.2": "mukhañca parisumbhati;",
  "ja350:1.3": "Sa ve rāja piyo hoti,",
  "ja350:1.4": "kaṃ tena tvābhipassasi”.",
  "ja350:2.1": "“Akkosati yathākāmaṃ,",
  "ja350:2.2": "āgamañcassa icchati;",
  "ja350:2.3": "Sa ve rāja piyo hoti,",
  "ja350:2.4": "kaṃ tena tvābhipassasi”.",
  "ja350:3.1": "“Abbhakkhāti abhūtena,",
  "ja350:3.2": "alikenābhisāraye;",
  "ja350:3.3": "Sa ve rāja piyo hoti,",
  "ja350:3.4": "kaṃ tena tvābhipassasi”.",
  "ja350:4.1": "“Haraṃ annañca pānañca,",
  "ja350:4.2": "Vatthasenāsanāni ca;",
  "ja350:4.3": "Aññadatthuharā santā,",
  "ja350:4.4": "Te ve rāja piyā honti;",
  "ja350:4.5": "Kaṃ tena tvābhipassasī”ti. (701)",
  "ja350:5.1": "Devatāpañhajātakaṃ dasamaṃ.",
  "ja350:5.2": "Cūḷakuṇālavaggo pañcamo.",
  "ja350:6.1": "Tassuddānaṃ",
  "ja350:7.1": "Narānaṃ asakkhivasimhavaro,",
  "ja350:7.2": "Nīliyamaggivarañca puna;",
  "ja350:7.3": "Puna rasāyasakūṭavaro,",
  "ja350:7.4": "Tathārañña sārathi hanti dasāti.",
  "ja350:8.1": "Atha vagguddānaṃ",
  "ja350:9.1": "Kāliṅgaṃ pucimandañca,",
  "ja350:9.2": "kuṭidūsaka kokilā;",
  "ja350:9.3": "Cūḷakuṇālavaggo so,",
  "ja350:9.4": "pañcamo suppakāsitoti.",
  "ja350:10.1": "Catukkanipātaṃ niṭṭhitaṃ."
}