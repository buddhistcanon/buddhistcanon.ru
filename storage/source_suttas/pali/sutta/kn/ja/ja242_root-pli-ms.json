{
  "ja242:0.1": "Jātaka",
  "ja242:0.2": "Dukanipāta",
  "ja242:0.3": "Siṅgālavagga",
  "ja242:0.4": "2. Sunakhajātaka",
  "ja242:1.1": "“Bālo vatāyaṃ sunakho,",
  "ja242:1.2": "yo varattaṃ na khādati;",
  "ja242:1.3": "Bandhanā ca pamuñceyya,",
  "ja242:1.4": "asito ca gharaṃ vaje”.",
  "ja242:2.1": "“Aṭṭhitaṃ me manasmiṃ me,",
  "ja242:2.2": "atho me hadaye kataṃ;",
  "ja242:2.3": "Kālañca paṭikaṅkhāmi,",
  "ja242:2.4": "yāva passupatū jano”ti.",
  "ja242:3.1": "Sunakhajātakaṃ dutiyaṃ."
}