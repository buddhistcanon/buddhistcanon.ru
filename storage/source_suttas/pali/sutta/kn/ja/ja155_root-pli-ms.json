{
  "ja155:0.1": "Jātaka",
  "ja155:0.2": "Dukanipāta",
  "ja155:0.3": "Daḷhavagga",
  "ja155:0.4": "5. Bhaggajātaka",
  "ja155:1.1": "“Jīva vassasataṃ bhagga,",
  "ja155:1.2": "aparāni ca vīsatiṃ;",
  "ja155:1.3": "Mā maṃ pisācā khādantu,",
  "ja155:1.4": "jīva tvaṃ sarado sataṃ”.",
  "ja155:2.1": "“Tvampi vassasataṃ jīva,",
  "ja155:2.2": "aparāni ca vīsatiṃ;",
  "ja155:2.3": "Visaṃ pisācā khādantu,",
  "ja155:2.4": "jīva tvaṃ sarado satan”ti.",
  "ja155:3.1": "Bhaggajātakaṃ pañcamaṃ."
}