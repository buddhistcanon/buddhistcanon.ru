{
  "ja521:0.1": "Jātaka",
  "ja521:0.2": "Cattālīsanipāta",
  "ja521:0.3": "Tesakuṇavagga",
  "ja521:0.4": "1. Tesakuṇajātaka",
  "ja521:1.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "ja521:2.1": "“Vessantaraṃ taṃ pucchāmi,",
  "ja521:2.2": "sakuṇa bhaddamatthu te;",
  "ja521:2.3": "Rajjaṃ kāretukāmena,",
  "ja521:2.4": "kiṃ su kiccaṃ kataṃ varaṃ”.",
  "ja521:3.1": "“Cirassaṃ vata maṃ tāto,",
  "ja521:3.2": "kaṃso bārāṇasiggaho;",
  "ja521:3.3": "Pamatto appamattaṃ maṃ,",
  "ja521:3.4": "pitā puttaṃ acodayi.",
  "ja521:4.1": "Paṭhameneva vitathaṃ,",
  "ja521:4.2": "kodhaṃ hāsaṃ nivāraye;",
  "ja521:4.3": "Tato kiccāni kāreyya,",
  "ja521:4.4": "taṃ vataṃ āhu khattiya.",
  "ja521:5.1": "Yaṃ tvaṃ tāta tapokammaṃ,",
  "ja521:5.2": "Pubbe katamasaṃsayaṃ;",
  "ja521:5.3": "Ratto duṭṭho ca yaṃ kayirā,",
  "ja521:5.4": "Na taṃ kayirā tato puna.",
  "ja521:6.1": "Khattiyassa pamattassa,",
  "ja521:6.2": "raṭṭhasmiṃ raṭṭhavaḍḍhana;",
  "ja521:6.3": "Sabbe bhogā vinassanti,",
  "ja521:6.4": "rañño taṃ vuccate aghaṃ.",
  "ja521:7.1": "Sirī tāta alakkhī ca,",
  "ja521:7.2": "pucchitā etadabravuṃ;",
  "ja521:7.3": "Uṭṭhānavīriye pose,",
  "ja521:7.4": "ramāhaṃ anusūyake.",
  "ja521:8.1": "Usūyake duhadaye,",
  "ja521:8.2": "purise kammadussake;",
  "ja521:8.3": "Kāḷakaṇṇī mahārāja,",
  "ja521:8.4": "ramati cakkabhañjanī.",
  "ja521:9.1": "So tvaṃ sabbesu suhadayo,",
  "ja521:9.2": "sabbesaṃ rakkhito bhava;",
  "ja521:9.3": "Alakkhiṃ nuda mahārāja,",
  "ja521:9.4": "lakkhyā bhava nivesanaṃ.",
  "ja521:10.1": "Sa lakkhīdhitisampanno,",
  "ja521:10.2": "puriso hi mahaggato;",
  "ja521:10.3": "Amittānaṃ kāsipati,",
  "ja521:10.4": "mūlaṃ aggañca chindati.",
  "ja521:11.1": "Sakkopi hi bhūtapati,",
  "ja521:11.2": "uṭṭhāne nappamajjati;",
  "ja521:11.3": "Sa kalyāṇe dhitiṃ katvā,",
  "ja521:11.4": "uṭṭhāne kurute mano.",
  "ja521:12.1": "Gandhabbā pitaro devā,",
  "ja521:12.2": "sājīvā honti tādino;",
  "ja521:12.3": "Uṭṭhāhato appamajjato,",
  "ja521:12.4": "anutiṭṭhanti devatā.",
  "ja521:13.1": "So appamatto akkuddho,",
  "ja521:13.2": "tāta kiccāni kāraya;",
  "ja521:13.3": "Vāyamassu ca kiccesu,",
  "ja521:13.4": "nālaso vindate sukhaṃ.",
  "ja521:14.1": "Tattheva te vattapadā,",
  "ja521:14.2": "esāva anusāsanī;",
  "ja521:14.3": "Alaṃ mitte sukhāpetuṃ,",
  "ja521:14.4": "amittānaṃ dukhāya ca”.",
  "ja521:15.1": "“Sakkhisi tvaṃ kuṇḍalini,",
  "ja521:15.2": "maññasi khattabandhuni;",
  "ja521:15.3": "Rajjaṃ kāretukāmena,",
  "ja521:15.4": "kiṃ su kiccaṃ kataṃ varaṃ”.",
  "ja521:16.1": "“Dveva tāta padakāni,",
  "ja521:16.2": "yattha sabbaṃ patiṭṭhitaṃ;",
  "ja521:16.3": "Aladdhassa ca yo lābho,",
  "ja521:16.4": "laddhassa cānurakkhaṇā.",
  "ja521:17.1": "Amacce tāta jānāhi,",
  "ja521:17.2": "dhīre atthassa kovide;",
  "ja521:17.3": "Anakkhākitave tāta,",
  "ja521:17.4": "asoṇḍe avināsake.",
  "ja521:18.1": "Yo ca taṃ tāta rakkheyya,",
  "ja521:18.2": "dhanaṃ yañceva te siyā;",
  "ja521:18.3": "Sūtova rathaṃ saṅgaṇhe,",
  "ja521:18.4": "so te kiccāni kāraye.",
  "ja521:19.1": "Susaṅgahitantajano,",
  "ja521:19.2": "sayaṃ vittaṃ avekkhiya;",
  "ja521:19.3": "Nidhiñca iṇadānañca,",
  "ja521:19.4": "na kare parapattiyā.",
  "ja521:20.1": "Sayaṃ āyaṃ vayaṃ jaññā,",
  "ja521:20.2": "sayaṃ jaññā katākataṃ;",
  "ja521:20.3": "Niggaṇhe niggahārahaṃ,",
  "ja521:20.4": "paggaṇhe paggahārahaṃ.",
  "ja521:21.1": "Sayaṃ jānapadaṃ atthaṃ,",
  "ja521:21.2": "anusāsa rathesabha;",
  "ja521:21.3": "Mā te adhammikā yuttā,",
  "ja521:21.4": "dhanaṃ raṭṭhañca nāsayuṃ.",
  "ja521:22.1": "Mā ca vegena kiccāni,",
  "ja521:22.2": "karosi kārayesi vā;",
  "ja521:22.3": "Vegasā hi kataṃ kammaṃ,",
  "ja521:22.4": "mando pacchānutappati.",
  "ja521:23.1": "Mā te adhisare muñca,",
  "ja521:23.2": "subāḷhamadhikopitaṃ;",
  "ja521:23.3": "Kodhasā hi bahū phītā,",
  "ja521:23.4": "kulā akulataṃ gatā.",
  "ja521:24.1": "Mā tāta issaromhīti,",
  "ja521:24.2": "anatthāya patārayi;",
  "ja521:24.3": "Itthīnaṃ purisānañca,",
  "ja521:24.4": "mā te āsi dukhudrayo.",
  "ja521:25.1": "Apetalomahaṃsassa,",
  "ja521:25.2": "rañño kāmānusārino;",
  "ja521:25.3": "Sabbe bhogā vinassanti,",
  "ja521:25.4": "rañño taṃ vuccate aghaṃ.",
  "ja521:26.1": "Tattheva te vattapadā,",
  "ja521:26.2": "esāva anusāsanī;",
  "ja521:26.3": "Dakkhassudāni puññakaro,",
  "ja521:26.4": "asoṇḍo avināsako;",
  "ja521:26.5": "Sīlavāssu mahārāja,",
  "ja521:26.6": "dussīlo vinipātiko”.",
  "ja521:27.1": "“Apucchimha kosiyagottaṃ,",
  "ja521:27.2": "kuṇḍaliniṃ tatheva ca;",
  "ja521:27.3": "Tvaṃ dāni vadehi jambuka,",
  "ja521:27.4": "balānaṃ balamuttamaṃ”.",
  "ja521:28.1": "“Balaṃ pañcavidhaṃ loke,",
  "ja521:28.2": "purisasmiṃ mahaggate;",
  "ja521:28.3": "Tattha bāhubalaṃ nāma,",
  "ja521:28.4": "carimaṃ vuccate balaṃ.",
  "ja521:29.1": "Bhogabalañca dīghāvu,",
  "ja521:29.2": "dutiyaṃ vuccate balaṃ;",
  "ja521:29.3": "Amaccabalañca dīghāvu,",
  "ja521:29.4": "tatiyaṃ vuccate balaṃ.",
  "ja521:30.1": "Abhijaccabalaṃ ceva,",
  "ja521:30.2": "taṃ catutthaṃ asaṃsayaṃ;",
  "ja521:30.3": "Yāni cetāni sabbāni,",
  "ja521:30.4": "adhigaṇhāti paṇḍito.",
  "ja521:31.1": "Taṃ balānaṃ balaṃ seṭṭhaṃ,",
  "ja521:31.2": "aggaṃ paññābalaṃ balaṃ;",
  "ja521:31.3": "Paññābalenupatthaddho,",
  "ja521:31.4": "atthaṃ vindati paṇḍito.",
  "ja521:32.1": "Api ce labhati mando,",
  "ja521:32.2": "phītaṃ dharaṇimuttamaṃ;",
  "ja521:32.3": "Akāmassa pasayhaṃ vā,",
  "ja521:32.4": "añño taṃ paṭipajjati.",
  "ja521:33.1": "Abhijātopi ce hoti,",
  "ja521:33.2": "rajjaṃ laddhāna khattiyo;",
  "ja521:33.3": "Duppañño hi kāsipati,",
  "ja521:33.4": "sabbenapi na jīvati.",
  "ja521:34.1": "Paññāva sutaṃ vinicchinī,",
  "ja521:34.2": "Paññā kitti silokavaḍḍhanī;",
  "ja521:34.3": "Paññāsahito naro idha,",
  "ja521:34.4": "Api dukkhe sukhāni vindati.",
  "ja521:35.1": "Paññañca kho asussūsaṃ,",
  "ja521:35.2": "na koci adhigacchati;",
  "ja521:35.3": "Bahussutaṃ anāgamma,",
  "ja521:35.4": "dhammaṭṭhaṃ avinibbhujaṃ.",
  "ja521:36.1": "Yo ca dhammavibhaṅgaññū,",
  "ja521:36.2": "kāluṭṭhāyī matandito;",
  "ja521:36.3": "Anuṭṭhahati kālena,",
  "ja521:36.4": "kammaphalaṃ tassijjhati.",
  "ja521:37.1": "Anāyatanasīlassa,",
  "ja521:37.2": "anāyatanasevino;",
  "ja521:37.3": "Na nibbindiyakārissa,",
  "ja521:37.4": "sammadattho vipaccati.",
  "ja521:38.1": "Ajjhattañca payuttassa,",
  "ja521:38.2": "tathāyatanasevino;",
  "ja521:38.3": "Anibbindiyakārissa,",
  "ja521:38.4": "sammadattho vipaccati.",
  "ja521:39.1": "Yogappayogasaṅkhātaṃ,",
  "ja521:39.2": "sambhatassānurakkhaṇaṃ;",
  "ja521:39.3": "Tāni tvaṃ tāta sevassu,",
  "ja521:39.4": "mā akammāya randhayi;",
  "ja521:39.5": "Akammunā hi dummedho,",
  "ja521:39.6": "naḷāgāraṃva sīdati”.",
  "ja521:40.1": "“Dhammaṃ cara mahārāja,",
  "ja521:40.2": "mātāpitūsu khattiya;",
  "ja521:40.3": "Idha dhammaṃ caritvāna,",
  "ja521:40.4": "rāja saggaṃ gamissasi.",
  "ja521:41.1": "Dhammaṃ cara mahārāja,",
  "ja521:41.2": "puttadāresu khattiya;",
  "ja521:41.3": "Idha dhammaṃ caritvāna,",
  "ja521:41.4": "rāja saggaṃ gamissasi.",
  "ja521:42.1": "Dhammaṃ cara mahārāja,",
  "ja521:42.2": "mittāmaccesu khattiya;",
  "ja521:42.3": "Idha dhammaṃ caritvāna,",
  "ja521:42.4": "rāja saggaṃ gamissasi.",
  "ja521:43.1": "Dhammaṃ cara mahārāja,",
  "ja521:43.2": "vāhanesu balesu ca;",
  "ja521:43.3": "Idha dhammaṃ caritvāna,",
  "ja521:43.4": "rāja saggaṃ gamissasi.",
  "ja521:44.1": "Dhammaṃ cara mahārāja,",
  "ja521:44.2": "gāmesu nigamesu ca;",
  "ja521:44.3": "…pe…",
  "ja521:45.1": "Dhammaṃ cara mahārāja,",
  "ja521:45.2": "raṭṭhesu janapadesu ca;",
  "ja521:45.3": "…pe…",
  "ja521:46.1": "Dhammaṃ cara mahārāja,",
  "ja521:46.2": "samaṇabrāhmaṇesu ca;",
  "ja521:46.3": "…pe…",
  "ja521:47.1": "Dhammaṃ cara mahārāja,",
  "ja521:47.2": "migapakkhīsu khattiya;",
  "ja521:47.3": "Idha dhammaṃ caritvāna,",
  "ja521:47.4": "rāja saggaṃ gamissasi.",
  "ja521:48.1": "Dhammaṃ cara mahārāja,",
  "ja521:48.2": "dhammo ciṇṇo sukhāvaho;",
  "ja521:48.3": "Idha dhammaṃ caritvāna,",
  "ja521:48.4": "rāja saggaṃ gamissasi.",
  "ja521:49.1": "Dhammaṃ cara mahārāja,",
  "ja521:49.2": "Saindā devā sabrahmakā;",
  "ja521:49.3": "Suciṇṇena divaṃ pattā,",
  "ja521:49.4": "Mā dhammaṃ rāja pāmado.",
  "ja521:50.1": "Tattheva te vattapadā,",
  "ja521:50.2": "Esāva anusāsanī;",
  "ja521:50.3": "Sappaññasevī kalyāṇī,",
  "ja521:50.4": "Samattaṃ sāma taṃ vidū”ti.",
  "ja521:51.1": "Tesakuṇajātakaṃ paṭhamaṃ."
}