{
  "ja433:0.1": "Jātaka",
  "ja433:0.2": "Navakanipāta",
  "ja433:0.3": "Gijjhavagga",
  "ja433:0.4": "7. Lomasakassapajātaka",
  "ja433:1.1": "“Assa indasamo rāja,",
  "ja433:1.2": "accantaṃ ajarāmaro;",
  "ja433:1.3": "Sace tvaṃ yaññaṃ yājeyya,",
  "ja433:1.4": "isiṃ lomasakassapaṃ”.",
  "ja433:2.1": "“Sasamuddapariyāyaṃ,",
  "ja433:2.2": "mahiṃ sāgarakuṇḍalaṃ;",
  "ja433:2.3": "Na icche saha nindāya,",
  "ja433:2.4": "evaṃ seyya vijānahi.",
  "ja433:3.1": "Dhiratthu taṃ yasalābhaṃ,",
  "ja433:3.2": "dhanalābhañca brāhmaṇa;",
  "ja433:3.3": "Yā vutti vinipātena,",
  "ja433:3.4": "adhammacaraṇena vā.",
  "ja433:4.1": "Api ce pattamādāya,",
  "ja433:4.2": "anagāro paribbaje;",
  "ja433:4.3": "Sāyeva jīvikā seyyo,",
  "ja433:4.4": "yā cādhammena esanā.",
  "ja433:5.1": "Api ce pattamādāya,",
  "ja433:5.2": "anagāro paribbaje;",
  "ja433:5.3": "Aññaṃ ahiṃsayaṃ loke,",
  "ja433:5.4": "api rajjena taṃ varaṃ”.",
  "ja433:6.1": "“Balaṃ cando balaṃ suriyo,",
  "ja433:6.2": "balaṃ samaṇabrāhmaṇā;",
  "ja433:6.3": "Balaṃ velā samuddassa,",
  "ja433:6.4": "balātibalamitthiyo.",
  "ja433:7.1": "Yathā uggatapaṃ santaṃ,",
  "ja433:7.2": "isiṃ lomasakassapaṃ;",
  "ja433:7.3": "Pitu atthā candavatī,",
  "ja433:7.4": "vājapeyyaṃ ayājayi”.",
  "ja433:8.1": "“Taṃ lobhapakataṃ kammaṃ,",
  "ja433:8.2": "Kaṭukaṃ kāmahetukaṃ;",
  "ja433:8.3": "Tassa mūlaṃ gavesissaṃ,",
  "ja433:8.4": "Checchaṃ rāgaṃ sabandhanaṃ.",
  "ja433:9.1": "Dhiratthu kāme subahūpi loke,",
  "ja433:9.2": "Tapova seyyo kāmaguṇehi rāja;",
  "ja433:9.3": "Tapo karissāmi pahāya kāme,",
  "ja433:9.4": "Taveva raṭṭhaṃ candavatī ca hotū”ti.",
  "ja433:10.1": "Lomasakassapajātakaṃ sattamaṃ."
}