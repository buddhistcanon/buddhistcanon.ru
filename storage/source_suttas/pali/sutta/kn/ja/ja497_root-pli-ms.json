{
  "ja497:0.1": "Jātaka",
  "ja497:0.2": "Vīsatinipāta",
  "ja497:0.3": "Mātaṅgavagga",
  "ja497:0.4": "1. Mātaṅgajātaka",
  "ja497:1.1": "“Kuto nu āgacchasi dummavāsī,",
  "ja497:1.2": "Otallako paṃsupisācakova;",
  "ja497:1.3": "Saṅkāracoḷaṃ paṭimuñca kaṇṭhe,",
  "ja497:1.4": "Ko re tuvaṃ hosi adakkhiṇeyyo”.",
  "ja497:2.1": "“Annaṃ tavedaṃ pakataṃ yasassi,",
  "ja497:2.2": "Taṃ khajjare bhuñjare piyyare ca;",
  "ja497:2.3": "Jānāsi maṃ tvaṃ paradattūpajīviṃ,",
  "ja497:2.4": "Uttiṭṭhapiṇḍaṃ labhataṃ sapāko”.",
  "ja497:3.1": "“Annaṃ mamedaṃ pakataṃ brāhmaṇānaṃ,",
  "ja497:3.2": "Attatthāya saddahato mamedaṃ;",
  "ja497:3.3": "Apehi etto kimidhaṭṭhitosi,",
  "ja497:3.4": "Na mādisā tuyhaṃ dadanti jamma”.",
  "ja497:4.1": "“Thale ca ninne ca vapanti bījaṃ,",
  "ja497:4.2": "Anūpakhette phalamāsamānā;",
  "ja497:4.3": "Etāya saddhāya dadāhi dānaṃ,",
  "ja497:4.4": "Appeva ārādhaye dakkhiṇeyye”.",
  "ja497:5.1": "“Khettāni mayhaṃ viditāni loke,",
  "ja497:5.2": "Yesāhaṃ bījāni patiṭṭhapemi;",
  "ja497:5.3": "Ye brāhmaṇā jātimantūpapannā,",
  "ja497:5.4": "Tānīdha khettāni supesalāni”.",
  "ja497:6.1": "“Jātimado ca atimānitā ca,",
  "ja497:6.2": "Lobho ca doso ca mado ca moho;",
  "ja497:6.3": "Ete aguṇā yesu ca santi sabbe,",
  "ja497:6.4": "Tānīdha khettāni apesalāni.",
  "ja497:7.1": "Jātimado ca atimānitā ca,",
  "ja497:7.2": "Lobho ca doso ca mado ca moho;",
  "ja497:7.3": "Ete aguṇā yesu na santi sabbe,",
  "ja497:7.4": "Tānīdha khettāni supesalāni”.",
  "ja497:8.1": "“Kvettha gatā upajotiyo ca,",
  "ja497:8.2": "Upajjhāyo ca atha vā gaṇḍakucchi;",
  "ja497:8.3": "Imassa daṇḍañca vadhañca datvā,",
  "ja497:8.4": "Gale gahetvā khalayātha jammaṃ”.",
  "ja497:9.1": "“Giriṃ nakhena khaṇasi,",
  "ja497:9.2": "ayo dantehi khādasi;",
  "ja497:9.3": "Jātavedaṃ padahasi,",
  "ja497:9.4": "yo isiṃ paribhāsasi”.",
  "ja497:10.1": "“Idaṃ vatvāna mātaṅgo,",
  "ja497:10.2": "isi saccaparakkamo;",
  "ja497:10.3": "Antalikkhasmiṃ pakkāmi,",
  "ja497:10.4": "brāhmaṇānaṃ udikkhataṃ”.",
  "ja497:11.1": "“Āvellitaṃ piṭṭhito uttamaṅgaṃ,",
  "ja497:11.2": "Bāhuṃ pasāreti akammaneyyaṃ;",
  "ja497:11.3": "Setāni akkhīni yathā matassa,",
  "ja497:11.4": "Ko me imaṃ puttamakāsi evaṃ”.",
  "ja497:12.1": "“Idhāgamā samaṇo dummavāsī,",
  "ja497:12.2": "Otallako paṃsupisācakova;",
  "ja497:12.3": "Saṅkāracoḷaṃ paṭimuñca kaṇṭhe,",
  "ja497:12.4": "So te imaṃ puttamakāsi evaṃ”.",
  "ja497:13.1": "“Katamaṃ disaṃ agamā bhūripañño,",
  "ja497:13.2": "Akkhātha me māṇavā etamatthaṃ;",
  "ja497:13.3": "Gantvāna taṃ paṭikaremu accayaṃ,",
  "ja497:13.4": "Appeva naṃ putta labhemu jīvitaṃ”.",
  "ja497:14.1": "“Vehāyasaṃ agamā bhūripañño,",
  "ja497:14.2": "Pathaddhuno pannaraseva cando;",
  "ja497:14.3": "Api cāpi so purimadisaṃ agacchi,",
  "ja497:14.4": "Saccappaṭiñño isi sādhurūpo”.",
  "ja497:15.1": "“Āvellitaṃ piṭṭhito uttamaṅgaṃ,",
  "ja497:15.2": "Bāhuṃ pasāreti akammaneyyaṃ;",
  "ja497:15.3": "Setāni akkhīni yathā matassa,",
  "ja497:15.4": "Ko me imaṃ puttamakāsi evaṃ”.",
  "ja497:16.1": "“Yakkhā have santi mahānubhāvā,",
  "ja497:16.2": "Anvāgatā isayo sādhurūpā;",
  "ja497:16.3": "Te duṭṭhacittaṃ kupitaṃ viditvā,",
  "ja497:16.4": "Yakkhā hi te puttamakaṃsu evaṃ”.",
  "ja497:17.1": "“Yakkhā ca me puttamakaṃsu evaṃ,",
  "ja497:17.2": "Tvaññeva me mā kuddho brahmacāri;",
  "ja497:17.3": "Tumheva pāde saraṇaṃ gatāsmi,",
  "ja497:17.4": "Anvāgatā puttasokena bhikkhu”.",
  "ja497:18.1": "“Tadeva hi etarahi ca mayhaṃ,",
  "ja497:18.2": "Manopadoso na mamatthi koci;",
  "ja497:18.3": "Putto ca te vedamadena matto,",
  "ja497:18.4": "Atthaṃ na jānāti adhicca vede”.",
  "ja497:19.1": "“Addhā have bhikkhu muhuttakena,",
  "ja497:19.2": "Sammuyhateva purisassa saññā;",
  "ja497:19.3": "Ekāparādhaṃ khama bhūripañña,",
  "ja497:19.4": "Na paṇḍitā kodhabalā bhavanti”.",
  "ja497:20.1": "“Idañca mayhaṃ uttiṭṭhapiṇḍaṃ,",
  "ja497:20.2": "Tava maṇḍabyo bhuñjatu appapañño;",
  "ja497:20.3": "Yakkhā ca te naṃ na viheṭhayeyyuṃ,",
  "ja497:20.4": "Putto ca te hessati so arogo”.",
  "ja497:21.1": "“Maṇḍabya bālosi parittapañño,",
  "ja497:21.2": "Yo puññakhettānamakovidosi;",
  "ja497:21.3": "Mahakkasāvesu dadāsi dānaṃ,",
  "ja497:21.4": "Kiliṭṭhakammesu asaññatesu.",
  "ja497:22.1": "Jaṭā ca kesā ajinā nivatthā,",
  "ja497:22.2": "Jarūdapānaṃva mukhaṃ parūḷhaṃ;",
  "ja497:22.3": "Pajaṃ imaṃ passatha dummarūpaṃ,",
  "ja497:22.4": "Na jaṭājinaṃ tāyati appapaññaṃ.",
  "ja497:23.1": "Yesaṃ rāgo ca doso ca,",
  "ja497:23.2": "avijjā ca virājitā;",
  "ja497:23.3": "Khīṇāsavā arahanto,",
  "ja497:23.4": "tesu dinnaṃ mahapphalan”ti.",
  "ja497:24.1": "Mātaṅgajātakaṃ paṭhamaṃ."
}