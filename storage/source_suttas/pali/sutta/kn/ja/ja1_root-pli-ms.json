{
  "ja1:0.1": "Jātaka",
  "ja1:0.2": "Ekakanipāta",
  "ja1:0.3": "Apaṇṇakavagga",
  "ja1:0.4": "1. Apaṇṇakajātaka",
  "ja1:1.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "ja1:2.1": "“Apaṇṇakaṃ ṭhānameke,",
  "ja1:2.2": "Dutiyaṃ āhu takkikā;",
  "ja1:2.3": "Etadaññāya medhāvī,",
  "ja1:2.4": "Taṃ gaṇhe yadapaṇṇakan”ti.",
  "ja1:3.1": "Apaṇṇakajātakaṃ paṭhamaṃ."
}