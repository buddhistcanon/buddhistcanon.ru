{
  "ja169:0.1": "Jātaka",
  "ja169:0.2": "Dukanipāta",
  "ja169:0.3": "Santhavavagga",
  "ja169:0.4": "9. Arakajātaka",
  "ja169:1.1": "“Yo ve mettena cittena,",
  "ja169:1.2": "sabbalokānukampati;",
  "ja169:1.3": "Uddhaṃ adho ca tiriyaṃ,",
  "ja169:1.4": "appamāṇena sabbaso.",
  "ja169:2.1": "Appamāṇaṃ hitaṃ cittaṃ,",
  "ja169:2.2": "paripuṇṇaṃ subhāvitaṃ;",
  "ja169:2.3": "Yaṃ pamāṇakataṃ kammaṃ,",
  "ja169:2.4": "na taṃ tatrāvasissatī”ti.",
  "ja169:3.1": "Arakajātakaṃ navamaṃ."
}