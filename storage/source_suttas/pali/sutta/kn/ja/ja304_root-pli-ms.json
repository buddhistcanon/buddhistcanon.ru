{
  "ja304:0.1": "Jātaka",
  "ja304:0.2": "Catukkanipāta",
  "ja304:0.3": "Kāliṅgavagga",
  "ja304:0.4": "4. Daddarajātaka",
  "ja304:1.1": "“Imāni maṃ daddara tāpayanti,",
  "ja304:1.2": "Vācāduruttāni manussaloke;",
  "ja304:1.3": "Maṇḍūkabhakkhā udakantasevī,",
  "ja304:1.4": "Āsīvisaṃ maṃ avisā sapanti”.",
  "ja304:2.1": "“Sakā raṭṭhā pabbājito,",
  "ja304:2.2": "aññaṃ janapadaṃ gato;",
  "ja304:2.3": "Mahantaṃ koṭṭhaṃ kayirātha,",
  "ja304:2.4": "duruttānaṃ nidhetave.",
  "ja304:3.1": "Yattha posaṃ na jānanti,",
  "ja304:3.2": "jātiyā vinayena vā;",
  "ja304:3.3": "Na tattha mānaṃ kayirātha,",
  "ja304:3.4": "vasamaññātake jane.",
  "ja304:4.1": "Videsavāsaṃ vasato,",
  "ja304:4.2": "jātavedasamenapi;",
  "ja304:4.3": "Khamitabbaṃ sapaññena,",
  "ja304:4.4": "api dāsassa tajjitan”ti.",
  "ja304:5.1": "Daddarajātakaṃ catutthaṃ."
}