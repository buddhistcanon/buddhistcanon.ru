{
  "ja204:0.1": "Jātaka",
  "ja204:0.2": "Dukanipāta",
  "ja204:0.3": "Nataṃdaḷhavagga",
  "ja204:0.4": "4. Vīrakajātaka",
  "ja204:1.1": "“Api vīraka passesi,",
  "ja204:1.2": "sakuṇaṃ mañjubhāṇakaṃ;",
  "ja204:1.3": "Mayūragīvasaṅkāsaṃ,",
  "ja204:1.4": "patiṃ mayhaṃ saviṭṭhakaṃ”.",
  "ja204:2.1": "“Udakathalacarassa pakkhino,",
  "ja204:2.2": "Niccaṃ āmakamacchabhojino;",
  "ja204:2.3": "Tassānukaraṃ saviṭṭhako,",
  "ja204:2.4": "Sevāle paliguṇṭhito mato”ti.",
  "ja204:3.1": "Vīrakajātakaṃ catutthaṃ."
}