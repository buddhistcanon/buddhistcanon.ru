{
  "ja438:0.1": "Jātaka",
  "ja438:0.2": "Navakanipāta",
  "ja438:0.3": "Gijjhavagga",
  "ja438:0.4": "12. Daddarajātaka",
  "ja438:1.1": "“Yo te puttake akhādi,",
  "ja438:1.2": "dinnabhatto adūsake;",
  "ja438:1.3": "Tasmiṃ dāṭhaṃ nipātehi,",
  "ja438:1.4": "mā te muccittha jīvato”.",
  "ja438:2.1": "“Ākiṇṇaluddo puriso,",
  "ja438:2.2": "dhāticelaṃva makkhito;",
  "ja438:2.3": "Padesaṃ taṃ na passāmi,",
  "ja438:2.4": "yattha dāṭhaṃ nipātaye.",
  "ja438:3.1": "Akataññussa posassa,",
  "ja438:3.2": "niccaṃ vivaradassino;",
  "ja438:3.3": "Sabbañce pathaviṃ dajjā,",
  "ja438:3.4": "neva naṃ abhirādhaye”.",
  "ja438:4.1": "“Kiṃ nu subāhu taramānarūpo,",
  "ja438:4.2": "Paccāgatosi saha māṇavena;",
  "ja438:4.3": "Kiṃ kiccamatthaṃ idhamatthi tuyhaṃ,",
  "ja438:4.4": "Akkhāhi me pucchito etamatthaṃ”.",
  "ja438:5.1": "“Yo te sakhā daddaro sādhurūpo,",
  "ja438:5.2": "Tassa vadhaṃ parisaṅkāmi ajja;",
  "ja438:5.3": "Purisassa kammāyatanāni sutvā,",
  "ja438:5.4": "Nāhaṃ sukhiṃ daddaraṃ ajja maññe”.",
  "ja438:6.1": "“Kānissa kammāyatanāni assu,",
  "ja438:6.2": "Purisassa vuttisamodhānatāya;",
  "ja438:6.3": "Kaṃ vā paṭiññaṃ purisassa sutvā,",
  "ja438:6.4": "Parisaṅkasi daddaraṃ māṇavena”.",
  "ja438:7.1": "“Ciṇṇā kaliṅgā caritā vaṇijjā,",
  "ja438:7.2": "Vettācaro saṅkupathopi ciṇṇo;",
  "ja438:7.3": "Naṭehi ciṇṇaṃ saha vākurehi,",
  "ja438:7.4": "Daṇḍena yuddhampi samajjamajjhe.",
  "ja438:8.1": "Baddhā kulīkā mitamāḷhakena,",
  "ja438:8.2": "Akkhā jitā saṃyamo abbhatīto;",
  "ja438:8.3": "Abbāhitaṃ pubbakaṃ aḍḍharattaṃ,",
  "ja438:8.4": "Hatthā daḍḍhā piṇḍapaṭiggahena.",
  "ja438:9.1": "Tānissa kammāyatanāni assu,",
  "ja438:9.2": "Purisassa vuttisamodhānatāya;",
  "ja438:9.3": "Yathā ayaṃ dissati lomapiṇḍo,",
  "ja438:9.4": "Gāvo hatā kiṃ pana daddarassā”ti. (1348)",
  "ja438:10.1": "Daddarajātakaṃ dvādasamaṃ.",
  "ja438:11.1": "Navakanipātaṃ niṭṭhitaṃ.",
  "ja438:12.1": "Tassuddānaṃ",
  "ja438:13.1": "Varagijjha samajjana haṃsavaro,",
  "ja438:13.2": "Nidhisavhaya hārita pāṭaliko;",
  "ja438:13.3": "Ajarāmara dhaṅka titikkha kuto,",
  "ja438:13.4": "Atha dvādasa pekkhana daddaribhīti."
}