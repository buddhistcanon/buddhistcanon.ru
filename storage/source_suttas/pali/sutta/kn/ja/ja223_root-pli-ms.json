{
  "ja223:0.1": "Jātaka",
  "ja223:0.2": "Dukanipāta",
  "ja223:0.3": "Kāsāvavagga",
  "ja223:0.4": "3. Puṭabhattajātaka",
  "ja223:1.1": "“Name namantassa bhaje bhajantaṃ,",
  "ja223:1.2": "Kiccānukubbassa kareyya kiccaṃ;",
  "ja223:1.3": "Nānatthakāmassa kareyya atthaṃ,",
  "ja223:1.4": "Asambhajantampi na sambhajeyya.",
  "ja223:2.1": "Caje cajantaṃ vanathaṃ na kayirā,",
  "ja223:2.2": "Apetacittena na sambhajeyya;",
  "ja223:2.3": "Dijo dumaṃ khīṇaphalanti ñatvā,",
  "ja223:2.4": "Aññaṃ samekkheyya mahā hi loko”ti.",
  "ja223:3.1": "Puṭabhattajātakaṃ tatiyaṃ."
}