{
  "ja156:0.1": "Jātaka",
  "ja156:0.2": "Dukanipāta",
  "ja156:0.3": "Daḷhavagga",
  "ja156:0.4": "6. Alīnacittajātaka",
  "ja156:1.1": "“Alīnacittaṃ nissāya,",
  "ja156:1.2": "pahaṭṭhā mahatī camū;",
  "ja156:1.3": "Kosalaṃ senāsantuṭṭhaṃ,",
  "ja156:1.4": "jīvaggāhaṃ agāhayi.",
  "ja156:2.1": "Evaṃ nissayasampanno,",
  "ja156:2.2": "bhikkhu āraddhavīriyo;",
  "ja156:2.3": "Bhāvayaṃ kusalaṃ dhammaṃ,",
  "ja156:2.4": "yogakkhemassa pattiyā;",
  "ja156:2.5": "Pāpuṇe anupubbena,",
  "ja156:2.6": "sabbasaṃyojanakkhayan”ti.",
  "ja156:3.1": "Alīnacittajātakaṃ chaṭṭhaṃ."
}