{
  "ja106:0.1": "Jātaka",
  "ja106:0.2": "Ekakanipāta",
  "ja106:0.3": "Parosatavagga",
  "ja106:0.4": "6. Udañcanījātaka",
  "ja106:1.1": "“Sukhaṃ vata maṃ jīvantaṃ,",
  "ja106:1.2": "Pacamānā udañcanī;",
  "ja106:1.3": "Corī jāyappavādena,",
  "ja106:1.4": "Telaṃ loṇañca yācatī”ti.",
  "ja106:2.1": "Udañcanījātakaṃ chaṭṭhaṃ."
}