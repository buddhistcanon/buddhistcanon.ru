{
  "ja152:0.1": "Jātaka",
  "ja152:0.2": "Dukanipāta",
  "ja152:0.3": "Daḷhavagga",
  "ja152:0.4": "2. Siṅgālajātaka",
  "ja152:1.1": "“Asamekkhitakammantaṃ,",
  "ja152:1.2": "turitābhinipātinaṃ;",
  "ja152:1.3": "Sāni kammāni tappenti,",
  "ja152:1.4": "uṇhaṃvajjhohitaṃ mukhe”.",
  "ja152:2.1": "“Sīho ca sīhanādena,",
  "ja152:2.2": "daddaraṃ abhinādayi;",
  "ja152:2.3": "Sutvā sīhassa nigghosaṃ,",
  "ja152:2.4": "siṅgālo daddare vasaṃ;",
  "ja152:2.5": "Bhīto santāsamāpādi,",
  "ja152:2.6": "hadayañcassa apphalī”ti.",
  "ja152:3.1": "Siṅgālajātakaṃ dutiyaṃ."
}