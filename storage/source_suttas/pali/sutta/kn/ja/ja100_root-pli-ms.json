{
  "ja100:0.1": "Jātaka",
  "ja100:0.2": "Ekakanipāta",
  "ja100:0.3": "Littavagga",
  "ja100:0.4": "10. Asātarūpajātaka",
  "ja100:1.1": "“Asātaṃ sātarūpena,",
  "ja100:1.2": "piyarūpena appiyaṃ;",
  "ja100:1.3": "Dukkhaṃ sukhassa rūpena,",
  "ja100:1.4": "pamattamativattatī”ti.",
  "ja100:2.1": "Asātarūpajātakaṃ dasamaṃ.",
  "ja100:2.2": "Littavaggo dasamo.",
  "ja100:3.1": "Tassuddānaṃ",
  "ja100:4.1": "Gilamakkhakutūhala mātukassā,",
  "ja100:4.2": "Muninā ca aniccata pattavaraṃ;",
  "ja100:4.3": "Dhanapālivaro atipaṇḍitako,",
  "ja100:4.4": "Saparosahassaasātadasāti.",
  "ja100:5.1": "Majjhimo paṇṇāsako."
}