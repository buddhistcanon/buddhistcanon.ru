{
  "ja96:0.1": "Jātaka",
  "ja96:0.2": "Ekakanipāta",
  "ja96:0.3": "Littavagga",
  "ja96:0.4": "6. Telapattajātaka",
  "ja96:1.1": "“Samatittikaṃ anavasekaṃ,",
  "ja96:1.2": "Telapattaṃ yathā parihareyya;",
  "ja96:1.3": "Evaṃ sacittamanurakkhe,",
  "ja96:1.4": "Patthayāno disaṃ agatapubban”ti.",
  "ja96:2.1": "Telapattajātakaṃ chaṭṭhaṃ."
}