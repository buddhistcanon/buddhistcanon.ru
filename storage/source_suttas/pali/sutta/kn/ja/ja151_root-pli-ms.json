{
  "ja151:0.1": "Jātaka",
  "ja151:0.2": "Dukanipāta",
  "ja151:0.3": "Daḷhavagga",
  "ja151:0.4": "1. Rājovādajātaka",
  "ja151:1.1": "“Daḷhaṃ daḷhassa khipati,",
  "ja151:1.2": "balliko mudunā muduṃ;",
  "ja151:1.3": "Sādhumpi sādhunā jeti,",
  "ja151:1.4": "asādhumpi asādhunā;",
  "ja151:1.5": "Etādiso ayaṃ rājā,",
  "ja151:1.6": "maggā uyyāhi sārathi”.",
  "ja151:2.1": "“Akkodhena jine kodhaṃ,",
  "ja151:2.2": "asādhuṃ sādhunā jine;",
  "ja151:2.3": "Jine kadariyaṃ dānena,",
  "ja151:2.4": "saccenālikavādinaṃ;",
  "ja151:2.5": "Etādiso ayaṃ rājā,",
  "ja151:2.6": "maggā uyyāhi sārathī”ti.",
  "ja151:3.1": "Rājovādajātakaṃ paṭhamaṃ."
}