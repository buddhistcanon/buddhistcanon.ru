{
  "ja426:0.1": "Jātaka",
  "ja426:0.2": "Aṭṭhakanipāta",
  "ja426:0.3": "Kaccānivagga",
  "ja426:0.4": "10. Dīpijātaka",
  "ja426:1.1": "“Khamanīyaṃ yāpanīyaṃ,",
  "ja426:1.2": "kacci mātula te sukhaṃ;",
  "ja426:1.3": "Sukhaṃ te ammā avaca,",
  "ja426:1.4": "sukhakāmāva te mayaṃ”.",
  "ja426:2.1": "“Naṅguṭṭhaṃ me avakkamma,",
  "ja426:2.2": "Heṭhayitvāna eḷike;",
  "ja426:2.3": "Sājja mātulavādena,",
  "ja426:2.4": "Muñcitabbā nu maññasi”.",
  "ja426:3.1": "“Puratthāmukho nisinnosi,",
  "ja426:3.2": "Ahaṃ te mukhamāgatā;",
  "ja426:3.3": "Pacchato tuyhaṃ naṅguṭṭhaṃ,",
  "ja426:3.4": "Kathaṃ khvāhaṃ avakkamiṃ”.",
  "ja426:4.1": "“Yāvatā caturo dīpā,",
  "ja426:4.2": "sasamuddā sapabbatā;",
  "ja426:4.3": "Tāvatā mayhaṃ naṅguṭṭhaṃ,",
  "ja426:4.4": "kathaṃ kho tvaṃ vivajjayi”.",
  "ja426:5.1": "“Pubbeva metamakkhiṃsu,",
  "ja426:5.2": "mātā pitā ca bhātaro;",
  "ja426:5.3": "Dīghaṃ duṭṭhassa naṅguṭṭhaṃ,",
  "ja426:5.4": "sāmhi vehāyasāgatā”.",
  "ja426:6.1": "“Tañca disvāna āyantiṃ,",
  "ja426:6.2": "antalikkhasmi eḷike;",
  "ja426:6.3": "Migasaṅgho palāyittha,",
  "ja426:6.4": "bhakkho me nāsito tayā”.",
  "ja426:7.1": "“Iccevaṃ vilapantiyā,",
  "ja426:7.2": "eḷakiyā ruhagghaso;",
  "ja426:7.3": "Galakaṃ anvāvamaddi,",
  "ja426:7.4": "natthi duṭṭhe subhāsitaṃ.",
  "ja426:8.1": "Neva duṭṭhe nayo atthi,",
  "ja426:8.2": "Na dhammo na subhāsitaṃ;",
  "ja426:8.3": "Nikkamaṃ duṭṭhe yuñjetha,",
  "ja426:8.4": "So ca sabbhiṃ na rañjatī”ti. (1235)",
  "ja426:9.1": "Dīpijātakaṃ dasamaṃ.",
  "ja426:10.1": "Aṭṭhakanipātaṃ niṭṭhitaṃ.",
  "ja426:11.1": "Tassuddānaṃ",
  "ja426:12.1": "Parisuddhā manāvilā vatthadharā,",
  "ja426:12.2": "Bakarājassa kāyuraṃ daṇḍavaro;",
  "ja426:12.3": "Atha aṅgāra cetiya devilinā,",
  "ja426:12.4": "Atha āditta gaṅgā daseḷakināti."
}