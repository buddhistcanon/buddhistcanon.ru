{
  "ja20:0.1": "Jātaka",
  "ja20:0.2": "Ekakanipāta",
  "ja20:0.3": "Sīlavagga",
  "ja20:0.4": "10. Naḷapānajātaka",
  "ja20:1.1": "“Disvā padamanuttiṇṇaṃ,",
  "ja20:1.2": "Disvānotaritaṃ padaṃ;",
  "ja20:1.3": "Naḷena vāriṃ pissāma,",
  "ja20:1.4": "Neva maṃ tvaṃ vadhissasī”ti.",
  "ja20:2.1": "Naḷapānajātakaṃ dasamaṃ.",
  "ja20:2.2": "Sīlavaggo dutiyo.",
  "ja20:3.1": "Tassuddānaṃ",
  "ja20:4.1": "Atha lakkhaṇa sākha dhiratthu puna,",
  "ja20:4.2": "Na kiratthi rasehi kharādiyā;",
  "ja20:4.3": "Atibhoti vara māluta pāṇa,",
  "ja20:4.4": "Muccena naḷaavhayanena bhavanti dasāti."
}