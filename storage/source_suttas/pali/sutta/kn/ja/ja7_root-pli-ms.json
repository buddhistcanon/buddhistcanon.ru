{
  "ja7:0.1": "Jātaka",
  "ja7:0.2": "Ekakanipāta",
  "ja7:0.3": "Apaṇṇakavagga",
  "ja7:0.4": "7. Kaṭṭhahārijātaka",
  "ja7:1.1": "“Putto tyāhaṃ mahārāja,",
  "ja7:1.2": "tvaṃ maṃ posa janādhipa;",
  "ja7:1.3": "Aññepi devo poseti,",
  "ja7:1.4": "kiñca devo sakaṃ pajan”ti.",
  "ja7:2.1": "Kaṭṭhahārijātakaṃ sattamaṃ."
}