{
  "ja54:0.1": "Jātaka",
  "ja54:0.2": "Ekakanipāta",
  "ja54:0.3": "Āsīsavagga",
  "ja54:0.4": "4. Kiṃphalajātaka",
  "ja54:1.1": "“Nāyaṃ rukkho durāruho,",
  "ja54:1.2": "napi gāmato ārakā;",
  "ja54:1.3": "Ākāraṇena jānāmi,",
  "ja54:1.4": "nāyaṃ sāduphalo dumo”ti.",
  "ja54:2.1": "Kiṃphalajātakaṃ catutthaṃ."
}