{
  "ja265:0.1": "Jātaka",
  "ja265:0.2": "Tikanipāta",
  "ja265:0.3": "Padumavagga",
  "ja265:0.4": "5. Khurappajātaka",
  "ja265:1.1": "“Disvā khurappe dhanuveganunne,",
  "ja265:1.2": "Khagge gahīte tikhiṇe teladhote;",
  "ja265:1.3": "Tasmiṃ bhayasmiṃ maraṇe viyūḷhe,",
  "ja265:1.4": "Kasmā nu te nāhu chambhitattaṃ”.",
  "ja265:2.1": "“Disvā khurappe dhanuveganunne,",
  "ja265:2.2": "Khagge gahīte tikhiṇe teladhote;",
  "ja265:2.3": "Tasmiṃ bhayasmiṃ maraṇe viyūḷhe,",
  "ja265:2.4": "Vedaṃ alatthaṃ vipulaṃ uḷāraṃ.",
  "ja265:3.1": "So vedajāto ajjhabhaviṃ amitte,",
  "ja265:3.2": "Pubbeva me jīvitamāsi cattaṃ;",
  "ja265:3.3": "Na hi jīvite ālayaṃ kubbamāno,",
  "ja265:3.4": "Sūro kayirā sūrakiccaṃ kadācī”ti.",
  "ja265:4.1": "Khurappajātakaṃ pañcamaṃ."
}