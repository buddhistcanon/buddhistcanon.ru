{
  "ja209:0.1": "Jātaka",
  "ja209:0.2": "Dukanipāta",
  "ja209:0.3": "Nataṃdaḷhavagga",
  "ja209:0.4": "9. Kukkuṭajātaka",
  "ja209:1.1": "“Diṭṭhā mayā vane rukkhā,",
  "ja209:1.2": "assakaṇṇā vibhīṭakā;",
  "ja209:1.3": "Na tāni evaṃ sakkanti,",
  "ja209:1.4": "yathā tvaṃ rukkha sakkasi”.",
  "ja209:2.1": "“Purāṇakukkuṭo ayaṃ,",
  "ja209:2.2": "bhetvā pañjaramāgato;",
  "ja209:2.3": "Kusalo vāḷapāsānaṃ,",
  "ja209:2.4": "apakkamati bhāsatī”ti.",
  "ja209:3.1": "Kukkuṭajātakaṃ navamaṃ."
}