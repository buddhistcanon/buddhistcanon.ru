{
  "ja501:0.1": "Jātaka",
  "ja501:0.2": "Vīsatinipāta",
  "ja501:0.3": "Mātaṅgavagga",
  "ja501:0.4": "5. Rohaṇamigajātaka",
  "ja501:1.1": "“Ete yūthā patiyanti,",
  "ja501:1.2": "bhītā maraṇassa cittaka;",
  "ja501:1.3": "Gaccha tuvampi mākaṅkhi,",
  "ja501:1.4": "jīvissanti tayā saha”.",
  "ja501:2.1": "“Nāhaṃ rohaṇa gacchāmi,",
  "ja501:2.2": "hadayaṃ me avakassati;",
  "ja501:2.3": "Na taṃ ahaṃ jahissāmi,",
  "ja501:2.4": "idha hissāmi jīvitaṃ”.",
  "ja501:3.1": "“Te hi nūna marissanti,",
  "ja501:3.2": "andhā apariṇāyakā;",
  "ja501:3.3": "Gaccha tuvampi mākaṅkhi,",
  "ja501:3.4": "jīvissanti tayā saha”.",
  "ja501:4.1": "“Nāhaṃ rohaṇa gacchāmi,",
  "ja501:4.2": "hadayaṃ me avakassati;",
  "ja501:4.3": "Na taṃ baddhaṃ jahissāmi,",
  "ja501:4.4": "idha hissāmi jīvitaṃ”.",
  "ja501:5.1": "“Gaccha bhīru palāyassu,",
  "ja501:5.2": "kūṭe baddhosmi āyase;",
  "ja501:5.3": "Gaccha tuvampi mākaṅkhi,",
  "ja501:5.4": "jīvissanti tayā saha”.",
  "ja501:6.1": "“Nāhaṃ rohaṇa gacchāmi,",
  "ja501:6.2": "hadayaṃ me avakassati;",
  "ja501:6.3": "Na taṃ ahaṃ jahissāmi,",
  "ja501:6.4": "idha hissāmi jīvitaṃ”.",
  "ja501:7.1": "“Te hi nūna marissanti,",
  "ja501:7.2": "andhā apariṇāyakā;",
  "ja501:7.3": "Gaccha tuvampi mākaṅkhi,",
  "ja501:7.4": "jīvissanti tayā saha”.",
  "ja501:8.1": "“Nāhaṃ rohaṇa gacchāmi,",
  "ja501:8.2": "hadayaṃ me avakassati;",
  "ja501:8.3": "Na taṃ baddhaṃ jahissāmi,",
  "ja501:8.4": "idha hissāmi jīvitaṃ”.",
  "ja501:9.1": "“Ayaṃ so luddako eti,",
  "ja501:9.2": "luddarūpo sahāvudho;",
  "ja501:9.3": "Yo no vadhissati ajja,",
  "ja501:9.4": "usunā sattiyā api”.",
  "ja501:10.1": "“Sā muhuttaṃ palāyitvā,",
  "ja501:10.2": "bhayaṭṭā bhayatajjitā;",
  "ja501:10.3": "Sudukkaraṃ akarā bhīru,",
  "ja501:10.4": "maraṇāyūpanivattatha”.",
  "ja501:11.1": "“Kiṃ nu teme migā honti,",
  "ja501:11.2": "muttā baddhaṃ upāsare;",
  "ja501:11.3": "Na taṃ cajitumicchanti,",
  "ja501:11.4": "jīvitassapi kāraṇā”.",
  "ja501:12.1": "“Bhātaro honti me ludda,",
  "ja501:12.2": "sodariyā ekamātukā;",
  "ja501:12.3": "Na maṃ cajitumicchanti,",
  "ja501:12.4": "jīvitassapi kāraṇā”.",
  "ja501:13.1": "“Te hi nūna marissanti,",
  "ja501:13.2": "andhā apariṇāyakā;",
  "ja501:13.3": "Pañcannaṃ jīvitaṃ dehi,",
  "ja501:13.4": "bhātaraṃ muñca luddaka”.",
  "ja501:14.1": "“So vo ahaṃ pamokkhāmi,",
  "ja501:14.2": "mātāpettibharaṃ migaṃ;",
  "ja501:14.3": "Nandantu mātāpitaro,",
  "ja501:14.4": "muttaṃ disvā mahāmigaṃ”.",
  "ja501:15.1": "“Evaṃ luddaka nandassu,",
  "ja501:15.2": "saha sabbehi ñātibhi;",
  "ja501:15.3": "Yathāhamajja nandāmi,",
  "ja501:15.4": "muttaṃ disvā mahāmigaṃ”.",
  "ja501:16.1": "“Kathaṃ tvaṃ pamokkho āsi,",
  "ja501:16.2": "Upanītasmi jīvite;",
  "ja501:16.3": "Kathaṃ putta amocesi,",
  "ja501:16.4": "Kūṭapāsamha luddako”.",
  "ja501:17.1": "“Bhaṇaṃ kaṇṇasukhaṃ vācaṃ,",
  "ja501:17.2": "hadayaṅgaṃ hadayassitaṃ;",
  "ja501:17.3": "Subhāsitāhi vācāhi,",
  "ja501:17.4": "cittako maṃ amocayi.",
  "ja501:18.1": "Bhaṇaṃ kaṇṇasukhaṃ vācaṃ,",
  "ja501:18.2": "hadayaṅgaṃ hadayassitaṃ;",
  "ja501:18.3": "Subhāsitāhi vācāhi,",
  "ja501:18.4": "sutanā maṃ amocayi.",
  "ja501:19.1": "Sutvā kaṇṇasukhaṃ vācaṃ,",
  "ja501:19.2": "hadayaṅgaṃ hadayassitaṃ;",
  "ja501:19.3": "Subhāsitāni sutvāna,",
  "ja501:19.4": "luddako maṃ amocayi”.",
  "ja501:20.1": "“Evaṃ ānandito hotu,",
  "ja501:20.2": "saha dārehi luddako;",
  "ja501:20.3": "Yathā mayajja nandāma,",
  "ja501:20.4": "disvā rohaṇamāgataṃ”.",
  "ja501:21.1": "“Nanu tvaṃ avaca ludda,",
  "ja501:21.2": "‘migacammāni āhariṃ’;",
  "ja501:21.3": "Atha kena nu vaṇṇena,",
  "ja501:21.4": "migacammāni nāhari”.",
  "ja501:22.1": "“Āgamā ceva hatthatthaṃ,",
  "ja501:22.2": "kūṭapāsañca so migo;",
  "ja501:22.3": "Abajjhi taṃ migarājaṃ,",
  "ja501:22.4": "tañca muttā upāsare.",
  "ja501:23.1": "Tassa me ahu saṃvego,",
  "ja501:23.2": "abbhuto lomahaṃsano;",
  "ja501:23.3": "Imañcāhaṃ migaṃ haññe,",
  "ja501:23.4": "ajja hissāmi jīvitaṃ”.",
  "ja501:24.1": "“Kīdisā te migā ludda,",
  "ja501:24.2": "kīdisā dhammikā migā;",
  "ja501:24.3": "Kathaṃvaṇṇā kathaṃsīlā,",
  "ja501:24.4": "bāḷhaṃ kho ne pasaṃsasi”.",
  "ja501:25.1": "“Odātasiṅgā sucivālā,",
  "ja501:25.2": "jātarūpatacūpamā;",
  "ja501:25.3": "Pādā lohitakā tesaṃ,",
  "ja501:25.4": "añjitakkhā manoramā”.",
  "ja501:26.1": "“Edisā te migā deva,",
  "ja501:26.2": "edisā dhammikā migā;",
  "ja501:26.3": "Mātāpettibharā deva,",
  "ja501:26.4": "na te so abhihārituṃ”.",
  "ja501:27.1": "“Dammi nikkhasataṃ ludda,",
  "ja501:27.2": "thūlañca maṇikuṇḍalaṃ;",
  "ja501:27.3": "Catussadañca pallaṅkaṃ,",
  "ja501:27.4": "umāpupphasarinnibhaṃ.",
  "ja501:28.1": "Dve ca sādisiyo bhariyā,",
  "ja501:28.2": "usabhañca gavaṃ sataṃ;",
  "ja501:28.3": "Dhammena rajjaṃ kāressaṃ,",
  "ja501:28.4": "bahukāro mesi luddaka.",
  "ja501:29.1": "Kasivāṇijjā iṇadānaṃ,",
  "ja501:29.2": "uñchācariyā ca luddaka;",
  "ja501:29.3": "Etena dāraṃ posehi,",
  "ja501:29.4": "mā pāpaṃ akarī punāti”.",
  "ja501:30.1": "Rohaṇamigajātakaṃ pañcamaṃ."
}