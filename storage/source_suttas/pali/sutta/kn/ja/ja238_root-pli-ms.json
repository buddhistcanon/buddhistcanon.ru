{
  "ja238:0.1": "Jātaka",
  "ja238:0.2": "Dukanipāta",
  "ja238:0.3": "Upāhanavagga",
  "ja238:0.4": "8. Ekapadajātaka",
  "ja238:1.1": "“Iṅgha ekapadaṃ tāta,",
  "ja238:1.2": "anekatthapadassitaṃ;",
  "ja238:1.3": "Kiñci saṅgāhikaṃ brūsi,",
  "ja238:1.4": "yenatthe sādhayemase”.",
  "ja238:2.1": "“Dakkheyyekapadaṃ tāta,",
  "ja238:2.2": "anekatthapadassitaṃ;",
  "ja238:2.3": "Tañca sīlena saññuttaṃ,",
  "ja238:2.4": "khantiyā upapāditaṃ;",
  "ja238:2.5": "Alaṃ mitte sukhāpetuṃ,",
  "ja238:2.6": "amittānaṃ dukhāya cā”ti.",
  "ja238:3.1": "Ekapadajātakaṃ aṭṭhamaṃ."
}