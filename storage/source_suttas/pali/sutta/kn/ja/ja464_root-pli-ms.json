{
  "ja464:0.1": "Jātaka",
  "ja464:0.2": "Dvādasakanipāta",
  "ja464:0.3": "Cūḷakuṇālavagga",
  "ja464:0.4": "1. Cūḷakuṇālajātaka",
  "ja464:1.1": "“Luddhānaṃ lahucittānaṃ,",
  "ja464:1.2": "akataññūna dubbhinaṃ;",
  "ja464:1.3": "Nādevasatto puriso,",
  "ja464:1.4": "thīnaṃ saddhātumarahati.",
  "ja464:2.1": "Na tā pajānanti kataṃ na kiccaṃ,",
  "ja464:2.2": "Na mātaraṃ pitaraṃ bhātaraṃ vā;",
  "ja464:2.3": "Anariyā samatikkantadhammā,",
  "ja464:2.4": "Sasseva cittassa vasaṃ vajanti.",
  "ja464:3.1": "Cirānuvutthampi piyaṃ manāpaṃ,",
  "ja464:3.2": "Anukampakaṃ pāṇasamampi bhattuṃ;",
  "ja464:3.3": "Āvāsu kiccesu ca naṃ jahanti,",
  "ja464:3.4": "Tasmāhamitthīnaṃ na vissasāmi.",
  "ja464:4.1": "Thīnañhi cittaṃ yathā vānarassa,",
  "ja464:4.2": "Kannappakannaṃ yathā rukkhachāyā;",
  "ja464:4.3": "Calācalaṃ hadayamitthiyānaṃ,",
  "ja464:4.4": "Cakkassa nemi viya parivattati.",
  "ja464:5.1": "Yadā tā passanti samekkhamānā,",
  "ja464:5.2": "Ādeyyarūpaṃ purisassa vittaṃ;",
  "ja464:5.3": "Saṇhāhi vācāhi nayanti menaṃ,",
  "ja464:5.4": "Kambojakā jalajeneva assaṃ.",
  "ja464:6.1": "Yadā na passanti samekkhamānā,",
  "ja464:6.2": "Ādeyyarūpaṃ purisassa vittaṃ;",
  "ja464:6.3": "Samantato naṃ parivajjayanti,",
  "ja464:6.4": "Tiṇṇo nadīpāragatova kullaṃ.",
  "ja464:7.1": "Silesūpamā sikhiriva sabbabhakkhā,",
  "ja464:7.2": "Tikkhamāyā nadīriva sīghasotā;",
  "ja464:7.3": "Sevanti hetā piyamappiyañca,",
  "ja464:7.4": "Nāvā yathā orakūlaṃ parañca.",
  "ja464:8.1": "Na tā ekassa na dvinnaṃ,",
  "ja464:8.2": "āpaṇova pasārito;",
  "ja464:8.3": "Yo tā mayhanti maññeyya,",
  "ja464:8.4": "vātaṃ jālena bādhaye.",
  "ja464:9.1": "Yathā nadī ca pantho ca,",
  "ja464:9.2": "pānāgāraṃ sabhā papā;",
  "ja464:9.3": "Evaṃ lokitthiyo nāma,",
  "ja464:9.4": "velā tāsaṃ na vijjati.",
  "ja464:10.1": "Ghatāsanasamā etā,",
  "ja464:10.2": "kaṇhasappasirūpamā;",
  "ja464:10.3": "Gāvo bahitiṇasseva,",
  "ja464:10.4": "omasanti varaṃ varaṃ.",
  "ja464:11.1": "Ghatāsanaṃ kuñjaraṃ kaṇhasappaṃ,",
  "ja464:11.2": "Muddhābhisittaṃ pamadā ca sabbā;",
  "ja464:11.3": "Ete naro niccayato bhajetha,",
  "ja464:11.4": "Tesaṃ have dubbidu sabbabhāvo.",
  "ja464:12.1": "Naccantavaṇṇā na bahūnaṃ kantā,",
  "ja464:12.2": "Na dakkhiṇā pamadā sevitabbā;",
  "ja464:12.3": "Na parassa bhariyā na dhanassa hetu,",
  "ja464:12.4": "Etitthiyo pañca na sevitabbā”ti.",
  "ja464:13.1": "Cūḷakuṇālajātakaṃ paṭhamaṃ."
}