{
  "ja86:0.1": "Jātaka",
  "ja86:0.2": "Ekakanipāta",
  "ja86:0.3": "Apāyimhavagga",
  "ja86:0.4": "6. Sīlavīmaṃsakajātaka",
  "ja86:1.1": "“Sīlaṃ kireva kalyāṇaṃ,",
  "ja86:1.2": "sīlaṃ loke anuttaraṃ;",
  "ja86:1.3": "Passa ghoraviso nāgo,",
  "ja86:1.4": "sīlavāti na haññatī”ti.",
  "ja86:2.1": "Sīlavīmaṃsakajātakaṃ chaṭṭhaṃ."
}