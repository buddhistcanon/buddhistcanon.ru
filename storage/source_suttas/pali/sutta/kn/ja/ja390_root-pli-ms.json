{
  "ja390:0.1": "Jātaka",
  "ja390:0.2": "Chakkanipāta",
  "ja390:0.3": "Kharaputtavagga",
  "ja390:0.4": "5. Mayhakajātaka",
  "ja390:1.1": "“Sakuṇo mayhako nāma,",
  "ja390:1.2": "girisānudarīcaro;",
  "ja390:1.3": "Pakkaṃ pipphalimāruyha,",
  "ja390:1.4": "mayhaṃ mayhanti kandati.",
  "ja390:2.1": "Tassevaṃ vilapantassa,",
  "ja390:2.2": "dijasaṅghā samāgatā;",
  "ja390:2.3": "Bhutvāna pipphaliṃ yanti,",
  "ja390:2.4": "vilapatveva so dijo.",
  "ja390:3.1": "Evameva idhekacco,",
  "ja390:3.2": "saṅgharitvā bahuṃ dhanaṃ;",
  "ja390:3.3": "Nevattano na ñātīnaṃ,",
  "ja390:3.4": "yathodhiṃ paṭipajjati.",
  "ja390:4.1": "Na so acchādanaṃ bhattaṃ,",
  "ja390:4.2": "na mālaṃ na vilepanaṃ;",
  "ja390:4.3": "Anubhoti sakiṃ kiñci,",
  "ja390:4.4": "na saṅgaṇhāti ñātake.",
  "ja390:5.1": "Tassevaṃ vilapantassa,",
  "ja390:5.2": "mayhaṃ mayhanti rakkhato;",
  "ja390:5.3": "Rājāno atha vā corā,",
  "ja390:5.4": "dāyādā ye va appiyā;",
  "ja390:5.5": "Dhanamādāya gacchanti,",
  "ja390:5.6": "vilapatveva so naro.",
  "ja390:6.1": "Dhīro bhoge adhigamma,",
  "ja390:6.2": "saṅgaṇhāti ca ñātake;",
  "ja390:6.3": "Tena so kittiṃ pappoti,",
  "ja390:6.4": "pecca sagge pamodatī”ti.",
  "ja390:7.1": "Mayhakajātakaṃ pañcamaṃ."
}