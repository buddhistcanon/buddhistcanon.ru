{
  "ja110:0.1": "Jātaka",
  "ja110:0.2": "Ekakanipāta",
  "ja110:0.3": "Parosatavagga",
  "ja110:0.4": "10. Sabbasaṃhārakapañhajātaka",
  "ja110:1.1": "“Sabbasaṃhārako natthi,",
  "ja110:1.2": "suddhaṃ kaṅgu pavāyati;",
  "ja110:1.3": "Alikaṃ bhāyatiyaṃ dhuttī,",
  "ja110:1.4": "saccamāha mahallikā”ti.",
  "ja110:2.1": "Sabbasaṃhārakapañhajātakaṃ dasamaṃ.",
  "ja110:2.2": "Parosatavaggo ekādasamo.",
  "ja110:3.1": "Tassuddānaṃ",
  "ja110:4.1": "Saparosatatāyitaverī puna,",
  "ja110:4.2": "Bhamacakkatha nāgasirivhayano;",
  "ja110:4.3": "Sukhakañca vata sippaka bāhiyā,",
  "ja110:4.4": "Kuṇḍapūva mahallikakā ca dasāti."
}