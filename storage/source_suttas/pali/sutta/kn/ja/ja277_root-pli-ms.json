{
  "ja277:0.1": "Jātaka",
  "ja277:0.2": "Tikanipāta",
  "ja277:0.3": "Udapānavagga",
  "ja277:0.4": "7. Romakajātaka",
  "ja277:1.1": "“Vassāni paññāsa samādhikāni,",
  "ja277:1.2": "Vasimha selassa guhāya romaka;",
  "ja277:1.3": "Asaṅkamānā abhinibbutattā,",
  "ja277:1.4": "Hatthattamāyanti mamaṇḍajā pure.",
  "ja277:2.1": "Te dāni vakkaṅga kimatthamussukā,",
  "ja277:2.2": "Bhajanti aññaṃ girikandaraṃ dijā;",
  "ja277:2.3": "Na nūna maññanti mamaṃ yathā pure,",
  "ja277:2.4": "Cirappavutthā atha vā na te ime”.",
  "ja277:3.1": "“Jānāma taṃ na mayaṃ sampamūḷhā,",
  "ja277:3.2": "Soyeva tvaṃ te mayamasma nāññe;",
  "ja277:3.3": "Cittañca te asmiṃ jane paduṭṭhaṃ,",
  "ja277:3.4": "Ājīvikā tena tamuttasāmā”ti.",
  "ja277:4.1": "Romakajātakaṃ sattamaṃ."
}