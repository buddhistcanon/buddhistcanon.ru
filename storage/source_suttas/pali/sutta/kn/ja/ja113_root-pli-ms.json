{
  "ja113:0.1": "Jātaka",
  "ja113:0.2": "Ekakanipāta",
  "ja113:0.3": "Haṃcivagga",
  "ja113:0.4": "3. Siṅgālajātaka",
  "ja113:1.1": "“Saddahāsi siṅgālassa,",
  "ja113:1.2": "surāpītassa brāhmaṇa;",
  "ja113:1.3": "Sippikānaṃ sataṃ natthi,",
  "ja113:1.4": "kuto kaṃsasatā duve”ti.",
  "ja113:2.1": "Siṅgālajātakaṃ tatiyaṃ."
}