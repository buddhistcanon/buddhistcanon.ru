{
  "ja214:0.1": "Jātaka",
  "ja214:0.2": "Dukanipāta",
  "ja214:0.3": "Bīraṇathambhavagga",
  "ja214:0.4": "4. Puṇṇanadījātaka",
  "ja214:1.1": "“Puṇṇaṃ nadiṃ yena ca peyyamāhu,",
  "ja214:1.2": "Jātaṃ yavaṃ yena ca guyhamāhu;",
  "ja214:1.3": "Dūraṃ gataṃ yena ca avhayanti,",
  "ja214:1.4": "So tyāgato handa ca bhuñja brāhmaṇa”.",
  "ja214:2.1": "“Yato maṃ saratī rājā,",
  "ja214:2.2": "vāyasampi pahetave;",
  "ja214:2.3": "Haṃsā koñcā mayūrā ca,",
  "ja214:2.4": "asatīyeva pāpiyā”ti.",
  "ja214:3.1": "Puṇṇanadījātakaṃ catutthaṃ."
}