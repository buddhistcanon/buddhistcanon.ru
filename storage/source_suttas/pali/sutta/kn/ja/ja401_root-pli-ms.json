{
  "ja401:0.1": "Jātaka",
  "ja401:0.2": "Sattakanipāta",
  "ja401:0.3": "Kukkuvagga",
  "ja401:0.4": "6. Paṇṇakajātaka",
  "ja401:1.1": "“Paṇṇakaṃ tikhiṇadhāraṃ,",
  "ja401:1.2": "Asiṃ sampannapāyinaṃ;",
  "ja401:1.3": "Parisāyaṃ puriso gilati,",
  "ja401:1.4": "Kiṃ dukkarataraṃ tato;",
  "ja401:1.5": "Yadaññaṃ dukkaraṃ ṭhānaṃ,",
  "ja401:1.6": "Taṃ me akkhāhi pucchito”.",
  "ja401:2.1": "“Gileyya puriso lobhā,",
  "ja401:2.2": "asiṃ sampannapāyinaṃ;",
  "ja401:2.3": "Yo ca vajjā dadāmīti,",
  "ja401:2.4": "taṃ dukkarataraṃ tato;",
  "ja401:2.5": "Sabbaññaṃ sukaraṃ ṭhānaṃ,",
  "ja401:2.6": "evaṃ jānāhi maddava”.",
  "ja401:3.1": "“Byākāsi āyuro pañhaṃ,",
  "ja401:3.2": "atthaṃ dhammassa kovido;",
  "ja401:3.3": "Pukkusaṃ dāni pucchāmi,",
  "ja401:3.4": "kiṃ dukkarataraṃ tato;",
  "ja401:3.5": "Yadaññaṃ dukkaraṃ ṭhānaṃ,",
  "ja401:3.6": "taṃ me akkhāhi pucchito”.",
  "ja401:4.1": "“Na vācamupajīvanti,",
  "ja401:4.2": "aphalaṃ giramudīritaṃ;",
  "ja401:4.3": "Yo ca datvā avākayirā,",
  "ja401:4.4": "taṃ dukkarataraṃ tato;",
  "ja401:4.5": "Sabbaññaṃ sukaraṃ ṭhānaṃ,",
  "ja401:4.6": "evaṃ jānāhi maddava”.",
  "ja401:5.1": "“Byākāsi pukkuso pañhaṃ,",
  "ja401:5.2": "atthaṃ dhammassa kovido;",
  "ja401:5.3": "Senakaṃ dāni pucchāmi,",
  "ja401:5.4": "kiṃ dukkarataraṃ tato;",
  "ja401:5.5": "Yadaññaṃ dukkaraṃ ṭhānaṃ,",
  "ja401:5.6": "taṃ me akkhāhi pucchito”.",
  "ja401:6.1": "“Dadeyya puriso dānaṃ,",
  "ja401:6.2": "appaṃ vā yadi vā bahuṃ;",
  "ja401:6.3": "Yo ca datvā nānutappe,",
  "ja401:6.4": "taṃ dukkarataraṃ tato;",
  "ja401:6.5": "Sabbaññaṃ sukaraṃ ṭhānaṃ,",
  "ja401:6.6": "evaṃ jānāhi maddava”.",
  "ja401:7.1": "“Byākāsi āyuro pañhaṃ,",
  "ja401:7.2": "atho pukkusaporiso;",
  "ja401:7.3": "Sabbe pañhe atibhoti,",
  "ja401:7.4": "yathā bhāsati senako”ti.",
  "ja401:8.1": "Paṇṇakajātakaṃ chaṭṭhaṃ."
}