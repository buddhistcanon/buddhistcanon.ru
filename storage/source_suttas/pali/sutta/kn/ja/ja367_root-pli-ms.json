{
  "ja367:0.1": "Jātaka",
  "ja367:0.2": "Pañcakanipāta",
  "ja367:0.3": "Vaṇṇārohavagga",
  "ja367:0.4": "7. Sāḷiyajātaka",
  "ja367:1.1": "“Yvāyaṃ sāḷiyachāpoti,",
  "ja367:1.2": "kaṇhasappaṃ agāhayi;",
  "ja367:1.3": "Tena sappenayaṃ daṭṭho,",
  "ja367:1.4": "hato pāpānusāsako.",
  "ja367:2.1": "Ahantāra mahantāraṃ,",
  "ja367:2.2": "yo naro hantumicchati;",
  "ja367:2.3": "Evaṃ so nihato seti,",
  "ja367:2.4": "yathāyaṃ puriso hato.",
  "ja367:3.1": "Ahantāra maghātentaṃ,",
  "ja367:3.2": "yo naro hantumicchati;",
  "ja367:3.3": "Evaṃ so nihato seti,",
  "ja367:3.4": "yathāyaṃ puriso hato.",
  "ja367:4.1": "Yathā paṃsumuṭṭhiṃ puriso,",
  "ja367:4.2": "paṭivātaṃ paṭikkhipe;",
  "ja367:4.3": "Tameva so rajo hanti,",
  "ja367:4.4": "tathāyaṃ puriso hato.",
  "ja367:5.1": "Yo appaduṭṭhassa narassa dussati,",
  "ja367:5.2": "Suddhassa posassa anaṅgaṇassa;",
  "ja367:5.3": "Tameva bālaṃ pacceti pāpaṃ,",
  "ja367:5.4": "Sukhumo rajo paṭivātaṃva khitto”ti.",
  "ja367:6.1": "Sāḷiyajātakaṃ sattamaṃ."
}