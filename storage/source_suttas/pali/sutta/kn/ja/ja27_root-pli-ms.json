{
  "ja27:0.1": "Jātaka",
  "ja27:0.2": "Ekakanipāta",
  "ja27:0.3": "Kuruṅgavagga",
  "ja27:0.4": "7. Abhiṇhajātaka",
  "ja27:1.1": "“Nālaṃ kabaḷaṃ padātave,",
  "ja27:1.2": "Na ca piṇḍaṃ na kuse na ghaṃsituṃ;",
  "ja27:1.3": "Maññāmi abhiṇhadassanā,",
  "ja27:1.4": "Nāgo snehamakāsi kukkure”ti.",
  "ja27:2.1": "Abhiṇhajātakaṃ sattamaṃ."
}