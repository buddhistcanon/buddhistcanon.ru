{
  "ja530:0.1": "Jātaka",
  "ja530:0.2": "Saṭṭhinipāta",
  "ja530:0.3": "Soṇakavagga",
  "ja530:0.4": "2. Saṅkiccajātaka",
  "ja530:1.1": "Disvā nisinnaṃ rājānaṃ,",
  "ja530:1.2": "brahmadattaṃ rathesabhaṃ;",
  "ja530:1.3": "Athassa paṭivedesi,",
  "ja530:1.4": "“yassāsi anukampako.",
  "ja530:2.1": "Saṅkiccāyaṃ anuppatto,",
  "ja530:2.2": "isīnaṃ sādhusammato;",
  "ja530:2.3": "Taramānarūpo niyyāhi,",
  "ja530:2.4": "khippaṃ passa mahesinaṃ”.",
  "ja530:3.1": "Tato ca rājā taramāno,",
  "ja530:3.2": "yuttamāruyha sandanaṃ;",
  "ja530:3.3": "Mittāmaccaparibyūḷho,",
  "ja530:3.4": "agamāsi rathesabho.",
  "ja530:4.1": "Nikkhippa pañca kakudhāni,",
  "ja530:4.2": "kāsīnaṃ raṭṭhavaḍḍhano;",
  "ja530:4.3": "Vālabījanimuṇhīsaṃ,",
  "ja530:4.4": "khaggaṃ chattañcupāhanaṃ.",
  "ja530:5.1": "Oruyha rājā yānamhā,",
  "ja530:5.2": "ṭhapayitvā paṭicchadaṃ;",
  "ja530:5.3": "Āsīnaṃ dāyapassasmiṃ,",
  "ja530:5.4": "saṅkiccamupasaṅkami.",
  "ja530:6.1": "Upasaṅkamitvā so rājā,",
  "ja530:6.2": "sammodi isinā saha;",
  "ja530:6.3": "Taṃ kathaṃ vītisāretvā,",
  "ja530:6.4": "ekamantaṃ upāvisi.",
  "ja530:7.1": "Ekamantaṃ nisinnova,",
  "ja530:7.2": "atha kālaṃ amaññatha;",
  "ja530:7.3": "Tato pāpāni kammāni,",
  "ja530:7.4": "pucchituṃ paṭipajjatha.",
  "ja530:8.1": "“Isiṃ pucchāma saṅkiccaṃ,",
  "ja530:8.2": "isīnaṃ sādhusammataṃ;",
  "ja530:8.3": "Āsīnaṃ dāyapassasmiṃ,",
  "ja530:8.4": "isisaṃghapurakkhataṃ.",
  "ja530:9.1": "Kaṃ gatiṃ pecca gacchanti,",
  "ja530:9.2": "narā dhammāticārino;",
  "ja530:9.3": "Aticiṇṇo mayā dhammo,",
  "ja530:9.4": "taṃ me akkhāhi pucchito”.",
  "ja530:10.1": "Isī avaca saṅkicco,",
  "ja530:10.2": "kāsīnaṃ raṭṭhavaḍḍhanaṃ;",
  "ja530:10.3": "Āsīnaṃ dāyapassasmiṃ,",
  "ja530:10.4": "“mahārāja suṇohi me.",
  "ja530:11.1": "Uppathena vajantassa,",
  "ja530:11.2": "yo maggamanusāsati;",
  "ja530:11.3": "Tassa ce vacanaṃ kayirā,",
  "ja530:11.4": "nāssa maggeyya kaṇṭako.",
  "ja530:12.1": "Adhammaṃ paṭipannassa,",
  "ja530:12.2": "yo dhammamanusāsati;",
  "ja530:12.3": "Tassa ce vacanaṃ kayirā,",
  "ja530:12.4": "na so gaccheyya duggatiṃ”.",
  "ja530:13.1": "“Dhammo patho mahārāja,",
  "ja530:13.2": "adhammo pana uppatho;",
  "ja530:13.3": "Adhammo nirayaṃ neti,",
  "ja530:13.4": "dhammo pāpeti suggatiṃ.",
  "ja530:14.1": "Adhammacārino rāja,",
  "ja530:14.2": "narā visamajīvino;",
  "ja530:14.3": "Yaṃ gatiṃ pecca gacchanti,",
  "ja530:14.4": "niraye te suṇohi me.",
  "ja530:15.1": "Sañjīvo kāḷasutto ca,",
  "ja530:15.2": "saṅghāto dve ca roruvā;",
  "ja530:15.3": "Athāparo mahāvīci,",
  "ja530:15.4": "tāpano ca patāpano.",
  "ja530:16.1": "Iccete aṭṭha nirayā,",
  "ja530:16.2": "akkhātā duratikkamā;",
  "ja530:16.3": "Ākiṇṇā luddakammehi,",
  "ja530:16.4": "paccekā soḷasussadā.",
  "ja530:17.1": "Kadariyatāpanā ghorā,",
  "ja530:17.2": "accimanto mahabbhayā;",
  "ja530:17.3": "Lomahaṃsanarūpā ca,",
  "ja530:17.4": "bhesmā paṭibhayā dukhā.",
  "ja530:18.1": "Catukkaṇṇā catudvārā,",
  "ja530:18.2": "vibhattā bhāgaso mitā;",
  "ja530:18.3": "Ayopākārapariyantā,",
  "ja530:18.4": "ayasā paṭikujjitā.",
  "ja530:19.1": "Tesaṃ ayomayā bhūmi,",
  "ja530:19.2": "jalitā tejasā yutā;",
  "ja530:19.3": "Samantā yojanasataṃ,",
  "ja530:19.4": "phuṭā tiṭṭhanti sabbadā.",
  "ja530:20.1": "Ete patanti niraye,",
  "ja530:20.2": "uddhampādā avaṃsirā;",
  "ja530:20.3": "Isīnaṃ ativattāro,",
  "ja530:20.4": "saññatānaṃ tapassinaṃ.",
  "ja530:21.1": "Te bhūnahuno paccanti,",
  "ja530:21.2": "macchā bilakatā yathā;",
  "ja530:21.3": "Saṃvacchare asaṅkheyye,",
  "ja530:21.4": "narā kibbisakārino.",
  "ja530:22.1": "Ḍayhamānena gattena,",
  "ja530:22.2": "niccaṃ santarabāhiraṃ;",
  "ja530:22.3": "Nirayā nādhigacchanti,",
  "ja530:22.4": "dvāraṃ nikkhamanesino.",
  "ja530:23.1": "Puratthimena dhāvanti,",
  "ja530:23.2": "tato dhāvanti pacchato;",
  "ja530:23.3": "Uttarenapi dhāvanti,",
  "ja530:23.4": "tato dhāvanti dakkhiṇaṃ;",
  "ja530:23.5": "Yaṃ yañhi dvāraṃ gacchanti,",
  "ja530:23.6": "taṃ tadeva pidhīyare.",
  "ja530:24.1": "Bahūni vassasahassāni,",
  "ja530:24.2": "janā nirayagāmino;",
  "ja530:24.3": "Bāhā paggayha kandanti,",
  "ja530:24.4": "patvā dukkhaṃ anappakaṃ.",
  "ja530:25.1": "Āsīvisaṃva kupitaṃ,",
  "ja530:25.2": "tejassiṃ duratikkamaṃ;",
  "ja530:25.3": "Na sādhurūpe āsīde,",
  "ja530:25.4": "saññatānaṃ tapassinaṃ.",
  "ja530:26.1": "Atikāyo mahissāso,",
  "ja530:26.2": "ajjuno kekakādhipo;",
  "ja530:26.3": "Sahassabāhu ucchinno,",
  "ja530:26.4": "isimāsajja gotamaṃ.",
  "ja530:27.1": "Arajaṃ rajasā vacchaṃ,",
  "ja530:27.2": "Kisaṃ avakiriya daṇḍakī;",
  "ja530:27.3": "Tālova mūlato chinno,",
  "ja530:27.4": "Sa rājā vibhavaṅgato.",
  "ja530:28.1": "Upahacca manaṃ majjho,",
  "ja530:28.2": "mātaṅgasmiṃ yasassine;",
  "ja530:28.3": "Sapārisajjo ucchinno,",
  "ja530:28.4": "majjhāraññaṃ tadā ahu.",
  "ja530:29.1": "Kaṇhadīpāyanāsajja,",
  "ja530:29.2": "Isiṃ andhakaveṇḍayo;",
  "ja530:29.3": "Aññoññaṃ musalā hantvā,",
  "ja530:29.4": "Sampattā yamasādhanaṃ.",
  "ja530:30.1": "Athāyaṃ isinā satto,",
  "ja530:30.2": "antalikkhacaro pure;",
  "ja530:30.3": "Pāvekkhi pathaviṃ cecco,",
  "ja530:30.4": "hīnatto pattapariyāyaṃ.",
  "ja530:31.1": "Tasmā hi chandāgamanaṃ,",
  "ja530:31.2": "nappasaṃsanti paṇḍitā;",
  "ja530:31.3": "Aduṭṭhacitto bhāseyya,",
  "ja530:31.4": "giraṃ saccūpasaṃhitaṃ.",
  "ja530:32.1": "Manasā ce paduṭṭhena,",
  "ja530:32.2": "yo naro pekkhate muniṃ;",
  "ja530:32.3": "Vijjācaraṇasampannaṃ,",
  "ja530:32.4": "gantā so nirayaṃ adho.",
  "ja530:33.1": "Ye vuḍḍhe paribhāsanti,",
  "ja530:33.2": "pharusūpakkamā janā;",
  "ja530:33.3": "Anapaccā adāyādā,",
  "ja530:33.4": "tālavatthu bhavanti te.",
  "ja530:34.1": "Yo ca pabbajitaṃ hanti,",
  "ja530:34.2": "katakiccaṃ mahesinaṃ;",
  "ja530:34.3": "Sa kāḷasutte niraye,",
  "ja530:34.4": "cirarattāya paccati.",
  "ja530:35.1": "Yo ca rājā adhammaṭṭho,",
  "ja530:35.2": "raṭṭhaviddhaṃsano mago;",
  "ja530:35.3": "Tāpayitvā janapadaṃ,",
  "ja530:35.4": "tāpane pecca paccati.",
  "ja530:36.1": "So ca vassasahassāni,",
  "ja530:36.2": "sataṃ dibbāni paccati;",
  "ja530:36.3": "Accisaṃghapareto so,",
  "ja530:36.4": "dukkhaṃ vedeti vedanaṃ.",
  "ja530:37.1": "Tassa aggisikhā kāyā,",
  "ja530:37.2": "niccharanti pabhassarā;",
  "ja530:37.3": "Tejobhakkhassa gattāni,",
  "ja530:37.4": "lomehi ca nakhehi ca.",
  "ja530:38.1": "Ḍayhamānena gattena,",
  "ja530:38.2": "niccaṃ santarabāhiraṃ;",
  "ja530:38.3": "Dukkhābhitunno nadati,",
  "ja530:38.4": "nāgo tuttaṭṭito yathā.",
  "ja530:39.1": "Yo lobhā pitaraṃ hanti,",
  "ja530:39.2": "dosā vā purisādhamo;",
  "ja530:39.3": "Sa kāḷasutte niraye,",
  "ja530:39.4": "cirarattāya paccati.",
  "ja530:40.1": "Sa tādiso paccati lohakumbhiyaṃ,",
  "ja530:40.2": "Pakkañca sattīhi hananti nittacaṃ;",
  "ja530:40.3": "Andhaṃ karitvā muttakarīsabhakkhaṃ,",
  "ja530:40.4": "Khāre nimujjanti tathāvidhaṃ naraṃ.",
  "ja530:41.1": "Tattaṃ pakkuthitamayoguḷañca,",
  "ja530:41.2": "Dīghe ca phāle cirarattatāpite;",
  "ja530:41.3": "Vikkhambhamādāya vibandharajjubhi,",
  "ja530:41.4": "Vivaṭe mukhe sampavisanti rakkhasā.",
  "ja530:42.1": "Sāmā ca soṇā sabalā ca gijjhā,",
  "ja530:42.2": "Kākoḷasaṅghā ca dijā ayomukhā;",
  "ja530:42.3": "Saṅgamma khādanti vipphandamānaṃ,",
  "ja530:42.4": "Jivhaṃ vibhajja vighāsaṃ salohitaṃ.",
  "ja530:43.1": "Taṃ daḍḍhatālaṃ paribhinnagattaṃ,",
  "ja530:43.2": "Nippothayantā anuvicaranti rakkhasā;",
  "ja530:43.3": "Ratī hi nesaṃ dukhino panītare,",
  "ja530:43.4": "Etādisasmiṃ niraye vasanti;",
  "ja530:43.5": "Ye keci loke idha pettighātino.",
  "ja530:44.1": "Putto ca mātaraṃ hantvā,",
  "ja530:44.2": "ito gantvā yamakkhayaṃ;",
  "ja530:44.3": "Bhusamāpajjate dukkhaṃ,",
  "ja530:44.4": "attakammaphalūpago.",
  "ja530:45.1": "Amanussā atibalā,",
  "ja530:45.2": "hantāraṃ janayantiyā;",
  "ja530:45.3": "Ayomayehi vālehi,",
  "ja530:45.4": "pīḷayanti punappunaṃ.",
  "ja530:46.1": "Tamassavaṃ sakā gattā,",
  "ja530:46.2": "rudhiraṃ attasambhavaṃ;",
  "ja530:46.3": "Tambalohavilīnaṃva,",
  "ja530:46.4": "tattaṃ pāyenti mattighaṃ.",
  "ja530:47.1": "Jigucchaṃ kuṇapaṃ pūtiṃ,",
  "ja530:47.2": "Duggandhaṃ gūthakaddamaṃ;",
  "ja530:47.3": "Pubbalohitasaṅkāsaṃ,",
  "ja530:47.4": "Rahadamogayha tiṭṭhati.",
  "ja530:48.1": "Tamenaṃ kimayo tattha,",
  "ja530:48.2": "atikāyā ayomukhā;",
  "ja530:48.3": "Chaviṃ bhetvāna khādanti,",
  "ja530:48.4": "saṅgiddhā maṃsalohite.",
  "ja530:49.1": "So ca taṃ nirayaṃ patto,",
  "ja530:49.2": "nimuggo sataporisaṃ;",
  "ja530:49.3": "Pūtikaṃ kuṇapaṃ vāti,",
  "ja530:49.4": "samantā satayojanaṃ.",
  "ja530:50.1": "Cakkhumāpi hi cakkhūhi,",
  "ja530:50.2": "tena gandhena jīyati;",
  "ja530:50.3": "Etādisaṃ brahmadatta,",
  "ja530:50.4": "mātugho labhate dukhaṃ.",
  "ja530:51.1": "Khuradhāramanukkamma,",
  "ja530:51.2": "tikkhaṃ durabhisambhavaṃ;",
  "ja530:51.3": "Patanti gabbhapātiyo,",
  "ja530:51.4": "duggaṃ vetaraṇiṃ nadiṃ.",
  "ja530:52.1": "Ayomayā simbaliyo,",
  "ja530:52.2": "soḷasaṅgulakaṇṭakā;",
  "ja530:52.3": "Ubhato abhilambanti,",
  "ja530:52.4": "duggaṃ vetaraṇiṃ nadiṃ.",
  "ja530:53.1": "Te accimanto tiṭṭhanti,",
  "ja530:53.2": "aggikkhandhāva ārakā;",
  "ja530:53.3": "Ādittā jātavedena,",
  "ja530:53.4": "uddhaṃ yojanamuggatā.",
  "ja530:54.1": "Ete vajanti niraye,",
  "ja530:54.2": "tatte tikhiṇakaṇṭake;",
  "ja530:54.3": "Nāriyo ca aticārā,",
  "ja530:54.4": "narā ca paradāragū.",
  "ja530:55.1": "Te patanti adhokkhandhā,",
  "ja530:55.2": "vivattā vihatā puthū;",
  "ja530:55.3": "Sayanti vinividdhaṅgā,",
  "ja530:55.4": "dīghaṃ jagganti sabbadā.",
  "ja530:56.1": "Tato ratyā vivasāne,",
  "ja530:56.2": "mahatiṃ pabbatūpamaṃ;",
  "ja530:56.3": "Lohakumbhiṃ pavajjanti,",
  "ja530:56.4": "tattaṃ aggisamūdakaṃ.",
  "ja530:57.1": "Evaṃ divā ca ratto ca,",
  "ja530:57.2": "dussīlā mohapārutā;",
  "ja530:57.3": "Anubhonti sakaṃ kammaṃ,",
  "ja530:57.4": "pubbe dukkaṭamattano.",
  "ja530:58.1": "Yā ca bhariyā dhanakkītā,",
  "ja530:58.2": "sāmikaṃ atimaññati;",
  "ja530:58.3": "Sassuṃ vā sasuraṃ vāpi,",
  "ja530:58.4": "jeṭṭhaṃ vāpi nanandaraṃ.",
  "ja530:59.1": "Tassā vaṅkena jivhaggaṃ,",
  "ja530:59.2": "nibbahanti sabandhanaṃ;",
  "ja530:59.3": "Sa byāmamattaṃ kiminaṃ,",
  "ja530:59.4": "jivhaṃ passati attani;",
  "ja530:59.5": "Viññāpetuṃ na sakkoti,",
  "ja530:59.6": "tāpane pecca paccati.",
  "ja530:60.1": "Orabbhikā sūkarikā,",
  "ja530:60.2": "macchikā migabandhakā;",
  "ja530:60.3": "Corā goghātakā luddā,",
  "ja530:60.4": "avaṇṇe vaṇṇakārakā.",
  "ja530:61.1": "Sattīhi lohakūṭehi,",
  "ja530:61.2": "nettiṃsehi usūhi ca;",
  "ja530:61.3": "Haññamānā khāranadiṃ,",
  "ja530:61.4": "papatanti avaṃsirā.",
  "ja530:62.1": "Sāyaṃ pāto kūṭakārī,",
  "ja530:62.2": "ayokūṭehi haññati;",
  "ja530:62.3": "Tato vantaṃ durattānaṃ,",
  "ja530:62.4": "paresaṃ bhuñjare sadā.",
  "ja530:63.1": "Dhaṅkā bheraṇḍakā gijjhā,",
  "ja530:63.2": "kākoḷā ca ayomukhā;",
  "ja530:63.3": "Vipphandamānaṃ khādanti,",
  "ja530:63.4": "naraṃ kibbisakārakaṃ.",
  "ja530:64.1": "Ye migena migaṃ hanti,",
  "ja530:64.2": "pakkhiṃ vā pana pakkhinā;",
  "ja530:64.3": "Asanto rajasā channā,",
  "ja530:64.4": "gantā te nirayussadaṃ.",
  "ja530:65.1": "Santo ca uddhaṃ gacchanti,",
  "ja530:65.2": "suciṇṇenidha kammunā;",
  "ja530:65.3": "Suciṇṇassa phalaṃ passa,",
  "ja530:65.4": "saindā devā sabrahmakā.",
  "ja530:66.1": "Taṃ taṃ brūmi mahārāja,",
  "ja530:66.2": "Dhammaṃ raṭṭhapatī cara;",
  "ja530:66.3": "Tathā rāja carāhi dhammaṃ,",
  "ja530:66.4": "Yathā taṃ suciṇṇaṃ nānutappeyya pacchā”ti. (3535)",
  "ja530:67.1": "Saṅkiccajātakaṃ dutiyaṃ.",
  "ja530:68.1": "Saṭṭhinipātaṃ niṭṭhitaṃ.",
  "ja530:69.1": "Tassuddānaṃ",
  "ja530:70.1": "Atha saṭṭhinipātamhi,",
  "ja530:70.2": "Suṇātha mama bhāsitaṃ;",
  "ja530:70.3": "Jātakasavhayano pavaro,",
  "ja530:70.4": "Soṇakaarindamasavhayano;",
  "ja530:70.5": "Tathā vuttarathesabhakiccavaroti."
}