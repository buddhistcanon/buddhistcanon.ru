{
  "ja325:0.1": "Jātaka",
  "ja325:0.2": "Catukkanipāta",
  "ja325:0.3": "Kuṭidūsakavagga",
  "ja325:0.4": "5. Godharājajātaka",
  "ja325:1.1": "“Samaṇaṃ taṃ maññamāno,",
  "ja325:1.2": "upagacchimasaññataṃ;",
  "ja325:1.3": "So maṃ daṇḍena pāhāsi,",
  "ja325:1.4": "yathā assamaṇo tathā”.",
  "ja325:2.1": "“Kiṃ te jaṭāhi dummedha,",
  "ja325:2.2": "kiṃ te ajinasāṭiyā;",
  "ja325:2.3": "Abbhantaraṃ te gahanaṃ,",
  "ja325:2.4": "bāhiraṃ parimajjasi”.",
  "ja325:3.1": "“Ehi godha nivattassu,",
  "ja325:3.2": "bhuñja sālīnamodanaṃ;",
  "ja325:3.3": "Telaṃ loṇañca me atthi,",
  "ja325:3.4": "pahūtaṃ mayha pipphali.",
  "ja325:4.1": "Esa bhiyyo pavekkhāmi,",
  "ja325:4.2": "vammikaṃ sataporisaṃ;",
  "ja325:4.3": "Telaṃ loṇañca kittesi,",
  "ja325:4.4": "ahitaṃ mayha pipphalī”ti.",
  "ja325:5.1": "Godharājajātakaṃ pañcamaṃ."
}