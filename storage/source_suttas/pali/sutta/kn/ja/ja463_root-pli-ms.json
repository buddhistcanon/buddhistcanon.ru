{
  "ja463:0.1": "Jātaka",
  "ja463:0.2": "Ekādasakanipāta",
  "ja463:0.3": "Mātuposakavagga",
  "ja463:0.4": "9. Suppārakajātaka",
  "ja463:1.1": "“Ummujjanti nimujjanti,",
  "ja463:1.2": "manussā khuranāsikā;",
  "ja463:1.3": "Suppārakaṃ taṃ pucchāma,",
  "ja463:1.4": "samuddo katamo ayaṃ”.",
  "ja463:2.1": "“Kurukacchā payātānaṃ,",
  "ja463:2.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja463:2.3": "Nāvāya vippanaṭṭhāya,",
  "ja463:2.4": "khuramālīti vuccati”.",
  "ja463:3.1": "“Yathā aggīva suriyova,",
  "ja463:3.2": "samuddo paṭidissati;",
  "ja463:3.3": "Suppārakaṃ taṃ pucchāma,",
  "ja463:3.4": "samuddo katamo ayaṃ”.",
  "ja463:4.1": "“Kurukacchā payātānaṃ,",
  "ja463:4.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja463:4.3": "Nāvāya vippanaṭṭhāya,",
  "ja463:4.4": "aggimālīti vuccati”.",
  "ja463:5.1": "“Yathā dadhīva khīraṃva,",
  "ja463:5.2": "samuddo paṭidissati;",
  "ja463:5.3": "Suppārakaṃ taṃ pucchāma,",
  "ja463:5.4": "samuddo katamo ayaṃ”.",
  "ja463:6.1": "“Kurukacchā payātānaṃ,",
  "ja463:6.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja463:6.3": "Nāvāya vippanaṭṭhāya,",
  "ja463:6.4": "dadhimālīti vuccati”.",
  "ja463:7.1": "“Yathā kusova sassova,",
  "ja463:7.2": "samuddo paṭidissati;",
  "ja463:7.3": "Suppārakaṃ taṃ pucchāma,",
  "ja463:7.4": "samuddo katamo ayaṃ”.",
  "ja463:8.1": "“Kurukacchā payātānaṃ,",
  "ja463:8.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja463:8.3": "Nāvāya vippanaṭṭhāya,",
  "ja463:8.4": "kusamālīti vuccati”.",
  "ja463:9.1": "“Yathā naḷova veḷūva,",
  "ja463:9.2": "samuddo paṭidissati;",
  "ja463:9.3": "Suppārakaṃ taṃ pucchāma,",
  "ja463:9.4": "samuddo katamo ayaṃ”.",
  "ja463:10.1": "“Kurukacchā payātānaṃ,",
  "ja463:10.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja463:10.3": "Nāvāya vippanaṭṭhāya,",
  "ja463:10.4": "naḷamālīti vuccati”.",
  "ja463:11.1": "“Mahabbhayo bhiṃsanako,",
  "ja463:11.2": "saddo suyyatimānuso;",
  "ja463:11.3": "Yathā sobbho papātova,",
  "ja463:11.4": "samuddo paṭidissati;",
  "ja463:11.5": "Suppārakaṃ taṃ pucchāma,",
  "ja463:11.6": "samuddo katamo ayaṃ”.",
  "ja463:12.1": "“Kurukacchā payātānaṃ,",
  "ja463:12.2": "vāṇijānaṃ dhanesinaṃ;",
  "ja463:12.3": "Nāvāya vippanaṭṭhāya,",
  "ja463:12.4": "baḷavāmukhīti vuccati.",
  "ja463:13.1": "Yato sarāmi attānaṃ,",
  "ja463:13.2": "Yato pattosmi viññutaṃ;",
  "ja463:13.3": "Nābhijānāmi sañcicca,",
  "ja463:13.4": "Ekapāṇampi hiṃsitaṃ;",
  "ja463:13.5": "Etena saccavajjena,",
  "ja463:13.6": "Sotthiṃ nāvā nivattatū”ti. (1647)",
  "ja463:14.1": "Suppārakajātakaṃ navamaṃ.",
  "ja463:15.1": "Ekādasakanipātaṃ niṭṭhitaṃ.",
  "ja463:16.1": "Tassuddānaṃ",
  "ja463:17.1": "Sirimātusuposakanāgavaro,",
  "ja463:17.2": "Puna juṇhaka dhammamudayavaro;",
  "ja463:17.3": "Atha pāni yudhañcayako ca dasa-",
  "ja463:17.4": "Ratha saṃvara pāragatena navāti."
}