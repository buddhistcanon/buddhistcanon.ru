{
  "ja505:0.1": "Jātaka",
  "ja505:0.2": "Vīsatinipāta",
  "ja505:0.3": "Mātaṅgavagga",
  "ja505:0.4": "9. Somanassajātaka",
  "ja505:1.1": "“Ko taṃ hiṃsati heṭheti,",
  "ja505:1.2": "Kiṃ dummano socasi appatīto;",
  "ja505:1.3": "Kassajja mātāpitaro rudantu,",
  "ja505:1.4": "Kvajja setu nihato pathabyā”.",
  "ja505:2.1": "“Tuṭṭhosmi deva tava dassanena,",
  "ja505:2.2": "Cirassaṃ passāmi taṃ bhūmipāla;",
  "ja505:2.3": "Ahiṃsako reṇumanuppavissa,",
  "ja505:2.4": "Puttena te heṭhayitosmi deva”.",
  "ja505:3.1": "“Āyantu dovārikā khaggabandhā,",
  "ja505:3.2": "Kāsāviyā yantu antepurantaṃ;",
  "ja505:3.3": "Hantvāna taṃ somanassaṃ kumāraṃ,",
  "ja505:3.4": "Chetvāna sīsaṃ varamāharantu”.",
  "ja505:4.1": "“Pesitā rājino dūtā,",
  "ja505:4.2": "kumāraṃ etadabravuṃ;",
  "ja505:4.3": "Issarena vitiṇṇosi,",
  "ja505:4.4": "vadhaṃ pattosi khattiya”.",
  "ja505:5.1": "“Sa rājaputto paridevayanto,",
  "ja505:5.2": "Dasaṅguliṃ añjaliṃ paggahetvā;",
  "ja505:5.3": "Ahampi icchāmi janinda daṭṭhuṃ,",
  "ja505:5.4": "Jīvaṃ maṃ netvā paṭidassayetha”.",
  "ja505:6.1": "Tassa taṃ vacanaṃ sutvā,",
  "ja505:6.2": "rañño puttaṃ adassayuṃ;",
  "ja505:6.3": "Putto ca pitaraṃ disvā,",
  "ja505:6.4": "dūratovajjhabhāsatha.",
  "ja505:7.1": "“Āgacchuṃ dovārikā khaggabandhā,",
  "ja505:7.2": "Kāsāviyā hantu mamaṃ janinda;",
  "ja505:7.3": "Akkhāhi me pucchito etamatthaṃ,",
  "ja505:7.4": "Aparādho ko nidha mamajja atthi”.",
  "ja505:8.1": "“Sāyañca pāto udakaṃ sajāti,",
  "ja505:8.2": "Aggiṃ sadā pāricaratappamatto;",
  "ja505:8.3": "Taṃ tādisaṃ saṃyataṃ brahmacāriṃ,",
  "ja505:8.4": "Kasmā tuvaṃ brūsi gahappatīti”.",
  "ja505:9.1": "“Tālā ca mūlā ca phalā ca deva,",
  "ja505:9.2": "Pariggahā vividhā santimassa;",
  "ja505:9.3": "Te rakkhati gopayatappamatto,",
  "ja505:9.4": "Tasmā ahaṃ brūmi gahappatīti”.",
  "ja505:10.1": "“Saccaṃ kho etaṃ vadasi kumāra,",
  "ja505:10.2": "Pariggahā vividhā santimassa;",
  "ja505:10.3": "Te rakkhati gopayatappamatto,",
  "ja505:10.4": "Sa brāhmaṇo gahapati tena hoti”.",
  "ja505:11.1": "“Suṇantu mayhaṃ parisā samāgatā,",
  "ja505:11.2": "Sanegamā jānapadā ca sabbe;",
  "ja505:11.3": "Bālāyaṃ bālassa vaco nisamma,",
  "ja505:11.4": "Ahetunā ghātayate maṃ janindo.",
  "ja505:12.1": "Daḷhasmi mūle visaṭe virūḷhe,",
  "ja505:12.2": "Dunnikkayo veḷu pasākhajāto;",
  "ja505:12.3": "Vandāmi pādāni tava janinda,",
  "ja505:12.4": "Anujāna maṃ pabbajissāmi deva”.",
  "ja505:13.1": "“Bhuñjassu bhoge vipule kumāra,",
  "ja505:13.2": "Sabbañca te issariyaṃ dadāmi;",
  "ja505:13.3": "Ajjeva tvaṃ kurūnaṃ hohi rājā,",
  "ja505:13.4": "Mā pabbajī pabbajjā hi dukkhā”.",
  "ja505:14.1": "“Kinnūdha deva tavamatthi bhogā,",
  "ja505:14.2": "Pubbevahaṃ devaloke ramissaṃ;",
  "ja505:14.3": "Rūpehi saddehi atho rasehi,",
  "ja505:14.4": "Gandhehi phassehi manoramehi.",
  "ja505:15.1": "Bhuttā ca me bhogā tidivasmiṃ deva,",
  "ja505:15.2": "Parivārito accharānaṃ gaṇena;",
  "ja505:15.3": "Tuvañca bālaṃ paraneyyaṃ viditvā,",
  "ja505:15.4": "Na tādise rājakule vaseyyaṃ”.",
  "ja505:16.1": "“Sacāhaṃ bālo paraneyyo asmi,",
  "ja505:16.2": "Ekāparādhaṃ khama putta mayhaṃ;",
  "ja505:16.3": "Punapi ce edisakaṃ bhaveyya,",
  "ja505:16.4": "Yathāmatiṃ somanassa karohi”.",
  "ja505:17.1": "“Anisamma kataṃ kammaṃ,",
  "ja505:17.2": "anavatthāya cintitaṃ;",
  "ja505:17.3": "Bhesajjasseva vebhaṅgo,",
  "ja505:17.4": "vipāko hoti pāpako.",
  "ja505:18.1": "Nisamma ca kataṃ kammaṃ,",
  "ja505:18.2": "sammāvatthāya cintitaṃ;",
  "ja505:18.3": "Bhesajjasseva sampatti,",
  "ja505:18.4": "vipāko hoti bhadrako.",
  "ja505:19.1": "Alaso gihī kāmabhogī na sādhu,",
  "ja505:19.2": "Asaññato pabbajito na sādhu;",
  "ja505:19.3": "Rājā na sādhu anisammakārī,",
  "ja505:19.4": "Yo paṇḍito kodhano taṃ na sādhu.",
  "ja505:20.1": "Nisamma khattiyo kayirā,",
  "ja505:20.2": "nānisamma disampati;",
  "ja505:20.3": "Nisammakārino rāja,",
  "ja505:20.4": "yaso kitti ca vaḍḍhati.",
  "ja505:21.1": "Nisamma daṇḍaṃ paṇayeyya issaro,",
  "ja505:21.2": "Vegā kataṃ tappati bhūmipāla;",
  "ja505:21.3": "Sammāpaṇīdhī ca narassa atthā,",
  "ja505:21.4": "Anānutappā te bhavanti pacchā.",
  "ja505:22.1": "Anānutappāni hi ye karonti,",
  "ja505:22.2": "Vibhajja kammāyatanāni loke;",
  "ja505:22.3": "Viññuppasatthāni sukhudrayāni,",
  "ja505:22.4": "Bhavanti buddhānumatāni tāni.",
  "ja505:23.1": "Āgacchuṃ dovārikā khaggabandhā,",
  "ja505:23.2": "Kāsāviyā hantu mamaṃ janinda;",
  "ja505:23.3": "Mātuñca aṅkasmimahaṃ nisinno,",
  "ja505:23.4": "Ākaḍḍhito sahasā tehi deva.",
  "ja505:24.1": "Kaṭukañhi sambādhaṃ sukicchaṃ patto,",
  "ja505:24.2": "Madhurampi yaṃ jīvitaṃ laddha rāja;",
  "ja505:24.3": "Kicchenahaṃ ajja vadhā pamutto,",
  "ja505:24.4": "Pabbajjamevābhimanohamasmi”.",
  "ja505:25.1": "“Putto tavāyaṃ taruṇo sudhamme,",
  "ja505:25.2": "Anukampako somanasso kumāro;",
  "ja505:25.3": "Taṃ yācamāno na labhāmi svajja,",
  "ja505:25.4": "Arahasi naṃ yācitave tuvampi”.",
  "ja505:26.1": "“Ramassu bhikkhācariyāya putta,",
  "ja505:26.2": "Nisamma dhammesu paribbajassu;",
  "ja505:26.3": "Sabbesu bhūtesu nidhāya daṇḍaṃ,",
  "ja505:26.4": "Anindito brahmamupehi ṭhānaṃ”.",
  "ja505:27.1": "“Acchera rūpaṃ vata yādisañca,",
  "ja505:27.2": "Dukkhitaṃ maṃ dukkhāpayase sudhamme;",
  "ja505:27.3": "Yācassu puttaṃ iti vuccamānā,",
  "ja505:27.4": "Bhiyyova ussāhayase kumāraṃ”.",
  "ja505:28.1": "“Ye vippamuttā anavajjabhogino,",
  "ja505:28.2": "Parinibbutā lokamimaṃ caranti;",
  "ja505:28.3": "Tamariyamaggaṃ paṭipajjamānaṃ,",
  "ja505:28.4": "Na ussahe vārayituṃ kumāraṃ”.",
  "ja505:29.1": "“Addhā have sevitabbā sapaññā,",
  "ja505:29.2": "Bahussutā ye bahuṭhānacintino;",
  "ja505:29.3": "Yesāyaṃ sutvāna subhāsitāni,",
  "ja505:29.4": "Appossukkā vītasokā sudhammā”ti.",
  "ja505:30.1": "Somanassajātakaṃ navamaṃ."
}