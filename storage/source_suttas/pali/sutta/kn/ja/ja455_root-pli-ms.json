{
  "ja455:0.1": "Jātaka",
  "ja455:0.2": "Ekādasakanipāta",
  "ja455:0.3": "Mātuposakavagga",
  "ja455:0.4": "1. Mātuposakajātaka",
  "ja455:1.1": "“Tassa nāgassa vippavāsena,",
  "ja455:1.2": "Virūḷhā sallakī ca kuṭajā ca;",
  "ja455:1.3": "Kuruvindakaravīrā bhisasāmā ca,",
  "ja455:1.4": "Nivāte pupphitā ca kaṇikārā.",
  "ja455:2.1": "Kocideva suvaṇṇakāyurā,",
  "ja455:2.2": "Nāgarājaṃ bharanti piṇḍena;",
  "ja455:2.3": "Yattha rājā rājakumāro vā,",
  "ja455:2.4": "Kavacamabhihessati achambhito”.",
  "ja455:3.1": "“Gaṇhāhi nāga kabaḷaṃ,",
  "ja455:3.2": "mā nāga kisako bhava;",
  "ja455:3.3": "Bahūni rājakiccāni,",
  "ja455:3.4": "tāni nāga karissasi”.",
  "ja455:4.1": "“Sā nūnasā kapaṇikā,",
  "ja455:4.2": "andhā apariṇāyikā;",
  "ja455:4.3": "Khāṇuṃ pādena ghaṭṭeti,",
  "ja455:4.4": "giriṃ caṇḍoraṇaṃ pati”.",
  "ja455:5.1": "“Kā nu te sā mahānāga,",
  "ja455:5.2": "andhā apariṇāyikā;",
  "ja455:5.3": "Khāṇuṃ pādena ghaṭṭeti,",
  "ja455:5.4": "giriṃ caṇḍoraṇaṃ pati”.",
  "ja455:6.1": "“Mātā me sā mahārāja,",
  "ja455:6.2": "andhā apariṇāyikā;",
  "ja455:6.3": "Khāṇuṃ pādena ghaṭṭeti,",
  "ja455:6.4": "giriṃ caṇḍoraṇaṃ pati”.",
  "ja455:7.1": "“Muñcathetaṃ mahānāgaṃ,",
  "ja455:7.2": "yoyaṃ bharati mātaraṃ;",
  "ja455:7.3": "Sametu mātarā nāgo,",
  "ja455:7.4": "saha sabbehi ñātibhi”.",
  "ja455:8.1": "“Mutto ca bandhanā nāgo,",
  "ja455:8.2": "muttamādāya kuñjaro;",
  "ja455:8.3": "Muhuttaṃ assāsayitvā,",
  "ja455:8.4": "agamā yena pabbato.",
  "ja455:9.1": "Tato so naḷiniṃ gantvā,",
  "ja455:9.2": "sītaṃ kuñjarasevitaṃ;",
  "ja455:9.3": "Soṇḍāyūdakamāhatvā,",
  "ja455:9.4": "mātaraṃ abhisiñcatha”.",
  "ja455:10.1": "“Koyaṃ anariyo devo,",
  "ja455:10.2": "akālenapi vassati;",
  "ja455:10.3": "Gato me atrajo putto,",
  "ja455:10.4": "yo mayhaṃ paricārako”.",
  "ja455:11.1": "“Uṭṭhehi amma kiṃ sesi,",
  "ja455:11.2": "āgato tyāhamatrajo;",
  "ja455:11.3": "Muttomhi kāsirājena,",
  "ja455:11.4": "vedehena yasassinā”.",
  "ja455:12.1": "“Ciraṃ jīvatu so rājā,",
  "ja455:12.2": "kāsīnaṃ raṭṭhavaḍḍhano;",
  "ja455:12.3": "Yo me puttaṃ pamocesi,",
  "ja455:12.4": "sadā vuddhāpacāyikan”ti.",
  "ja455:13.1": "Mātuposakajātakaṃ paṭhamaṃ."
}