{
  "ja163:0.1": "Jātaka",
  "ja163:0.2": "Dukanipāta",
  "ja163:0.3": "Santhavavagga",
  "ja163:0.4": "3. Susīmajātaka",
  "ja163:1.1": "“Kāḷā migā setadantā tavīme,",
  "ja163:1.2": "Parosataṃ hemajālābhichannā;",
  "ja163:1.3": "Te te dadāmīti susīma brūsi,",
  "ja163:1.4": "Anussaraṃ pettipitāmahānaṃ”.",
  "ja163:2.1": "“Kāḷā migā setadantā mamīme,",
  "ja163:2.2": "Parosataṃ hemajālābhichannā;",
  "ja163:2.3": "Te te dadāmīti vadāmi māṇava,",
  "ja163:2.4": "Anussaraṃ pettipitāmahānan”ti.",
  "ja163:3.1": "Susīmajātakaṃ tatiyaṃ."
}