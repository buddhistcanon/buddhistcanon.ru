{
  "ja141:0.1": "Jātaka",
  "ja141:0.2": "Ekakanipāta",
  "ja141:0.3": "Kakaṇṭakavagga",
  "ja141:0.4": "1. Godhajātaka",
  "ja141:1.1": "“Na pāpajanasaṃsevī,",
  "ja141:1.2": "accantasukhamedhati;",
  "ja141:1.3": "Godhākulaṃ kakaṇṭāva,",
  "ja141:1.4": "kaliṃ pāpeti attanan”ti.",
  "ja141:2.1": "Godhajātakaṃ paṭhamaṃ."
}