{
  "ja215:0.1": "Jātaka",
  "ja215:0.2": "Dukanipāta",
  "ja215:0.3": "Bīraṇathambhavagga",
  "ja215:0.4": "5. Kacchapajātaka",
  "ja215:1.1": "“Avadhī vata attānaṃ,",
  "ja215:1.2": "kacchapo byāharaṃ giraṃ;",
  "ja215:1.3": "Suggahītasmiṃ kaṭṭhasmiṃ,",
  "ja215:1.4": "vācāya sakiyāvadhi.",
  "ja215:2.1": "Etampi disvā naravīriyaseṭṭha,",
  "ja215:2.2": "Vācaṃ pamuñce kusalaṃ nātivelaṃ;",
  "ja215:2.3": "Passasi bahubhāṇena,",
  "ja215:2.4": "Kacchapaṃ byasanaṃ gatan”ti.",
  "ja215:3.1": "Kacchapajātakaṃ pañcamaṃ."
}