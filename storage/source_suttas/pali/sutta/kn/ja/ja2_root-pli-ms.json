{
  "ja2:0.1": "Jātaka",
  "ja2:0.2": "Ekakanipāta",
  "ja2:0.3": "Apaṇṇakavagga",
  "ja2:0.4": "2. Vaṇṇupathajātaka",
  "ja2:1.1": "“Akilāsuno vaṇṇupathe khaṇantā,",
  "ja2:1.2": "Udaṅgaṇe tattha papaṃ avinduṃ;",
  "ja2:1.3": "Evaṃ munī viriyabalūpapanno,",
  "ja2:1.4": "Akilāsu vinde hadayassa santin”ti.",
  "ja2:2.1": "Vaṇṇupathajātakaṃ dutiyaṃ."
}