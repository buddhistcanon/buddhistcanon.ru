{
  "ja477:0.1": "Jātaka",
  "ja477:0.2": "Terasakanipāta",
  "ja477:0.3": "Ambavagga",
  "ja477:0.4": "4. Cūḷanāradajātaka",
  "ja477:1.1": "“Na te kaṭṭhāni bhinnāni,",
  "ja477:1.2": "na te udakamābhataṃ;",
  "ja477:1.3": "Aggīpi te na hāpito,",
  "ja477:1.4": "kiṃ nu mandova jhāyasi”.",
  "ja477:2.1": "“Na ussahe vane vatthuṃ,",
  "ja477:2.2": "kassapāmantayāmi taṃ;",
  "ja477:2.3": "Dukkho vāso araññasmiṃ,",
  "ja477:2.4": "raṭṭhaṃ icchāmi gantave.",
  "ja477:3.1": "Yathā ahaṃ ito gantvā,",
  "ja477:3.2": "Yasmiṃ janapade vasaṃ;",
  "ja477:3.3": "Ācāraṃ brahme sikkheyyaṃ,",
  "ja477:3.4": "Taṃ dhammaṃ anusāsa maṃ”.",
  "ja477:4.1": "“Sace araññaṃ hitvāna,",
  "ja477:4.2": "vanamūlaphalāni ca;",
  "ja477:4.3": "Raṭṭhe rocayase vāsaṃ,",
  "ja477:4.4": "taṃ dhammaṃ nisāmehi me.",
  "ja477:5.1": "Visaṃ mā paṭisevittho,",
  "ja477:5.2": "papātaṃ parivajjaya;",
  "ja477:5.3": "Paṅke ca mā visīdittho,",
  "ja477:5.4": "yatto cāsīvise care”.",
  "ja477:6.1": "“Kiṃ nu visaṃ papāto vā,",
  "ja477:6.2": "paṅko vā brahmacārinaṃ;",
  "ja477:6.3": "Kaṃ tvaṃ āsīvisaṃ brūsi,",
  "ja477:6.4": "taṃ me akkhāhi pucchito”.",
  "ja477:7.1": "“Āsavo tāta lokasmiṃ,",
  "ja477:7.2": "surā nāma pavuccati;",
  "ja477:7.3": "Manuñño surabhī vaggu,",
  "ja477:7.4": "sādu khuddarasūpamo;",
  "ja477:7.5": "Visaṃ tadāhu ariyā se,",
  "ja477:7.6": "brahmacariyassa nārada.",
  "ja477:8.1": "Itthiyo tāta lokasmiṃ,",
  "ja477:8.2": "pamattaṃ pamathenti tā;",
  "ja477:8.3": "Haranti yuvino cittaṃ,",
  "ja477:8.4": "tūlaṃ bhaṭṭhaṃva māluto;",
  "ja477:8.5": "Papāto eso akkhāto,",
  "ja477:8.6": "brahmacariyassa nārada.",
  "ja477:9.1": "Lābho siloko sakkāro,",
  "ja477:9.2": "pūjā parakulesu ca;",
  "ja477:9.3": "Paṅko eso ca akkhāto,",
  "ja477:9.4": "brahmacariyassa nārada.",
  "ja477:10.1": "Sasatthā tāta rājāno,",
  "ja477:10.2": "āvasanti mahiṃ imaṃ;",
  "ja477:10.3": "Te tādise manussinde,",
  "ja477:10.4": "mahante tāta nārada.",
  "ja477:11.1": "Issarānaṃ adhipatīnaṃ,",
  "ja477:11.2": "na tesaṃ pādato care;",
  "ja477:11.3": "Āsīvisoti akkhāto,",
  "ja477:11.4": "brahmacariyassa nārada.",
  "ja477:12.1": "Bhattattho bhattakāle ca,",
  "ja477:12.2": "yaṃ gehamupasaṅkame;",
  "ja477:12.3": "Yadettha kusalaṃ jaññā,",
  "ja477:12.4": "tattha ghāsesanaṃ care.",
  "ja477:13.1": "Pavisitvā parakulaṃ,",
  "ja477:13.2": "pānatthaṃ bhojanāya vā;",
  "ja477:13.3": "Mitaṃ khāde mitaṃ bhuñje,",
  "ja477:13.4": "na ca rūpe manaṃ kare.",
  "ja477:14.1": "Goṭṭhaṃ majjaṃ kirāṭañca,",
  "ja477:14.2": "sabhā nikiraṇāni ca;",
  "ja477:14.3": "Ārakā parivajjehi,",
  "ja477:14.4": "yānīva visamaṃ pathan”ti.",
  "ja477:15.1": "Cūḷanāradajātakaṃ catutthaṃ."
}