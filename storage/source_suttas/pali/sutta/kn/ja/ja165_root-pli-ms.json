{
  "ja165:0.1": "Jātaka",
  "ja165:0.2": "Dukanipāta",
  "ja165:0.3": "Santhavavagga",
  "ja165:0.4": "5. Nakulajātaka",
  "ja165:1.1": "“Sandhiṃ katvā amittena,",
  "ja165:1.2": "aṇḍajena jalābuja;",
  "ja165:1.3": "Vivariya dāṭhaṃ sesi,",
  "ja165:1.4": "kuto te bhayamāgataṃ”.",
  "ja165:2.1": "“Saṅketheva amittasmiṃ,",
  "ja165:2.2": "mittasmimpi na vissase;",
  "ja165:2.3": "Abhayā bhayamuppannaṃ,",
  "ja165:2.4": "api mūlāni kantatī”ti.",
  "ja165:3.1": "Nakulajātakaṃ pañcamaṃ."
}