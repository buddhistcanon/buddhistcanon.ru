{
  "ja15:0.1": "Jātaka",
  "ja15:0.2": "Ekakanipāta",
  "ja15:0.3": "Sīlavagga",
  "ja15:0.4": "5. Kharādiyajātaka",
  "ja15:1.1": "“Aṭṭhakkhuraṃ kharādiye,",
  "ja15:1.2": "migaṃ vaṅkātivaṅkinaṃ;",
  "ja15:1.3": "Sattahi kālātikkantaṃ,",
  "ja15:1.4": "na naṃ ovaditussahe”ti.",
  "ja15:2.1": "Kharādiyajātakaṃ pañcamaṃ."
}