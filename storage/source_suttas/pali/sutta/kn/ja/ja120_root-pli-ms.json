{
  "ja120:0.1": "Jātaka",
  "ja120:0.2": "Ekakanipāta",
  "ja120:0.3": "Haṃcivagga",
  "ja120:0.4": "10. Bandhanamokkhajātaka",
  "ja120:1.1": "“Abaddhā tattha bajjhanti,",
  "ja120:1.2": "yattha bālā pabhāsare;",
  "ja120:1.3": "Baddhāpi tattha muccanti,",
  "ja120:1.4": "yattha dhīrā pabhāsare”ti.",
  "ja120:2.1": "Bandhanamokkhajātakaṃ dasamaṃ.",
  "ja120:2.2": "Haṃcivaggo dvādasamo.",
  "ja120:3.1": "Tassuddānaṃ",
  "ja120:4.1": "Atha gadrabha sattuva kaṃsasataṃ,",
  "ja120:4.2": "Bahucinti sāsikāyātikara;",
  "ja120:4.3": "Ativela visesamanācariyova,",
  "ja120:4.4": "Dhīrāpabhāsaratena dasāti."
}