{
  "ja36:0.1": "Jātaka",
  "ja36:0.2": "Ekakanipāta",
  "ja36:0.3": "Kulāvakavagga",
  "ja36:0.4": "6. Sakuṇajātaka",
  "ja36:1.1": "“Yaṃ nissitā jagatiruhaṃ vihaṅgamā,",
  "ja36:1.2": "Svāyaṃ aggiṃ pamuñcati;",
  "ja36:1.3": "Disā bhajatha vakkaṅgā,",
  "ja36:1.4": "Jātaṃ saraṇato bhayan”ti.",
  "ja36:2.1": "Sakuṇajātakaṃ chaṭṭhaṃ."
}