{
  "ja347:0.1": "Jātaka",
  "ja347:0.2": "Catukkanipāta",
  "ja347:0.3": "Cūḷakuṇālavagga",
  "ja347:0.4": "7. Ayakūṭajātaka",
  "ja347:1.1": "“Sabbāyasaṃ kūṭamatippamāṇaṃ,",
  "ja347:1.2": "Paggayha yo tiṭṭhasi antalikkhe;",
  "ja347:1.3": "Rakkhāya me tvaṃ vihito nusajja,",
  "ja347:1.4": "Udāhu me cetayase vadhāya”.",
  "ja347:2.1": "“Dūto ahaṃ rājidha rakkhasānaṃ,",
  "ja347:2.2": "Vadhāya tuyhaṃ pahitohamasmi;",
  "ja347:2.3": "Indo ca taṃ rakkhati devarājā,",
  "ja347:2.4": "Tenuttamaṅgaṃ na te phālayāmi”.",
  "ja347:3.1": "“Sace ca maṃ rakkhati devarājā,",
  "ja347:3.2": "Devānamindo maghavā sujampati;",
  "ja347:3.3": "Kāmaṃ pisācā vinadantu sabbe,",
  "ja347:3.4": "Na santase rakkhasiyā pajāya.",
  "ja347:4.1": "Kāmaṃ kandantu kumbhaṇḍā,",
  "ja347:4.2": "Sabbe paṃsupisācakā;",
  "ja347:4.3": "Nālaṃ pisācā yuddhāya,",
  "ja347:4.4": "Mahatī sā vibhiṃsikā”ti.",
  "ja347:5.1": "Ayakūṭajātakaṃ sattamaṃ."
}