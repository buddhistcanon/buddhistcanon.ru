{
  "ja205:0.1": "Jātaka",
  "ja205:0.2": "Dukanipāta",
  "ja205:0.3": "Nataṃdaḷhavagga",
  "ja205:0.4": "5. Gaṅgeyyajātaka",
  "ja205:1.1": "“Sobhati maccho gaṅgeyyo,",
  "ja205:1.2": "atho sobhati yāmuno;",
  "ja205:1.3": "Catuppadoyaṃ puriso,",
  "ja205:1.4": "nigrodhaparimaṇḍalo;",
  "ja205:1.5": "Īsakāyatagīvo ca,",
  "ja205:1.6": "sabbeva atirocati”.",
  "ja205:2.1": "“Yaṃ pucchito na taṃ akkhāsi,",
  "ja205:2.2": "Aññaṃ akkhāsi pucchito;",
  "ja205:2.3": "Attappasaṃsako poso,",
  "ja205:2.4": "Nāyaṃ asmāka ruccatī”ti.",
  "ja205:3.1": "Gaṅgeyyajātakaṃ pañcamaṃ."
}