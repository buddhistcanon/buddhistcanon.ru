{
  "ja268:0.1": "Jātaka",
  "ja268:0.2": "Tikanipāta",
  "ja268:0.3": "Padumavagga",
  "ja268:0.4": "8. Ārāmadūsakajātaka",
  "ja268:1.1": "“Yo ve sabbasametānaṃ,",
  "ja268:1.2": "ahuvā seṭṭhasammato;",
  "ja268:1.3": "Tassāyaṃ edisī paññā,",
  "ja268:1.4": "kimeva itarā pajā”.",
  "ja268:2.1": "“Evameva tuvaṃ brahme,",
  "ja268:2.2": "anaññāya vinindasi;",
  "ja268:2.3": "Kathaṃ mūlaṃ adisvāna,",
  "ja268:2.4": "rukkhaṃ jaññā patiṭṭhitaṃ”.",
  "ja268:3.1": "“Nāhaṃ tumhe vinindāmi,",
  "ja268:3.2": "ye caññe vānarā vane;",
  "ja268:3.3": "Vissasenova gārayho,",
  "ja268:3.4": "yassatthā rukkharopakā”ti.",
  "ja268:4.1": "Ārāmadūsakajātakaṃ aṭṭhamaṃ."
}