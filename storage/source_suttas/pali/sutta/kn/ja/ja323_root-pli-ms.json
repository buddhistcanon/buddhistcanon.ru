{
  "ja323:0.1": "Jātaka",
  "ja323:0.2": "Catukkanipāta",
  "ja323:0.3": "Kuṭidūsakavagga",
  "ja323:0.4": "3. Brahmadattajātaka",
  "ja323:1.1": "“Dvayaṃ yācanako rāja,",
  "ja323:1.2": "brahmadatta nigacchati;",
  "ja323:1.3": "Alābhaṃ dhanalābhaṃ vā,",
  "ja323:1.4": "evaṃ dhammā hi yācanā.",
  "ja323:2.1": "Yācanaṃ rodanaṃ āhu,",
  "ja323:2.2": "pañcālānaṃ rathesabha;",
  "ja323:2.3": "Yo yācanaṃ paccakkhāti,",
  "ja323:2.4": "tamāhu paṭirodanaṃ.",
  "ja323:3.1": "Mā maddasaṃsu rodantaṃ,",
  "ja323:3.2": "pañcālā susamāgatā;",
  "ja323:3.3": "Tuvaṃ vā paṭirodantaṃ,",
  "ja323:3.4": "tasmā icchāmahaṃ raho”.",
  "ja323:4.1": "“Dadāmi te brāhmaṇa rohiṇīnaṃ,",
  "ja323:4.2": "Gavaṃ sahassaṃ saha puṅgavena;",
  "ja323:4.3": "Ariyo hi ariyassa kathaṃ na dajjā,",
  "ja323:4.4": "Sutvāna gāthā tava dhammayuttā”ti.",
  "ja323:5.1": "Brahmadattajātakaṃ tatiyaṃ."
}