{
  "ja535:0.1": "Jātaka",
  "ja535:0.2": "Asītinipāta",
  "ja535:0.3": "Cūḷahaṃsavagga",
  "ja535:0.4": "3. Sudhābhojanajātaka",
  "ja535:1.1": "“Neva kiṇāmi napi vikkiṇāmi,",
  "ja535:1.2": "Na cāpi me sannicayo ca atthi;",
  "ja535:1.3": "Sukiccharūpaṃ vatidaṃ parittaṃ,",
  "ja535:1.4": "Patthodano nālamayaṃ duvinnaṃ”.",
  "ja535:2.1": "“Appamhā appakaṃ dajjā,",
  "ja535:2.2": "anumajjhato majjhakaṃ;",
  "ja535:2.3": "Bahumhā bahukaṃ dajjā,",
  "ja535:2.4": "adānaṃ nupapajjati.",
  "ja535:3.1": "Taṃ taṃ vadāmi kosiya,",
  "ja535:3.2": "dehi dānāni bhuñja ca;",
  "ja535:3.3": "Ariyamaggaṃ samārūha,",
  "ja535:3.4": "nekāsī labhate sukhaṃ”.",
  "ja535:4.1": "“Moghañcassa hutaṃ hoti,",
  "ja535:4.2": "moghañcāpi samīhitaṃ;",
  "ja535:4.3": "Atithismiṃ yo nisinnasmiṃ,",
  "ja535:4.4": "eko bhuñjati bhojanaṃ.",
  "ja535:5.1": "Taṃ taṃ vadāmi kosiya,",
  "ja535:5.2": "dehi dānāni bhuñja ca;",
  "ja535:5.3": "Ariyamaggaṃ samārūha,",
  "ja535:5.4": "nekāsī labhate sukhaṃ”.",
  "ja535:6.1": "“Saccañcassa hutaṃ hoti,",
  "ja535:6.2": "saccañcāpi samīhitaṃ;",
  "ja535:6.3": "Atithismiṃ yo nisinnasmiṃ,",
  "ja535:6.4": "neko bhuñjati bhojanaṃ.",
  "ja535:7.1": "Taṃ taṃ vadāmi kosiya,",
  "ja535:7.2": "dehi dānāni bhuñja ca;",
  "ja535:7.3": "Ariyamaggaṃ samārūha,",
  "ja535:7.4": "nekāsī labhate sukhaṃ”.",
  "ja535:8.1": "“Sarañca juhati poso,",
  "ja535:8.2": "bahukāya gayāya ca;",
  "ja535:8.3": "Doṇe timbarutitthasmiṃ,",
  "ja535:8.4": "sīghasote mahāvahe.",
  "ja535:9.1": "Atra cassa hutaṃ hoti,",
  "ja535:9.2": "atra cassa samīhitaṃ;",
  "ja535:9.3": "Atithismiṃ yo nisinnasmiṃ,",
  "ja535:9.4": "neko bhuñjati bhojanaṃ.",
  "ja535:10.1": "Taṃ taṃ vadāmi kosiya,",
  "ja535:10.2": "dehi dānāni bhuñja ca;",
  "ja535:10.3": "Ariyamaggaṃ samārūha,",
  "ja535:10.4": "nekāsī labhate sukhaṃ”.",
  "ja535:11.1": "“Baḷisañhi so nigilati,",
  "ja535:11.2": "dīghasuttaṃ sabandhanaṃ;",
  "ja535:11.3": "Atithismiṃ yo nisinnasmiṃ,",
  "ja535:11.4": "eko bhuñjati bhojanaṃ.",
  "ja535:12.1": "Taṃ taṃ vadāmi kosiya,",
  "ja535:12.2": "dehi dānāni bhuñja ca;",
  "ja535:12.3": "Ariyamaggaṃ samārūha,",
  "ja535:12.4": "nekāsī labhate sukhaṃ”.",
  "ja535:13.1": "“Uḷāravaṇṇā vata brāhmaṇā ime,",
  "ja535:13.2": "Ayañca vo sunakho kissa hetu;",
  "ja535:13.3": "Uccāvacaṃ vaṇṇanibhaṃ vikubbati,",
  "ja535:13.4": "Akkhātha no brāhmaṇā ke nu tumhe”.",
  "ja535:14.1": "“Cando ca suriyo ca ubho idhāgatā,",
  "ja535:14.2": "Ayaṃ pana mātali devasārathi;",
  "ja535:14.3": "Sakkohamasmi tidasānamindo,",
  "ja535:14.4": "Eso ca kho pañcasikhoti vuccati.",
  "ja535:15.1": "Pāṇissarā mudiṅgā ca,",
  "ja535:15.2": "murajālambarāni ca;",
  "ja535:15.3": "Suttamenaṃ pabodhenti,",
  "ja535:15.4": "paṭibuddho ca nandati.",
  "ja535:16.1": "Ye kecime maccharino kadariyā,",
  "ja535:16.2": "Paribhāsakā samaṇabrāhmaṇānaṃ;",
  "ja535:16.3": "Idheva nikkhippa sarīradehaṃ,",
  "ja535:16.4": "Kāyassa bhedā nirayaṃ vajanti.",
  "ja535:17.1": "Ye kecime suggatimāsamānā,",
  "ja535:17.2": "Dhamme ṭhitā saṃyame saṃvibhāge;",
  "ja535:17.3": "Idheva nikkhippa sarīradehaṃ,",
  "ja535:17.4": "Kāyassa bhedā sugatiṃ vajanti.",
  "ja535:18.1": "Tvaṃ nosi ñāti purimāsu jātisu,",
  "ja535:18.2": "So maccharī rosako pāpadhammo;",
  "ja535:18.3": "Taveva atthāya idhāgatamhā,",
  "ja535:18.4": "Mā pāpadhammo nirayaṃ gamittha”.",
  "ja535:19.1": "“Addhā maṃ vo hitakāmā,",
  "ja535:19.2": "yaṃ maṃ samanusāsatha;",
  "ja535:19.3": "Sohaṃ tathā karissāmi,",
  "ja535:19.4": "sabbaṃ vuttaṃ hitesibhi.",
  "ja535:20.1": "Esāhamajjeva upāramāmi,",
  "ja535:20.2": "Na cāpihaṃ kiñci kareyya pāpaṃ;",
  "ja535:20.3": "Na cāpi me kiñci adeyyamatthi,",
  "ja535:20.4": "Na cāpidatvā udakaṃ pivāmi.",
  "ja535:21.1": "Evañca me dadato sabbakālaṃ,",
  "ja535:21.2": "Bhogā ime vāsava khīyissanti;",
  "ja535:21.3": "Tato ahaṃ pabbajissāmi sakka,",
  "ja535:21.4": "Hitvāna kāmāni yathodhikāni”.",
  "ja535:22.1": "Naguttame girivare gandhamādane,",
  "ja535:22.2": "Modanti tā devavarābhipālitā;",
  "ja535:22.3": "Athāgamā isivaro sabbalokagū,",
  "ja535:22.4": "Supupphitaṃ dumavarasākhamādiya.",
  "ja535:23.1": "Suciṃ sugandhaṃ tidasehi sakkataṃ,",
  "ja535:23.2": "Pupphuttamaṃ amaravarehi sevitaṃ;",
  "ja535:23.3": "Aladdha maccehi va dānavehi vā,",
  "ja535:23.4": "Aññatra devehi tadārahaṃ hidaṃ.",
  "ja535:24.1": "Tato catasso kanakattacūpamā,",
  "ja535:24.2": "Uṭṭhāya nāriyo pamadādhipā muniṃ;",
  "ja535:24.3": "Āsā ca saddhā ca sirī tato hirī,",
  "ja535:24.4": "Iccabravuṃ nāradadeva brāhmaṇaṃ.",
  "ja535:25.1": "“Sace anuddiṭṭhaṃ tayā mahāmuni,",
  "ja535:25.2": "Pupphaṃ imaṃ pārichattassa brahme;",
  "ja535:25.3": "Dadāhi no sabbā gati te ijjhatu,",
  "ja535:25.4": "Tuvampi no hohi yatheva vāsavo”.",
  "ja535:26.1": "Taṃ yācamānābhisamekkha nārado,",
  "ja535:26.2": "Iccabravī saṅkalahaṃ udīrayi;",
  "ja535:26.3": "“Na mayhamatthatthi imehi koci naṃ,",
  "ja535:26.4": "Yāyeva vo seyyasi sā piḷandhatha”.",
  "ja535:27.1": "“Tvaṃ nottamevābhisamekkha nārada,",
  "ja535:27.2": "Yassicchasi tassā anuppavecchasu;",
  "ja535:27.3": "Yassā hi no nārada tvaṃ padassasi,",
  "ja535:27.4": "Sāyeva no hehiti seṭṭhasammatā”.",
  "ja535:28.1": "“Akallametaṃ vacanaṃ sugatte,",
  "ja535:28.2": "Ko brāhmaṇo saṅkalahaṃ udīraye;",
  "ja535:28.3": "Gantvāna bhūtādhipameva pucchatha,",
  "ja535:28.4": "Sace na jānātha idhuttamādhamaṃ”.",
  "ja535:29.1": "Tā nāradena paramappakopitā,",
  "ja535:29.2": "Udīritā vaṇṇamadena mattā;",
  "ja535:29.3": "Sakāse gantvāna sahassacakkhuno,",
  "ja535:29.4": "“Pucchiṃsu bhūtādhipaṃ kā nu seyyasi”.",
  "ja535:30.1": "Tā disvā āyattamanā purindado,",
  "ja535:30.2": "Iccabravī devavaro katañjalī;",
  "ja535:30.3": "“Sabbāva vo hotha sugatte sādisī,",
  "ja535:30.4": "Ko neva bhadde kalahaṃ udīrayi”.",
  "ja535:31.1": "“Yo sabbalokaccarito mahāmuni,",
  "ja535:31.2": "Dhamme ṭhito nārado saccanikkamo;",
  "ja535:31.3": "So nobravi girivare gandhamādane,",
  "ja535:31.4": "Gantvāna bhūtādhipameva pucchatha;",
  "ja535:31.5": "Sace na jānātha idhuttamādhamaṃ”.",
  "ja535:32.1": "“Asu brahāraññacaro mahāmuni,",
  "ja535:32.2": "Nādatvā bhattaṃ varagatte bhuñjati;",
  "ja535:32.3": "Viceyya dānāni dadāti kosiyo,",
  "ja535:32.4": "Yassā hi so dassati sāva seyyasi”.",
  "ja535:33.1": "“Asū hi yo sammati dakkhiṇaṃ disaṃ,",
  "ja535:33.2": "Gaṅgāya tīre himavantapassani;",
  "ja535:33.3": "Sa kosiyo dullabhapānabhojano,",
  "ja535:33.4": "Tassa sudhaṃ pāpaya devasārathi”.",
  "ja535:34.1": "Sa mātalī devavarena pesito,",
  "ja535:34.2": "Sahassayuttaṃ abhiruyha sandanaṃ;",
  "ja535:34.3": "Sukhippameva upagamma assamaṃ,",
  "ja535:34.4": "Adissamāno munino sudhaṃ adā.",
  "ja535:35.1": "“Udaggihuttaṃ upatiṭṭhato hi me,",
  "ja535:35.2": "Pabhaṅkaraṃ lokatamonuduttamaṃ;",
  "ja535:35.3": "Sabbāni bhūtāni adhicca vāsavo,",
  "ja535:35.4": "Ko neva me pāṇisu kiṃ sudhodahi.",
  "ja535:36.1": "Saṅkhūpamaṃ setamatulyadassanaṃ,",
  "ja535:36.2": "Suciṃ sugandhaṃ piyarūpamabbhutaṃ;",
  "ja535:36.3": "Adiṭṭhapubbaṃ mama jātu cakkhubhi,",
  "ja535:36.4": "Kā devatā pāṇisu kiṃ sudhodahi”.",
  "ja535:37.1": "“Ahaṃ mahindena mahesi pesito,",
  "ja535:37.2": "Sudhābhihāsiṃ turito mahāmuni;",
  "ja535:37.3": "Jānāsi maṃ mātali devasārathi,",
  "ja535:37.4": "Bhuñjassu bhattuttama mābhivārayi.",
  "ja535:38.1": "Bhuttā ca sā dvādasa hanti pāpake,",
  "ja535:38.2": "Khudaṃ pipāsaṃ aratiṃ daraklamaṃ;",
  "ja535:38.3": "Kodhūpanāhañca vivādapesuṇaṃ,",
  "ja535:38.4": "Sītuṇha tandiñca rasuttamaṃ idaṃ”.",
  "ja535:39.1": "“Na kappatī mātali mayha bhuñjituṃ,",
  "ja535:39.2": "Pubbe adatvā iti me vatuttamaṃ;",
  "ja535:39.3": "Na cāpi ekāsnamarīyapūjitaṃ,",
  "ja535:39.4": "Asaṃvibhāgī ca sukhaṃ na vindati”.",
  "ja535:40.1": "“Thīghātakā ye cime pāradārikā,",
  "ja535:40.2": "Mittadduno ye ca sapanti subbate;",
  "ja535:40.3": "Sabbe ca te maccharipañcamādhamā,",
  "ja535:40.4": "Tasmā adatvā udakampi nāsniye.",
  "ja535:41.1": "Sohitthiyā vā purisassa vā pana,",
  "ja535:41.2": "Dassāmi dānaṃ vidusampavaṇṇitaṃ;",
  "ja535:41.3": "Saddhā vadaññū idha vītamaccharā,",
  "ja535:41.4": "Bhavanti hete sucisaccasammatā”.",
  "ja535:42.1": "Ato matā devavarena pesitā,",
  "ja535:42.2": "Kaññā catasso kanakattacūpamā;",
  "ja535:42.3": "Āsā ca saddhā ca sirī tato hirī,",
  "ja535:42.4": "Taṃ assamaṃ āgamu yattha kosiyo.",
  "ja535:43.1": "Tā disvā sabbo paramappamodito,",
  "ja535:43.2": "Subhena vaṇṇena sikhārivaggino;",
  "ja535:43.3": "Kaññā catasso caturo catuddisā,",
  "ja535:43.4": "Iccabravī mātalino ca sammukhā.",
  "ja535:44.1": "“Purimaṃ disaṃ kā tvaṃ pabhāsi devate,",
  "ja535:44.2": "Alaṅkatā tāravarāva osadhī;",
  "ja535:44.3": "Pucchāmi taṃ kañcanavelliviggahe,",
  "ja535:44.4": "Ācikkha me tvaṃ katamāsi devatā”.",
  "ja535:45.1": "“Sirāha devī manujebhi pūjitā,",
  "ja535:45.2": "Apāpasattūpanisevinī sadā;",
  "ja535:45.3": "Sudhāvivādena tavantimāgatā,",
  "ja535:45.4": "Taṃ maṃ sudhāya varapañña bhājaya.",
  "ja535:46.1": "Yassāhamicchāmi sudhaṃ mahāmuni,",
  "ja535:46.2": "So sabbakāmehi naro pamodati;",
  "ja535:46.3": "Sirīti maṃ jānahi jūhatuttama,",
  "ja535:46.4": "Taṃ maṃ sudhāya varapañña bhājaya”.",
  "ja535:47.1": "“Sippena vijjācaraṇena buddhiyā,",
  "ja535:47.2": "Narā upetā paguṇā sakammunā;",
  "ja535:47.3": "Tayā vihīnā na labhanti kiñcanaṃ,",
  "ja535:47.4": "Tayidaṃ na sādhu yadidaṃ tayā kataṃ.",
  "ja535:48.1": "Passāmi posaṃ alasaṃ mahagghasaṃ,",
  "ja535:48.2": "Sudukkulīnampi arūpimaṃ naraṃ;",
  "ja535:48.3": "Tayānugutto siri jātimāmapi,",
  "ja535:48.4": "Peseti dāsaṃ viya bhogavā sukhī.",
  "ja535:49.1": "Taṃ taṃ asaccaṃ avibhajjaseviniṃ,",
  "ja535:49.2": "Jānāmi mūḷhaṃ vidurānupātiniṃ;",
  "ja535:49.3": "Na tādisī arahati āsanūdakaṃ,",
  "ja535:49.4": "Kuto sudhā gaccha na mayha ruccasi”.",
  "ja535:50.1": "“Kā sukkadāṭhā paṭimukkakuṇḍalā,",
  "ja535:50.2": "Cittaṅgadā kambuvimaṭṭhadhārinī;",
  "ja535:50.3": "Osittavaṇṇaṃ paridayha sobhasi,",
  "ja535:50.4": "Kusaggirattaṃ apiḷayha mañjariṃ.",
  "ja535:51.1": "Migīva bhantā saracāpadhārinā,",
  "ja535:51.2": "Virādhitā mandamiva udikkhasi;",
  "ja535:51.3": "Ko te dutīyo idha mandalocane,",
  "ja535:51.4": "Na bhāyasi ekikā kānane vane”.",
  "ja535:52.1": "“Na me dutīyo idha matthi kosiya,",
  "ja535:52.2": "Masakkasārappabhavamhi devatā;",
  "ja535:52.3": "Āsā sudhāsāya tavantimāgatā,",
  "ja535:52.4": "Taṃ maṃ sudhāya varapañña bhājaya”.",
  "ja535:53.1": "“Āsāya yanti vāṇijā dhanesino,",
  "ja535:53.2": "Nāvaṃ samāruyha parenti aṇṇave;",
  "ja535:53.3": "Te tattha sīdanti athopi ekadā,",
  "ja535:53.4": "Jīnādhanā enti vinaṭṭhapābhatā.",
  "ja535:54.1": "Āsāya khettāni kasanti kassakā,",
  "ja535:54.2": "Vapanti bījāni karontupāyaso;",
  "ja535:54.3": "Ītīnipātena avuṭṭhitāya vā,",
  "ja535:54.4": "Na kiñci vindanti tato phalāgamaṃ.",
  "ja535:55.1": "Athattakārāni karonti bhattusu,",
  "ja535:55.2": "Āsaṃ purakkhatvā narā sukhesino;",
  "ja535:55.3": "Te bhatturatthā atigāḷhitā puna,",
  "ja535:55.4": "Disā panassanti aladdha kiñcanaṃ.",
  "ja535:56.1": "Hitvāna dhaññañca dhanañca ñātake,",
  "ja535:56.2": "Āsāya saggādhimanā sukhesino;",
  "ja535:56.3": "Tapanti lūkhampi tapaṃ cirantaraṃ,",
  "ja535:56.4": "Kumaggamāruyha parenti duggatiṃ.",
  "ja535:57.1": "Āsā visaṃvādikasammatā ime,",
  "ja535:57.2": "Āse sudhāsaṃ vinayassu attani;",
  "ja535:57.3": "Na tādisī arahati āsanūdakaṃ,",
  "ja535:57.4": "Kuto sudhā gaccha na mayha ruccasi”.",
  "ja535:58.1": "“Daddallamānā yasasā yasassinī,",
  "ja535:58.2": "Jighaññanāmavhayanaṃ disaṃ pati;",
  "ja535:58.3": "Pucchāmi taṃ kañcanavelliviggahe,",
  "ja535:58.4": "Ācikkha me tvaṃ katamāsi devatā”.",
  "ja535:59.1": "“Saddhāha devī manujehi pūjitā,",
  "ja535:59.2": "Apāpasattūpanisevinī sadā;",
  "ja535:59.3": "Sudhāvivādena tavantimāgatā,",
  "ja535:59.4": "Taṃ maṃ sudhāya varapañña bhājaya”.",
  "ja535:60.1": "“Dānaṃ damaṃ cāgamathopi saṃyamaṃ,",
  "ja535:60.2": "Ādāya saddhāya karonti hekadā;",
  "ja535:60.3": "Theyyaṃ musā kūṭamathopi pesuṇaṃ,",
  "ja535:60.4": "Karonti heke puna viccutā tayā.",
  "ja535:61.1": "Bhariyāsu poso sadisīsu pekkhavā,",
  "ja535:61.2": "Sīlūpapannāsu patibbatāsupi;",
  "ja535:61.3": "Vinetvāna chandaṃ kulitthiyāsupi,",
  "ja535:61.4": "Karoti saddhaṃ puna kumbhadāsiyā.",
  "ja535:62.1": "Tvameva saddhe paradārasevinī,",
  "ja535:62.2": "Pāpaṃ karosi kusalampi riñcasi;",
  "ja535:62.3": "Na tādisī arahati āsanūdakaṃ,",
  "ja535:62.4": "Kuto sudhā gaccha na mayha ruccasi”.",
  "ja535:63.1": "“Jighaññarattiṃ aruṇasmimūhate,",
  "ja535:63.2": "Yā dissati uttamarūpavaṇṇinī;",
  "ja535:63.3": "Tathūpamā maṃ paṭibhāsi devate,",
  "ja535:63.4": "Ācikkha me tvaṃ katamāsi accharā.",
  "ja535:64.1": "Kālā nidāgheriva aggijāriva,",
  "ja535:64.2": "Anileritā lohitapattamālinī;",
  "ja535:64.3": "Kā tiṭṭhasi mandamigāvalokayaṃ,",
  "ja535:64.4": "Bhāsesamānāva giraṃ na muñcasi”.",
  "ja535:65.1": "“Hirāha devī manujehi pūjitā,",
  "ja535:65.2": "Apāpasattūpanisevinī sadā;",
  "ja535:65.3": "Sudhāvivādena tavantimāgatā,",
  "ja535:65.4": "Sāhaṃ na sakkomi sudhampi yācituṃ;",
  "ja535:65.5": "Kopīnarūpā viya yācanitthiyā”.",
  "ja535:66.1": "“Dhammena ñāyena sugatte lacchasi,",
  "ja535:66.2": "Eso hi dhammo na hi yācanā sudhā;",
  "ja535:66.3": "Taṃ taṃ ayācantimahaṃ nimantaye,",
  "ja535:66.4": "Sudhāya yañcicchasi tampi dammi te.",
  "ja535:67.1": "Sā tvaṃ mayā ajja sakamhi assame,",
  "ja535:67.2": "Nimantitā kañcanavelliviggahe;",
  "ja535:67.3": "Tuvañhi me sabbarasehi pūjiyā,",
  "ja535:67.4": "Taṃ pūjayitvāna sudhampi asniye”.",
  "ja535:68.1": "Sā kosiyenānumatā jutīmatā,",
  "ja535:68.2": "Addhā hiri rammaṃ pāvisi yassamaṃ;",
  "ja535:68.3": "Udakavantaṃ phalamariyapūjitaṃ,",
  "ja535:68.4": "Apāpasattūpanisevitaṃ sadā.",
  "ja535:69.1": "Rukkhaggahānā bahukettha pupphitā,",
  "ja535:69.2": "Ambā piyālā panasā ca kiṃsukā;",
  "ja535:69.3": "Sobhañjanā loddamathopi padmakā,",
  "ja535:69.4": "Kekā ca bhaṅgā tilakā supupphitā.",
  "ja535:70.1": "Sālā karerī bahukettha jambuyo,",
  "ja535:70.2": "Assatthanigrodhamadhukavetasā;",
  "ja535:70.3": "Uddālakā pāṭali sinduvārakā,",
  "ja535:70.4": "Manuññagandhā mucalindaketakā.",
  "ja535:71.1": "Hareṇukā veḷukā keṇu tindukā,",
  "ja535:71.2": "Sāmākanīvāramathopi cīnakā;",
  "ja535:71.3": "Mocā kadalī bahukettha sāliyo,",
  "ja535:71.4": "Pavīhayo ābhūjino ca taṇḍulā.",
  "ja535:72.1": "Tassevuttarapassena,",
  "ja535:72.2": "jātā pokkharaṇī sivā;",
  "ja535:72.3": "Akakkasā apabbhārā,",
  "ja535:72.4": "sādhu appaṭigandhikā.",
  "ja535:73.1": "Tattha macchā sanniratā,",
  "ja535:73.2": "khemino bahubhojanā;",
  "ja535:73.3": "Siṅgū savaṅkā saṅkulā,",
  "ja535:73.4": "satavaṅkā ca rohitā;",
  "ja535:73.5": "Āḷigaggarakākiṇṇā,",
  "ja535:73.6": "pāṭhīnā kākamacchakā.",
  "ja535:74.1": "Tattha pakkhī sanniratā,",
  "ja535:74.2": "khemino bahubhojanā;",
  "ja535:74.3": "Haṃsā koñcā mayūrā ca,",
  "ja535:74.4": "cakkavākā ca kukkuhā;",
  "ja535:74.5": "Kuṇālakā bahū citrā,",
  "ja535:74.6": "sikhaṇḍī jīvajīvakā.",
  "ja535:75.1": "Tattha pānāya māyanti,",
  "ja535:75.2": "nānā migagaṇā bahū;",
  "ja535:75.3": "Sīhā byagghā varāhā ca,",
  "ja535:75.4": "acchakokataracchayo.",
  "ja535:76.1": "Palāsādā gavajā ca,",
  "ja535:76.2": "mahiṃsā rohitā rurū;",
  "ja535:76.3": "Eṇeyyā ca varāhā ca,",
  "ja535:76.4": "gaṇino nīkasūkarā;",
  "ja535:76.5": "Kadalimigā bahukettha,",
  "ja535:76.6": "biḷārā sasakaṇṇikā.",
  "ja535:77.1": "Chamāgirī pupphavicitrasanthatā,",
  "ja535:77.2": "Dijābhighuṭṭhā dijasaṅghasevitā.",
  "ja535:78.1": "Sā suttacā nīladumābhilambitā,",
  "ja535:78.2": "Vijju mahāmegharivānupajjatha;",
  "ja535:78.3": "Tassā susambandhasiraṃ kusāmayaṃ,",
  "ja535:78.4": "Suciṃ sugandhaṃ ajinūpasevitaṃ;",
  "ja535:78.5": "Atricca kocchaṃ hirimetadabravi,",
  "ja535:78.6": "“Nisīda kalyāṇi sukhayidamāsanaṃ”.",
  "ja535:79.1": "Tassā tadā kocchagatāya kosiyo,",
  "ja535:79.2": "Yadicchamānāya jaṭājinandharo;",
  "ja535:79.3": "Navehi pattehi sayaṃ sahūdakaṃ,",
  "ja535:79.4": "Sudhābhihāsī turito mahāmuni.",
  "ja535:80.1": "Sā taṃ paṭiggayha ubhohi pāṇibhi,",
  "ja535:80.2": "Iccabravi attamanā jaṭādharaṃ;",
  "ja535:80.3": "“Handāhaṃ etarahi pūjitā tayā,",
  "ja535:80.4": "Gaccheyyaṃ brahme tidivaṃ jitāvinī”.",
  "ja535:81.1": "Sā kosiyenānumatā jutīmatā,",
  "ja535:81.2": "Udīritā vaṇṇamadena mattā;",
  "ja535:81.3": "Sakāse gantvāna sahassacakkhuno,",
  "ja535:81.4": "“Ayaṃ sudhā vāsava dehi me jayaṃ”.",
  "ja535:82.1": "Tamena sakkopi tadā apūjayi,",
  "ja535:82.2": "Sahindadevā surakaññamuttamaṃ;",
  "ja535:82.3": "Sā pañjalī devamanussapūjitā,",
  "ja535:82.4": "Navamhi kocchamhi yadā upāvisi.",
  "ja535:83.1": "Tameva saṃsī punadeva mātaliṃ,",
  "ja535:83.2": "Sahassanetto tidasānamindo;",
  "ja535:83.3": "“Gantvāna vākyaṃ mama brūhi kosiyaṃ,",
  "ja535:83.4": "‘Āsāya saddhā siriyā ca kosiya;",
  "ja535:83.5": "Hirī sudhaṃ kena malattha hetunā’”.",
  "ja535:84.1": "Taṃ suplavatthaṃ udatārayī rathaṃ,",
  "ja535:84.2": "Daddallamānaṃ upakāriyasādisaṃ;",
  "ja535:84.3": "Jambonadīsaṃ tapaneyyasannibhaṃ,",
  "ja535:84.4": "Alaṅkataṃ kañcanacittasannibhaṃ.",
  "ja535:85.1": "Suvaṇṇacandettha bahū nipātitā,",
  "ja535:85.2": "Hatthī gavāssā kikibyagghadīpiyo;",
  "ja535:85.3": "Eṇeyyakā laṅghamayettha pakkhino,",
  "ja535:85.4": "Migettha veḷuriyamayā yudhā yutā.",
  "ja535:86.1": "Tatthassarājaharayo ayojayuṃ,",
  "ja535:86.2": "Dasasatāni susunāgasādise;",
  "ja535:86.3": "Alaṅkate kañcanajāluracchade,",
  "ja535:86.4": "Āveḷine saddagame asaṅgite.",
  "ja535:87.1": "Taṃ yānaseṭṭhaṃ abhiruyha mātali,",
  "ja535:87.2": "Disā imāyo abhinādayittha;",
  "ja535:87.3": "Nabhañca selañca vanappatiniñca,",
  "ja535:87.4": "Sasāgaraṃ pabyathayittha mediniṃ.",
  "ja535:88.1": "Sa khippameva upagamma assamaṃ,",
  "ja535:88.2": "Pāvāramekaṃsakato katañjalī;",
  "ja535:88.3": "Bahussutaṃ vuddhaṃ vinītavantaṃ,",
  "ja535:88.4": "Iccabravī mātali devabrāhmaṇaṃ.",
  "ja535:89.1": "“Indassa vākyaṃ nisāmehi kosiya,",
  "ja535:89.2": "Dūto ahaṃ pucchati taṃ purindado;",
  "ja535:89.3": "‘Āsāya saddhā siriyā ca kosiya,",
  "ja535:89.4": "Hirī sudhaṃ kena malattha hetunā’”.",
  "ja535:90.1": "“Andhā sirī maṃ paṭibhāti mātali,",
  "ja535:90.2": "Saddhā aniccā pana devasārathi;",
  "ja535:90.3": "Āsā visaṃvādikasammatā hi me,",
  "ja535:90.4": "Hirī ca ariyamhi guṇe patiṭṭhitā”.",
  "ja535:91.1": "“Kumāriyo yācimā gottarakkhitā,",
  "ja535:91.2": "Jiṇṇā ca yā yā ca sabhattuitthiyo;",
  "ja535:91.3": "Tā chandarāgaṃ purisesu uggataṃ,",
  "ja535:91.4": "Hiriyā nivārenti sacittamattano.",
  "ja535:92.1": "Saṅgāmasīse sarasattisaṃyute,",
  "ja535:92.2": "Parājitānaṃ patataṃ palāyinaṃ;",
  "ja535:92.3": "Hiriyā nivattanti jahitva jīvitaṃ,",
  "ja535:92.4": "Te sampaṭicchanti punā hirīmanā.",
  "ja535:93.1": "Velā yathā sāgaravegavārinī,",
  "ja535:93.2": "Hirāya hi pāpajanaṃ nivārinī;",
  "ja535:93.3": "Taṃ sabbaloke hirimariyapūjitaṃ,",
  "ja535:93.4": "Indassa taṃ vedaya devasārathi”.",
  "ja535:94.1": "“Ko te imaṃ kosiya diṭṭhimodahi,",
  "ja535:94.2": "Brahmā mahindo atha vā pajāpati;",
  "ja535:94.3": "Hirāya devesu hi seṭṭhasammatā,",
  "ja535:94.4": "Dhītā mahindassa mahesi jāyatha”.",
  "ja535:95.1": "“Handehi dāni tidivaṃ apakkama,",
  "ja535:95.2": "Rathaṃ samāruyha mamāyitaṃ imaṃ;",
  "ja535:95.3": "Indo ca taṃ indasagotta kaṅkhati,",
  "ja535:95.4": "Ajjeva tvaṃ indasahabyataṃ vaja”.",
  "ja535:96.1": "“Evaṃ visujjhanti apāpakammino,",
  "ja535:96.2": "Atho suciṇṇassa phalaṃ na nassati;",
  "ja535:96.3": "Ye keci maddakkhu sudhāya bhojanaṃ,",
  "ja535:96.4": "Sabbeva te indasahabyataṃ gatā.",
  "ja535:97.1": "Hirī uppalavaṇṇāsi,",
  "ja535:97.2": "kosiyo dānapati bhikkhu;",
  "ja535:97.3": "Anuruddho pañcasikho,",
  "ja535:97.4": "ānando āsi mātali.",
  "ja535:98.1": "Sūriyo kassapo bhikkhu,",
  "ja535:98.2": "moggallānosi candimā;",
  "ja535:98.3": "Nārado sāriputtosi,",
  "ja535:98.4": "sambuddho āsi vāsavo”ti.",
  "ja535:99.1": "Sudhābhojanajātakaṃ tatiyaṃ."
}