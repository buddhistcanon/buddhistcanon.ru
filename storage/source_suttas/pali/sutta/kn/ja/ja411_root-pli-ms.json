{
  "ja411:0.1": "Jātaka",
  "ja411:0.2": "Sattakanipāta",
  "ja411:0.3": "Gandhāravagga",
  "ja411:0.4": "6. Susīmajātaka",
  "ja411:1.1": "“Kāḷāni kesāni pure ahesuṃ,",
  "ja411:1.2": "Jātāni sīsamhi yathāpadese;",
  "ja411:1.3": "Tānajja setāni susīma disvā,",
  "ja411:1.4": "Dhammaṃ cara brahmacariyassa kālo”.",
  "ja411:2.1": "“Mameva deva palitaṃ na tuyhaṃ,",
  "ja411:2.2": "Mameva sīsaṃ mama uttamaṅgaṃ;",
  "ja411:2.3": "‘Atthaṃ karissan’ti musā abhāṇiṃ,",
  "ja411:2.4": "Ekāparādhaṃ khama rājaseṭṭha.",
  "ja411:3.1": "Daharo tuvaṃ dassaniyosi rāja,",
  "ja411:3.2": "Paṭhamuggato hosi yathā kaḷīro;",
  "ja411:3.3": "Rajjañca kārehi mamañca passa,",
  "ja411:3.4": "Mā kālikaṃ anudhāvī janinda”.",
  "ja411:4.1": "“Passāmi vohaṃ dahariṃ kumāriṃ,",
  "ja411:4.2": "Sāmaṭṭhapassaṃ sutanuṃ sumajjhaṃ;",
  "ja411:4.3": "Kāḷappavāḷāva pavellamānā,",
  "ja411:4.4": "Palobhayantīva naresu gacchati.",
  "ja411:5.1": "Tamena passāmiparena nāriṃ,",
  "ja411:5.2": "Āsītikaṃ nāvutikaṃ va jaccā;",
  "ja411:5.3": "Daṇḍaṃ gahetvāna pavedhamānaṃ,",
  "ja411:5.4": "Gopānasībhoggasamaṃ carantiṃ.",
  "ja411:6.1": "Sohaṃ tamevānuvicintayanto,",
  "ja411:6.2": "Eko sayāmi sayanassa majjhe;",
  "ja411:6.3": "‘Ahampi evaṃ’ iti pekkhamāno,",
  "ja411:6.4": "Na gahe rame brahmacariyassa kālo.",
  "ja411:7.1": "Rajjuvālambanī cesā,",
  "ja411:7.2": "Yā gehe vasato rati;",
  "ja411:7.3": "Evampi chetvāna vajanti dhīrā,",
  "ja411:7.4": "Anapekkhino kāmasukhaṃ pahāyā”ti.",
  "ja411:8.1": "Susīmajātakaṃ chaṭṭhaṃ."
}