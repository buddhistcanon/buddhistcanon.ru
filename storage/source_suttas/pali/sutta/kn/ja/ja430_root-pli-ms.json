{
  "ja430:0.1": "Jātaka",
  "ja430:0.2": "Navakanipāta",
  "ja430:0.3": "Gijjhavagga",
  "ja430:0.4": "4. Cūḷasuvajātaka",
  "ja430:1.1": "“Santi rukkhā haripattā,",
  "ja430:1.2": "dumā nekaphalā bahū;",
  "ja430:1.3": "Kasmā nu sukkhe koḷāpe,",
  "ja430:1.4": "suvassa nirato mano”.",
  "ja430:2.1": "“Phalassa upabhuñjimhā,",
  "ja430:2.2": "nekavassagaṇe bahū;",
  "ja430:2.3": "Aphalampi viditvāna,",
  "ja430:2.4": "sāva metti yathā pure”.",
  "ja430:3.1": "“Sukkhañca rukkhaṃ koḷāpaṃ,",
  "ja430:3.2": "opattamaphalaṃ dumaṃ;",
  "ja430:3.3": "Ohāya sakuṇā yanti,",
  "ja430:3.4": "kiṃ dosaṃ passase dija”.",
  "ja430:4.1": "“Ye phalatthā sambhajanti,",
  "ja430:4.2": "aphaloti jahanti naṃ;",
  "ja430:4.3": "Attatthapaññā dummedhā,",
  "ja430:4.4": "te honti pakkhapātino”.",
  "ja430:5.1": "“Sādhu sakkhi kataṃ hoti,",
  "ja430:5.2": "metti saṃsati santhavo;",
  "ja430:5.3": "Sacetaṃ dhammaṃ rocesi,",
  "ja430:5.4": "pāsaṃsosi vijānataṃ.",
  "ja430:6.1": "So te suva varaṃ dammi,",
  "ja430:6.2": "pattayāna vihaṅgama;",
  "ja430:6.3": "Varaṃ varassu vakkaṅga,",
  "ja430:6.4": "yaṃ kiñci manasicchasi”.",
  "ja430:7.1": "“Api nāma naṃ passeyyaṃ,",
  "ja430:7.2": "sapattaṃ saphalaṃ dumaṃ;",
  "ja430:7.3": "Daliddova nidhiṃ laddhā,",
  "ja430:7.4": "nandeyyāhaṃ punappunaṃ”.",
  "ja430:8.1": "“Tato amatamādāya,",
  "ja430:8.2": "abhisiñci mahīruhaṃ;",
  "ja430:8.3": "Tassa sākhā virūhiṃsu,",
  "ja430:8.4": "sītacchāyā manoramā”.",
  "ja430:9.1": "“Evaṃ sakka sukhī hohi,",
  "ja430:9.2": "saha sabbehi ñātibhi;",
  "ja430:9.3": "Yathāhamajja sukhito,",
  "ja430:9.4": "disvāna saphalaṃ dumaṃ.",
  "ja430:10.1": "Suvassa ca varaṃ datvā,",
  "ja430:10.2": "katvāna saphalaṃ dumaṃ;",
  "ja430:10.3": "Pakkāmi saha bhariyāya,",
  "ja430:10.4": "devānaṃ nandanaṃ vanan”ti.",
  "ja430:11.1": "Cūḷasuvajātakaṃ catutthaṃ."
}