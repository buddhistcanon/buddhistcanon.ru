{
  "ja161:0.1": "Jātaka",
  "ja161:0.2": "Dukanipāta",
  "ja161:0.3": "Santhavavagga",
  "ja161:0.4": "1. Indasamānagottajātaka",
  "ja161:1.1": "“Na santhavaṃ kāpurisena kayirā,",
  "ja161:1.2": "Ariyo anariyena pajānamatthaṃ;",
  "ja161:1.3": "Cirānuvutthopi karoti pāpaṃ,",
  "ja161:1.4": "Gajo yathā indasamānagottaṃ.",
  "ja161:2.1": "Yaṃ tveva jaññā sadiso mamanti,",
  "ja161:2.2": "Sīlena paññāya sutena cāpi;",
  "ja161:2.3": "Teneva mettiṃ kayirātha saddhiṃ,",
  "ja161:2.4": "Sukho have sappurisena saṅgamo”ti.",
  "ja161:3.1": "Indasamānagottajātakaṃ paṭhamaṃ."
}