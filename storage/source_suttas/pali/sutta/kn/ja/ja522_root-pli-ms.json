{
  "ja522:0.1": "Jātaka",
  "ja522:0.2": "Cattālīsanipāta",
  "ja522:0.3": "Tesakuṇavagga",
  "ja522:0.4": "2. Sarabhaṅgajātaka",
  "ja522:1.1": "“Alaṅkatā kuṇḍalino suvatthā,",
  "ja522:1.2": "Veḷuriyamuttātharukhaggabandhā;",
  "ja522:1.3": "Rathesabhā tiṭṭhatha ke nu tumhe,",
  "ja522:1.4": "Kathaṃ vo jānanti manussaloke”.",
  "ja522:2.1": "“Ahamaṭṭhako bhīmaratho panāyaṃ,",
  "ja522:2.2": "Kāliṅgarājā pana uggatoyaṃ;",
  "ja522:2.3": "Susaññatānaṃ isīnaṃ dassanāya,",
  "ja522:2.4": "Idhāgatā pucchitāyemha pañhe”.",
  "ja522:3.1": "“Vehāyasaṃ tiṭṭhasi antalikkhe,",
  "ja522:3.2": "Pathaddhuno pannaraseva cando;",
  "ja522:3.3": "Pucchāmi taṃ yakkha mahānubhāva,",
  "ja522:3.4": "Kathaṃ taṃ jānanti manussaloke”.",
  "ja522:4.1": "“Yamāhu devesu sujampatīti,",
  "ja522:4.2": "Maghavāti taṃ āhu manussaloke;",
  "ja522:4.3": "Sa devarājā idamajja patto,",
  "ja522:4.4": "Susaññatānaṃ isīnaṃ dassanāya”.",
  "ja522:5.1": "“Dūre sutā no isayo samāgatā,",
  "ja522:5.2": "Mahiddhikā iddhiguṇūpapannā;",
  "ja522:5.3": "Vandāmi te ayire pasannacitto,",
  "ja522:5.4": "Ye jīvalokettha manussaseṭṭhā”.",
  "ja522:6.1": "“Gandho isīnaṃ ciradikkhitānaṃ,",
  "ja522:6.2": "Kāyā cuto gacchati mālutena;",
  "ja522:6.3": "Ito paṭikkamma sahassanetta,",
  "ja522:6.4": "Gandho isīnaṃ asuci devarāja”.",
  "ja522:7.1": "“Gandho isīnaṃ ciradikkhitānaṃ,",
  "ja522:7.2": "Kāyā cuto gacchatu mālutena;",
  "ja522:7.3": "Vicitrapupphaṃ surabhiṃva mālaṃ,",
  "ja522:7.4": "Gandhañca etaṃ pāṭikaṅkhāma bhante;",
  "ja522:7.5": "Na hettha devā paṭikkūlasaññino”.",
  "ja522:8.1": "“Purindado bhūtapatī yasassī,",
  "ja522:8.2": "Devānamindo sakko maghavā sujampati;",
  "ja522:8.3": "Sa devarājā asuragaṇappamaddano,",
  "ja522:8.4": "Okāsamākaṅkhati pañha pucchituṃ.",
  "ja522:9.1": "Ko nevimesaṃ idha paṇḍitānaṃ,",
  "ja522:9.2": "Pañhe puṭṭho nipuṇe byākarissati;",
  "ja522:9.3": "Tiṇṇañca raññaṃ manujādhipānaṃ,",
  "ja522:9.4": "Devānamindassa ca vāsavassa”.",
  "ja522:10.1": "“Ayaṃ isi sarabhaṅgo tapassī,",
  "ja522:10.2": "Yato jāto virato methunasmā;",
  "ja522:10.3": "Āceraputto suvinītarūpo,",
  "ja522:10.4": "So nesaṃ pañhāni viyākarissati”.",
  "ja522:11.1": "“Koṇḍañña pañhāni viyākarohi,",
  "ja522:11.2": "Yācanti taṃ isayo sādhurūpā;",
  "ja522:11.3": "Koṇḍañña eso manujesu dhammo,",
  "ja522:11.4": "Yaṃ vuddhamāgacchati esa bhāro”.",
  "ja522:12.1": "“Katāvakāsā pucchantu bhonto,",
  "ja522:12.2": "Yaṃ kiñci pañhaṃ manasābhipatthitaṃ;",
  "ja522:12.3": "Ahañhi taṃ taṃ vo viyākarissaṃ,",
  "ja522:12.4": "Ñatvā sayaṃ lokamimaṃ parañca”.",
  "ja522:13.1": "Tato ca maghavā sakko,",
  "ja522:13.2": "atthadassī purindado;",
  "ja522:13.3": "Apucchi paṭhamaṃ pañhaṃ,",
  "ja522:13.4": "yañcāsi abhipatthitaṃ.",
  "ja522:14.1": "“Kiṃ sū vadhitvā na kadāci socati,",
  "ja522:14.2": "Kissappahānaṃ isayo vaṇṇayanti;",
  "ja522:14.3": "Kassīdha vuttaṃ pharusaṃ khametha,",
  "ja522:14.4": "Akkhāhi me koṇḍañña etamatthaṃ”.",
  "ja522:15.1": "“Kodhaṃ vadhitvā na kadāci socati,",
  "ja522:15.2": "Makkhappahānaṃ isayo vaṇṇayanti;",
  "ja522:15.3": "Sabbesaṃ vuttaṃ pharusaṃ khametha,",
  "ja522:15.4": "Etaṃ khantiṃ uttamamāhu santo”.",
  "ja522:16.1": "“Sakkā ubhinnaṃ vacanaṃ titikkhituṃ,",
  "ja522:16.2": "Sadisassa vā seṭṭhatarassa vāpi;",
  "ja522:16.3": "Kathaṃ nu hīnassa vaco khametha,",
  "ja522:16.4": "Akkhāhi me koṇḍañña etamatthaṃ”.",
  "ja522:17.1": "“Bhayā hi seṭṭhassa vaco khametha,",
  "ja522:17.2": "Sārambhahetū pana sādisassa;",
  "ja522:17.3": "Yo cīdha hīnassa vaco khametha,",
  "ja522:17.4": "Etaṃ khantiṃ uttamamāhu santo”.",
  "ja522:18.1": "“Kathaṃ vijaññā catupattharūpaṃ,",
  "ja522:18.2": "Seṭṭhaṃ sarikkhaṃ athavāpi hīnaṃ;",
  "ja522:18.3": "Virūparūpena caranti santo,",
  "ja522:18.4": "Tasmā hi sabbesaṃ vaco khametha”.",
  "ja522:19.1": "“Na hetamatthaṃ mahatīpi senā,",
  "ja522:19.2": "Sarājikā yujjhamānā labhetha;",
  "ja522:19.3": "Yaṃ khantimā sappuriso labhetha,",
  "ja522:19.4": "Khantī balassūpasamanti verā”.",
  "ja522:20.1": "“Subhāsitaṃ te anumodiyāna,",
  "ja522:20.2": "Aññaṃ taṃ pucchāmi tadiṅgha brūhi;",
  "ja522:20.3": "Yathā ahuṃ daṇḍakī nāḷikero,",
  "ja522:20.4": "Athajjuno kalābu cāpi rājā;",
  "ja522:20.5": "Tesaṃ gatiṃ brūhi supāpakamminaṃ,",
  "ja522:20.6": "Katthūpapannā isinaṃ viheṭhakā”.",
  "ja522:21.1": "“Kisañhi vacchaṃ avakiriya daṇḍakī,",
  "ja522:21.2": "Ucchinnamūlo sajano saraṭṭho;",
  "ja522:21.3": "Kukkuḷanāme nirayamhi paccati,",
  "ja522:21.4": "Tassa phuliṅgāni patanti kāye.",
  "ja522:22.1": "Yo saññate pabbajite aheṭhayi,",
  "ja522:22.2": "Dhammaṃ bhaṇante samaṇe adūsake;",
  "ja522:22.3": "Taṃ nāḷikeraṃ sunakhā parattha,",
  "ja522:22.4": "Saṅgamma khādanti viphandamānaṃ.",
  "ja522:23.1": "Athajjuno niraye sattisūle,",
  "ja522:23.2": "Avaṃsiro patito uddhampādo;",
  "ja522:23.3": "Aṅgīrasaṃ gotamaṃ heṭhayitvā,",
  "ja522:23.4": "Khantiṃ tapassiṃ cirabrahmacāriṃ.",
  "ja522:24.1": "Yo khaṇḍaso pabbajitaṃ achedayi,",
  "ja522:24.2": "Khantiṃ vadantaṃ samaṇaṃ adūsakaṃ;",
  "ja522:24.3": "Kalābuvīciṃ upapajja paccati,",
  "ja522:24.4": "Mahāpatāpaṃ kaṭukaṃ bhayānakaṃ.",
  "ja522:25.1": "Etāni sutvā nirayāni paṇḍito,",
  "ja522:25.2": "Aññāni pāpiṭṭhatarāni cettha;",
  "ja522:25.3": "Dhammaṃ care samaṇabrāhmaṇesu,",
  "ja522:25.4": "Evaṃkaro saggamupeti ṭhānaṃ”.",
  "ja522:26.1": "“Subhāsitaṃ te anumodiyāna,",
  "ja522:26.2": "Aññaṃ taṃ pucchāmi tadiṅgha brūhi;",
  "ja522:26.3": "Kathaṃvidhaṃ sīlavantaṃ vadanti,",
  "ja522:26.4": "Kathaṃvidhaṃ paññavantaṃ vadanti;",
  "ja522:26.5": "Kathaṃvidhaṃ sappurisaṃ vadanti,",
  "ja522:26.6": "Kathaṃvidhaṃ no siri no jahāti”.",
  "ja522:27.1": "“Kāyena vācāya ca yodha saññato,",
  "ja522:27.2": "Manasā ca kiñci na karoti pāpaṃ;",
  "ja522:27.3": "Na attahetū alikaṃ bhaṇeti,",
  "ja522:27.4": "Tathāvidhaṃ sīlavantaṃ vadanti.",
  "ja522:28.1": "Gambhīrapañhaṃ manasābhicintayaṃ,",
  "ja522:28.2": "Nāccāhitaṃ kamma karoti luddaṃ;",
  "ja522:28.3": "Kālāgataṃ atthapadaṃ na riñcati,",
  "ja522:28.4": "Tathāvidhaṃ paññavantaṃ vadanti.",
  "ja522:29.1": "Yo ve kataññū katavedi dhīro,",
  "ja522:29.2": "Kalyāṇamitto daḷhabhatti ca hoti;",
  "ja522:29.3": "Dukhitassa sakkacca karoti kiccaṃ,",
  "ja522:29.4": "Tathāvidhaṃ sappurisaṃ vadanti.",
  "ja522:30.1": "Etehi sabbehi guṇehupeto,",
  "ja522:30.2": "Saddho mudū saṃvibhāgī vadaññū;",
  "ja522:30.3": "Saṅgāhakaṃ sakhilaṃ saṇhavācaṃ,",
  "ja522:30.4": "Tathāvidhaṃ no siri no jahāti”.",
  "ja522:31.1": "“Subhāsitaṃ te anumodiyāna,",
  "ja522:31.2": "Aññaṃ taṃ pucchāmi tadiṅgha brūhi;",
  "ja522:31.3": "Sīlaṃ siriñcāpi satañca dhammaṃ,",
  "ja522:31.4": "Paññañca kaṃ seṭṭhataraṃ vadanti”.",
  "ja522:32.1": "“Paññā hi seṭṭhā kusalā vadanti,",
  "ja522:32.2": "Nakkhattarājāriva tārakānaṃ;",
  "ja522:32.3": "Sīlaṃ sīrī cāpi satañca dhammo,",
  "ja522:32.4": "Anvāyikā paññavato bhavanti”.",
  "ja522:33.1": "“Subhāsitaṃ te anumodiyāna,",
  "ja522:33.2": "Aññaṃ taṃ pucchāmi tadiṅgha brūhi;",
  "ja522:33.3": "Kathaṃkaro kintikaro kimācaraṃ,",
  "ja522:33.4": "Kiṃ sevamāno labhatīdha paññaṃ;",
  "ja522:33.5": "Paññāya dāni paṭipadaṃ vadehi,",
  "ja522:33.6": "Kathaṃkaro paññavā hoti macco”.",
  "ja522:34.1": "“Sevetha vuddhe nipuṇe bahussute,",
  "ja522:34.2": "Uggāhako ca paripucchako siyā;",
  "ja522:34.3": "Suṇeyya sakkacca subhāsitāni,",
  "ja522:34.4": "Evaṃkaro paññavā hoti macco.",
  "ja522:35.1": "Sa paññavā kāmaguṇe avekkhati,",
  "ja522:35.2": "Aniccato dukkhato rogato ca;",
  "ja522:35.3": "Evaṃ vipassī pajahāti chandaṃ,",
  "ja522:35.4": "Dukkhesu kāmesu mahabbhayesu.",
  "ja522:36.1": "Sa vītarāgo pavineyya dosaṃ,",
  "ja522:36.2": "Mettaṃ cittaṃ bhāvaye appamāṇaṃ;",
  "ja522:36.3": "Sabbesu bhūtesu nidhāya daṇḍaṃ,",
  "ja522:36.4": "Anindito brahmamupeti ṭhānaṃ”.",
  "ja522:37.1": "“Mahatthiyaṃ āgamanaṃ ahosi,",
  "ja522:37.2": "Tavamaṭṭhakā bhīmarathassa cāpi;",
  "ja522:37.3": "Kāliṅgarājassa ca uggatassa,",
  "ja522:37.4": "Sabbesa vo kāmarāgo pahīno”.",
  "ja522:38.1": "“Evametaṃ paracittavedi,",
  "ja522:38.2": "Sabbesa no kāmarāgo pahīno;",
  "ja522:38.3": "Karohi okāsamanuggahāya,",
  "ja522:38.4": "Yathā gatiṃ te abhisambhavema”.",
  "ja522:39.1": "“Karomi okāsamanuggahāya,",
  "ja522:39.2": "Tathā hi vo kāmarāgo pahīno;",
  "ja522:39.3": "Pharātha kāyaṃ vipulāya pītiyā,",
  "ja522:39.4": "Yathā gatiṃ me abhisambhavetha”.",
  "ja522:40.1": "“Sabbaṃ karissāma tavānusāsaniṃ,",
  "ja522:40.2": "Yaṃ yaṃ tuvaṃ vakkhasi bhūripañña;",
  "ja522:40.3": "Pharāma kāyaṃ vipulāya pītiyā,",
  "ja522:40.4": "Yathā gatiṃ te abhisambhavema”.",
  "ja522:41.1": "“Katāya vacchassa kisassa pūjā,",
  "ja522:41.2": "Gacchantu bhonto isayo sādhurūpā;",
  "ja522:41.3": "Jhāne ratā hotha sadā samāhitā,",
  "ja522:41.4": "Esā ratī pabbajitassa seṭṭhā”.",
  "ja522:42.1": "Sutvāna gāthā paramatthasaṃhitā,",
  "ja522:42.2": "Subhāsitā isinā paṇḍitena;",
  "ja522:42.3": "Te vedajātā anumodamānā,",
  "ja522:42.4": "Pakkāmu devā devapuraṃ yasassino.",
  "ja522:43.1": "Gāthā imā atthavatī subyañjanā,",
  "ja522:43.2": "Subhāsitā isinā paṇḍitena;",
  "ja522:43.3": "Yo kocimā aṭṭhikatvā suṇeyya,",
  "ja522:43.4": "Labhetha pubbāpariyaṃ visesaṃ;",
  "ja522:43.5": "Laddhāna pubbāpariyaṃ visesaṃ,",
  "ja522:43.6": "Adassanaṃ maccurājassa gacche.",
  "ja522:44.1": "“Sālissaro sāriputto,",
  "ja522:44.2": "meṇḍissaro ca kassapo;",
  "ja522:44.3": "Pabbato anuruddho ca,",
  "ja522:44.4": "kaccāyano ca devalo.",
  "ja522:45.1": "Anusisso ca ānando,",
  "ja522:45.2": "kisavaccho ca kolito;",
  "ja522:45.3": "Nārado udāyitthero,",
  "ja522:45.4": "parisā buddhaparisā;",
  "ja522:45.5": "Sarabhaṅgo lokanātho,",
  "ja522:45.6": "evaṃ dhāretha jātakan”ti.",
  "ja522:46.1": "Sarabhaṅgajātakaṃ dutiyaṃ."
}