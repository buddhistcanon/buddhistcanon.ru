{
  "ja327:0.1": "Jātaka",
  "ja327:0.2": "Catukkanipāta",
  "ja327:0.3": "Kuṭidūsakavagga",
  "ja327:0.4": "7. Kākavatījātaka",
  "ja327:1.1": "“Vāti cāyaṃ tato gandho,",
  "ja327:1.2": "yattha me vasatī piyā;",
  "ja327:1.3": "Dūre ito hi kākavatī,",
  "ja327:1.4": "yattha me nirato mano”.",
  "ja327:2.1": "“Kathaṃ samuddamatarī,",
  "ja327:2.2": "kathaṃ atari kepukaṃ;",
  "ja327:2.3": "Kathaṃ satta samuddāni,",
  "ja327:2.4": "kathaṃ simbalimāruhi”.",
  "ja327:3.1": "“Tayā samuddamatariṃ,",
  "ja327:3.2": "tayā atari kepukaṃ;",
  "ja327:3.3": "Tayā satta samuddāni,",
  "ja327:3.4": "tayā simbalimāruhiṃ”.",
  "ja327:4.1": "“Dhiratthumaṃ mahākāyaṃ,",
  "ja327:4.2": "dhiratthumaṃ acetanaṃ;",
  "ja327:4.3": "Yattha jāyāyahaṃ jāraṃ,",
  "ja327:4.4": "āvahāmi vahāmi cā”ti.",
  "ja327:5.1": "Kākavatījātakaṃ sattamaṃ."
}