{
  "ja450:0.1": "Jātaka",
  "ja450:0.2": "Dasakanipāta",
  "ja450:0.3": "Catudvāravagga",
  "ja450:0.4": "12. Bilārakosiyajātaka",
  "ja450:1.1": "“Apacantāpi dicchanti,",
  "ja450:1.2": "santo laddhāna bhojanaṃ;",
  "ja450:1.3": "Kimeva tvaṃ pacamāno,",
  "ja450:1.4": "yaṃ na dajjā na taṃ samaṃ.",
  "ja450:2.1": "Maccherā ca pamādā ca,",
  "ja450:2.2": "evaṃ dānaṃ na diyyati;",
  "ja450:2.3": "Puññaṃ ākaṅkhamānena,",
  "ja450:2.4": "deyyaṃ hoti vijānatā.",
  "ja450:3.1": "Yasseva bhīto na dadāti maccharī,",
  "ja450:3.2": "Tadevādadato bhayaṃ;",
  "ja450:3.3": "Jighacchā ca pipāsā ca,",
  "ja450:3.4": "Yassa bhāyati maccharī;",
  "ja450:3.5": "Tameva bālaṃ phusati,",
  "ja450:3.6": "Asmiṃ loke paramhi ca.",
  "ja450:4.1": "Tasmā vineyya maccheraṃ,",
  "ja450:4.2": "dajjā dānaṃ malābhibhū;",
  "ja450:4.3": "Puññāni paralokasmiṃ,",
  "ja450:4.4": "patiṭṭhā honti pāṇinaṃ”.",
  "ja450:5.1": "“Duddadaṃ dadamānānaṃ,",
  "ja450:5.2": "dukkaraṃ kamma kubbataṃ;",
  "ja450:5.3": "Asanto nānukubbanti,",
  "ja450:5.4": "sataṃ dhammo durannayo.",
  "ja450:6.1": "Tasmā satañca asataṃ,",
  "ja450:6.2": "nānā hoti ito gati;",
  "ja450:6.3": "Asanto nirayaṃ yanti,",
  "ja450:6.4": "santo saggaparāyaṇā”.",
  "ja450:7.1": "“Appasmeke pavecchanti,",
  "ja450:7.2": "bahuneke na dicchare;",
  "ja450:7.3": "Appasmā dakkhiṇā dinnā,",
  "ja450:7.4": "sahassena samaṃ mitā”.",
  "ja450:8.1": "“Dhammaṃ care yopi samuñchakaṃ care,",
  "ja450:8.2": "Dārañca posaṃ dadamappakasmiṃ;",
  "ja450:8.3": "Sataṃ sahassānaṃ sahassayāginaṃ,",
  "ja450:8.4": "Kalampi nāgghanti tathāvidhassa te”.",
  "ja450:9.1": "“Kenesa yañño vipulo mahagghato,",
  "ja450:9.2": "Samena dinnassa na agghameti;",
  "ja450:9.3": "Kathaṃ sataṃ sahassānaṃ sahassayāginaṃ,",
  "ja450:9.4": "Kalampi nāgghanti tathāvidhassa te”.",
  "ja450:10.1": "“Dadanti heke visame niviṭṭhā,",
  "ja450:10.2": "Chetvā vadhitvā atha socayitvā;",
  "ja450:10.3": "Sā dakkhiṇā assumukhā sadaṇḍā,",
  "ja450:10.4": "Samena dinnassa na agghameti;",
  "ja450:10.5": "Evaṃ sataṃ sahassānaṃ sahassayāginaṃ,",
  "ja450:10.6": "Kalampi nāgghanti tathāvidhassa te”ti.",
  "ja450:11.1": "Bilārakosiyajātakaṃ dvādasamaṃ."
}