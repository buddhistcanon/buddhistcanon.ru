{
  "ja479:0.1": "Jātaka",
  "ja479:0.2": "Terasakanipāta",
  "ja479:0.3": "Ambavagga",
  "ja479:0.4": "6. Kāliṅgabodhijātaka",
  "ja479:1.1": "Rājā kāliṅgo cakkavatti,",
  "ja479:1.2": "Dhammena pathavimanusāsaṃ;",
  "ja479:1.3": "Agamā bodhisamīpaṃ,",
  "ja479:1.4": "Nāgena mahānubhāvena.",
  "ja479:2.1": "Kāliṅgo bhāradvājo ca,",
  "ja479:2.2": "Rājānaṃ kāliṅgaṃ samaṇakolaññaṃ;",
  "ja479:2.3": "Cakkaṃ vattayato pariggahetvā,",
  "ja479:2.4": "Pañjalī idamavoca.",
  "ja479:3.1": "“Paccoroha mahārāja,",
  "ja479:3.2": "Bhūmibhāgo yathā samaṇuggato;",
  "ja479:3.3": "Idha anadhivarā buddhā,",
  "ja479:3.4": "Abhisambuddhā virocanti.",
  "ja479:4.1": "Padakkhiṇato āvaṭṭā,",
  "ja479:4.2": "Tiṇalatā asmiṃ bhūmibhāgasmiṃ;",
  "ja479:4.3": "Pathaviyā nābhiyaṃ maṇḍo,",
  "ja479:4.4": "Iti no sutaṃ mante mahārāja.",
  "ja479:5.1": "Sāgarapariyantāya,",
  "ja479:5.2": "Mediniyā sabbabhūtadharaṇiyā;",
  "ja479:5.3": "Pathaviyā ayaṃ maṇḍo,",
  "ja479:5.4": "Orohitvā namo karohi.",
  "ja479:6.1": "Ye te bhavanti nāgā ca,",
  "ja479:6.2": "abhijātā ca kuñjarā;",
  "ja479:6.3": "Ettāvatā padesaṃ te,",
  "ja479:6.4": "nāgā neva mupayanti.",
  "ja479:7.1": "Abhijāto nāgo kāmaṃ,",
  "ja479:7.2": "Pesehi kuñjaraṃ dantiṃ;",
  "ja479:7.3": "Ettāvatā padeso,",
  "ja479:7.4": "Sakkā nāgena mupagantuṃ.",
  "ja479:8.1": "Taṃ sutvā rājā kāliṅgo,",
  "ja479:8.2": "Veyyañjanikavaco nisāmetvā;",
  "ja479:8.3": "Sampesesi nāgaṃ ñassāma,",
  "ja479:8.4": "Mayaṃ yathimassidaṃ vacanaṃ”.",
  "ja479:9.1": "Sampesito ca raññā,",
  "ja479:9.2": "Nāgo koñcova abhinaditvāna;",
  "ja479:9.3": "Paṭisakkitvā nisīdi,",
  "ja479:9.4": "Garuṃva bhāraṃ asahamāno.",
  "ja479:10.1": "Kāliṅgabhāradvājo,",
  "ja479:10.2": "Nāgaṃ khīṇāyukaṃ viditvāna;",
  "ja479:10.3": "Rājānaṃ kāliṅgaṃ,",
  "ja479:10.4": "Taramāno ajjhabhāsittha;",
  "ja479:10.5": "“Aññaṃ saṅkama nāgaṃ,",
  "ja479:10.6": "Nāgo khīṇāyuko mahārāja”.",
  "ja479:11.1": "Taṃ sutvā kāliṅgo,",
  "ja479:11.2": "taramāno saṅkamī nāgaṃ;",
  "ja479:11.3": "Saṅkanteva raññe,",
  "ja479:11.4": "nāgo tattheva pati bhumyā;",
  "ja479:11.5": "Veyyañjanikavaco,",
  "ja479:11.6": "yathā tathā ahu nāgo.",
  "ja479:12.1": "Kāliṅgo rājā kāliṅgaṃ,",
  "ja479:12.2": "brāhmaṇaṃ etadavoca;",
  "ja479:12.3": "“Tvameva asi sambuddho,",
  "ja479:12.4": "sabbaññū sabbadassāvī”.",
  "ja479:13.1": "Taṃ anadhivāsento kāliṅgaṃ,",
  "ja479:13.2": "Brāhmaṇo idamavoca;",
  "ja479:13.3": "“Veyyañjanikā hi mayaṃ,",
  "ja479:13.4": "Buddhā sabbaññuno mahārāja.",
  "ja479:14.1": "Sabbaññū sabbavidū ca,",
  "ja479:14.2": "Buddhā na lakkhaṇena jānanti;",
  "ja479:14.3": "Āgamabalasā hi mayaṃ,",
  "ja479:14.4": "Buddhā sabbaṃ pajānanti”.",
  "ja479:15.1": "Mahayitvā sambodhiṃ,",
  "ja479:15.2": "Nānāturiyehi vajjamānehi;",
  "ja479:15.3": "Mālāvilepanaṃ abhiharitvā,",
  "ja479:15.4": "Atha rājā manupāyāsi.",
  "ja479:16.1": "Saṭṭhi vāhasahassāni,",
  "ja479:16.2": "pupphānaṃ sannipātayi;",
  "ja479:16.3": "Pūjesi rājā kāliṅgo,",
  "ja479:16.4": "bodhimaṇḍamanuttaranti.",
  "ja479:17.1": "Kāliṅgabodhijātakaṃ chaṭṭhaṃ."
}