{
  "ja405:0.1": "Jātaka",
  "ja405:0.2": "Sattakanipāta",
  "ja405:0.3": "Kukkuvagga",
  "ja405:0.4": "10. Bakajātaka",
  "ja405:1.1": "“Dvāsattati gotama puññakammā,",
  "ja405:1.2": "Vasavattino jātijaraṃ atītā;",
  "ja405:1.3": "Ayamantimā vedagū brahmapatti,",
  "ja405:1.4": "Asmābhijappanti janā anekā”.",
  "ja405:2.1": "“Appañhi etaṃ na hi dīghamāyu,",
  "ja405:2.2": "Yaṃ tvaṃ baka maññasi dīghamāyuṃ;",
  "ja405:2.3": "Sataṃ sahassāni nirabbudānaṃ,",
  "ja405:2.4": "Āyuṃ pajānāmi tavāha brahme”.",
  "ja405:3.1": "“Anantadassī bhagavāhamasmi,",
  "ja405:3.2": "Jātijjaraṃ sokamupātivatto;",
  "ja405:3.3": "Kiṃ me purāṇaṃ vatasīlavattaṃ,",
  "ja405:3.4": "Ācikkha me taṃ yamahaṃ vijaññaṃ”.",
  "ja405:4.1": "“Yaṃ tvaṃ apāyesi bahū manusse,",
  "ja405:4.2": "Pipāsite ghammani samparete;",
  "ja405:4.3": "Taṃ te purāṇaṃ vatasīlavattaṃ,",
  "ja405:4.4": "Suttappabuddhova anussarāmi.",
  "ja405:5.1": "Yaṃ eṇikūlasmi janaṃ gahītaṃ,",
  "ja405:5.2": "Amocayī gayhaka niyyamānaṃ;",
  "ja405:5.3": "Taṃ te purāṇaṃ vatasīlavattaṃ,",
  "ja405:5.4": "Suttappabuddhova anussarāmi.",
  "ja405:6.1": "Gaṅgāya sotasmiṃ gahītanāvaṃ,",
  "ja405:6.2": "Luddena nāgena manussakappā;",
  "ja405:6.3": "Amocayi tvaṃ balasā pasayha,",
  "ja405:6.4": "Taṃ te purāṇaṃ vatasīlavattaṃ;",
  "ja405:6.5": "Suttappabuddhova anussarāmi.",
  "ja405:7.1": "Kappo ca te baddhacaro ahosiṃ,",
  "ja405:7.2": "Sambuddhimantaṃ vatinaṃ amaññaṃ;",
  "ja405:7.3": "Taṃ te purāṇaṃ vatasīlavattaṃ,",
  "ja405:7.4": "Suttappabuddhova anussarāmi”.",
  "ja405:8.1": "“Addhā pajānāsi mametamāyuṃ,",
  "ja405:8.2": "Aññampi jānāsi tathā hi buddho;",
  "ja405:8.3": "Tathā hi tāyaṃ jalitānubhāvo,",
  "ja405:8.4": "Obhāsayaṃ tiṭṭhati brahmalokan”ti.",
  "ja405:9.1": "Bakajātakaṃ dasamaṃ.",
  "ja405:9.2": "Kukkuvaggo paṭhamo.",
  "ja405:10.1": "Tassuddānaṃ",
  "ja405:11.1": "Varakaṇṇika cāpavaro sutano,",
  "ja405:11.2": "Atha gijjha sarohitamacchavaro;",
  "ja405:11.3": "Puna paṇṇaka senaka yācanako,",
  "ja405:11.4": "Atha veri sabrahmabakena dasāti."
}