{
  "ja399:0.1": "Jātaka",
  "ja399:0.2": "Sattakanipāta",
  "ja399:0.3": "Kukkuvagga",
  "ja399:0.4": "4. Mātuposakagijjhajātaka",
  "ja399:1.1": "“Te kathaṃ nu karissanti,",
  "ja399:1.2": "vuddhā giridarīsayā;",
  "ja399:1.3": "Ahaṃ baddhosmi pāsena,",
  "ja399:1.4": "nilīyassa vasaṃ gato”.",
  "ja399:2.1": "“Kiṃ gijjha paridevasi,",
  "ja399:2.2": "kā nu te paridevanā;",
  "ja399:2.3": "Na me suto vā diṭṭho vā,",
  "ja399:2.4": "bhāsanto mānusiṃ dijo”.",
  "ja399:3.1": "“Bharāmi mātāpitaro,",
  "ja399:3.2": "vuddhe giridarīsaye;",
  "ja399:3.3": "Te kathaṃ nu karissanti,",
  "ja399:3.4": "ahaṃ vasaṃ gato tava”.",
  "ja399:4.1": "“Yannu gijjho yojanasataṃ,",
  "ja399:4.2": "kuṇapāni avekkhati;",
  "ja399:4.3": "Kasmā jālañca pāsañca,",
  "ja399:4.4": "āsajjāpi na bujjhasi”.",
  "ja399:5.1": "“Yadā parābhavo hoti,",
  "ja399:5.2": "poso jīvitasaṅkhaye;",
  "ja399:5.3": "Atha jālañca pāsañca,",
  "ja399:5.4": "āsajjāpi na bujjhati”.",
  "ja399:6.1": "“Bharassu mātāpitaro,",
  "ja399:6.2": "vuddhe giridarīsaye;",
  "ja399:6.3": "Mayā tvaṃ samanuññāto,",
  "ja399:6.4": "sotthiṃ passāhi ñātake”.",
  "ja399:7.1": "“Evaṃ luddaka nandassu,",
  "ja399:7.2": "saha sabbehi ñātibhi;",
  "ja399:7.3": "Bharissaṃ mātāpitaro,",
  "ja399:7.4": "vuddhe giridarīsaye”ti.",
  "ja399:8.1": "Mātuposakagijjhajātakaṃ catutthaṃ."
}