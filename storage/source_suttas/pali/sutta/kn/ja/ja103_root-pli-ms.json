{
  "ja103:0.1": "Jātaka",
  "ja103:0.2": "Ekakanipāta",
  "ja103:0.3": "Parosatavagga",
  "ja103:0.4": "3. Verijātaka",
  "ja103:1.1": "“Yattha verī nivisati,",
  "ja103:1.2": "na vase tattha paṇḍito;",
  "ja103:1.3": "Ekarattaṃ dirattaṃ vā,",
  "ja103:1.4": "dukkhaṃ vasati verisū”ti.",
  "ja103:2.1": "Verijātakaṃ tatiyaṃ."
}