{
  "ja329:0.1": "Jātaka",
  "ja329:0.2": "Catukkanipāta",
  "ja329:0.3": "Kuṭidūsakavagga",
  "ja329:0.4": "9. Kāḷabāhujātaka",
  "ja329:1.1": "“Yaṃ annapānassa pure labhāma,",
  "ja329:1.2": "Taṃ dāni sākhamigameva gacchati;",
  "ja329:1.3": "Gacchāma dāni vanameva rādha,",
  "ja329:1.4": "Asakkatā casma dhanañjayāya”.",
  "ja329:2.1": "“Lābho alābho yaso ayaso ca,",
  "ja329:2.2": "Nindā pasaṃsā ca sukhañca dukkhaṃ;",
  "ja329:2.3": "Ete aniccā manujesu dhammā,",
  "ja329:2.4": "Mā soci kiṃ socasi poṭṭhapāda”.",
  "ja329:3.1": "“Addhā tuvaṃ paṇḍitakosi rādha,",
  "ja329:3.2": "Jānāsi atthāni anāgatāni;",
  "ja329:3.3": "Kathaṃ nu sākhāmigaṃ dakkhisāma,",
  "ja329:3.4": "Niddhāvitaṃ rājakulatova jammaṃ”.",
  "ja329:4.1": "“Cāleti kaṇṇaṃ bhakuṭiṃ karoti,",
  "ja329:4.2": "Muhuṃ muhuṃ bhāyayate kumāre;",
  "ja329:4.3": "Sayameva taṃ kāhati kāḷabāhu,",
  "ja329:4.4": "Yenārakā ṭhassati annapānā”ti.",
  "ja329:5.1": "Kāḷabāhujātakaṃ navamaṃ."
}