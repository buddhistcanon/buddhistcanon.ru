{
  "ja439:0.1": "Jātaka",
  "ja439:0.2": "Dasakanipāta",
  "ja439:0.3": "Catudvāravagga",
  "ja439:0.4": "1. Catudvārajātaka",
  "ja439:1.1": "“Catudvāramidaṃ nagaraṃ,",
  "ja439:1.2": "āyasaṃ daḷhapākāraṃ;",
  "ja439:1.3": "Oruddhapaṭiruddhosmi,",
  "ja439:1.4": "kiṃ pāpaṃ pakataṃ mayā”.",
  "ja439:2.1": "“Sabbe apihitā dvārā,",
  "ja439:2.2": "oruddhosmi yathā dijo;",
  "ja439:2.3": "Kimādhikaraṇaṃ yakkha,",
  "ja439:2.4": "cakkābhinihato ahaṃ.",
  "ja439:3.1": "Laddhā satasahassāni,",
  "ja439:3.2": "atirekāni vīsati;",
  "ja439:3.3": "Anukampakānaṃ ñātīnaṃ,",
  "ja439:3.4": "vacanaṃ samma nākari.",
  "ja439:4.1": "Laṅghiṃ samuddaṃ pakkhandi,",
  "ja439:4.2": "sāgaraṃ appasiddhikaṃ;",
  "ja439:4.3": "Catubbhi aṭṭhajjhagamā,",
  "ja439:4.4": "aṭṭhāhipi ca soḷasa.",
  "ja439:5.1": "Soḷasāhi ca bāttiṃsa,",
  "ja439:5.2": "atricchaṃ cakkamāsado;",
  "ja439:5.3": "Icchāhatassa posassa,",
  "ja439:5.4": "cakkaṃ bhamati matthake.",
  "ja439:6.1": "Uparivisālā duppūrā,",
  "ja439:6.2": "icchā visaṭagāminī;",
  "ja439:6.3": "Ye ca taṃ anugijjhanti,",
  "ja439:6.4": "te honti cakkadhārino.",
  "ja439:7.1": "Bahubhaṇḍaṃ avahāya,",
  "ja439:7.2": "maggaṃ appaṭivekkhiya;",
  "ja439:7.3": "Yesañcetaṃ asaṅkhātaṃ,",
  "ja439:7.4": "te honti cakkadhārino.",
  "ja439:8.1": "Kammaṃ samekkhe vipulañca bhogaṃ,",
  "ja439:8.2": "Icchaṃ na seveyya anatthasaṃhitaṃ;",
  "ja439:8.3": "Kareyya vākyaṃ anukampakānaṃ,",
  "ja439:8.4": "Taṃ tādisaṃ nātivatteyya cakkaṃ”.",
  "ja439:9.1": "“Kīvaciraṃ nu me yakkha,",
  "ja439:9.2": "cakkaṃ sirasi ṭhassati;",
  "ja439:9.3": "Kati vassasahassāni,",
  "ja439:9.4": "taṃ me akkhāhi pucchito”.",
  "ja439:10.1": "“Atisaro paccasaro,",
  "ja439:10.2": "mittavinda suṇohi me;",
  "ja439:10.3": "Cakkaṃ te sirasi māviddhaṃ,",
  "ja439:10.4": "na taṃ jīvaṃ pamokkhasī”ti.",
  "ja439:11.1": "Catudvārajātakaṃ paṭhamaṃ."
}