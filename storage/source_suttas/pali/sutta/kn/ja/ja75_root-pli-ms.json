{
  "ja75:0.1": "Jātaka",
  "ja75:0.2": "Ekakanipāta",
  "ja75:0.3": "Varuṇavagga",
  "ja75:0.4": "5. Macchajātaka",
  "ja75:1.1": "“Abhitthanaya pajjunna,",
  "ja75:1.2": "nidhiṃ kākassa nāsaya;",
  "ja75:1.3": "Kākaṃ sokāya randhehi,",
  "ja75:1.4": "mañca sokā pamocayā”ti.",
  "ja75:2.1": "Macchajātakaṃ pañcamaṃ."
}