{
  "ja73:0.1": "Jātaka",
  "ja73:0.2": "Ekakanipāta",
  "ja73:0.3": "Varuṇavagga",
  "ja73:0.4": "3. Saccaṃkirajātaka",
  "ja73:1.1": "“Saccaṃ kirevamāhaṃsu,",
  "ja73:1.2": "narā ekacciyā idha;",
  "ja73:1.3": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "ja73:1.4": "na tvevekacciyo naro”ti.",
  "ja73:2.1": "Saccaṃkirajātakaṃ tatiyaṃ."
}