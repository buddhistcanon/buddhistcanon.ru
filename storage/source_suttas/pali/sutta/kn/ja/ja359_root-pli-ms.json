{
  "ja359:0.1": "Jātaka",
  "ja359:0.2": "Pañcakanipāta",
  "ja359:0.3": "Maṇikuṇḍalavagga",
  "ja359:0.4": "9. Suvaṇṇamigajātaka",
  "ja359:1.1": "“Vikkama re haripāda,",
  "ja359:1.2": "vikkama re mahāmiga;",
  "ja359:1.3": "Chinda vārattikaṃ pāsaṃ,",
  "ja359:1.4": "nāhaṃ ekā vane rame”.",
  "ja359:2.1": "“Vikkamāmi na pāremi,",
  "ja359:2.2": "bhūmiṃ sumbhāmi vegasā;",
  "ja359:2.3": "Daḷho vārattiko pāso,",
  "ja359:2.4": "pādaṃ me parikantati”.",
  "ja359:3.1": "“Attharassu palāsāni,",
  "ja359:3.2": "asiṃ nibbāha luddaka;",
  "ja359:3.3": "Paṭhamaṃ maṃ vadhitvāna,",
  "ja359:3.4": "hana pacchā mahāmigaṃ”.",
  "ja359:4.1": "“Na me sutaṃ vā diṭṭhaṃ vā,",
  "ja359:4.2": "Bhāsantiṃ mānusiṃ migiṃ;",
  "ja359:4.3": "Tvañca bhadde sukhī hohi,",
  "ja359:4.4": "Eso cāpi mahāmigo”.",
  "ja359:5.1": "“Evaṃ luddaka nandassu,",
  "ja359:5.2": "saha sabbehi ñātibhi;",
  "ja359:5.3": "Yathāhamajja nandāmi,",
  "ja359:5.4": "muttaṃ disvā mahāmigan”ti.",
  "ja359:6.1": "Suvaṇṇamigajātakaṃ navamaṃ."
}