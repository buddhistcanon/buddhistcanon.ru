{
  "ja283:0.1": "Jātaka",
  "ja283:0.2": "Tikanipāta",
  "ja283:0.3": "Abbhantaravagga",
  "ja283:0.4": "3. Vaḍḍhakīsūkarajātaka",
  "ja283:1.1": "“Varaṃ varaṃ tvaṃ nihanaṃ pure cari,",
  "ja283:1.2": "Asmiṃ padese abhibhuyya sūkare;",
  "ja283:1.3": "So dāni eko byapagamma jhāyasi,",
  "ja283:1.4": "Balaṃ nu te byaggha na cajja vijjati”.",
  "ja283:2.1": "“Ime sudaṃ yanti disodisaṃ pure,",
  "ja283:2.2": "Bhayaṭṭitā leṇagavesino puthū;",
  "ja283:2.3": "Te dāni saṅgamma vasanti ekato,",
  "ja283:2.4": "Yatthaṭṭhitā duppasahajjame mayā”.",
  "ja283:3.1": "“Namatthu saṃghāna samāgatānaṃ,",
  "ja283:3.2": "Disvā sayaṃ sakhya vadāmi abbhutaṃ;",
  "ja283:3.3": "Byagghaṃ migā yattha jiniṃsu dāṭhino,",
  "ja283:3.4": "Sāmaggiyā dāṭhabalesu muccare”ti.",
  "ja283:4.1": "Vaḍḍhakīsūkarajātakaṃ tatiyaṃ."
}