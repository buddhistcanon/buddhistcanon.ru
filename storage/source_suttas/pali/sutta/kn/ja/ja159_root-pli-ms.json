{
  "ja159:0.1": "Jātaka",
  "ja159:0.2": "Dukanipāta",
  "ja159:0.3": "Daḷhavagga",
  "ja159:0.4": "9. Morajātaka",
  "ja159:1.1": "“Udetayañ cakkhumā ekarājā,",
  "ja159:1.2": "Harissavaṇṇo pathavippabhāso;",
  "ja159:1.3": "Taṃ taṃ namassāmi harissavaṇṇaṃ pathavippabhāsaṃ,",
  "ja159:1.4": "Tayājja guttā viharemu divasaṃ.",
  "ja159:2.1": "Ye brāhmaṇā vedagū sabbadhamme,",
  "ja159:2.2": "Te me namo te ca maṃ pālayantu;",
  "ja159:2.3": "Namatthu buddhānaṃ namatthu bodhiyā,",
  "ja159:2.4": "Namo vimuttānaṃ namo vimuttiyā;",
  "ja159:2.5": "Imaṃ so parittaṃ katvā,",
  "ja159:2.6": "Moro carati esanā.",
  "ja159:3.1": "Apetayaṃ cakkhumā ekarājā,",
  "ja159:3.2": "Harissavaṇṇo pathavippabhāso;",
  "ja159:3.3": "Taṃ taṃ namassāmi harissavaṇṇaṃ pathavippabhāsaṃ,",
  "ja159:3.4": "Tayājja guttā viharemu rattiṃ.",
  "ja159:4.1": "Ye brāhmaṇā vedagū sabbadhamme,",
  "ja159:4.2": "Te me namo te ca maṃ pālayantu;",
  "ja159:4.3": "Namatthu buddhānaṃ namatthu bodhiyā,",
  "ja159:4.4": "Namo vimuttānaṃ namo vimuttiyā;",
  "ja159:4.5": "Imaṃ so parittaṃ katvā,",
  "ja159:4.6": "Moro vāsamakappayī”ti.",
  "ja159:5.1": "Morajātakaṃ navamaṃ."
}