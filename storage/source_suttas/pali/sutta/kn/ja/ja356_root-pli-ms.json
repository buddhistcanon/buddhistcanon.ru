{
  "ja356:0.1": "Jātaka",
  "ja356:0.2": "Pañcakanipāta",
  "ja356:0.3": "Maṇikuṇḍalavagga",
  "ja356:0.4": "6. Koraṇḍiyajātaka",
  "ja356:1.1": "“Eko araññe girikandarāyaṃ,",
  "ja356:1.2": "Paggayha paggayha silaṃ pavecchasi;",
  "ja356:1.3": "Punappunaṃ santaramānarūpo,",
  "ja356:1.4": "Koraṇḍiya ko nu tava yidhattho”.",
  "ja356:2.1": "“Ahañhimaṃ sāgara sevitantaṃ,",
  "ja356:2.2": "Samaṃ karissāmi yathāpi pāṇi;",
  "ja356:2.3": "Vikiriya sānūni ca pabbatāni ca,",
  "ja356:2.4": "Tasmā silaṃ dariyā pakkhipāmi”.",
  "ja356:3.1": "“Nayimaṃ mahiṃ arahati pāṇikappaṃ,",
  "ja356:3.2": "Samaṃ manusso karaṇāya meko;",
  "ja356:3.3": "Maññāmimaññeva dariṃ jigīsaṃ,",
  "ja356:3.4": "Koraṇḍiya hāhasi jīvalokaṃ”.",
  "ja356:4.1": "“Sace ahaṃ bhūtadharaṃ na sakkā,",
  "ja356:4.2": "Samaṃ manusso karaṇāya meko;",
  "ja356:4.3": "Evameva tvaṃ brahme ime manusse,",
  "ja356:4.4": "Nānādiṭṭhike nānayissasi te”.",
  "ja356:5.1": "“Saṃkhittarūpena bhavaṃ mamatthaṃ,",
  "ja356:5.2": "Akkhāsi koraṇḍiya evametaṃ;",
  "ja356:5.3": "Yathā na sakkā pathavī samāyaṃ,",
  "ja356:5.4": "Kattuṃ manussena tathā manussā”ti.",
  "ja356:6.1": "Koraṇḍiyajātakaṃ chaṭṭhaṃ."
}