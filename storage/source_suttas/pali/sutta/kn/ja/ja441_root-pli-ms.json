{
  "ja441:0.1": "Jātaka",
  "ja441:0.2": "Dasakanipāta",
  "ja441:0.3": "Catudvāravagga",
  "ja441:0.4": "3. Catuposathiyajātaka",
  "ja441:1.1": "“Yo kopaneyye na karoti kopaṃ,",
  "ja441:1.2": "Na kujjhati sappuriso kadāci;",
  "ja441:1.3": "Kuddhopi so nāvikaroti kopaṃ,",
  "ja441:1.4": "Taṃ ve naraṃ samaṇamāhu loke”.",
  "ja441:2.1": "“Ūnūdaro yo sahate jighacchaṃ,",
  "ja441:2.2": "Danto tapassī mitapānabhojano;",
  "ja441:2.3": "Āhārahetu na karoti pāpaṃ,",
  "ja441:2.4": "Taṃ ve naraṃ samaṇamāhu loke”.",
  "ja441:3.1": "“Khiḍḍaṃ ratiṃ vippajahitvāna sabbaṃ,",
  "ja441:3.2": "Na cālikaṃ bhāsasi kiñci loke;",
  "ja441:3.3": "Vibhūsaṭṭhānā virato methunasmā,",
  "ja441:3.4": "Taṃ ve naraṃ samaṇamāhu loke”.",
  "ja441:4.1": "“Pariggahaṃ lobhadhammañca sabbaṃ,",
  "ja441:4.2": "Yo ve pariññāya pariccajeti;",
  "ja441:4.3": "Dantaṃ ṭhitattaṃ amamaṃ nirāsaṃ,",
  "ja441:4.4": "Taṃ ve naraṃ samaṇamāhu loke”.",
  "ja441:5.1": "“Pucchāma kattāramanomapaññaṃ,",
  "ja441:5.2": "Kathāsu no viggaho atthi jāto;",
  "ja441:5.3": "Chindajja kaṅkhaṃ vicikicchitāni,",
  "ja441:5.4": "Tadajja kaṅkhaṃ vitaremu sabbe”.",
  "ja441:6.1": "“Ye paṇḍitā atthadasā bhavanti,",
  "ja441:6.2": "Bhāsanti te yoniso tattha kāle;",
  "ja441:6.3": "Kathaṃ nu kathānaṃ abhāsitānaṃ,",
  "ja441:6.4": "Atthaṃ nayeyyuṃ kusalā janindā.",
  "ja441:7.1": "Kathaṃ have bhāsati nāgarājā,",
  "ja441:7.2": "Garuḷo pana venateyyo kimāha;",
  "ja441:7.3": "Gandhabbarājā pana kiṃ vadesi,",
  "ja441:7.4": "Kathaṃ pana kurūnaṃ rājaseṭṭho”.",
  "ja441:8.1": "“Khantiṃ have bhāsati nāgarājā,",
  "ja441:8.2": "Appāhāraṃ garuḷo venateyyo;",
  "ja441:8.3": "Gandhabbarājā rativippahānaṃ,",
  "ja441:8.4": "Akiñcanaṃ kurūnaṃ rājaseṭṭho”.",
  "ja441:9.1": "“Sabbāni etāni subhāsitāni,",
  "ja441:9.2": "Na hettha dubbhāsitamatthi kiñci;",
  "ja441:9.3": "Yasmiñca etāni patiṭṭhitāni,",
  "ja441:9.4": "Arāva nābhyā susamohitāni;",
  "ja441:9.5": "Catubbhi dhammehi samaṅgibhūtaṃ,",
  "ja441:9.6": "Taṃ ve naraṃ samaṇamāhu loke”.",
  "ja441:10.1": "“Tuvañhi seṭṭho tvamanuttarosi,",
  "ja441:10.2": "Tvaṃ dhammagū dhammavidū sumedho;",
  "ja441:10.3": "Paññāya pañhaṃ samadhiggahetvā,",
  "ja441:10.4": "Acchecchi dhīro vicikicchitāni;",
  "ja441:10.5": "Acchecchi kaṅkhaṃ vicikicchitāni,",
  "ja441:10.6": "Cundo yathā nāgadantaṃ kharena.",
  "ja441:11.1": "Nīluppalābhaṃ vimalaṃ anagghaṃ,",
  "ja441:11.2": "Vatthaṃ idaṃ dhūmasamānavaṇṇaṃ;",
  "ja441:11.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:11.4": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:12.1": "Suvaṇṇamālaṃ satapattaphullitaṃ,",
  "ja441:12.2": "Sakesaraṃ ratnasahassamaṇḍitaṃ;",
  "ja441:12.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:12.4": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:13.1": "Maṇiṃ anagghaṃ ruciraṃ pabhassaraṃ,",
  "ja441:13.2": "Kaṇṭhāvasattaṃ maṇibhūsitaṃ me;",
  "ja441:13.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:13.4": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:14.1": "Gavaṃ sahassaṃ usabhañca nāgaṃ,",
  "ja441:14.2": "Ājaññayutte ca rathe dasa ime;",
  "ja441:14.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:14.4": "Dadāmi te gāmavarāni soḷasa”.",
  "ja441:15.1": "“Sāriputto tadā nāgo,",
  "ja441:15.2": "supaṇṇo pana kolito;",
  "ja441:15.3": "Gandhabbarājā anuruddho,",
  "ja441:15.4": "rājā ānandapaṇḍito;",
  "ja441:15.5": "Vidhuro bodhisatto ca,",
  "ja441:15.6": "evaṃ dhāretha jātakan”ti.",
  "ja441:16.1": "Catuposathiyajātakaṃ tatiyaṃ."
}