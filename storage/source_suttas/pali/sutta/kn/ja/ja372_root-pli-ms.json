{
  "ja372:0.1": "Jātaka",
  "ja372:0.2": "Pañcakanipāta",
  "ja372:0.3": "Aḍḍhavagga",
  "ja372:0.4": "2. Migapotakajātaka",
  "ja372:1.1": "“Agārā paccupetassa,",
  "ja372:1.2": "anagārassa te sato;",
  "ja372:1.3": "Samaṇassa na taṃ sādhu,",
  "ja372:1.4": "yaṃ petamanusocasi”.",
  "ja372:2.1": "“Saṃvāsena have sakka,",
  "ja372:2.2": "manussassa migassa vā;",
  "ja372:2.3": "Hadaye jāyate pemaṃ,",
  "ja372:2.4": "na taṃ sakkā asocituṃ”.",
  "ja372:3.1": "“Mataṃ marissaṃ rodanti,",
  "ja372:3.2": "ye rudanti lapanti ca;",
  "ja372:3.3": "Tasmā tvaṃ isi mā rodi,",
  "ja372:3.4": "roditaṃ moghamāhu santo.",
  "ja372:4.1": "Roditena have brahme,",
  "ja372:4.2": "mato peto samuṭṭhahe;",
  "ja372:4.3": "Sabbe saṅgamma rodāma,",
  "ja372:4.4": "aññamaññassa ñātake”.",
  "ja372:5.1": "“Ādittaṃ vata maṃ santaṃ,",
  "ja372:5.2": "ghatasittaṃva pāvakaṃ;",
  "ja372:5.3": "Vārinā viya osiñcaṃ,",
  "ja372:5.4": "sabbaṃ nibbāpaye daraṃ.",
  "ja372:6.1": "Abbahi vata me sallaṃ,",
  "ja372:6.2": "yamāsi hadayassitaṃ;",
  "ja372:6.3": "Yo me sokaparetassa,",
  "ja372:6.4": "puttasokaṃ apānudi.",
  "ja372:7.1": "Sohaṃ abbūḷhasallosmi,",
  "ja372:7.2": "vītasoko anāvilo;",
  "ja372:7.3": "Na socāmi na rodāmi,",
  "ja372:7.4": "tava sutvāna vāsavā”ti.",
  "ja372:8.1": "Migapotakajātakaṃ dutiyaṃ."
}