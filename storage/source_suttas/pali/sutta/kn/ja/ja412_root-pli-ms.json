{
  "ja412:0.1": "Jātaka",
  "ja412:0.2": "Sattakanipāta",
  "ja412:0.3": "Gandhāravagga",
  "ja412:0.4": "7. Koṭasimbalijātaka",
  "ja412:1.1": "“Ahaṃ dasasataṃbyāmaṃ,",
  "ja412:1.2": "uragamādāya āgato;",
  "ja412:1.3": "Tañca mañca mahākāyaṃ,",
  "ja412:1.4": "dhārayaṃ nappavedhasi.",
  "ja412:2.1": "Athimaṃ khuddakaṃ pakkhiṃ,",
  "ja412:2.2": "Appamaṃsataraṃ mayā;",
  "ja412:2.3": "Dhārayaṃ byathasi bhītā,",
  "ja412:2.4": "Kamatthaṃ koṭasimbali”.",
  "ja412:3.1": "“Maṃsabhakkho tuvaṃ rāja,",
  "ja412:3.2": "phalabhakkho ayaṃ dijo;",
  "ja412:3.3": "Ayaṃ nigrodhabījāni,",
  "ja412:3.4": "pilakkhudumbarāni ca;",
  "ja412:3.5": "Assatthāni ca bhakkhitvā,",
  "ja412:3.6": "khandhe me ohadissati.",
  "ja412:4.1": "Te rukkhā saṃvirūhanti,",
  "ja412:4.2": "mama passe nivātajā;",
  "ja412:4.3": "Te maṃ pariyonandhissanti,",
  "ja412:4.4": "arukkhaṃ maṃ karissare.",
  "ja412:5.1": "Santi aññepi rukkhā se,",
  "ja412:5.2": "mūlino khandhino dumā;",
  "ja412:5.3": "Iminā sakuṇajātena,",
  "ja412:5.4": "bījamāharitā hatā.",
  "ja412:6.1": "Ajjhārūhābhivaḍḍhanti,",
  "ja412:6.2": "brahantampi vanappatiṃ;",
  "ja412:6.3": "Tasmā rāja pavedhāmi,",
  "ja412:6.4": "sampassaṃnāgataṃ bhayaṃ”.",
  "ja412:7.1": "“Saṅkeyya saṅkitabbāni,",
  "ja412:7.2": "rakkheyyānāgataṃ bhayaṃ;",
  "ja412:7.3": "Anāgatabhayā dhīro,",
  "ja412:7.4": "ubho loke avekkhatī”ti.",
  "ja412:8.1": "Koṭasimbalijātakaṃ sattamaṃ."
}