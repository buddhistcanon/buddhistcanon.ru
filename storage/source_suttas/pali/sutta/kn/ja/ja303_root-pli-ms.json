{
  "ja303:0.1": "Jātaka",
  "ja303:0.2": "Catukkanipāta",
  "ja303:0.3": "Kāliṅgavagga",
  "ja303:0.4": "3. Ekarājajātaka",
  "ja303:1.1": "“Anuttare kāmaguṇe samiddhe,",
  "ja303:1.2": "Bhutvāna pubbe vasī ekarāja;",
  "ja303:1.3": "So dāni dugge narakamhi khitto,",
  "ja303:1.4": "Nappajjahe vaṇṇabalaṃ purāṇaṃ”.",
  "ja303:2.1": "“Pubbeva khantī ca tapo ca mayhaṃ,",
  "ja303:2.2": "Sampatthitā dubbhisena ahosi;",
  "ja303:2.3": "Taṃ dāni laddhāna kathaṃ nu rāja,",
  "ja303:2.4": "Jahe ahaṃ vaṇṇabalaṃ purāṇaṃ.",
  "ja303:3.1": "Sabbā kirevaṃ pariniṭṭhitāni,",
  "ja303:3.2": "Yasassinaṃ paññavantaṃ visayha;",
  "ja303:3.3": "Yaso ca laddhā purimaṃ uḷāraṃ,",
  "ja303:3.4": "Nappajjahe vaṇṇabalaṃ purāṇaṃ.",
  "ja303:4.1": "Panujja dukkhena sukhaṃ janinda,",
  "ja303:4.2": "Sukhena vā dukkhamasayhasāhi;",
  "ja303:4.3": "Ubhayattha santo abhinibbutattā,",
  "ja303:4.4": "Sukhe ca dukkhe ca bhavanti tulyā”ti.",
  "ja303:5.1": "Ekarājajātakaṃ tatiyaṃ."
}