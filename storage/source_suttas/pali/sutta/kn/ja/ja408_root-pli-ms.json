{
  "ja408:0.1": "Jātaka",
  "ja408:0.2": "Sattakanipāta",
  "ja408:0.3": "Gandhāravagga",
  "ja408:0.4": "3. Kumbhakārajātaka",
  "ja408:1.1": "“Ambāhamaddaṃ vanamantarasmiṃ,",
  "ja408:1.2": "Nīlobhāsaṃ phalitaṃ saṃvirūḷhaṃ;",
  "ja408:1.3": "Tamaddasaṃ phalahetu vibhaggaṃ,",
  "ja408:1.4": "Taṃ disvā bhikkhācariyaṃ carāmi.",
  "ja408:2.1": "Selaṃ sumaṭṭhaṃ naravīraniṭṭhitaṃ,",
  "ja408:2.2": "Nārī yugaṃ dhārayi appasaddaṃ;",
  "ja408:2.3": "Dutiyañca āgamma ahosi saddo,",
  "ja408:2.4": "Taṃ disvā bhikkhācariyaṃ carāmi.",
  "ja408:3.1": "Dijā dijaṃ kuṇapamāharantaṃ,",
  "ja408:3.2": "Ekaṃ samānaṃ bahukā samecca;",
  "ja408:3.3": "Āhārahetū paripātayiṃsu,",
  "ja408:3.4": "Taṃ disvā bhikkhācariyaṃ carāmi.",
  "ja408:4.1": "Usabhāhamaddaṃ yūthassa majjhe,",
  "ja408:4.2": "Calakkakuṃ vaṇṇabalūpapannaṃ;",
  "ja408:4.3": "Tamaddasaṃ kāmahetu vitunnaṃ,",
  "ja408:4.4": "Taṃ disvā bhikkhācariyaṃ carāmi”.",
  "ja408:5.1": "“Karaṇḍako kaliṅgānaṃ,",
  "ja408:5.2": "gandhārānañca naggaji;",
  "ja408:5.3": "Nimirājā videhānaṃ,",
  "ja408:5.4": "pañcālānañca dummukho;",
  "ja408:5.5": "Ete raṭṭhāni hitvāna,",
  "ja408:5.6": "pabbajiṃsu akiñcanā.",
  "ja408:6.1": "Sabbepime devasamā samāgatā,",
  "ja408:6.2": "Aggī yathā pajjalito tathevime;",
  "ja408:6.3": "Ahampi eko carissāmi bhaggavi,",
  "ja408:6.4": "Hitvāna kāmāni yathodhikāni”.",
  "ja408:7.1": "“Ayameva kālo na hi añño atthi,",
  "ja408:7.2": "Anusāsitā me na bhaveyya pacchā;",
  "ja408:7.3": "Ahampi ekā carissāmi bhaggava,",
  "ja408:7.4": "Sakuṇīva muttā purisassa hatthā”.",
  "ja408:8.1": "“Āmaṃ pakkañca jānanti,",
  "ja408:8.2": "atho loṇaṃ aloṇakaṃ;",
  "ja408:8.3": "Tamahaṃ disvāna pabbajiṃ,",
  "ja408:8.4": "careva tvaṃ carāmahan”ti.",
  "ja408:9.1": "Kumbhakārajātakaṃ tatiyaṃ."
}