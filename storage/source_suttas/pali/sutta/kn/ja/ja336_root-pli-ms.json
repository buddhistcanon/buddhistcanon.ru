{
  "ja336:0.1": "Jātaka",
  "ja336:0.2": "Catukkanipāta",
  "ja336:0.3": "Kokilavagga",
  "ja336:0.4": "6. Brahāchattajātaka",
  "ja336:1.1": "“Tiṇaṃ tiṇanti lapasi,",
  "ja336:1.2": "ko nu te tiṇamāhari;",
  "ja336:1.3": "Kiṃ nu te tiṇakiccatthi,",
  "ja336:1.4": "tiṇameva pabhāsasi”.",
  "ja336:2.1": "“Idhāgamā brahmacārī,",
  "ja336:2.2": "brahā chatto bahussuto;",
  "ja336:2.3": "So me sabbaṃ samādāya,",
  "ja336:2.4": "tiṇaṃ nikkhippa gacchati”.",
  "ja336:3.1": "“Evetaṃ hoti kattabbaṃ,",
  "ja336:3.2": "appena bahumicchatā;",
  "ja336:3.3": "Sabbaṃ sakassa ādānaṃ,",
  "ja336:3.4": "anādānaṃ tiṇassa ca”.",
  "ja336:3.5": "()",
  "ja336:4.1": "“Sīlavanto na kubbanti,",
  "ja336:4.2": "bālo sīlāni kubbati;",
  "ja336:4.3": "Aniccasīlaṃ dussīlyaṃ,",
  "ja336:4.4": "kiṃ paṇḍiccaṃ karissatī”ti.",
  "ja336:5.1": "Brahāchattajātakaṃ chaṭṭhaṃ."
}