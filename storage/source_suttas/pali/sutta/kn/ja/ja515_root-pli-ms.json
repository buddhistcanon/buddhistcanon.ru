{
  "ja515:0.1": "Jātaka",
  "ja515:0.2": "Tiṃsanipāta",
  "ja515:0.3": "Kiṃchandavagga",
  "ja515:0.4": "5. Sambhavajātaka",
  "ja515:1.1": "“Rajjañca paṭipannāsma,",
  "ja515:1.2": "ādhipaccaṃ sucīrata;",
  "ja515:1.3": "Mahattaṃ pattumicchāmi,",
  "ja515:1.4": "vijetuṃ pathaviṃ imaṃ.",
  "ja515:2.1": "Dhammena no adhammena,",
  "ja515:2.2": "adhammo me na ruccati;",
  "ja515:2.3": "Kiccova dhammo carito,",
  "ja515:2.4": "rañño hoti sucīrata.",
  "ja515:3.1": "Idha cevāninditā yena,",
  "ja515:3.2": "pecca yena aninditā;",
  "ja515:3.3": "Yasaṃ devamanussesu,",
  "ja515:3.4": "yena pappomu brāhmaṇa.",
  "ja515:4.1": "Yohaṃ atthañca dhammañca,",
  "ja515:4.2": "kattumicchāmi brāhmaṇa;",
  "ja515:4.3": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:4.4": "brāhmaṇakkhāhi pucchito”.",
  "ja515:5.1": "“Nāññatra vidhurā rāja,",
  "ja515:5.2": "etadakkhātumarahati;",
  "ja515:5.3": "Yaṃ tvaṃ atthañca dhammañca,",
  "ja515:5.4": "kattumicchasi khattiya”.",
  "ja515:6.1": "“Ehi kho pahito gaccha,",
  "ja515:6.2": "vidhurassa upantikaṃ;",
  "ja515:6.3": "Nikkhañcimaṃ suvaṇṇassa,",
  "ja515:6.4": "haraṃ gaccha sucīrata;",
  "ja515:6.5": "Abhihāraṃ imaṃ dajjā,",
  "ja515:6.6": "atthadhammānusiṭṭhiyā”.",
  "ja515:7.1": "Svādhippāgā bhāradvājo,",
  "ja515:7.2": "vidhurassa upantikaṃ;",
  "ja515:7.3": "Tamaddasa mahābrahmā,",
  "ja515:7.4": "asamānaṃ sake ghare.",
  "ja515:8.1": "“Raññohaṃ pahito dūto,",
  "ja515:8.2": "korabyassa yasassino;",
  "ja515:8.3": "‘Atthaṃ dhammañca pucchesi’,",
  "ja515:8.4": "iccabravi yudhiṭṭhilo;",
  "ja515:8.5": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:8.6": "vidhurakkhāhi pucchito”.",
  "ja515:9.1": "“Gaṅgaṃ me pidahissanti,",
  "ja515:9.2": "na taṃ sakkomi brāhmaṇa;",
  "ja515:9.3": "Apidhetuṃ mahāsindhuṃ,",
  "ja515:9.4": "taṃ kathaṃ so bhavissati;",
  "ja515:9.5": "Na te sakkomi akkhātuṃ,",
  "ja515:9.6": "atthaṃ dhammañca pucchito.",
  "ja515:10.1": "Bhadrakāro ca me putto,",
  "ja515:10.2": "oraso mama atrajo;",
  "ja515:10.3": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:10.4": "gantvā pucchassu brāhmaṇa”.",
  "ja515:11.1": "Svādhippāgā bhāradvājo,",
  "ja515:11.2": "bhadrakārassupantikaṃ;",
  "ja515:11.3": "Tamaddasa mahābrahmā,",
  "ja515:11.4": "nisinnaṃ samhi vesmani.",
  "ja515:12.1": "“Raññohaṃ pahito dūto,",
  "ja515:12.2": "korabyassa yasassino;",
  "ja515:12.3": "‘Atthaṃ dhammañca pucchesi’,",
  "ja515:12.4": "iccabravi yudhiṭṭhilo;",
  "ja515:12.5": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:12.6": "bhadrakāra pabrūhi me”.",
  "ja515:13.1": "“Maṃsakājaṃ avahāya,",
  "ja515:13.2": "godhaṃ anupatāmahaṃ;",
  "ja515:13.3": "Na te sakkomi akkhātuṃ,",
  "ja515:13.4": "atthaṃ dhammañca pucchito.",
  "ja515:14.1": "Sañcayo nāma me bhātā,",
  "ja515:14.2": "kaniṭṭho me sucīrata;",
  "ja515:14.3": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:14.4": "gantvā pucchassu brāhmaṇa”.",
  "ja515:15.1": "Svādhippāgā bhāradvājo,",
  "ja515:15.2": "sañcayassa upantikaṃ;",
  "ja515:15.3": "Tamaddasa mahābrahmā,",
  "ja515:15.4": "nisinnaṃ samhi vesmani.",
  "ja515:16.1": "“Raññohaṃ pahito dūto,",
  "ja515:16.2": "korabyassa yasassino;",
  "ja515:16.3": "‘Atthaṃ dhammañca pucchesi’,",
  "ja515:16.4": "iccabravi yudhiṭṭhilo;",
  "ja515:16.5": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:16.6": "sañcayakkhāhi pucchito”.",
  "ja515:17.1": "“Sadā maṃ gilate maccu,",
  "ja515:17.2": "sāyaṃ pāto sucīrata;",
  "ja515:17.3": "Na te sakkomi akkhātuṃ,",
  "ja515:17.4": "atthaṃ dhammañca pucchito.",
  "ja515:18.1": "Sambhavo nāma me bhātā,",
  "ja515:18.2": "kaniṭṭho me sucīrata;",
  "ja515:18.3": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:18.4": "gantvā pucchassu brāhmaṇa”.",
  "ja515:19.1": "“Abbhuto vata bho dhammo,",
  "ja515:19.2": "nāyaṃ asmāka ruccati;",
  "ja515:19.3": "Tayo janā pitāputtā,",
  "ja515:19.4": "te su paññāya no vidū.",
  "ja515:20.1": "Na taṃ sakkotha akkhātuṃ,",
  "ja515:20.2": "atthaṃ dhammañca pucchitā;",
  "ja515:20.3": "Kathaṃ nu daharo jaññā,",
  "ja515:20.4": "atthaṃ dhammañca pucchito”.",
  "ja515:21.1": "“Mā naṃ daharoti uññāsi,",
  "ja515:21.2": "apucchitvāna sambhavaṃ;",
  "ja515:21.3": "Pucchitvā sambhavaṃ jaññā,",
  "ja515:21.4": "atthaṃ dhammañca brāhmaṇa.",
  "ja515:22.1": "Yathāpi cando vimalo,",
  "ja515:22.2": "gacchaṃ ākāsadhātuyā;",
  "ja515:22.3": "Sabbe tārāgaṇe loke,",
  "ja515:22.4": "ābhāya atirocati.",
  "ja515:23.1": "Evampi daharūpeto,",
  "ja515:23.2": "paññāyogena sambhavo;",
  "ja515:23.3": "Mā naṃ daharoti uññāsi,",
  "ja515:23.4": "apucchitvāna sambhavaṃ;",
  "ja515:23.5": "Pucchitvā sambhavaṃ jaññā,",
  "ja515:23.6": "atthaṃ dhammañca brāhmaṇa.",
  "ja515:24.1": "Yathāpi rammako māso,",
  "ja515:24.2": "gimhānaṃ hoti brāhmaṇa;",
  "ja515:24.3": "Atevaññehi māsehi,",
  "ja515:24.4": "dumapupphehi sobhati.",
  "ja515:25.1": "Evampi daharūpeto,",
  "ja515:25.2": "paññāyogena sambhavo;",
  "ja515:25.3": "Mā naṃ daharoti uññāsi,",
  "ja515:25.4": "apucchitvāna sambhavaṃ;",
  "ja515:25.5": "Pucchitvā sambhavaṃ jaññā,",
  "ja515:25.6": "atthaṃ dhammañca brāhmaṇa.",
  "ja515:26.1": "Yathāpi himavā brahme,",
  "ja515:26.2": "pabbato gandhamādano;",
  "ja515:26.3": "Nānārukkhehi sañchanno,",
  "ja515:26.4": "mahābhūtagaṇālayo;",
  "ja515:26.5": "Osadhehi ca dibbehi,",
  "ja515:26.6": "disā bhāti pavāti ca.",
  "ja515:27.1": "Evampi daharūpeto,",
  "ja515:27.2": "paññāyogena sambhavo;",
  "ja515:27.3": "Mā naṃ daharoti uññāsi,",
  "ja515:27.4": "apucchitvāna sambhavaṃ;",
  "ja515:27.5": "Pucchitvā sambhavaṃ jaññā,",
  "ja515:27.6": "atthaṃ dhammañca brāhmaṇa.",
  "ja515:28.1": "Yathāpi pāvako brahme,",
  "ja515:28.2": "accimālī yasassimā;",
  "ja515:28.3": "Jalamāno vane gacche,",
  "ja515:28.4": "analo kaṇhavattanī.",
  "ja515:29.1": "Ghatāsano dhūmaketu,",
  "ja515:29.2": "uttamāhevanandaho;",
  "ja515:29.3": "Nisīthe pabbataggasmiṃ,",
  "ja515:29.4": "pahūtedho virocati.",
  "ja515:30.1": "Evampi daharūpeto,",
  "ja515:30.2": "paññāyogena sambhavo;",
  "ja515:30.3": "Mā naṃ daharoti uññāsi,",
  "ja515:30.4": "apucchitvāna sambhavaṃ;",
  "ja515:30.5": "Pucchitvā sambhavaṃ jaññā,",
  "ja515:30.6": "atthaṃ dhammañca brāhmaṇa.",
  "ja515:31.1": "Javena bhadraṃ jānanti,",
  "ja515:31.2": "balībaddañca vāhiye;",
  "ja515:31.3": "Dohena dhenuṃ jānanti,",
  "ja515:31.4": "bhāsamānañca paṇḍitaṃ.",
  "ja515:32.1": "Evampi daharūpeto,",
  "ja515:32.2": "paññāyogena sambhavo;",
  "ja515:32.3": "Mā naṃ daharoti uññāsi,",
  "ja515:32.4": "apucchitvāna sambhavaṃ;",
  "ja515:32.5": "Pucchitvā sambhavaṃ jaññā,",
  "ja515:32.6": "atthaṃ dhammañca brāhmaṇa”.",
  "ja515:33.1": "Svādhippāgā bhāradvājo,",
  "ja515:33.2": "sambhavassa upantikaṃ;",
  "ja515:33.3": "Tamaddasa mahābrahmā,",
  "ja515:33.4": "kīḷamānaṃ bahīpure.",
  "ja515:34.1": "“Raññohaṃ pahito dūto,",
  "ja515:34.2": "korabyassa yasassino;",
  "ja515:34.3": "‘Atthaṃ dhammañca pucchesi’,",
  "ja515:34.4": "iccabravi yudhiṭṭhilo;",
  "ja515:34.5": "Taṃ tvaṃ atthañca dhammañca,",
  "ja515:34.6": "sambhavakkhāhi pucchito”.",
  "ja515:35.1": "“Taggha te ahamakkhissaṃ,",
  "ja515:35.2": "yathāpi kusalo tathā;",
  "ja515:35.3": "Rājā ca kho taṃ jānāti,",
  "ja515:35.4": "yadi kāhati vā na vā.",
  "ja515:36.1": "‘Ajja suve’ti saṃseyya,",
  "ja515:36.2": "raññā puṭṭho sucīrata;",
  "ja515:36.3": "Mā katvā avasī rājā,",
  "ja515:36.4": "atthe jāte yudhiṭṭhilo.",
  "ja515:37.1": "Ajjhattaññeva saṃseyya,",
  "ja515:37.2": "raññā puṭṭho sucīrata;",
  "ja515:37.3": "Kummaggaṃ na niveseyya,",
  "ja515:37.4": "yathā mūḷho acetaso.",
  "ja515:38.1": "Attānaṃ nātivatteyya,",
  "ja515:38.2": "adhammaṃ na samācare;",
  "ja515:38.3": "Atitthe nappatāreyya,",
  "ja515:38.4": "anatthe na yuto siyā.",
  "ja515:39.1": "Yo ca etāni ṭhānāni,",
  "ja515:39.2": "kattuṃ jānāti khattiyo;",
  "ja515:39.3": "Sadā so vaḍḍhate rājā,",
  "ja515:39.4": "sukkapakkheva candimā.",
  "ja515:40.1": "Ñātīnañca piyo hoti,",
  "ja515:40.2": "mittesu ca virocati;",
  "ja515:40.3": "Kāyassa bhedā sappañño,",
  "ja515:40.4": "saggaṃ so upapajjatī”ti.",
  "ja515:41.1": "Sambhavajātakaṃ pañcamaṃ."
}