{
  "ja406:0.1": "Jātaka",
  "ja406:0.2": "Sattakanipāta",
  "ja406:0.3": "Gandhāravagga",
  "ja406:0.4": "1. Gandhārajātaka",
  "ja406:1.1": "“Hitvā gāmasahassāni,",
  "ja406:1.2": "paripuṇṇāni soḷasa;",
  "ja406:1.3": "Koṭṭhāgārāni phītāni,",
  "ja406:1.4": "sannidhiṃ dāni kubbasi”.",
  "ja406:2.1": "“Hitvā gandhāravisayaṃ,",
  "ja406:2.2": "pahūtadhanadhāriyaṃ;",
  "ja406:2.3": "Pasāsanato nikkhanto,",
  "ja406:2.4": "idha dāni pasāsasi”.",
  "ja406:3.1": "“Dhammaṃ bhaṇāmi vedeha,",
  "ja406:3.2": "adhammo me na ruccati;",
  "ja406:3.3": "Dhammaṃ me bhaṇamānassa,",
  "ja406:3.4": "na pāpamupalimpati”.",
  "ja406:4.1": "“Yena kenaci vaṇṇena,",
  "ja406:4.2": "paro labhati ruppanaṃ;",
  "ja406:4.3": "Mahatthiyampi ce vācaṃ,",
  "ja406:4.4": "na taṃ bhāseyya paṇḍito”.",
  "ja406:5.1": "“Kāmaṃ ruppatu vā mā vā,",
  "ja406:5.2": "bhusaṃva vikirīyatu;",
  "ja406:5.3": "Dhammaṃ me bhaṇamānassa,",
  "ja406:5.4": "na pāpamupalimpati”.",
  "ja406:6.1": "“No ce assa sakā buddhi,",
  "ja406:6.2": "vinayo vā susikkhito;",
  "ja406:6.3": "Vane andhamahiṃsova,",
  "ja406:6.4": "careyya bahuko jano.",
  "ja406:7.1": "Yasmā ca panidhekacce,",
  "ja406:7.2": "āceramhi susikkhitā;",
  "ja406:7.3": "Tasmā vinītavinayā,",
  "ja406:7.4": "caranti susamāhitā”ti.",
  "ja406:8.1": "Gandhārajātakaṃ paṭhamaṃ."
}