{
  "ja278:0.1": "Jātaka",
  "ja278:0.2": "Tikanipāta",
  "ja278:0.3": "Udapānavagga",
  "ja278:0.4": "8. Mahiṃsarājajātaka",
  "ja278:1.1": "“Kimatthamabhisandhāya,",
  "ja278:1.2": "lahucittassa dubbhino;",
  "ja278:1.3": "Sabbakāmadadasseva,",
  "ja278:1.4": "imaṃ dukkhaṃ titikkhasi.",
  "ja278:2.1": "Siṅgena nihanāhetaṃ,",
  "ja278:2.2": "padasā ca adhiṭṭhaha;",
  "ja278:2.3": "Bhiyyo bālā pakujjheyyuṃ,",
  "ja278:2.4": "no cassa paṭisedhako”.",
  "ja278:3.1": "“Mamevāyaṃ maññamāno,",
  "ja278:3.2": "aññepevaṃ karissati;",
  "ja278:3.3": "Te naṃ tattha vadhissanti,",
  "ja278:3.4": "sā me mutti bhavissatī”ti.",
  "ja278:4.1": "Mahiṃsarājajātakaṃ aṭṭhamaṃ."
}