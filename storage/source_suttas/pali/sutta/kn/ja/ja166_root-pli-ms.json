{
  "ja166:0.1": "Jātaka",
  "ja166:0.2": "Dukanipāta",
  "ja166:0.3": "Santhavavagga",
  "ja166:0.4": "6. Upasāḷakajātaka",
  "ja166:1.1": "“Upasāḷakanāmāni,",
  "ja166:1.2": "sahassāni catuddasa;",
  "ja166:1.3": "Asmiṃ padese daḍḍhāni,",
  "ja166:1.4": "natthi loke anāmataṃ.",
  "ja166:2.1": "Yamhi saccañca dhammo ca,",
  "ja166:2.2": "ahiṃsā saṃyamo damo;",
  "ja166:2.3": "Etaṃ ariyā sevanti,",
  "ja166:2.4": "etaṃ loke anāmatan”ti.",
  "ja166:3.1": "Upasāḷakajātakaṃ chaṭṭhaṃ."
}