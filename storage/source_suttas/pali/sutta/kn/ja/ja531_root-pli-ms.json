{
  "ja531:0.1": "Jātaka",
  "ja531:0.2": "Sattatinipāta",
  "ja531:0.3": "Kusavagga",
  "ja531:0.4": "1. Kusajātaka",
  "ja531:1.1": "“Idaṃ te raṭṭhaṃ sadhanaṃ sayoggaṃ,",
  "ja531:1.2": "Sakāyuraṃ sabbakāmūpapannaṃ;",
  "ja531:1.3": "Idaṃ te rajjaṃ anusāsa amma,",
  "ja531:1.4": "Gacchāmahaṃ yattha piyā pabhāvatī”.",
  "ja531:2.1": "“Anujjubhūtena haraṃ mahantaṃ,",
  "ja531:2.2": "Divā ca ratto ca nisīthakāle;",
  "ja531:2.3": "Paṭigaccha tvaṃ khippaṃ kusāvatiṃ kusa,",
  "ja531:2.4": "Nicchāmi dubbaṇṇamahaṃ vasantaṃ”.",
  "ja531:3.1": "“Nāhaṃ gamissāmi ito kusāvatiṃ,",
  "ja531:3.2": "Pabhāvatī vaṇṇapalobhito tava;",
  "ja531:3.3": "Ramāmi maddassa niketaramme,",
  "ja531:3.4": "Hitvāna raṭṭhaṃ tava dassane rato.",
  "ja531:4.1": "Pabhāvatī vaṇṇapalobhito tava,",
  "ja531:4.2": "Sammūḷharūpo vicarāmi mediniṃ;",
  "ja531:4.3": "Disaṃ na jānāmi kutomhi āgato,",
  "ja531:4.4": "Tayamhi matto migamandalocane.",
  "ja531:5.1": "Suvaṇṇacīravasane,",
  "ja531:5.2": "jātarūpasumekhale;",
  "ja531:5.3": "Sussoṇi tava kāmā hi,",
  "ja531:5.4": "nāhaṃ rajjena matthiko”.",
  "ja531:6.1": "“Abbhūti tassa bho hoti,",
  "ja531:6.2": "yo anicchantamicchati;",
  "ja531:6.3": "Akāmaṃ rāja kāmesi,",
  "ja531:6.4": "akantaṃ kantumicchasi”.",
  "ja531:7.1": "“Akāmaṃ vā sakāmaṃ vā,",
  "ja531:7.2": "yo naro labhate piyaṃ;",
  "ja531:7.3": "Lābhamettha pasaṃsāma,",
  "ja531:7.4": "alābho tattha pāpako”.",
  "ja531:8.1": "“Pāsāṇasāraṃ khaṇasi,",
  "ja531:8.2": "kaṇikārassa dārunā;",
  "ja531:8.3": "Vātaṃ jālena bādhesi,",
  "ja531:8.4": "yo anicchantamicchasi”.",
  "ja531:9.1": "“Pāsāṇo nūna te hadaye,",
  "ja531:9.2": "ohito mudulakkhaṇe;",
  "ja531:9.3": "Yo te sātaṃ na vindāmi,",
  "ja531:9.4": "tirojanapadāgato.",
  "ja531:10.1": "Yadā maṃ bhakuṭiṃ katvā,",
  "ja531:10.2": "rājaputtī udikkhati;",
  "ja531:10.3": "Āḷāriko tadā homi,",
  "ja531:10.4": "rañño maddassantepure.",
  "ja531:11.1": "Yadā umhayamānā maṃ,",
  "ja531:11.2": "rājaputtī udikkhati;",
  "ja531:11.3": "Nāḷāriko tadā homi,",
  "ja531:11.4": "rājā homi tadā kuso”.",
  "ja531:12.1": "“Sace hi vacanaṃ saccaṃ,",
  "ja531:12.2": "nemittānaṃ bhavissati;",
  "ja531:12.3": "Neva me tvaṃ patī assa,",
  "ja531:12.4": "kāmaṃ chindantu sattadhā”.",
  "ja531:13.1": "“Sace hi vacanaṃ saccaṃ,",
  "ja531:13.2": "aññesaṃ yadi vā mamaṃ;",
  "ja531:13.3": "Neva tuyhaṃ patī atthi,",
  "ja531:13.4": "añño sīhassarā kusā”.",
  "ja531:14.1": "“Nekkhaṃ gīvaṃ te kāressaṃ,",
  "ja531:14.2": "patvā khujje kusāvatiṃ;",
  "ja531:14.3": "Sace maṃ nāganāsūrū,",
  "ja531:14.4": "olokeyya pabhāvatī.",
  "ja531:15.1": "Nekkhaṃ gīvaṃ te kāressaṃ,",
  "ja531:15.2": "patvā khujje kusāvatiṃ;",
  "ja531:15.3": "Sace maṃ nāganāsūrū,",
  "ja531:15.4": "ālapeyya pabhāvatī.",
  "ja531:16.1": "Nekkhaṃ gīvaṃ te kāressaṃ,",
  "ja531:16.2": "patvā khujje kusāvatiṃ;",
  "ja531:16.3": "Sace maṃ nāganāsūrū,",
  "ja531:16.4": "umhāyeyya pabhāvatī.",
  "ja531:17.1": "Nekkhaṃ gīvaṃ te kāressaṃ,",
  "ja531:17.2": "patvā khujje kusāvatiṃ;",
  "ja531:17.3": "Sace maṃ nāganāsūrū,",
  "ja531:17.4": "pamhāyeyya pabhāvatī.",
  "ja531:18.1": "Nekkhaṃ gīvaṃ te kāressaṃ,",
  "ja531:18.2": "patvā khujje kusāvatiṃ;",
  "ja531:18.3": "Sace maṃ nāganāsūrū,",
  "ja531:18.4": "pāṇīhi upasamphuse”.",
  "ja531:19.1": "“Na hi nūnāyaṃ rājaputtī,",
  "ja531:19.2": "kuse sātampi vindati;",
  "ja531:19.3": "Āḷārike bhate pose,",
  "ja531:19.4": "vetanena anatthike”.",
  "ja531:20.1": "“Na hi nūnāyaṃ sā khujjā,",
  "ja531:20.2": "labhati jivhāya chedanaṃ;",
  "ja531:20.3": "Sunisitena satthena,",
  "ja531:20.4": "evaṃ dubbhāsitaṃ bhaṇaṃ”.",
  "ja531:21.1": "“Mā naṃ rūpena pāmesi,",
  "ja531:21.2": "ārohena pabhāvati;",
  "ja531:21.3": "Mahāyasoti katvāna,",
  "ja531:21.4": "karassu rucire piyaṃ.",
  "ja531:22.1": "Mā naṃ rūpena pāmesi,",
  "ja531:22.2": "ārohena pabhāvati;",
  "ja531:22.3": "Mahaddhanoti katvāna,",
  "ja531:22.4": "karassu rucire piyaṃ.",
  "ja531:23.1": "Mā naṃ rūpena pāmesi,",
  "ja531:23.2": "ārohena pabhāvati;",
  "ja531:23.3": "Mahabbaloti katvāna,",
  "ja531:23.4": "karassu rucire piyaṃ.",
  "ja531:24.1": "Mā naṃ rūpena pāmesi,",
  "ja531:24.2": "ārohena pabhāvati;",
  "ja531:24.3": "Mahāraṭṭhoti katvāna,",
  "ja531:24.4": "karassu rucire piyaṃ.",
  "ja531:25.1": "Mā naṃ rūpena pāmesi,",
  "ja531:25.2": "ārohena pabhāvati;",
  "ja531:25.3": "Mahārājāti katvāna,",
  "ja531:25.4": "karassu rucire piyaṃ.",
  "ja531:26.1": "Mā naṃ rūpena pāmesi,",
  "ja531:26.2": "ārohena pabhāvati;",
  "ja531:26.3": "Sīhassaroti katvāna,",
  "ja531:26.4": "karassu rucire piyaṃ.",
  "ja531:27.1": "Mā naṃ rūpena pāmesi,",
  "ja531:27.2": "ārohena pabhāvati;",
  "ja531:27.3": "Vaggussaroti katvāna,",
  "ja531:27.4": "karassu rucire piyaṃ.",
  "ja531:28.1": "Mā naṃ rūpena pāmesi,",
  "ja531:28.2": "ārohena pabhāvati;",
  "ja531:28.3": "Bindussaroti katvāna,",
  "ja531:28.4": "karassu rucire piyaṃ.",
  "ja531:29.1": "Mā naṃ rūpena pāmesi,",
  "ja531:29.2": "ārohena pabhāvati;",
  "ja531:29.3": "Mañjussaroti katvāna,",
  "ja531:29.4": "karassu rucire piyaṃ.",
  "ja531:30.1": "Mā naṃ rūpena pāmesi,",
  "ja531:30.2": "ārohena pabhāvati;",
  "ja531:30.3": "Madhussaroti katvāna,",
  "ja531:30.4": "karassu rucire piyaṃ.",
  "ja531:31.1": "Mā naṃ rūpena pāmesi,",
  "ja531:31.2": "ārohena pabhāvati;",
  "ja531:31.3": "Satasippoti katvāna,",
  "ja531:31.4": "karassu rucire piyaṃ.",
  "ja531:32.1": "Mā naṃ rūpena pāmesi,",
  "ja531:32.2": "ārohena pabhāvati;",
  "ja531:32.3": "Khattiyotipi katvāna,",
  "ja531:32.4": "karassu rucire piyaṃ.",
  "ja531:33.1": "Mā naṃ rūpena pāmesi,",
  "ja531:33.2": "ārohena pabhāvati;",
  "ja531:33.3": "Kusarājāti katvāna,",
  "ja531:33.4": "karassu rucire piyaṃ”.",
  "ja531:34.1": "“Ete nāgā upatthaddhā,",
  "ja531:34.2": "sabbe tiṭṭhanti vammitā;",
  "ja531:34.3": "Purā maddanti pākāraṃ,",
  "ja531:34.4": "ānentetaṃ pabhāvatiṃ”.",
  "ja531:35.1": "“Satta bile karitvāna,",
  "ja531:35.2": "ahametaṃ pabhāvatiṃ;",
  "ja531:35.3": "Khattiyānaṃ padassāmi,",
  "ja531:35.4": "ye maṃ hantuṃ idhāgatā”.",
  "ja531:36.1": "“Avuṭṭhahi rājaputtī,",
  "ja531:36.2": "sāmā koseyyavāsinī;",
  "ja531:36.3": "Assupuṇṇehi nettehi,",
  "ja531:36.4": "dāsīgaṇapurakkhatā”.",
  "ja531:37.1": "“Taṃ nūna kakkūpanisevitaṃ mukhaṃ,",
  "ja531:37.2": "Ādāsadantātharupaccavekkhitaṃ;",
  "ja531:37.3": "Subhaṃ sunettaṃ virajaṃ anaṅgaṇaṃ,",
  "ja531:37.4": "Chuddhaṃ vane ṭhassati khattiyehi.",
  "ja531:38.1": "Te nūna me asite vellitagge,",
  "ja531:38.2": "Kese mudū candanasāralitte;",
  "ja531:38.3": "Samākule sīvathikāya majjhe,",
  "ja531:38.4": "Pādehi gijjhā parikaḍḍhissanti.",
  "ja531:39.1": "Tā nūna me tambanakhā sulomā,",
  "ja531:39.2": "Bāhā mudū candanasāralittā;",
  "ja531:39.3": "Chinnā vane ujjhitā khattiyehi,",
  "ja531:39.4": "Gayha dhaṅko gacchati yenakāmaṃ.",
  "ja531:40.1": "Te nūna tālūpanibhe alambe,",
  "ja531:40.2": "Nisevite kāsikacandanena;",
  "ja531:40.3": "Thanesu me lambissati siṅgālo,",
  "ja531:40.4": "Mātūva putto taruṇo tanūjo.",
  "ja531:41.1": "Taṃ nūna soṇiṃ puthulaṃ sukoṭṭitaṃ,",
  "ja531:41.2": "Nisevitaṃ kañcanamekhalāhi;",
  "ja531:41.3": "Chinnaṃ vane khattiyehī avatthaṃ,",
  "ja531:41.4": "Siṅgālasaṅghā parikaḍḍhissanti.",
  "ja531:42.1": "Soṇā dhaṅkā siṅgālā ca,",
  "ja531:42.2": "ye caññe santi dāṭhino;",
  "ja531:42.3": "Ajarā nūna hessanti,",
  "ja531:42.4": "bhakkhayitvā pabhāvatiṃ.",
  "ja531:43.1": "Sace maṃsāni hariṃsu,",
  "ja531:43.2": "khattiyā dūragāmino;",
  "ja531:43.3": "Aṭṭhīni amma yācitvā,",
  "ja531:43.4": "anupathe dahātha naṃ.",
  "ja531:44.1": "Khettāni amma kāretvā,",
  "ja531:44.2": "kaṇikārettha ropaya;",
  "ja531:44.3": "Yadā te pupphitā assu,",
  "ja531:44.4": "hemantānaṃ himaccaye;",
  "ja531:44.5": "Sareyyātha mamaṃ amma,",
  "ja531:44.6": "evaṃvaṇṇā pabhāvatī”.",
  "ja531:45.1": "“Tassā mātā udaṭṭhāsi,",
  "ja531:45.2": "khattiyā devavaṇṇinī;",
  "ja531:45.3": "Disvā asiñca sūnañca,",
  "ja531:45.4": "rañño maddassantepure”.",
  "ja531:46.1": "“Iminā nūna asinā,",
  "ja531:46.2": "susaññaṃ tanumajjhimaṃ;",
  "ja531:46.3": "Dhītaraṃ madda hantvāna,",
  "ja531:46.4": "khattiyānaṃ padassasi.",
  "ja531:47.1": "Na me akāsi vacanaṃ,",
  "ja531:47.2": "atthakāmāya puttike;",
  "ja531:47.3": "Sājja lohitasañchannā,",
  "ja531:47.4": "gacchasi yamasādhanaṃ.",
  "ja531:48.1": "Evamāpajjatī poso,",
  "ja531:48.2": "pāpiyañca nigacchati;",
  "ja531:48.3": "Yo ve hitānaṃ vacanaṃ,",
  "ja531:48.4": "na karoti atthadassinaṃ.",
  "ja531:49.1": "Sace ca ajja dhāresi,",
  "ja531:49.2": "kumāraṃ cārudassanaṃ;",
  "ja531:49.3": "Kusena jātaṃ khattiyaṃ,",
  "ja531:49.4": "suvaṇṇamaṇimekhalaṃ;",
  "ja531:49.5": "Pūjitaṃ ñātisaṅghehi,",
  "ja531:49.6": "na gacchasi yamakkhayaṃ.",
  "ja531:50.1": "Yatthassu bherī nadati,",
  "ja531:50.2": "kuñjaro ca nikūjati;",
  "ja531:50.3": "Khattiyānaṃ kule bhadde,",
  "ja531:50.4": "kiṃ nu sukhataraṃ tato.",
  "ja531:51.1": "Asso ca sisati dvāre,",
  "ja531:51.2": "kumāro uparodati;",
  "ja531:51.3": "Khattiyānaṃ kule bhadde,",
  "ja531:51.4": "kiṃ nu sukhataraṃ tato.",
  "ja531:52.1": "Mayūrakoñcābhirude,",
  "ja531:52.2": "kokilābhinikūjite;",
  "ja531:52.3": "Khattiyānaṃ kule bhadde,",
  "ja531:52.4": "kiṃ nu sukhataraṃ tato”.",
  "ja531:53.1": "“Kahaṃ nu so sattumaddano,",
  "ja531:53.2": "pararaṭṭhappamaddano;",
  "ja531:53.3": "Kuso soḷārapaññāṇo,",
  "ja531:53.4": "yo no dukkhā pamocaye”.",
  "ja531:54.1": "“Idheva so sattumaddano,",
  "ja531:54.2": "pararaṭṭhappamaddano;",
  "ja531:54.3": "Kuso soḷārapaññāṇo,",
  "ja531:54.4": "yo te sabbe vadhissati”.",
  "ja531:55.1": "“Ummattikā nu bhaṇasi,",
  "ja531:55.2": "Andhabālā pabhāsasi;",
  "ja531:55.3": "Kuso ce āgato assa,",
  "ja531:55.4": "Kiṃ na jānemu taṃ mayaṃ”.",
  "ja531:56.1": "“Eso āḷāriko poso,",
  "ja531:56.2": "kumārīpuramantare;",
  "ja531:56.3": "Daḷhaṃ katvāna saṃvelliṃ,",
  "ja531:56.4": "kumbhiṃ dhovati oṇato”.",
  "ja531:57.1": "“Veṇī tvamasi caṇḍālī,",
  "ja531:57.2": "adūsi kulagandhinī;",
  "ja531:57.3": "Kathaṃ maddakule jātā,",
  "ja531:57.4": "dāsaṃ kayirāsi kāmukaṃ”.",
  "ja531:58.1": "“Namhi veṇī na caṇḍālī,",
  "ja531:58.2": "na camhi kulagandhinī;",
  "ja531:58.3": "Okkākaputto bhaddante,",
  "ja531:58.4": "tvaṃ nu dāsoti maññasi”.",
  "ja531:59.1": "“Yo brāhmaṇasahassāni,",
  "ja531:59.2": "sadā bhojeti vīsatiṃ;",
  "ja531:59.3": "Okkākaputto bhaddante,",
  "ja531:59.4": "tvaṃ nu dāsoti maññasi.",
  "ja531:60.1": "Yassa nāgasahassāni,",
  "ja531:60.2": "sadā yojenti vīsatiṃ;",
  "ja531:60.3": "Okkākaputto bhaddante,",
  "ja531:60.4": "tvaṃ nu dāsoti maññasi.",
  "ja531:61.1": "Yassa assasahassāni,",
  "ja531:61.2": "sadā yojenti vīsatiṃ;",
  "ja531:61.3": "Okkākaputto bhaddante,",
  "ja531:61.4": "tvaṃ nu dāsoti maññasi.",
  "ja531:62.1": "Yassa rathasahassāni,",
  "ja531:62.2": "sadā yojenti vīsatiṃ;",
  "ja531:62.3": "Okkākaputto bhaddante,",
  "ja531:62.4": "tvaṃ nu dāsoti maññasi.",
  "ja531:63.1": "Yassa usabhasahassāni,",
  "ja531:63.2": "sadā yojenti vīsatiṃ;",
  "ja531:63.3": "Okkākaputto bhaddante,",
  "ja531:63.4": "tvaṃ nu dāsoti maññasi.",
  "ja531:64.1": "Yassa dhenusahassāni,",
  "ja531:64.2": "sadā duhanti vīsatiṃ;",
  "ja531:64.3": "Okkākaputto bhaddante,",
  "ja531:64.4": "tvaṃ nu dāsoti maññasi”.",
  "ja531:65.1": "“Taggha te dukkaṭaṃ bāle,",
  "ja531:65.2": "yaṃ khattiyaṃ mahabbalaṃ;",
  "ja531:65.3": "Nāgaṃ maṇḍūkavaṇṇena,",
  "ja531:65.4": "na naṃ akkhāsidhāgataṃ”.",
  "ja531:66.1": "“Aparādhaṃ mahārāja,",
  "ja531:66.2": "tvaṃ no khama rathesabha;",
  "ja531:66.3": "Yaṃ taṃ aññātavesena,",
  "ja531:66.4": "nāññāsimhā idhāgataṃ”.",
  "ja531:67.1": "“Mādisassa na taṃ channaṃ,",
  "ja531:67.2": "yohaṃ āḷāriko bhave;",
  "ja531:67.3": "Tvaññeva me pasīdassu,",
  "ja531:67.4": "natthi te deva dukkaṭaṃ”.",
  "ja531:68.1": "“Gaccha bāle khamāpehi,",
  "ja531:68.2": "kusarājaṃ mahabbalaṃ;",
  "ja531:68.3": "Khamāpito kuso rājā,",
  "ja531:68.4": "so te dassati jīvitaṃ”.",
  "ja531:69.1": "“Pitussa vacanaṃ sutvā,",
  "ja531:69.2": "devavaṇṇī pabhāvatī;",
  "ja531:69.3": "Sirasā aggahī pāde,",
  "ja531:69.4": "kusarājaṃ mahabbalaṃ”.",
  "ja531:70.1": "“Yāmā ratyo atikkantā,",
  "ja531:70.2": "tāmā deva tayā vinā;",
  "ja531:70.3": "Vande te sirasā pāde,",
  "ja531:70.4": "mā me kujjha rathesabha.",
  "ja531:71.1": "Sabbaṃ te paṭijānāmi,",
  "ja531:71.2": "mahārāja suṇohi me;",
  "ja531:71.3": "Na cāpi appiyaṃ tuyhaṃ,",
  "ja531:71.4": "kareyyāmi ahaṃ puna.",
  "ja531:72.1": "Evañce yācamānāya,",
  "ja531:72.2": "vacanaṃ me na kāhasi;",
  "ja531:72.3": "Idāni maṃ tāto hantvā,",
  "ja531:72.4": "khattiyānaṃ padassati”.",
  "ja531:73.1": "“Evaṃ te yācamānāya,",
  "ja531:73.2": "kiṃ na kāhāmi te vaco;",
  "ja531:73.3": "Vikuddho tyasmi kalyāṇi,",
  "ja531:73.4": "mā tvaṃ bhāyi pabhāvati.",
  "ja531:74.1": "Sabbaṃ te paṭijānāmi,",
  "ja531:74.2": "rājaputti suṇohi me;",
  "ja531:74.3": "Na cāpi appiyaṃ tuyhaṃ,",
  "ja531:74.4": "kareyyāmi ahaṃ puna.",
  "ja531:75.1": "Tava kāmā hi sussoṇi,",
  "ja531:75.2": "pahu dukkhaṃ titikkhisaṃ;",
  "ja531:75.3": "Bahuṃ maddakulaṃ hantvā,",
  "ja531:75.4": "nayituṃ taṃ pabhāvati”.",
  "ja531:76.1": "“Yojayantu rathe asse,",
  "ja531:76.2": "nānācitte samāhite;",
  "ja531:76.3": "Atha dakkhatha me vegaṃ,",
  "ja531:76.4": "vidhamantassa sattavo”.",
  "ja531:77.1": "“Tañca tattha udikkhiṃsu,",
  "ja531:77.2": "rañño maddassantepure;",
  "ja531:77.3": "Vijambhamānaṃ sīhaṃva,",
  "ja531:77.4": "phoṭentaṃ diguṇaṃ bhujaṃ.",
  "ja531:78.1": "Hatthikkhandhañca āruyha,",
  "ja531:78.2": "āropetvā pabhāvatiṃ;",
  "ja531:78.3": "Saṅgāmaṃ otaritvāna,",
  "ja531:78.4": "sīhanādaṃ nadī kuso.",
  "ja531:79.1": "Tassa taṃ nadato sutvā,",
  "ja531:79.2": "sīhassevitare migā;",
  "ja531:79.3": "Khattiyā vipalāyiṃsu,",
  "ja531:79.4": "kusasaddabhayaṭṭitā.",
  "ja531:80.1": "Hatthārohā anīkaṭṭhā,",
  "ja531:80.2": "rathikā pattikārakā;",
  "ja531:80.3": "Aññamaññassa chindanti,",
  "ja531:80.4": "kusasaddabhayaṭṭitā.",
  "ja531:81.1": "Tasmiṃ saṅgāmasīsasmiṃ,",
  "ja531:81.2": "passitvā haṭṭhamānaso;",
  "ja531:81.3": "Kusassa rañño devindo,",
  "ja531:81.4": "adā verocanaṃ maṇiṃ.",
  "ja531:82.1": "So taṃ vijitvā saṅgāmaṃ,",
  "ja531:82.2": "laddhā verocanaṃ maṇiṃ;",
  "ja531:82.3": "Hatthikkhandhagato rājā,",
  "ja531:82.4": "pāvekkhi nagaraṃ puraṃ.",
  "ja531:83.1": "Jīvaggāhaṃ gahetvāna,",
  "ja531:83.2": "bandhitvā satta khattiye;",
  "ja531:83.3": "Sasurassupanāmesi,",
  "ja531:83.4": "ime te deva sattavo.",
  "ja531:84.1": "Sabbeva te vasaṃ gatā,",
  "ja531:84.2": "amittā vihatā tava;",
  "ja531:84.3": "Kāmaṃ karohi te tayā,",
  "ja531:84.4": "muñca vā te hanassu vā”.",
  "ja531:85.1": "“Tuyheva sattavo ete,",
  "ja531:85.2": "na hi te mayha sattavo;",
  "ja531:85.3": "Tvaññeva no mahārāja,",
  "ja531:85.4": "muñca vā te hanassu vā”.",
  "ja531:86.1": "“Imā te dhītaro satta,",
  "ja531:86.2": "devakaññūpamā subhā;",
  "ja531:86.3": "Dadāhi nesaṃ ekekaṃ,",
  "ja531:86.4": "hontu jāmātaro tava”.",
  "ja531:87.1": "“Amhākañceva tāsañca,",
  "ja531:87.2": "tvaṃ no sabbesamissaro;",
  "ja531:87.3": "Tvaññeva no mahārāja,",
  "ja531:87.4": "dehi nesaṃ yadicchasi”.",
  "ja531:88.1": "Ekamekassa ekekaṃ,",
  "ja531:88.2": "adā sīhassaro kuso;",
  "ja531:88.3": "Khattiyānaṃ tadā tesaṃ,",
  "ja531:88.4": "rañño maddassa dhītaro.",
  "ja531:89.1": "Pīṇitā tena lābhena,",
  "ja531:89.2": "tuṭṭhā sīhassare kuse;",
  "ja531:89.3": "Sakaraṭṭhāni pāyiṃsu,",
  "ja531:89.4": "khattiyā satta tāvade.",
  "ja531:90.1": "Pabhāvatiñca ādāya,",
  "ja531:90.2": "maṇiṃ verocanaṃ subhaṃ;",
  "ja531:90.3": "Kusāvatiṃ kuso rājā,",
  "ja531:90.4": "agamāsi mahabbalo.",
  "ja531:91.1": "Tyassu ekarathe yantā,",
  "ja531:91.2": "pavisantā kusāvatiṃ;",
  "ja531:91.3": "Samānā vaṇṇarūpena,",
  "ja531:91.4": "nāññamaññātirocisuṃ.",
  "ja531:92.1": "Mātā puttena saṅgacchi,",
  "ja531:92.2": "ubhayo ca jayampatī;",
  "ja531:92.3": "Samaggā te tadā āsuṃ,",
  "ja531:92.4": "phītaṃ dharaṇimāvasunti.",
  "ja531:93.1": "Kusajātakaṃ paṭhamaṃ."
}