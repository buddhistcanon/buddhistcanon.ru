{
  "ja318:0.1": "Jātaka",
  "ja318:0.2": "Catukkanipāta",
  "ja318:0.3": "Pucimandavagga",
  "ja318:0.4": "8. Kaṇaverajātaka",
  "ja318:1.1": "“Yaṃ taṃ vasantasamaye,",
  "ja318:1.2": "kaṇaveresu bhāṇusu;",
  "ja318:1.3": "Sāmaṃ bāhāya pīḷesi,",
  "ja318:1.4": "sā taṃ ārogyamabravi”.",
  "ja318:2.1": "“Ambho na kira saddheyyaṃ,",
  "ja318:2.2": "yaṃ vāto pabbataṃ vahe;",
  "ja318:2.3": "Pabbatañce vahe vāto,",
  "ja318:2.4": "sabbampi pathaviṃ vahe;",
  "ja318:2.5": "Yattha sāmā kālakatā,",
  "ja318:2.6": "sā maṃ ārogyamabravi”.",
  "ja318:3.1": "“Na ceva sā kālakatā,",
  "ja318:3.2": "na ca sā aññamicchati;",
  "ja318:3.3": "Ekabhattikinī sāmā,",
  "ja318:3.4": "tameva abhikaṅkhati”.",
  "ja318:4.1": "“Asanthutaṃ maṃ cirasanthutena,",
  "ja318:4.2": "Nimīni sāmā adhuvaṃ dhuvena;",
  "ja318:4.3": "Mayāpi sāmā nimineyya aññaṃ,",
  "ja318:4.4": "Ito ahaṃ dūrataraṃ gamissan”ti.",
  "ja318:5.1": "Kaṇaverajātakaṃ aṭṭhamaṃ."
}