{
  "ja136:0.1": "Jātaka",
  "ja136:0.2": "Ekakanipāta",
  "ja136:0.3": "Asampadānavagga",
  "ja136:0.4": "6. Suvaṇṇahaṃsajātaka",
  "ja136:1.1": "“Yaṃ laddhaṃ tena tuṭṭhabbaṃ,",
  "ja136:1.2": "Atilobho hi pāpako;",
  "ja136:1.3": "Haṃsarājaṃ gahetvāna,",
  "ja136:1.4": "Suvaṇṇā parihāyathā”ti.",
  "ja136:2.1": "Suvaṇṇahaṃsajātakaṃ chaṭṭhaṃ."
}