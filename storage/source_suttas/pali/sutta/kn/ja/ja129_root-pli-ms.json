{
  "ja129:0.1": "Jātaka",
  "ja129:0.2": "Ekakanipāta",
  "ja129:0.3": "Kusanāḷivagga",
  "ja129:0.4": "9. Aggikabhāradvājajātaka",
  "ja129:1.1": "“Nāyaṃ sikhā puññahetu,",
  "ja129:1.2": "ghāsahetu ayaṃ sikhā;",
  "ja129:1.3": "Nāguṭṭhigaṇanaṃ yāti,",
  "ja129:1.4": "alaṃ te hotu aggikā”ti.",
  "ja129:2.1": "Aggikabhāradvājajātakaṃ navamaṃ."
}