{
  "ja222:0.1": "Jātaka",
  "ja222:0.2": "Dukanipāta",
  "ja222:0.3": "Kāsāvavagga",
  "ja222:0.4": "2. Cūḷanandiyajātaka",
  "ja222:1.1": "“Idaṃ tadācariyavaco,",
  "ja222:1.2": "pārāsariyo yadabravi;",
  "ja222:1.3": "Māsu tvaṃ akari pāpaṃ,",
  "ja222:1.4": "yaṃ tvaṃ pacchā kataṃ tape.",
  "ja222:2.1": "Yāni karoti puriso,",
  "ja222:2.2": "tāni attani passati;",
  "ja222:2.3": "Kalyāṇakārī kalyāṇaṃ,",
  "ja222:2.4": "pāpakārī ca pāpakaṃ;",
  "ja222:2.5": "Yādisaṃ vapate bījaṃ,",
  "ja222:2.6": "tādisaṃ harate phalan”ti.",
  "ja222:3.1": "Cūḷanandiyajātakaṃ dutiyaṃ."
}