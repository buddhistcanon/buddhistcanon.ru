{
  "ja260:0.1": "Jātaka",
  "ja260:0.2": "Tikanipāta",
  "ja260:0.3": "Saṅkappavagga",
  "ja260:0.4": "10. Dūtajātaka",
  "ja260:1.1": "“Yassatthā dūramāyanti,",
  "ja260:1.2": "amittamapi yācituṃ;",
  "ja260:1.3": "Tassūdarassahaṃ dūto,",
  "ja260:1.4": "mā me kujjha rathesabha”.",
  "ja260:2.1": "“Yassa divā ca ratto ca,",
  "ja260:2.2": "vasamāyanti māṇavā;",
  "ja260:2.3": "Tassūdarassahaṃ dūto,",
  "ja260:2.4": "mā me kujjha rathesabha.",
  "ja260:3.1": "Dadāmi te brāhmaṇa rohiṇīnaṃ,",
  "ja260:3.2": "Gavaṃ sahassaṃ saha puṅgavena;",
  "ja260:3.3": "Dūto hi dūtassa kathaṃ na dajjaṃ,",
  "ja260:3.4": "Mayampi tasseva bhavāma dūtā”ti.",
  "ja260:4.1": "Dūtajātakaṃ dasamaṃ.",
  "ja260:4.2": "Saṅkappavaggo paṭhamo.",
  "ja260:5.1": "Tassuddānaṃ",
  "ja260:6.1": "Usukāravaro tilamuṭṭhi maṇi,",
  "ja260:6.2": "Hayarāja vihaṅgama āsiviso;",
  "ja260:6.3": "Janasandha kahāpaṇavassa puna,",
  "ja260:6.4": "Tiriṭaṃ puna dūtavarena dasāti."
}