{
  "ja377:0.1": "Jātaka",
  "ja377:0.2": "Chakkanipāta",
  "ja377:0.3": "Avāriyavagga",
  "ja377:0.4": "2. Setaketujātaka",
  "ja377:1.1": "“Mā tāta kujjhi na hi sādhu kodho,",
  "ja377:1.2": "Bahumpi te adiṭṭhamassutañca;",
  "ja377:1.3": "Mātā pitā disatā setaketu,",
  "ja377:1.4": "Ācariyamāhu disataṃ pasatthā.",
  "ja377:2.1": "Agārino annadapānavatthadā,",
  "ja377:2.2": "Avhāyikā tampi disaṃ vadanti;",
  "ja377:2.3": "Esā disā paramā setaketu,",
  "ja377:2.4": "Yaṃ patvā dukkhī sukhino bhavanti”.",
  "ja377:3.1": "“Kharājinā jaṭilā paṅkadantā,",
  "ja377:3.2": "Dummakkharūpā yeme jappanti mante;",
  "ja377:3.3": "Kacci nu te mānusake payoge,",
  "ja377:3.4": "Idaṃ vidū parimuttā apāyā”.",
  "ja377:4.1": "“Pāpāni kammāni katvāna rāja,",
  "ja377:4.2": "Bahussuto ce na careyya dhammaṃ;",
  "ja377:4.3": "Sahassavedopi na taṃ paṭicca,",
  "ja377:4.4": "Dukkhā pamucce caraṇaṃ apatvā”.",
  "ja377:5.1": "“Sahassavedopi na taṃ paṭicca,",
  "ja377:5.2": "Dukkhā pamucce caraṇaṃ apatvā;",
  "ja377:5.3": "Maññāmi vedā aphalā bhavanti,",
  "ja377:5.4": "Sasaṃyamaṃ caraṇameva saccaṃ”.",
  "ja377:6.1": "“Na heva vedā aphalā bhavanti,",
  "ja377:6.2": "Sasaṃyamaṃ caraṇameva saccaṃ;",
  "ja377:6.3": "Kittiñhi pappoti adhicca vede,",
  "ja377:6.4": "Santiṃ puṇeti caraṇena danto”ti.",
  "ja377:7.1": "Setaketujātakaṃ dutiyaṃ."
}