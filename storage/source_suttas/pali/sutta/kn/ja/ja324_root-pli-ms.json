{
  "ja324:0.1": "Jātaka",
  "ja324:0.2": "Catukkanipāta",
  "ja324:0.3": "Kuṭidūsakavagga",
  "ja324:0.4": "4. Cammasāṭakajātaka",
  "ja324:1.1": "“Kalyāṇarūpo vatayaṃ catuppado,",
  "ja324:1.2": "Subhaddako ceva supesalo ca;",
  "ja324:1.3": "Yo brāhmaṇaṃ jātimantūpapannaṃ,",
  "ja324:1.4": "Apacāyati meṇḍavaro yasassī”.",
  "ja324:2.1": "“Mā brāhmaṇa ittaradassanena,",
  "ja324:2.2": "Vissāsamāpajji catuppadassa;",
  "ja324:2.3": "Daḷhappahāraṃ abhikaṅkhamāno,",
  "ja324:2.4": "Avasakkatī dassati suppahāraṃ”.",
  "ja324:3.1": "Ūruṭṭhi bhaggaṃ pavaṭṭito khāribhāro,",
  "ja324:3.2": "Sabbañca bhaṇḍaṃ brāhmaṇassa bhinnaṃ;",
  "ja324:3.3": "Ubhopi bāhā paggayha kandati,",
  "ja324:3.4": "“Abhidhāvatha haññate brahmacārī.",
  "ja324:4.1": "Evaṃ so nihato seti,",
  "ja324:4.2": "yo apūjaṃ pasaṃsati;",
  "ja324:4.3": "Yathāhamajja pahato,",
  "ja324:4.4": "hato meṇḍena dummatī”ti.",
  "ja324:5.1": "Cammasāṭakajātakaṃ catutthaṃ."
}