{
  "ja94:0.1": "Jātaka",
  "ja94:0.2": "Ekakanipāta",
  "ja94:0.3": "Littavagga",
  "ja94:0.4": "4. Lomahaṃsajātaka",
  "ja94:1.1": "Sotatto sosindo ceva,",
  "ja94:1.2": "eko bhiṃsanake vane;",
  "ja94:1.3": "Naggo na caggimāsīno,",
  "ja94:1.4": "esanāpasuto munī”ti.",
  "ja94:2.1": "Lomahaṃsajātakaṃ catutthaṃ."
}