{
  "ja90:0.1": "Jātaka",
  "ja90:0.2": "Ekakanipāta",
  "ja90:0.3": "Apāyimhavagga",
  "ja90:0.4": "10. Akataññujātaka",
  "ja90:1.1": "“Yo pubbe katakalyāṇo,",
  "ja90:1.2": "katattho nāvabujjhati;",
  "ja90:1.3": "Pacchā kicce samuppanne,",
  "ja90:1.4": "kattāraṃ nādhigacchatī”ti.",
  "ja90:2.1": "Akataññujātakaṃ dasamaṃ.",
  "ja90:2.2": "Apāyimhavaggo navamo.",
  "ja90:3.1": "Tassuddānaṃ",
  "ja90:4.1": "Apāyimha ca dūbhaka sattapadaṃ,",
  "ja90:4.2": "Chaḷadvara ca āyatinā ca puna;",
  "ja90:4.3": "Ahisīlava maṅgali pāpikassā,",
  "ja90:4.4": "Sataṃnikkha katatthavarena dasāti."
}