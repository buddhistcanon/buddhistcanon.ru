{
  "ja252:0.1": "Jātaka",
  "ja252:0.2": "Tikanipāta",
  "ja252:0.3": "Saṅkappavagga",
  "ja252:0.4": "2. Tilamuṭṭhijātaka",
  "ja252:1.1": "“Ajjāpi me taṃ manasi,",
  "ja252:1.2": "yaṃ maṃ tvaṃ tilamuṭṭhiyā;",
  "ja252:1.3": "Bāhāya maṃ gahetvāna,",
  "ja252:1.4": "laṭṭhiyā anutāḷayi.",
  "ja252:2.1": "Nanu jīvite na ramasi,",
  "ja252:2.2": "yenāsi brāhmaṇāgato;",
  "ja252:2.3": "Yaṃ maṃ bāhā gahetvāna,",
  "ja252:2.4": "tikkhattuṃ anutāḷayi”.",
  "ja252:3.1": "“Ariyo anariyaṃ kubbantaṃ,",
  "ja252:3.2": "Yo daṇḍena nisedhati;",
  "ja252:3.3": "Sāsanaṃ taṃ na taṃ veraṃ,",
  "ja252:3.4": "Iti naṃ paṇḍitā vidū”ti.",
  "ja252:4.1": "Tilamuṭṭhijātakaṃ dutiyaṃ."
}