{
  "ja173:0.1": "Jātaka",
  "ja173:0.2": "Dukanipāta",
  "ja173:0.3": "Kalyāṇavagga",
  "ja173:0.4": "3. Makkaṭajātaka",
  "ja173:1.1": "“Tāta māṇavako eso,",
  "ja173:1.2": "tālamūlaṃ apassito;",
  "ja173:1.3": "Agārakañcidaṃ atthi,",
  "ja173:1.4": "handa demassagārakaṃ”.",
  "ja173:2.1": "“Mā kho tvaṃ tāta pakkosi,",
  "ja173:2.2": "dūseyya no agārakaṃ;",
  "ja173:2.3": "Netādisaṃ mukhaṃ hoti,",
  "ja173:2.4": "brāhmaṇassa susīlino”ti.",
  "ja173:3.1": "Makkaṭajātakaṃ tatiyaṃ."
}