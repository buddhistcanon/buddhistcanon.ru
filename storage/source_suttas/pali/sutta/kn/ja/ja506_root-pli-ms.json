{
  "ja506:0.1": "Jātaka",
  "ja506:0.2": "Vīsatinipāta",
  "ja506:0.3": "Mātaṅgavagga",
  "ja506:0.4": "10. Campeyyajātaka",
  "ja506:1.1": "“Kā nu vijjurivābhāsi,",
  "ja506:1.2": "Osadhī viya tārakā;",
  "ja506:1.3": "Devatā nusi gandhabbī,",
  "ja506:1.4": "Na taṃ maññāmi mānusiṃ”.",
  "ja506:2.1": "“Namhi devī na gandhabbī,",
  "ja506:2.2": "na mahārāja mānusī;",
  "ja506:2.3": "Nāgakaññāsmi bhaddante,",
  "ja506:2.4": "atthenamhi idhāgatā”.",
  "ja506:3.1": "“Vibbhantacittā kupitindriyāsi,",
  "ja506:3.2": "Nettehi te vārigaṇā savanti;",
  "ja506:3.3": "Kiṃ te naṭṭhaṃ kiṃ pana patthayānā,",
  "ja506:3.4": "Idhāgatā nāri tadiṅgha brūhi”.",
  "ja506:4.1": "“Yamuggatejo uragoti cāhu,",
  "ja506:4.2": "Nāgoti naṃ āhu janā janinda;",
  "ja506:4.3": "Tamaggahī puriso jīvikattho,",
  "ja506:4.4": "Taṃ bandhanā muñca patī mameso”.",
  "ja506:5.1": "“Kathaṃ nvayaṃ balaviriyūpapanno,",
  "ja506:5.2": "Hatthattamāgacchi vanibbakassa;",
  "ja506:5.3": "Akkhāhi me nāgakaññe tamatthaṃ,",
  "ja506:5.4": "Kathaṃ vijānemu gahītanāgaṃ”.",
  "ja506:6.1": "“Nagarampi nāgo bhasmaṃ kareyya,",
  "ja506:6.2": "Tathā hi so balaviriyūpapanno;",
  "ja506:6.3": "Dhammañca nāgo apacāyamāno,",
  "ja506:6.4": "Tasmā parakkamma tapo karoti.",
  "ja506:7.1": "Cātuddasiṃ pañcadasiñca rāja,",
  "ja506:7.2": "Catuppathe sammati nāgarājā;",
  "ja506:7.3": "Tamaggahī puriso jīvikattho,",
  "ja506:7.4": "Taṃ bandhanā muñca patī mameso”.",
  "ja506:8.1": "“Soḷasitthisahassāni,",
  "ja506:8.2": "āmuttamaṇikuṇḍalā;",
  "ja506:8.3": "Vārigehasayā nārī,",
  "ja506:8.4": "tāpi taṃ saraṇaṃ gatā.",
  "ja506:9.1": "Dhammena mocehi asāhasena,",
  "ja506:9.2": "Gāmena nikkhena gavaṃ satena;",
  "ja506:9.3": "Ossaṭṭhakāyo urago carātu,",
  "ja506:9.4": "Puññatthiko muñcatu bandhanasmā”.",
  "ja506:10.1": "“Dhammena mocemi asāhasena,",
  "ja506:10.2": "Gāmena nikkhena gavaṃ satena;",
  "ja506:10.3": "Ossaṭṭhakāyo urago carātu,",
  "ja506:10.4": "Puññatthiko muñcatu bandhanasmā.",
  "ja506:11.1": "Dammi nikkhasataṃ ludda,",
  "ja506:11.2": "thūlañca maṇikuṇḍalaṃ;",
  "ja506:11.3": "Catussadañca pallaṅkaṃ,",
  "ja506:11.4": "umāpupphasarinnibhaṃ.",
  "ja506:12.1": "Dve ca sādisiyo bhariyā,",
  "ja506:12.2": "Usabhañca gavaṃ sataṃ;",
  "ja506:12.3": "Ossaṭṭhakāyo urago carātu,",
  "ja506:12.4": "Puññatthiko muñcatu bandhanasmā”.",
  "ja506:13.1": "“Vināpi dānā tava vacanaṃ janinda,",
  "ja506:13.2": "Muñcemu naṃ uragaṃ bandhanasmā;",
  "ja506:13.3": "Ossaṭṭhakāyo urago carātu,",
  "ja506:13.4": "Puññatthiko muñcatu bandhanasmā”.",
  "ja506:14.1": "Mutto campeyyako nāgo,",
  "ja506:14.2": "rājānaṃ etadabravi;",
  "ja506:14.3": "“Namo te kāsirājatthu,",
  "ja506:14.4": "namo te kāsivaḍḍhana;",
  "ja506:14.5": "Añjaliṃ te paggaṇhāmi,",
  "ja506:14.6": "passeyyaṃ me nivesanaṃ”.",
  "ja506:15.1": "“Addhā hi dubbissasametamāhu,",
  "ja506:15.2": "Yaṃ mānuso vissase amānusamhi;",
  "ja506:15.3": "Sace ca maṃ yācasi etamatthaṃ,",
  "ja506:15.4": "Dakkhemu te nāga nivesanāni”.",
  "ja506:16.1": "“Sacepi vāto girimāvaheyya,",
  "ja506:16.2": "Cando ca suriyo ca chamā pateyyuṃ;",
  "ja506:16.3": "Sabbā ca najjo paṭisotaṃ vajeyyuṃ,",
  "ja506:16.4": "Na tvevahaṃ rāja musā bhaṇeyyaṃ.",
  "ja506:17.1": "Nabhaṃ phaleyya udadhīpi susse,",
  "ja506:17.2": "Saṃvaṭṭaye bhūtadharā vasundharā;",
  "ja506:17.3": "Siluccayo meru samūlamuppate,",
  "ja506:17.4": "Na tvevahaṃ rāja musā bhaṇeyyaṃ”.",
  "ja506:18.1": "“Addhā hi dubbissasametamāhu,",
  "ja506:18.2": "Yaṃ mānuso vissase amānusamhi;",
  "ja506:18.3": "Sace ca maṃ yācasi etamatthaṃ,",
  "ja506:18.4": "Dakkhemu te nāga nivesanāni.",
  "ja506:19.1": "Tumhe khottha ghoravisā uḷārā,",
  "ja506:19.2": "Mahātejā khippakopī ca hotha;",
  "ja506:19.3": "Maṃkāraṇā bandhanasmā pamutto,",
  "ja506:19.4": "Arahasi no jānituye katāni”.",
  "ja506:20.1": "“So paccataṃ niraye ghorarūpe,",
  "ja506:20.2": "Mā kāyikaṃ sātamalattha kiñci;",
  "ja506:20.3": "Peḷāya baddho maraṇaṃ upetu,",
  "ja506:20.4": "Yo tādisaṃ kammakataṃ na jāne”.",
  "ja506:21.1": "“Saccappaṭiññā tava mesa hotu,",
  "ja506:21.2": "Akkodhano hohi anupanāhī;",
  "ja506:21.3": "Sabbañca te nāgakulaṃ supaṇṇā,",
  "ja506:21.4": "Aggiṃva gimhesu vivajjayantu”.",
  "ja506:22.1": "“Anukampasī nāgakulaṃ janinda,",
  "ja506:22.2": "Mātā yathā suppiyaṃ ekaputtaṃ;",
  "ja506:22.3": "Ahañca te nāgakulena saddhiṃ,",
  "ja506:22.4": "Kāhāmi veyyāvaṭikaṃ uḷāraṃ”.",
  "ja506:23.1": "“Yojentu ve rājarathe sucitte,",
  "ja506:23.2": "Kambojake assatare sudante;",
  "ja506:23.3": "Nāge ca yojentu suvaṇṇakappane,",
  "ja506:23.4": "Dakkhemu nāgassa nivesanāni”.",
  "ja506:24.1": "Bherī mudiṅgā paṇavā ca saṅkhā,",
  "ja506:24.2": "Avajjayiṃsu uggasenassa rañño;",
  "ja506:24.3": "Pāyāsi rājā bahusobhamāno,",
  "ja506:24.4": "Purakkhato nārigaṇassa majjhe.",
  "ja506:25.1": "Suvaṇṇacitakaṃ bhūmiṃ,",
  "ja506:25.2": "addakkhi kāsivaḍḍhano;",
  "ja506:25.3": "Sovaṇṇamayapāsāde,",
  "ja506:25.4": "veḷuriyaphalakatthate.",
  "ja506:26.1": "Sa rājā pāvisi byamhaṃ,",
  "ja506:26.2": "campeyyassa nivesanaṃ;",
  "ja506:26.3": "Ādiccavaṇṇasannibhaṃ,",
  "ja506:26.4": "kaṃsavijju pabhassaraṃ.",
  "ja506:27.1": "Nānārukkhehi sañchannaṃ,",
  "ja506:27.2": "nānāgandhasamīritaṃ;",
  "ja506:27.3": "So pāvekkhi kāsirājā,",
  "ja506:27.4": "campeyyassa nivesanaṃ.",
  "ja506:28.1": "Paviṭṭhasmiṃ kāsiraññe,",
  "ja506:28.2": "campeyyassa nivesanaṃ;",
  "ja506:28.3": "Dibbā tūriyā pavajjiṃsu,",
  "ja506:28.4": "nāgakaññā ca naccisuṃ.",
  "ja506:29.1": "Taṃ nāgakaññā caritaṃ gaṇena,",
  "ja506:29.2": "Anvāruhī kāsirājā pasanno;",
  "ja506:29.3": "Nisīdi sovaṇṇamayamhi pīṭhe,",
  "ja506:29.4": "Sāpassaye candanasāralitte.",
  "ja506:30.1": "So tattha bhutvā ca atho ramitvā,",
  "ja506:30.2": "Campeyyakaṃ kāsirājā avoca;",
  "ja506:30.3": "“Vimānaseṭṭhāni imāni tuyhaṃ,",
  "ja506:30.4": "Ādiccavaṇṇāni pabhassarāni;",
  "ja506:30.5": "Netādisaṃ atthi manussaloke,",
  "ja506:30.6": "Kiṃ patthayaṃ nāga tapo karosi.",
  "ja506:31.1": "Tā kambukāyūradharā suvatthā,",
  "ja506:31.2": "Vaṭṭaṅgulī tambatalūpapannā;",
  "ja506:31.3": "Paggayha pāyenti anomavaṇṇā,",
  "ja506:31.4": "Netādisaṃ atthi manussaloke;",
  "ja506:31.5": "Kiṃ patthayaṃ nāga tapo karosi.",
  "ja506:32.1": "Najjo ca temā puthulomamacchā,",
  "ja506:32.2": "Āṭā sakuntābhirudā sutitthā;",
  "ja506:32.3": "Netādisaṃ atthi manussaloke,",
  "ja506:32.4": "Kiṃ patthayaṃ nāga tapo karosi.",
  "ja506:33.1": "Koñcā mayūrā diviyā ca haṃsā,",
  "ja506:33.2": "Vaggussarā kokilā sampatanti;",
  "ja506:33.3": "Netādisaṃ atthi manussaloke,",
  "ja506:33.4": "Kiṃ patthayaṃ nāga tapo karosi.",
  "ja506:34.1": "Ambā ca sālā tilakā ca jambuyo,",
  "ja506:34.2": "Uddālakā pāṭaliyo ca phullā;",
  "ja506:34.3": "Netādisaṃ atthi manussaloke,",
  "ja506:34.4": "Kiṃ patthayaṃ nāga tapo karosi.",
  "ja506:35.1": "Imā ca te pokkharañño samantato,",
  "ja506:35.2": "Dibbā ca gandhā satataṃ pavāyanti;",
  "ja506:35.3": "Netādisaṃ atthi manussaloke,",
  "ja506:35.4": "Kiṃ patthayaṃ nāga tapo karosi”.",
  "ja506:36.1": "“Na puttahetu na dhanassa hetu,",
  "ja506:36.2": "Na āyuno cāpi janinda hetu;",
  "ja506:36.3": "Manussayoniṃ abhipatthayāno,",
  "ja506:36.4": "Tasmā parakkamma tapo karomi”.",
  "ja506:37.1": "“Tvaṃ lohitakkho vihatantaraṃso,",
  "ja506:37.2": "Alaṅkato kappitakesamassu;",
  "ja506:37.3": "Surosito lohitacandanena,",
  "ja506:37.4": "Gandhabbarājāva disā pabhāsasi.",
  "ja506:38.1": "Deviddhipattosi mahānubhāvo,",
  "ja506:38.2": "Sabbehi kāmehi samaṅgibhūto;",
  "ja506:38.3": "Pucchāmi taṃ nāgarājetamatthaṃ,",
  "ja506:38.4": "Seyyo ito kena manussaloko”.",
  "ja506:39.1": "“Janinda nāññatra manussalokā,",
  "ja506:39.2": "Suddhī va saṃvijjati saṃyamo vā;",
  "ja506:39.3": "Ahañca laddhāna manussayoniṃ,",
  "ja506:39.4": "Kāhāmi jātimaraṇassa antaṃ”.",
  "ja506:40.1": "“Addhā have sevitabbā sapaññā,",
  "ja506:40.2": "Bahussutā ye bahuṭhānacintino;",
  "ja506:40.3": "Nāriyo ca disvāna tuvañca nāga,",
  "ja506:40.4": "Kāhāmi puññāni anappakāni”.",
  "ja506:41.1": "“Addhā have sevitabbā sapaññā,",
  "ja506:41.2": "Bahussutā ye bahuṭhānacintino;",
  "ja506:41.3": "Nāriyo ca disvāna mamañca rāja,",
  "ja506:41.4": "Karohi puññāni anappakāni”.",
  "ja506:42.1": "“Idañca me jātarūpaṃ pahūtaṃ,",
  "ja506:42.2": "Rāsī suvaṇṇassa ca tālamattā;",
  "ja506:42.3": "Ito haritvāna suvaṇṇagharāni,",
  "ja506:42.4": "Karassu rūpiyapākāraṃ karontu.",
  "ja506:43.1": "Muttā ca vāhasahassāni pañca,",
  "ja506:43.2": "Veḷuriyamissāni ito haritvā;",
  "ja506:43.3": "Antepure bhūmiyaṃ santharantu,",
  "ja506:43.4": "Nikkaddamā hehiti nīrajā ca.",
  "ja506:44.1": "Etādisaṃ āvasa rājaseṭṭha,",
  "ja506:44.2": "Vimānaseṭṭhaṃ bahu sobhamānaṃ;",
  "ja506:44.3": "Bārāṇasiṃ nagaraṃ iddhaṃ phītaṃ,",
  "ja506:44.4": "Rajjañca kārehi anomapaññā”ti.",
  "ja506:45.1": "Campeyyajātakaṃ dasamaṃ."
}