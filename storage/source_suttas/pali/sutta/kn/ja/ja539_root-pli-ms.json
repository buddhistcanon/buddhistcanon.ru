{
  "ja539:0.1": "Jātaka",
  "ja539:0.2": "Mahānipāta",
  "ja539:0.3": "Mūgapakkhavagga",
  "ja539:0.4": "2. Mahājanakajātaka",
  "ja539:1.1": "“Koyaṃ majjhe samuddasmiṃ,",
  "ja539:1.2": "apassaṃ tīramāyuhe;",
  "ja539:1.3": "Kaṃ tvaṃ atthavasaṃ ñatvā,",
  "ja539:1.4": "evaṃ vāyamase bhusaṃ”.",
  "ja539:2.1": "“Nisamma vattaṃ lokassa,",
  "ja539:2.2": "vāyāmassa ca devate;",
  "ja539:2.3": "Tasmā majjhe samuddasmiṃ,",
  "ja539:2.4": "apassaṃ tīramāyuhe”.",
  "ja539:3.1": "“Gambhīre appameyyasmiṃ,",
  "ja539:3.2": "tīraṃ yassa na dissati;",
  "ja539:3.3": "Mogho te purisavāyāmo,",
  "ja539:3.4": "appatvāva marissasi”.",
  "ja539:4.1": "“Anaṇo ñātinaṃ hoti,",
  "ja539:4.2": "devānaṃ pitunañca so;",
  "ja539:4.3": "Karaṃ purisakiccāni,",
  "ja539:4.4": "na ca pacchānutappati”.",
  "ja539:5.1": "“Apāraneyyaṃ yaṃ kammaṃ,",
  "ja539:5.2": "Aphalaṃ kilamathuddayaṃ;",
  "ja539:5.3": "Tattha ko vāyamenattho,",
  "ja539:5.4": "Maccu yassābhinippataṃ”.",
  "ja539:6.1": "“Apāraneyyamaccantaṃ,",
  "ja539:6.2": "yo viditvāna devate;",
  "ja539:6.3": "Na rakkhe attano pāṇaṃ,",
  "ja539:6.4": "jaññā so yadi hāpaye.",
  "ja539:7.1": "Adhippāyaphalaṃ eke,",
  "ja539:7.2": "asmiṃ lokasmi devate;",
  "ja539:7.3": "Payojayanti kammāni,",
  "ja539:7.4": "tāni ijjhanti vā na vā.",
  "ja539:8.1": "Sandiṭṭhikaṃ kammaphalaṃ,",
  "ja539:8.2": "nanu passasi devate;",
  "ja539:8.3": "Sannā aññe tarāmahaṃ,",
  "ja539:8.4": "tañca passāmi santike.",
  "ja539:9.1": "So ahaṃ vāyamissāmi,",
  "ja539:9.2": "yathāsatti yathābalaṃ;",
  "ja539:9.3": "Gacchaṃ pāraṃ samuddassa,",
  "ja539:9.4": "kassaṃ purisakāriyaṃ”.",
  "ja539:10.1": "“Yo tvaṃ evaṃ gate oghe,",
  "ja539:10.2": "appameyye mahaṇṇave;",
  "ja539:10.3": "Dhammavāyāmasampanno,",
  "ja539:10.4": "kammunā nāvasīdasi;",
  "ja539:10.5": "So tvaṃ tattheva gacchāhi,",
  "ja539:10.6": "yattha te nirato mano.",
  "ja539:11.1": "Suriyuggamane nidhi,",
  "ja539:11.2": "atho oggamane nidhi;",
  "ja539:11.3": "Anto nidhi bahi nidhi,",
  "ja539:11.4": "na anto na bahi nidhi.",
  "ja539:12.1": "Ārohane mahānidhi,",
  "ja539:12.2": "atho orohane nidhi;",
  "ja539:12.3": "Catūsu mahāsālesu,",
  "ja539:12.4": "samantā yojane nidhi.",
  "ja539:13.1": "Dantaggesu mahānidhi,",
  "ja539:13.2": "bālaggesu ca kepuke;",
  "ja539:13.3": "Rukkhaggesu mahānidhi,",
  "ja539:13.4": "soḷasete mahānidhī;",
  "ja539:13.5": "Sahassathāmo pallaṅko,",
  "ja539:13.6": "sīvalārādhanena ca.",
  "ja539:14.1": "Āsīsetheva puriso,",
  "ja539:14.2": "na nibbindeyya paṇḍito;",
  "ja539:14.3": "Passāmi vohaṃ attānaṃ,",
  "ja539:14.4": "yathā icchiṃ tathā ahu.",
  "ja539:15.1": "Āsīsetheva puriso,",
  "ja539:15.2": "na nibbindeyya paṇḍito;",
  "ja539:15.3": "Passāmi vohaṃ attānaṃ,",
  "ja539:15.4": "udakā thalamubbhataṃ.",
  "ja539:16.1": "Vāyametheva puriso,",
  "ja539:16.2": "na nibbindeyya paṇḍito;",
  "ja539:16.3": "Passāmi vohaṃ attānaṃ,",
  "ja539:16.4": "yathā icchiṃ tathā ahu.",
  "ja539:17.1": "Vāyametheva puriso,",
  "ja539:17.2": "na nibbindeyya paṇḍito;",
  "ja539:17.3": "Passāmi vohaṃ attānaṃ,",
  "ja539:17.4": "udakā thalamubbhataṃ.",
  "ja539:18.1": "Dukkhūpanītopi naro sapañño,",
  "ja539:18.2": "Āsaṃ na chindeyya sukhāgamāya;",
  "ja539:18.3": "Bahū hi phassā ahitā hitā ca,",
  "ja539:18.4": "Avitakkitā maccumupabbajanti.",
  "ja539:19.1": "Acintitampi bhavati,",
  "ja539:19.2": "cintitampi vinassati;",
  "ja539:19.3": "Na hi cintāmayā bhogā,",
  "ja539:19.4": "itthiyā purisassa vā”.",
  "ja539:20.1": "“Aporāṇaṃ vata bho rājā,",
  "ja539:20.2": "sabbabhummo disampati;",
  "ja539:20.3": "Nājja nacce nisāmeti,",
  "ja539:20.4": "na gīte kurute mano.",
  "ja539:21.1": "Na mige napi uyyāne,",
  "ja539:21.2": "napi haṃse udikkhati;",
  "ja539:21.3": "Mūgova tuṇhimāsīno,",
  "ja539:21.4": "na atthamanusāsati”.",
  "ja539:22.1": "“Sukhakāmā rahosīlā,",
  "ja539:22.2": "vadhabandhā upāratā;",
  "ja539:22.3": "Kassa nu ajja ārāme,",
  "ja539:22.4": "daharā vuddhā ca acchare.",
  "ja539:23.1": "Atikkantavanathā dhīrā,",
  "ja539:23.2": "namo tesaṃ mahesinaṃ;",
  "ja539:23.3": "Ye ussukamhi lokamhi,",
  "ja539:23.4": "viharanti manussukā.",
  "ja539:24.1": "Te chetvā maccuno jālaṃ,",
  "ja539:24.2": "tataṃ māyāvino daḷaṃ;",
  "ja539:24.3": "Chinnālayattā gacchanti,",
  "ja539:24.4": "ko tesaṃ gatimāpaye.",
  "ja539:25.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:25.2": "vibhattaṃ bhāgaso mitaṃ;",
  "ja539:25.3": "Pahāya pabbajissāmi,",
  "ja539:25.4": "taṃ kudāssu bhavissati.",
  "ja539:26.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:26.2": "visālaṃ sabbatopabhaṃ;",
  "ja539:26.3": "Pahāya pabbajissāmi,",
  "ja539:26.4": "taṃ kudāssu bhavissati.",
  "ja539:27.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:27.2": "bahupākāratoraṇaṃ;",
  "ja539:27.3": "Pahāya pabbajissāmi,",
  "ja539:27.4": "taṃ kudāssu bhavissati.",
  "ja539:28.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:28.2": "daḷhamaṭṭālakoṭṭhakaṃ;",
  "ja539:28.3": "Pahāya pabbajissāmi,",
  "ja539:28.4": "taṃ kudāssu bhavissati.",
  "ja539:29.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:29.2": "suvibhattaṃ mahāpathaṃ;",
  "ja539:29.3": "Pahāya pabbajissāmi,",
  "ja539:29.4": "taṃ kudāssu bhavissati.",
  "ja539:30.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:30.2": "suvibhattantarāpaṇaṃ;",
  "ja539:30.3": "Pahāya pabbajissāmi,",
  "ja539:30.4": "taṃ kudāssu bhavissati.",
  "ja539:31.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:31.2": "gavassarathapīḷitaṃ;",
  "ja539:31.3": "Pahāya pabbajissāmi,",
  "ja539:31.4": "taṃ kudāssu bhavissati.",
  "ja539:32.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:32.2": "ārāmavanamāliniṃ;",
  "ja539:32.3": "Pahāya pabbajissāmi,",
  "ja539:32.4": "taṃ kudāssu bhavissati.",
  "ja539:33.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:33.2": "uyyānavanamāliniṃ;",
  "ja539:33.3": "Pahāya pabbajissāmi,",
  "ja539:33.4": "taṃ kudāssu bhavissati.",
  "ja539:34.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:34.2": "pāsādavanamāliniṃ;",
  "ja539:34.3": "Pahāya pabbajissāmi,",
  "ja539:34.4": "taṃ kudāssu bhavissati.",
  "ja539:35.1": "Kadāhaṃ mithilaṃ phītaṃ,",
  "ja539:35.2": "tipuraṃ rājabandhuniṃ;",
  "ja539:35.3": "Māpitaṃ somanassena,",
  "ja539:35.4": "vedehena yasassinā;",
  "ja539:35.5": "Pahāya pabbajissāmi,",
  "ja539:35.6": "taṃ kudāssu bhavissati.",
  "ja539:36.1": "Kadāhaṃ vedehe phīte,",
  "ja539:36.2": "nicite dhammarakkhite;",
  "ja539:36.3": "Pahāya pabbajissāmi,",
  "ja539:36.4": "taṃ kudāssu bhavissati.",
  "ja539:37.1": "Kadāhaṃ vedehe phīte,",
  "ja539:37.2": "ajeyye dhammarakkhite;",
  "ja539:37.3": "Pahāya pabbajissāmi,",
  "ja539:37.4": "taṃ kudāssu bhavissati.",
  "ja539:38.1": "Kadāhaṃ antepuraṃ rammaṃ,",
  "ja539:38.2": "Vibhattaṃ bhāgaso mitaṃ;",
  "ja539:38.3": "Pahāya pabbajissāmi,",
  "ja539:38.4": "Taṃ kudāssu bhavissati.",
  "ja539:39.1": "Kadāhaṃ antepuraṃ rammaṃ,",
  "ja539:39.2": "sudhāmattikalepanaṃ;",
  "ja539:39.3": "Pahāya pabbajissāmi,",
  "ja539:39.4": "taṃ kudāssu bhavissati.",
  "ja539:40.1": "Kadāhaṃ antepuraṃ rammaṃ,",
  "ja539:40.2": "sucigandhaṃ manoramaṃ;",
  "ja539:40.3": "Pahāya pabbajissāmi,",
  "ja539:40.4": "taṃ kudāssu bhavissati.",
  "ja539:41.1": "Kadāhaṃ kūṭāgāre ca,",
  "ja539:41.2": "vibhatte bhāgaso mite;",
  "ja539:41.3": "Pahāya pabbajissāmi,",
  "ja539:41.4": "taṃ kudāssu bhavissati.",
  "ja539:42.1": "Kadāhaṃ kūṭāgāre ca,",
  "ja539:42.2": "sudhāmattikalepane;",
  "ja539:42.3": "Pahāya pabbajissāmi,",
  "ja539:42.4": "taṃ kudāssu bhavissati.",
  "ja539:43.1": "Kadāhaṃ kūṭāgāre ca,",
  "ja539:43.2": "sucigandhe manorame;",
  "ja539:43.3": "Pahāya pabbajissāmi,",
  "ja539:43.4": "taṃ kudāssu bhavissati.",
  "ja539:44.1": "Kadāhaṃ kūṭāgāre ca,",
  "ja539:44.2": "litte candanaphosite;",
  "ja539:44.3": "Pahāya pabbajissāmi,",
  "ja539:44.4": "taṃ kudāssu bhavissati.",
  "ja539:45.1": "Kadāhaṃ soṇṇapallaṅke,",
  "ja539:45.2": "gonake cittasanthate;",
  "ja539:45.3": "Pahāya pabbajissāmi,",
  "ja539:45.4": "taṃ kudāssu bhavissati.",
  "ja539:46.1": "Kadāhaṃ maṇipallaṅke,",
  "ja539:46.2": "gonake cittasanthate;",
  "ja539:46.3": "Pahāya pabbajissāmi,",
  "ja539:46.4": "taṃ kudāssu bhavissati.",
  "ja539:47.1": "Kadāhaṃ kappāsakoseyyaṃ,",
  "ja539:47.2": "khomakoṭumbarāni ca;",
  "ja539:47.3": "Pahāya pabbajissāmi,",
  "ja539:47.4": "taṃ kudāssu bhavissati.",
  "ja539:48.1": "Kadāhaṃ pokkharaṇī rammā,",
  "ja539:48.2": "cakkavākapakūjitā;",
  "ja539:48.3": "Mandālakehi sañchannā,",
  "ja539:48.4": "padumuppalakehi ca;",
  "ja539:48.5": "Pahāya pabbajissāmi,",
  "ja539:48.6": "taṃ kudāssu bhavissati.",
  "ja539:49.1": "Kadāhaṃ hatthigumbe ca,",
  "ja539:49.2": "sabbālaṅkārabhūsite;",
  "ja539:49.3": "Suvaṇṇakacche mātaṅge,",
  "ja539:49.4": "hemakappanavāsase.",
  "ja539:50.1": "Ārūḷhe gāmaṇīyehi,",
  "ja539:50.2": "tomaraṅkusapāṇibhi;",
  "ja539:50.3": "Pahāya pabbajissāmi,",
  "ja539:50.4": "taṃ kudāssu bhavissati.",
  "ja539:51.1": "Kadāhaṃ assagumbe ca,",
  "ja539:51.2": "sabbālaṅkārabhūsite;",
  "ja539:51.3": "Ājānīyeva jātiyā,",
  "ja539:51.4": "sindhave sīghavāhane.",
  "ja539:52.1": "Ārūḷhe gāmaṇīyehi,",
  "ja539:52.2": "illiyācāpadhāribhi;",
  "ja539:52.3": "Pahāya pabbajissāmi,",
  "ja539:52.4": "taṃ kudāssu bhavissati.",
  "ja539:53.1": "Kadāhaṃ rathaseniyo,",
  "ja539:53.2": "sannaddhe ussitaddhaje;",
  "ja539:53.3": "Dīpe athopi veyyagghe,",
  "ja539:53.4": "sabbālaṅkārabhūsite.",
  "ja539:54.1": "Ārūḷhe gāmaṇīyehi,",
  "ja539:54.2": "cāpahatthehi vammibhi;",
  "ja539:54.3": "Pahāya pabbajissāmi,",
  "ja539:54.4": "taṃ kudāssu bhavissati.",
  "ja539:55.1": "Kadāhaṃ sovaṇṇarathe,",
  "ja539:55.2": "sannaddhe ussitaddhaje;",
  "ja539:55.3": "Dīpe athopi veyyagghe,",
  "ja539:55.4": "sabbālaṅkārabhūsite.",
