{
  "ja148:0.1": "Jātaka",
  "ja148:0.2": "Ekakanipāta",
  "ja148:0.3": "Kakaṇṭakavagga",
  "ja148:0.4": "8. Siṅgālajātaka",
  "ja148:1.1": "“Nāhaṃ punaṃ na ca punaṃ,",
  "ja148:1.2": "na cāpi apunappunaṃ;",
  "ja148:1.3": "Hatthibondiṃ pavekkhāmi,",
  "ja148:1.4": "tathā hi bhayatajjito”ti.",
  "ja148:2.1": "Siṅgālajātakaṃ aṭṭhamaṃ."
}