{
  "ja123:0.1": "Jātaka",
  "ja123:0.2": "Ekakanipāta",
  "ja123:0.3": "Kusanāḷivagga",
  "ja123:0.4": "3. Naṅgalīsajātaka",
  "ja123:1.1": "“Asabbatthagāmiṃ vācaṃ,",
  "ja123:1.2": "Bālo sabbattha bhāsati;",
  "ja123:1.3": "Nāyaṃ dadhiṃ vedi na naṅgalīsaṃ,",
  "ja123:1.4": "Dadhippayaṃ maññati naṅgalīsan”ti.",
  "ja123:2.1": "Naṅgalīsajātakaṃ tatiyaṃ."
}