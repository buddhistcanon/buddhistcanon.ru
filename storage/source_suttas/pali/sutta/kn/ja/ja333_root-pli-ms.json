{
  "ja333:0.1": "Jātaka",
  "ja333:0.2": "Catukkanipāta",
  "ja333:0.3": "Kokilavagga",
  "ja333:0.4": "3. Pakkagodhajātaka",
  "ja333:1.1": "“Tadeva me tvaṃ vidito,",
  "ja333:1.2": "vanamajjhe rathesabha;",
  "ja333:1.3": "Yassa te khaggabaddhassa,",
  "ja333:1.4": "sannaddhassa tirīṭino;",
  "ja333:1.5": "Assatthadumasākhāya,",
  "ja333:1.6": "pakkā godhā palāyatha”.",
  "ja333:2.1": "“Name namantassa bhaje bhajantaṃ,",
  "ja333:2.2": "Kiccānukubbassa kareyya kiccaṃ;",
  "ja333:2.3": "Nānatthakāmassa kareyya atthaṃ,",
  "ja333:2.4": "Asambhajantampi na sambhajeyya.",
  "ja333:3.1": "Caje cajantaṃ vanathaṃ na kayirā,",
  "ja333:3.2": "Apetacittena na sambhajeyya;",
  "ja333:3.3": "Dijo dumaṃ khīṇaphalanti ñatvā,",
  "ja333:3.4": "Aññaṃ samekkheyya mahā hi loko”.",
  "ja333:4.1": "“So te karissāmi yathānubhāvaṃ,",
  "ja333:4.2": "Kataññutaṃ khattiye pekkhamāno;",
  "ja333:4.3": "Sabbañca te issariyaṃ dadāmi,",
  "ja333:4.4": "Yassicchasī tassa tuvaṃ dadāmī”ti.",
  "ja333:5.1": "Pakkagodhajātakaṃ tatiyaṃ."
}