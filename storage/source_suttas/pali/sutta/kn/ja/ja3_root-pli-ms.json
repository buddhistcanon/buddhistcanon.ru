{
  "ja3:0.1": "Jātaka",
  "ja3:0.2": "Ekakanipāta",
  "ja3:0.3": "Apaṇṇakavagga",
  "ja3:0.4": "3. Serivavāṇijajātaka",
  "ja3:1.1": "“Idha ce naṃ virādhesi,",
  "ja3:1.2": "saddhammassa niyāmataṃ;",
  "ja3:1.3": "Ciraṃ tvaṃ anutappesi,",
  "ja3:1.4": "serivāyaṃva vāṇijo”ti.",
  "ja3:2.1": "Serivavāṇijajātakaṃ tatiyaṃ."
}