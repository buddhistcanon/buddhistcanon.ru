{
  "ja534:0.1": "Jātaka",
  "ja534:0.2": "Asītinipāta",
  "ja534:0.3": "Cūḷahaṃsavagga",
  "ja534:0.4": "2. Mahāhaṃsajātaka",
  "ja534:1.1": "“Ete haṃsā pakkamanti,",
  "ja534:1.2": "vakkaṅgā bhayameritā;",
  "ja534:1.3": "Harittaca hemavaṇṇa,",
  "ja534:1.4": "kāmaṃ sumukha pakkama.",
  "ja534:2.1": "Ohāya maṃ ñātigaṇā,",
  "ja534:2.2": "ekaṃ pāsavasaṃ gataṃ;",
  "ja534:2.3": "Anapekkhamānā gacchanti,",
  "ja534:2.4": "kiṃ eko avahīyasi.",
  "ja534:3.1": "Pateva patataṃ seṭṭha,",
  "ja534:3.2": "natthi baddhe sahāyatā;",
  "ja534:3.3": "Mā anīghāya hāpesi,",
  "ja534:3.4": "kāmaṃ sumukha pakkama”.",
  "ja534:4.1": "“Nāhaṃ dukkhaparetopi,",
  "ja534:4.2": "dhataraṭṭha tuvaṃ jahe;",
  "ja534:4.3": "Jīvitaṃ maraṇaṃ vā me,",
  "ja534:4.4": "tayā saddhiṃ bhavissati.",
  "ja534:5.1": "Nāhaṃ dukkhaparetopi,",
  "ja534:5.2": "dhataraṭṭha tuvaṃ jahe;",
  "ja534:5.3": "Na maṃ anariyasaṃyutte,",
  "ja534:5.4": "kamme yojetumarahasi.",
  "ja534:6.1": "Sakumāro sakhā tyasmi,",
  "ja534:6.2": "sacitte casmi te ṭhito;",
  "ja534:6.3": "Ñāto senāpati tyāhaṃ,",
  "ja534:6.4": "haṃsānaṃ pavaruttama.",
  "ja534:7.1": "Kathaṃ ahaṃ vikatthissaṃ,",
  "ja534:7.2": "ñātimajjhe ito gato;",
  "ja534:7.3": "Taṃ hitvā patataṃ seṭṭha,",
  "ja534:7.4": "kiṃ te vakkhāmito gato;",
  "ja534:7.5": "Idha pāṇaṃ cajissāmi,",
  "ja534:7.6": "nānariyaṃ kattumussahe”.",
  "ja534:8.1": "“Eso hi dhammo sumukha,",
  "ja534:8.2": "yaṃ tvaṃ ariyapathe ṭhito;",
  "ja534:8.3": "Yo bhattāraṃ sakhāraṃ maṃ,",
  "ja534:8.4": "na pariccattumussahe.",
  "ja534:9.1": "Tañhi me pekkhamānassa,",
  "ja534:9.2": "bhayaṃ na tveva jāyati;",
  "ja534:9.3": "Adhigacchasi tvaṃ mayhaṃ,",
  "ja534:9.4": "evaṃ bhūtassa jīvitaṃ”.",
  "ja534:10.1": "Iccevaṃ mantayantānaṃ,",
  "ja534:10.2": "Ariyānaṃ ariyavuttinaṃ;",
  "ja534:10.3": "Daṇḍamādāya nesādo,",
  "ja534:10.4": "Āpatī turito bhusaṃ.",
  "ja534:11.1": "Tamāpatantaṃ disvāna,",
  "ja534:11.2": "Sumukho atibrūhayi;",
  "ja534:11.3": "Aṭṭhāsi purato rañño,",
  "ja534:11.4": "Haṃso vissāsayaṃ byathaṃ.",
  "ja534:12.1": "“Mā bhāyi patataṃ seṭṭha,",
  "ja534:12.2": "na hi bhāyanti tādisā;",
  "ja534:12.3": "Ahaṃ yogaṃ payuñjissaṃ,",
  "ja534:12.4": "yuttaṃ dhammūpasaṃhitaṃ;",
  "ja534:12.5": "Tena pariyāpadānena,",
  "ja534:12.6": "khippaṃ pāsā pamokkhasi”.",
  "ja534:13.1": "Tassa taṃ vacanaṃ sutvā,",
  "ja534:13.2": "sumukhassa subhāsitaṃ;",
  "ja534:13.3": "Pahaṭṭhalomo nesādo,",
  "ja534:13.4": "añjalissa paṇāmayi.",
  "ja534:14.1": "“Na me sutaṃ vā diṭṭhaṃ vā,",
  "ja534:14.2": "bhāsanto mānusiṃ dijo;",
  "ja534:14.3": "Ariyaṃ bruvāno vakkaṅgo,",
  "ja534:14.4": "cajanto mānusiṃ giraṃ.",
  "ja534:15.1": "Kiṃ nu tāyaṃ dijo hoti,",
  "ja534:15.2": "mutto baddhaṃ upāsasi;",
  "ja534:15.3": "Ohāya sakuṇā yanti,",
  "ja534:15.4": "kiṃ eko avahīyasi”.",
  "ja534:16.1": "“Rājā me so dijāmitta,",
  "ja534:16.2": "senāpaccassa kārayiṃ;",
  "ja534:16.3": "Tamāpade pariccattuṃ,",
  "ja534:16.4": "nussahe vihagādhipaṃ.",
  "ja534:17.1": "Mahāgaṇāya bhattā me,",
  "ja534:17.2": "mā eko byasanaṃ agā;",
  "ja534:17.3": "Tathā taṃ samma nesāda,",
  "ja534:17.4": "bhattāyaṃ abhito rame”.",
  "ja534:18.1": "“Ariyavattasi vakkaṅga,",
  "ja534:18.2": "yo piṇḍamapacāyasi;",
  "ja534:18.3": "Cajāmi te taṃ bhattāraṃ,",
  "ja534:18.4": "gacchathūbho yathāsukhaṃ”.",
  "ja534:19.1": "“Sace attappayogena,",
  "ja534:19.2": "ohito haṃsapakkhinaṃ;",
  "ja534:19.3": "Paṭigaṇhāma te samma,",
  "ja534:19.4": "etaṃ abhayadakkhiṇaṃ.",
  "ja534:20.1": "No ce attappayogena,",
  "ja534:20.2": "ohito haṃsapakkhinaṃ;",
  "ja534:20.3": "Anissaro muñcamamhe,",
  "ja534:20.4": "theyyaṃ kayirāsi luddaka”.",
  "ja534:21.1": "“Yassa tvaṃ bhatako rañño,",
  "ja534:21.2": "kāmaṃ tasseva pāpaya;",
  "ja534:21.3": "Tattha saṃyamano rājā,",
  "ja534:21.4": "yathābhiññaṃ karissati”.",
  "ja534:22.1": "Iccevaṃ vutto nesādo,",
  "ja534:22.2": "hemavaṇṇe harittace;",
  "ja534:22.3": "Ubho hatthehi saṅgayha,",
  "ja534:22.4": "pañjare ajjhavodahi.",
  "ja534:23.1": "Te pañjaragate pakkhī,",
  "ja534:23.2": "ubho bhassaravaṇṇine;",
  "ja534:23.3": "Sumukhaṃ dhataraṭṭhañca,",
  "ja534:23.4": "luddo ādāya pakkami.",
  "ja534:24.1": "Harīyamāno dhataraṭṭho,",
  "ja534:24.2": "sumukhaṃ etadabravi;",
  "ja534:24.3": "“Bāḷhaṃ bhāyāmi sumukha,",
  "ja534:24.4": "sāmāya lakkhaṇūruyā;",
  "ja534:24.5": "Asmākaṃ vadhamaññāya,",
  "ja534:24.6": "athattānaṃ vadhissati.",
  "ja534:25.1": "Pākahaṃsā ca sumukha,",
  "ja534:25.2": "suhemā hemasuttacā;",
  "ja534:25.3": "Koñcī samuddatīreva,",
  "ja534:25.4": "kapaṇā nūna rucchati”.",
  "ja534:26.1": "“Evaṃ mahanto lokassa,",
  "ja534:26.2": "appameyyo mahāgaṇī;",
  "ja534:26.3": "Ekitthimanusoceyya,",
  "ja534:26.4": "nayidaṃ paññavatāmiva.",
  "ja534:27.1": "Vātova gandhamādeti,",
  "ja534:27.2": "ubhayaṃ chekapāpakaṃ;",
  "ja534:27.3": "Bālo āmakapakkaṃva,",
  "ja534:27.4": "lolo andhova āmisaṃ.",
  "ja534:28.1": "Avinicchayaññu atthesu,",
  "ja534:28.2": "mandova paṭibhāsi maṃ;",
  "ja534:28.3": "Kiccākiccaṃ na jānāsi,",
  "ja534:28.4": "sampatto kālapariyāyaṃ.",
  "ja534:29.1": "Aḍḍhummatto udīresi,",
  "ja534:29.2": "yo seyyā maññasitthiyo;",
  "ja534:29.3": "Bahusādhāraṇā hetā,",
  "ja534:29.4": "soṇḍānaṃva surāgharaṃ.",
  "ja534:30.1": "Māyā cesā marīcī ca,",
  "ja534:30.2": "soko rogo cupaddavo;",
  "ja534:30.3": "Kharā ca bandhanā cetā,",
  "ja534:30.4": "maccupāsā guhāsayā;",
  "ja534:30.5": "Tāsu yo vissase poso,",
  "ja534:30.6": "so naresu narādhamo”.",
  "ja534:31.1": "“Yaṃ vuddhehi upaññātaṃ,",
  "ja534:31.2": "ko taṃ ninditumarahati;",
  "ja534:31.3": "Mahābhūtitthiyo nāma,",
  "ja534:31.4": "lokasmiṃ udapajjisuṃ.",
  "ja534:32.1": "Khiḍḍā paṇihitā tyāsu,",
  "ja534:32.2": "rati tyāsu patiṭṭhitā;",
  "ja534:32.3": "Bījāni tyāsu rūhanti,",
  "ja534:32.4": "yadidaṃ sattā pajāyare;",
  "ja534:32.5": "Tāsu ko nibbide poso,",
  "ja534:32.6": "pāṇamāsajja pāṇibhi.",
  "ja534:33.1": "Tvameva nañño sumukha,",
  "ja534:33.2": "thīnaṃ atthesu yuñjasi;",
  "ja534:33.3": "Tassa tyajja bhaye jāte,",
  "ja534:33.4": "bhītena jāyate mati.",
  "ja534:34.1": "Sabbo hi saṃsayaṃ patto,",
  "ja534:34.2": "bhayaṃ bhīru titikkhati;",
  "ja534:34.3": "Paṇḍitā ca mahantāno,",
  "ja534:34.4": "atthe yuñjanti duyyuje.",
  "ja534:35.1": "Etadatthāya rājāno,",
  "ja534:35.2": "sūramicchanti mantinaṃ;",
  "ja534:35.3": "Paṭibāhati yaṃ sūro,",
  "ja534:35.4": "āpadaṃ attapariyāyaṃ.",
  "ja534:36.1": "Mā no ajja vikantiṃsu,",
  "ja534:36.2": "rañño sūdā mahānase;",
  "ja534:36.3": "Tathā hi vaṇṇo pattānaṃ,",
  "ja534:36.4": "phalaṃ veḷuṃva taṃ vadhi.",
  "ja534:37.1": "Muttopi na icchi uḍḍetuṃ,",
  "ja534:37.2": "Sayaṃ bandhaṃ upāgami;",
  "ja534:37.3": "Sopajja saṃsayaṃ patto,",
  "ja534:37.4": "Atthaṃ gaṇhāhi mā mukhaṃ.",
  "ja534:38.1": "So taṃ yogaṃ payuñjassu,",
  "ja534:38.2": "yuttaṃ dhammūpasaṃhitaṃ;",
  "ja534:38.3": "Tava pariyāpadānena,",
  "ja534:38.4": "mama pāṇesanaṃ cara”.",
  "ja534:39.1": "“Mā bhāyi patataṃ seṭṭha,",
  "ja534:39.2": "na hi bhāyanti tādisā;",
  "ja534:39.3": "Ahaṃ yogaṃ payuñjissaṃ,",
  "ja534:39.4": "yuttaṃ dhammūpasaṃhitaṃ;",
  "ja534:39.5": "Mama pariyāpadānena,",
  "ja534:39.6": "khippaṃ pāsā pamokkhasi”.",
  "ja534:40.1": "So luddo haṃsakājena,",
  "ja534:40.2": "rājadvāraṃ upāgami;",
  "ja534:40.3": "“Paṭivedetha maṃ rañño,",
  "ja534:40.4": "dhataraṭṭhāyamāgato”.",
  "ja534:41.1": "Te disvā puññasaṅkāse,",
  "ja534:41.2": "ubho lakkhaṇasammate;",
  "ja534:41.3": "Khalu saṃyamano rājā,",
  "ja534:41.4": "amacce ajjhabhāsatha.",
  "ja534:42.1": "“Detha luddassa vatthāni,",
  "ja534:42.2": "annaṃ pānañca bhojanaṃ;",
  "ja534:42.3": "Kāmaṅkaro hiraññassa,",
  "ja534:42.4": "yāvanto esa icchati”.",
  "ja534:43.1": "Disvā luddaṃ pasannattaṃ,",
  "ja534:43.2": "Kāsirājā tadabravi;",
  "ja534:43.3": "“Yadyāyaṃ samma khemaka,",
  "ja534:43.4": "Puṇṇā haṃsehi tiṭṭhati.",
  "ja534:44.1": "Kathaṃ rucimajjhagataṃ,",
  "ja534:44.2": "pāsahattho upāgami;",
  "ja534:44.3": "Okiṇṇaṃ ñātisaṅghehi,",
  "ja534:44.4": "nimmajjhimaṃ kathaṃ gahi”.",
  "ja534:45.1": "“Ajja me sattamā ratti,",
  "ja534:45.2": "adanāni upāsato;",
  "ja534:45.3": "Padametassa anvesaṃ,",
  "ja534:45.4": "appamatto ghaṭassito.",
  "ja534:46.1": "Athassa padamaddakkhiṃ,",
  "ja534:46.2": "carato adanesanaṃ;",
  "ja534:46.3": "Tatthāhaṃ odahiṃ pāsaṃ,",
  "ja534:46.4": "evaṃ taṃ dijamaggahiṃ”.",
  "ja534:47.1": "“Ludda dve ime sakuṇā,",
  "ja534:47.2": "atha ekoti bhāsasi;",
  "ja534:47.3": "Cittaṃ nu te vipariyattaṃ,",
  "ja534:47.4": "adu kiṃ nu jigīsasi”.",
  "ja534:48.1": "“Yassa lohitakā tālā,",
  "ja534:48.2": "tapanīyanibhā subhā;",
  "ja534:48.3": "Uraṃ saṃhacca tiṭṭhanti,",
  "ja534:48.4": "so me bandhaṃ upāgami.",
  "ja534:49.1": "Athāyaṃ bhassaro pakkhī,",
  "ja534:49.2": "abaddho baddhamāturaṃ;",
  "ja534:49.3": "Ariyaṃ bruvāno aṭṭhāsi,",
  "ja534:49.4": "cajanto mānusiṃ giraṃ”.",
  "ja534:50.1": "“Atha kiṃ dāni sumukha,",
  "ja534:50.2": "Hanuṃ saṃhacca tiṭṭhasi;",
  "ja534:50.3": "Adu me parisaṃ patto,",
  "ja534:50.4": "Bhayā bhīto na bhāsasi”.",
  "ja534:51.1": "“Nāhaṃ kāsipati bhīto,",
  "ja534:51.2": "ogayha parisaṃ tava;",
  "ja534:51.3": "Nāhaṃ bhayā na bhāsissaṃ,",
  "ja534:51.4": "vākyaṃ atthamhi tādise”.",
  "ja534:52.1": "“Na te abhisaraṃ passe,",
  "ja534:52.2": "na rathe napi pattike;",
  "ja534:52.3": "Nāssa cammaṃ va kīṭaṃ vā,",
  "ja534:52.4": "vammite ca dhanuggahe.",
  "ja534:53.1": "Na hiraññaṃ suvaṇṇaṃ vā,",
  "ja534:53.2": "nagaraṃ vā sumāpitaṃ;",
  "ja534:53.3": "Okiṇṇaparikhaṃ duggaṃ,",
  "ja534:53.4": "daḷhamaṭṭālakoṭṭhakaṃ;",
  "ja534:53.5": "Yattha paviṭṭho sumukha,",
  "ja534:53.6": "bhāyitabbaṃ na bhāyasi”.",
  "ja534:54.1": "“Na me abhisarenattho,",
  "ja534:54.2": "nagarena dhanena vā;",
  "ja534:54.3": "Apathena pathaṃ yāma,",
  "ja534:54.4": "antalikkhecarā mayaṃ.",
  "ja534:55.1": "Sutā ca paṇḍitā tyamhā,",
  "ja534:55.2": "nipuṇā atthacintakā;",
  "ja534:55.3": "Bhāsematthavatiṃ vācaṃ,",
  "ja534:55.4": "sacce cassa patiṭṭhito.",
  "ja534:56.1": "Kiñca tuyhaṃ asaccassa,",
  "ja534:56.2": "anariyassa karissati;",
  "ja534:56.3": "Musāvādissa luddassa,",
  "ja534:56.4": "bhaṇitampi subhāsitaṃ.",
  "ja534:57.1": "Taṃ brāhmaṇānaṃ vacanā,",
  "ja534:57.2": "imaṃ khemamakārayi;",
  "ja534:57.3": "Abhayañca tayā ghuṭṭhaṃ,",
  "ja534:57.4": "imāyo dasadhā disā.",
  "ja534:58.1": "Ogayha te pokkharaṇiṃ,",
  "ja534:58.2": "vippasannodakaṃ suciṃ;",
  "ja534:58.3": "Pahūtaṃ cādanaṃ tattha,",
  "ja534:58.4": "ahiṃsā cettha pakkhinaṃ.",
  "ja534:59.1": "Idaṃ sutvāna nigghosaṃ,",
  "ja534:59.2": "āgatamha tavantike;",
  "ja534:59.3": "Te te bandhasma pāsena,",
  "ja534:59.4": "etaṃ te bhāsitaṃ musā.",
  "ja534:60.1": "Musāvādaṃ purakkhatvā,",
  "ja534:60.2": "icchālobhañca pāpakaṃ;",
  "ja534:60.3": "Ubho sandhimatikkamma,",
  "ja534:60.4": "asātaṃ upapajjati”.",
  "ja534:61.1": "“Nāparajjhāma sumukha,",
  "ja534:61.2": "napi lobhāva maggahiṃ;",
  "ja534:61.3": "Sutā ca paṇḍitātyattha-",
  "ja534:61.4": "nipuṇā atthacintakā.",
  "ja534:62.1": "Appevatthavatiṃ vācaṃ,",
  "ja534:62.2": "byāhareyyuṃ idhāgatā;",
  "ja534:62.3": "Tathā taṃ samma nesādo,",
  "ja534:62.4": "vutto sumukha maggahi”.",
  "ja534:63.1": "“Neva bhītā kāsipati,",
  "ja534:63.2": "upanītasmi jīvite;",
  "ja534:63.3": "Bhāsematthavatiṃ vācaṃ,",
  "ja534:63.4": "sampattā kālapariyāyaṃ.",
  "ja534:64.1": "Yo migena migaṃ hanti,",
  "ja534:64.2": "pakkhiṃ vā pana pakkhinā;",
  "ja534:64.3": "Sutena vā sutaṃ kiṇyā,",
  "ja534:64.4": "kiṃ anariyataraṃ tato.",
  "ja534:65.1": "Yo cāriyarudaṃ bhāse,",
  "ja534:65.2": "anariyadhammavassito;",
  "ja534:65.3": "Ubho so dhaṃsate lokā,",
  "ja534:65.4": "idha ceva parattha ca.",
  "ja534:66.1": "Na majjetha yasaṃ patto,",
  "ja534:66.2": "na byādhe pattasaṃsayaṃ;",
  "ja534:66.3": "Vāyametheva kiccesu,",
  "ja534:66.4": "saṃvare vivarāni ca.",
  "ja534:67.1": "Ye vuddhā abbhatikkantā,",
  "ja534:67.2": "sampattā kālapariyāyaṃ;",
  "ja534:67.3": "Idha dhammaṃ caritvāna,",
  "ja534:67.4": "evaṃte tidivaṃ gatā.",
  "ja534:68.1": "Idaṃ sutvā kāsipati,",
  "ja534:68.2": "dhammamattani pālaya;",
  "ja534:68.3": "Dhataraṭṭhañca muñcāhi,",
  "ja534:68.4": "haṃsānaṃ pavaruttamaṃ”.",
  "ja534:69.1": "“Āharantudakaṃ pajjaṃ,",
  "ja534:69.2": "āsanañca mahārahaṃ;",
  "ja534:69.3": "Pañjarato pamokkhāmi,",
  "ja534:69.4": "dhataraṭṭhaṃ yasassinaṃ.",
  "ja534:70.1": "Tañca senāpatiṃ dhīraṃ,",
  "ja534:70.2": "nipuṇaṃ atthacintakaṃ;",
  "ja534:70.3": "Yo sukhe sukhito rañño,",
  "ja534:70.4": "dukkhite hoti dukkhito.",
  "ja534:71.1": "Ediso kho arahati,",
  "ja534:71.2": "piṇḍamasnātu bhattuno;",
  "ja534:71.3": "Yathāyaṃ sumukho rañño,",
  "ja534:71.4": "pāṇasādhāraṇo sakhā”.",
  "ja534:72.1": "Pīṭhañca sabbasovaṇṇaṃ,",
  "ja534:72.2": "Aṭṭhapādaṃ manoramaṃ;",
  "ja534:72.3": "Maṭṭhaṃ kāsikamatthannaṃ,",
  "ja534:72.4": "Dhataraṭṭho upāvisi.",
  "ja534:73.1": "Kocchañca sabbasovaṇṇaṃ,",
  "ja534:73.2": "veyyagghaparisibbitaṃ;",
  "ja534:73.3": "Sumukho ajjhupāvekkhi,",
  "ja534:73.4": "dhataraṭṭhassanantarā.",
  "ja534:74.1": "Tesaṃ kañcanapattehi,",
  "ja534:74.2": "puthū ādāya kāsiyo;",
  "ja534:74.3": "Haṃsānaṃ abhihāresuṃ,",
  "ja534:74.4": "aggarañño pavāsitaṃ.",
  "ja534:75.1": "Disvā abhihaṭaṃ aggaṃ,",
  "ja534:75.2": "kāsirājena pesitaṃ;",
  "ja534:75.3": "Kusalo khattadhammānaṃ,",
  "ja534:75.4": "tato pucchi anantarā.",
  "ja534:76.1": "“Kaccinnu bhoto kusalaṃ,",
  "ja534:76.2": "kacci bhoto anāmayaṃ;",
  "ja534:76.3": "Kacci raṭṭhamidaṃ phītaṃ,",
  "ja534:76.4": "dhammena manusāsasi”.",
  "ja534:77.1": "“Kusalañceva me haṃsa,",
  "ja534:77.2": "atho haṃsa anāmayaṃ;",
  "ja534:77.3": "Atho raṭṭhamidaṃ phītaṃ,",
  "ja534:77.4": "dhammenaṃ manusāsahaṃ”.",
  "ja534:78.1": "“Kacci bhoto amaccesu,",
  "ja534:78.2": "doso koci na vijjati;",
  "ja534:78.3": "Kacci ca te tavatthesu,",
  "ja534:78.4": "nāvakaṅkhanti jīvitaṃ”.",
  "ja534:79.1": "“Athopi me amaccesu,",
  "ja534:79.2": "doso koci na vijjati;",
  "ja534:79.3": "Athopi te mamatthesu,",
  "ja534:79.4": "nāvakaṅkhanti jīvitaṃ”.",
  "ja534:80.1": "“Kacci te sādisī bhariyā,",
  "ja534:80.2": "assavā piyabhāṇinī;",
  "ja534:80.3": "Puttarūpayasūpetā,",
  "ja534:80.4": "tava chandavasānugā”.",
  "ja534:81.1": "“Atho me sādisī bhariyā,",
  "ja534:81.2": "assavā piyabhāṇinī;",
  "ja534:81.3": "Puttarūpayasūpetā,",
  "ja534:81.4": "mama chandavasānugā”.",
  "ja534:82.1": "“Kacci raṭṭhaṃ anuppīḷaṃ,",
  "ja534:82.2": "akutociupaddavaṃ;",
  "ja534:82.3": "Asāhasena dhammena,",
  "ja534:82.4": "samena manusāsasi”.",
  "ja534:83.1": "“Atho raṭṭhaṃ anuppīḷaṃ,",
  "ja534:83.2": "akutociupaddavaṃ;",
  "ja534:83.3": "Asāhasena dhammena,",
  "ja534:83.4": "samena manusāsahaṃ”.",
  "ja534:84.1": "“Kacci santo apacitā,",
  "ja534:84.2": "Asanto parivajjitā;",
  "ja534:84.3": "No ce dhammaṃ niraṃkatvā,",
  "ja534:84.4": "Adhammamanuvattasi”.",
  "ja534:85.1": "“Santo ca me apacitā,",
  "ja534:85.2": "asanto parivajjitā;",
  "ja534:85.3": "Dhammamevānuvattāmi,",
  "ja534:85.4": "adhammo me niraṅkato”.",
  "ja534:86.1": "“Kacci nānāgataṃ dīghaṃ,",
  "ja534:86.2": "samavekkhasi khattiya;",
  "ja534:86.3": "Kacci matto madanīye,",
  "ja534:86.4": "paralokaṃ na santasi”.",
  "ja534:87.1": "“Nāhaṃ anāgataṃ dīghaṃ,",
  "ja534:87.2": "samavekkhāmi pakkhima;",
  "ja534:87.3": "Ṭhito dasasu dhammesu,",
  "ja534:87.4": "paralokaṃ na santase.",
  "ja534:88.1": "Dānaṃ sīlaṃ pariccāgaṃ,",
  "ja534:88.2": "ajjavaṃ maddavaṃ tapaṃ;",
  "ja534:88.3": "Akkodhaṃ avihiṃsañca,",
  "ja534:88.4": "khantiñca avirodhanaṃ.",
  "ja534:89.1": "Iccete kusale dhamme,",
  "ja534:89.2": "ṭhite passāmi attani;",
  "ja534:89.3": "Tato me jāyate pīti,",
  "ja534:89.4": "somanassañcanappakaṃ.",
  "ja534:90.1": "Sumukho ca acintetvā,",
  "ja534:90.2": "visajji pharusaṃ giraṃ;",
  "ja534:90.3": "Bhāvadosamanaññāya,",
  "ja534:90.4": "asmākāyaṃ vihaṅgamo.",
  "ja534:91.1": "So kuddho pharusaṃ vācaṃ,",
  "ja534:91.2": "nicchāresi ayoniso;",
  "ja534:91.3": "Yānasmesu na vijjanti,",
  "ja534:91.4": "nayidaṃ paññavatāmiva”.",
  "ja534:92.1": "“Atthi me taṃ atisāraṃ,",
  "ja534:92.2": "vegena manujādhipa;",
  "ja534:92.3": "Dhataraṭṭhe ca baddhasmiṃ,",
  "ja534:92.4": "dukkhaṃ me vipulaṃ ahu.",
  "ja534:93.1": "Tvaṃ no pitāva puttānaṃ,",
  "ja534:93.2": "bhūtānaṃ dharaṇīriva;",
  "ja534:93.3": "Asmākaṃ adhipannānaṃ,",
  "ja534:93.4": "khamassu rājakuñjara”.",
  "ja534:94.1": "“Etaṃ te anumodāma,",
  "ja534:94.2": "yaṃ bhāvaṃ na nigūhasi;",
  "ja534:94.3": "Khilaṃ pabhindasi pakkhi,",
  "ja534:94.4": "ujukosi vihaṅgama.",
  "ja534:95.1": "Yaṃ kiñci ratanaṃ atthi,",
  "ja534:95.2": "kāsirājanivesane;",
  "ja534:95.3": "Rajataṃ jātarūpañca,",
  "ja534:95.4": "muttā veḷuriyā bahū.",
  "ja534:96.1": "Maṇayo saṅkhamuttā ca,",
  "ja534:96.2": "vatthakaṃ haricandanaṃ;",
  "ja534:96.3": "Ajinaṃ dantabhaṇḍañca,",
  "ja534:96.4": "lohaṃ kāḷāyasaṃ bahuṃ;",
  "ja534:96.5": "Etaṃ dadāmi vo vittaṃ,",
  "ja534:96.6": "issaraṃ vissajāmi vo”.",
  "ja534:97.1": "“Addhā apacitā tyamhā,",
  "ja534:97.2": "sakkatā ca rathesabha;",
  "ja534:97.3": "Dhammesu vattamānānaṃ,",
  "ja534:97.4": "tvaṃ no ācariyo bhava.",
  "ja534:98.1": "Ācariya samanuññātā,",
  "ja534:98.2": "Tayā anumatā mayaṃ;",
  "ja534:98.3": "Taṃ padakkhiṇato katvā,",
  "ja534:98.4": "Ñātiṃ passemurindama”.",
  "ja534:99.1": "Sabbarattiṃ cintayitvā,",
  "ja534:99.2": "mantayitvā yathātathaṃ;",
  "ja534:99.3": "Kāsirājā anuññāsi,",
  "ja534:99.4": "haṃsānaṃ pavaruttamaṃ.",
  "ja534:100.1": "Tato ratyā vivasāne,",
  "ja534:100.2": "sūriyuggamanaṃ pati;",
  "ja534:100.3": "Pekkhato kāsirājassa,",
  "ja534:100.4": "bhavanā te vigāhisuṃ.",
  "ja534:101.1": "Te aroge anuppatte,",
  "ja534:101.2": "disvāna parame dije;",
  "ja534:101.3": "Kekāti makaruṃ haṃsā,",
  "ja534:101.4": "puthusaddo ajāyatha.",
  "ja534:102.1": "Te patītā pamuttena,",
  "ja534:102.2": "bhattunā bhattugāravā;",
  "ja534:102.3": "Samantā parikiriṃsu,",
  "ja534:102.4": "aṇḍajā laddhapaccayā.",
  "ja534:103.1": "Evaṃ mittavataṃ atthā,",
  "ja534:103.2": "sabbe honti padakkhiṇā;",
  "ja534:103.3": "Haṃsā yathā dhataraṭṭhā,",
  "ja534:103.4": "ñātisaṅghaṃ upāgamunti.",
  "ja534:104.1": "Mahāhaṃsajātakaṃ dutiyaṃ."
}