{
  "ja484:0.1": "Jātaka",
  "ja484:0.2": "Pakiṇṇakanipāta",
  "ja484:0.3": "Sālikedāravagga",
  "ja484:0.4": "1. Sālikedārajātaka",
  "ja484:1.1": "“Sampannaṃ sālikedāraṃ,",
  "ja484:1.2": "suvā bhuñjanti kosiya;",
  "ja484:1.3": "Paṭivedemi te brahme,",
  "ja484:1.4": "na ne vāretumussahe.",
  "ja484:2.1": "Eko ca tattha sakuṇo,",
  "ja484:2.2": "yo nesaṃ sabbasundaro;",
  "ja484:2.3": "Bhutvā sāliṃ yathākāmaṃ,",
  "ja484:2.4": "tuṇḍenādāya gacchati”.",
  "ja484:3.1": "“Oḍḍentu vāḷapāsāni,",
  "ja484:3.2": "yathā bajjhetha so dijo;",
  "ja484:3.3": "Jīvañca naṃ gahetvāna,",
  "ja484:3.4": "ānayehi mamantike”.",
  "ja484:4.1": "“Ete bhutvā pivitvā ca,",
  "ja484:4.2": "pakkamanti vihaṅgamā;",
  "ja484:4.3": "Eko baddhosmi pāsena,",
  "ja484:4.4": "kiṃ pāpaṃ pakataṃ mayā”.",
  "ja484:5.1": "“Udaraṃ nūna aññesaṃ,",
  "ja484:5.2": "suva accodaraṃ tava;",
  "ja484:5.3": "Bhutvā sāliṃ yathākāmaṃ,",
  "ja484:5.4": "tuṇḍenādāya gacchasi.",
  "ja484:6.1": "Koṭṭhaṃ nu tattha pūresi,",
  "ja484:6.2": "suva veraṃ nu te mayā;",
  "ja484:6.3": "Puṭṭho me samma akkhāhi,",
  "ja484:6.4": "kuhiṃ sāliṃ nidāhasi”.",
  "ja484:7.1": "“Na me veraṃ tayā saddhiṃ,",
  "ja484:7.2": "koṭṭho mayhaṃ na vijjati;",
  "ja484:7.3": "Iṇaṃ muñcāmiṇaṃ dammi,",
  "ja484:7.4": "sampatto koṭasimbaliṃ;",
  "ja484:7.5": "Nidhimpi tattha nidahāmi,",
  "ja484:7.6": "evaṃ jānāhi kosiya”.",
  "ja484:8.1": "“Kīdisaṃ te iṇadānaṃ,",
  "ja484:8.2": "iṇamokkho ca kīdiso;",
  "ja484:8.3": "Nidhinidhānamakkhāhi,",
  "ja484:8.4": "atha pāsā pamokkhasi”.",
  "ja484:9.1": "“Ajātapakkhā taruṇā,",
  "ja484:9.2": "puttakā mayha kosiya;",
  "ja484:9.3": "Te maṃ bhatā bharissanti,",
  "ja484:9.4": "tasmā tesaṃ iṇaṃ dade.",
  "ja484:10.1": "Mātā pitā ca me vuddhā,",
  "ja484:10.2": "jiṇṇakā gatayobbanā;",
  "ja484:10.3": "Tesaṃ tuṇḍena hātūna,",
  "ja484:10.4": "muñce pubbakataṃ iṇaṃ.",
  "ja484:11.1": "Aññepi tattha sakuṇā,",
  "ja484:11.2": "khīṇapakkhā sudubbalā;",
  "ja484:11.3": "Tesaṃ puññatthiko dammi,",
  "ja484:11.4": "taṃ nidhiṃ āhu paṇḍitā.",
  "ja484:12.1": "Īdisaṃ me iṇadānaṃ,",
  "ja484:12.2": "iṇamokkho ca īdiso;",
  "ja484:12.3": "Nidhinidhānamakkhāmi,",
  "ja484:12.4": "evaṃ jānāhi kosiya”.",
  "ja484:13.1": "“Bhaddako vatayaṃ pakkhī,",
  "ja484:13.2": "dijo paramadhammiko;",
  "ja484:13.3": "Ekaccesu manussesu,",
  "ja484:13.4": "ayaṃ dhammo na vijjati.",
  "ja484:14.1": "Bhuñja sāliṃ yathākāmaṃ,",
  "ja484:14.2": "saha sabbehi ñātibhi;",
  "ja484:14.3": "Punāpi suva passemu,",
  "ja484:14.4": "piyaṃ me tava dassanaṃ”.",
  "ja484:15.1": "“Bhuttañca pītañca tavassamamhi,",
  "ja484:15.2": "Ratī ca no kosiya te sakāse;",
  "ja484:15.3": "Nikkhittadaṇḍesu dadāhi dānaṃ,",
  "ja484:15.4": "Jiṇṇe ca mātāpitaro bharassu”.",
  "ja484:16.1": "“Lakkhī vata me udapādi ajja,",
  "ja484:16.2": "Yo addasāsiṃ pavaraṃ dijānaṃ;",
  "ja484:16.3": "Suvassa sutvāna subhāsitāni,",
  "ja484:16.4": "Kāhāmi puññāni anappakāni”.",
  "ja484:17.1": "So kosiyo attamano udaggo,",
  "ja484:17.2": "Annañca pānañcabhisaṅkharitvā;",
  "ja484:17.3": "Annena pānena pasannacitto,",
  "ja484:17.4": "Santappayi samaṇabrāhmaṇe cāti.",
  "ja484:18.1": "Sālikedārajātakaṃ paṭhamaṃ."
}