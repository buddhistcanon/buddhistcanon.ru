{
  "ja509:0.1": "Jātaka",
  "ja509:0.2": "Vīsatinipāta",
  "ja509:0.3": "Mātaṅgavagga",
  "ja509:0.4": "13. Hatthipālajātaka",
  "ja509:1.1": "“Cirassaṃ vata passāma,",
  "ja509:1.2": "brāhmaṇaṃ devavaṇṇinaṃ;",
  "ja509:1.3": "Mahājaṭaṃ khāridharaṃ,",
  "ja509:1.4": "paṅkadantaṃ rajassiraṃ.",
  "ja509:2.1": "Cirassaṃ vata passāma,",
  "ja509:2.2": "isiṃ dhammaguṇe rataṃ;",
  "ja509:2.3": "Kāsāyavatthavasanaṃ,",
  "ja509:2.4": "vākacīraṃ paṭicchadaṃ.",
  "ja509:3.1": "Āsanaṃ udakaṃ pajjaṃ,",
  "ja509:3.2": "paṭigaṇhātu no bhavaṃ;",
  "ja509:3.3": "Agghe bhavantaṃ pucchāma,",
  "ja509:3.4": "agghaṃ kurutu no bhavaṃ”.",
  "ja509:4.1": "“Adhicca vede pariyesa vittaṃ,",
  "ja509:4.2": "Putte gahe tāta patiṭṭhapetvā;",
  "ja509:4.3": "Gandhe rase paccanubhuyya sabbaṃ,",
  "ja509:4.4": "Araññaṃ sādhu muni so pasattho”.",
  "ja509:5.1": "“Vedā na saccā na ca vittalābho,",
  "ja509:5.2": "Na puttalābhena jaraṃ vihanti;",
  "ja509:5.3": "Gandhe rase muccanamāhu santo,",
  "ja509:5.4": "Sakammunā hoti phalūpapatti”.",
  "ja509:6.1": "“Addhā hi saccaṃ vacanaṃ tavetaṃ,",
  "ja509:6.2": "Sakammunā hoti phalūpapatti;",
  "ja509:6.3": "Jiṇṇā ca mātāpitaro tavīme,",
  "ja509:6.4": "Passeyyuṃ taṃ vassasataṃ arogaṃ”.",
  "ja509:7.1": "“Yassassa sakkhī maraṇena rāja,",
  "ja509:7.2": "Jarāya mettī naravīraseṭṭha;",
  "ja509:7.3": "Yo cāpi jaññā na marissaṃ kadāci,",
  "ja509:7.4": "Passeyyuṃ taṃ vassasataṃ arogaṃ.",
  "ja509:8.1": "Yathāpi nāvaṃ puriso dakamhi,",
  "ja509:8.2": "Ereti ce naṃ upaneti tīraṃ;",
  "ja509:8.3": "Evampi byādhī satataṃ jarā ca,",
  "ja509:8.4": "Upaneti maccaṃ vasamantakassa”.",
  "ja509:9.1": "“Paṅko ca kāmā palipo ca kāmā,",
  "ja509:9.2": "Manoharā duttarā maccudheyyā;",
  "ja509:9.3": "Etasmiṃ paṅke palipe byasannā,",
  "ja509:9.4": "Hīnattarūpā na taranti pāraṃ.",
  "ja509:10.1": "Ayaṃ pure luddamakāsi kammaṃ,",
  "ja509:10.2": "Svāyaṃ gahīto na hi mokkhito me;",
  "ja509:10.3": "Orundhiyā naṃ parirakkhissāmi,",
  "ja509:10.4": "Māyaṃ puna luddamakāsi kammaṃ”.",
  "ja509:11.1": "“Gavaṃva naṭṭhaṃ puriso yathā vane,",
  "ja509:11.2": "Anvesatī rāja apassamāno;",
  "ja509:11.3": "Evaṃ naṭṭho esukārī mamattho,",
  "ja509:11.4": "Sohaṃ kathaṃ na gaveseyyaṃ rāja.",
  "ja509:12.1": "Hiyyoti hiyyati poso,",
  "ja509:12.2": "Pareti parihāyati;",
  "ja509:12.3": "Anāgataṃ netamatthīti ñatvā,",
  "ja509:12.4": "Uppannachandaṃ ko panudeyya dhīro”.",
  "ja509:13.1": "“Passāmi vohaṃ daharaṃ kumāriṃ,",
  "ja509:13.2": "Mattūpamaṃ ketakapupphanettaṃ;",
  "ja509:13.3": "Abhuttabhoge paṭhame vayasmiṃ,",
  "ja509:13.4": "Ādāya maccu vajate kumāriṃ.",
  "ja509:14.1": "Yuvā sujāto sumukho sudassano,",
  "ja509:14.2": "Sāmo kusumbhaparikiṇṇamassu;",
  "ja509:14.3": "Hitvāna kāme paṭikacca gehaṃ,",
  "ja509:14.4": "Anujāna maṃ pabbajissāmi deva”.",
  "ja509:15.1": "“Sākhāhi rukkho labhate samaññaṃ,",
  "ja509:15.2": "Pahīnasākhaṃ pana khāṇumāhu;",
  "ja509:15.3": "Pahīnaputtassa mamajja bhoti,",
  "ja509:15.4": "Vāseṭṭhi bhikkhācariyāya kālo”.",
  "ja509:16.1": "“Aghasmi koñcāva yathā himaccaye,",
  "ja509:16.2": "Katāni jālāni padāliya haṃsā;",
  "ja509:16.3": "Gacchanti puttā ca patī ca mayhaṃ,",
  "ja509:16.4": "Sāhaṃ kathaṃ nānuvaje pajānaṃ”.",
  "ja509:17.1": "“Ete bhutvā vamitvā ca,",
  "ja509:17.2": "pakkamanti vihaṅgamā;",
  "ja509:17.3": "Ye ca bhutvāna vamiṃsu,",
  "ja509:17.4": "te me hatthattamāgatā.",
  "ja509:18.1": "Avamī brāhmaṇo kāme,",
  "ja509:18.2": "so tvaṃ paccāvamissasi;",
  "ja509:18.3": "Vantādo puriso rāja,",
  "ja509:18.4": "na so hoti pasaṃsiyo”.",
  "ja509:19.1": "“Paṅke ca posaṃ palipe byasannaṃ,",
  "ja509:19.2": "Balī yathā dubbalamuddhareyya;",
  "ja509:19.3": "Evampi maṃ tvaṃ udatāri bhoti,",
  "ja509:19.4": "Pañcāli gāthāhi subhāsitāhi”.",
  "ja509:20.1": "Idaṃ vatvā mahārājā,",
  "ja509:20.2": "esukārī disampati;",
  "ja509:20.3": "Raṭṭhaṃ hitvāna pabbaji,",
  "ja509:20.4": "nāgo chetvāva bandhanaṃ.",
  "ja509:21.1": "“Rājā ca pabbajjamarocayittha,",
  "ja509:21.2": "Raṭṭhaṃ pahāya naravīraseṭṭho;",
  "ja509:21.3": "Tuvampi no hohi yatheva rājā,",
  "ja509:21.4": "Amhehi guttā anusāsa rajjaṃ”.",
  "ja509:22.1": "“Rājā ca pabbajjamarocayittha,",
  "ja509:22.2": "Raṭṭhaṃ pahāya naravīraseṭṭho;",
  "ja509:22.3": "Ahampi ekā carissāmi loke,",
  "ja509:22.4": "Hitvāna kāmāni manoramāni.",
  "ja509:23.1": "Rājā ca pabbajjamarocayittha,",
  "ja509:23.2": "Raṭṭhaṃ pahāya naravīraseṭṭho;",
  "ja509:23.3": "Ahampi ekā carissāmi loke,",
  "ja509:23.4": "Hitvāna kāmāni yathodhikāni.",
  "ja509:24.1": "Accenti kālā tarayanti rattiyo,",
  "ja509:24.2": "Vayoguṇā anupubbaṃ jahanti;",
  "ja509:24.3": "Ahampi ekā carissāmi loke,",
  "ja509:24.4": "Hitvāna kāmāni manoramāni.",
  "ja509:25.1": "Accenti kālā tarayanti rattiyo,",
  "ja509:25.2": "Vayoguṇā anupubbaṃ jahanti;",
  "ja509:25.3": "Ahampi ekā carissāmi loke,",
  "ja509:25.4": "Hitvāna kāmāni yathodhikāni.",
  "ja509:26.1": "Accenti kālā tarayanti rattiyo,",
  "ja509:26.2": "Vayoguṇā anupubbaṃ jahanti;",
  "ja509:26.3": "Ahampi ekā carissāmi loke,",
  "ja509:26.4": "Sītibhūtā sabbamaticca saṅgan”ti.",
  "ja509:27.1": "Hatthipālajātakaṃ terasamaṃ."
}