{
  "ja513:0.1": "Jātaka",
  "ja513:0.2": "Tiṃsanipāta",
  "ja513:0.3": "Kiṃchandavagga",
  "ja513:0.4": "3. Jayaddisajātaka",
  "ja513:1.1": "“Cirassaṃ vata me udapādi ajja,",
  "ja513:1.2": "Bhakkho mahā sattamibhattakāle;",
  "ja513:1.3": "Kutosi ko vāsi tadiṅgha brūhi,",
  "ja513:1.4": "Ācikkha jātiṃ vidito yathāsi”.",
  "ja513:2.1": "“Pañcālarājā migavaṃ paviṭṭho,",
  "ja513:2.2": "Jayaddiso nāma yadissuto te;",
  "ja513:2.3": "Carāmi kacchāni vanāni cāhaṃ,",
  "ja513:2.4": "Pasadaṃ imaṃ khāda mamajja muñca”.",
  "ja513:3.1": "“Seneva tvaṃ paṇasi sassamāno,",
  "ja513:3.2": "Mamesa bhakkho pasado yaṃ vadesi;",
  "ja513:3.3": "Taṃ khādiyāna pasadaṃ jighaññaṃ,",
  "ja513:3.4": "Khādissaṃ pacchā na vilāpakālo”.",
  "ja513:4.1": "“Na catthi mokkho mama nikkayena,",
  "ja513:4.2": "Gantvāna paccāgamanāya paṇhe;",
  "ja513:4.3": "Taṃ saṅgaraṃ brāhmaṇassappadāya,",
  "ja513:4.4": "Saccānurakkhī punarāvajissaṃ”.",
  "ja513:5.1": "“Kiṃ kammajātaṃ anutappate tvaṃ,",
  "ja513:5.2": "Pattaṃ samīpaṃ maraṇassa rāja;",
  "ja513:5.3": "Ācikkha me taṃ api sakkuṇemu,",
  "ja513:5.4": "Anujānituṃ āgamanāya paṇhe”.",
  "ja513:6.1": "“Katā mayā brāhmaṇassa dhanāsā,",
  "ja513:6.2": "Taṃ saṅgaraṃ paṭimukkaṃ na muttaṃ;",
  "ja513:6.3": "Taṃ saṅgaraṃ brāhmaṇassappadāya,",
  "ja513:6.4": "Saccānurakkhī punarāvajissaṃ”.",
  "ja513:7.1": "“Yā te katā brāhmaṇassa dhanāsā,",
  "ja513:7.2": "Taṃ saṅgaraṃ paṭimukkaṃ na muttaṃ;",
  "ja513:7.3": "Taṃ saṅgaraṃ brāhmaṇassappadāya,",
  "ja513:7.4": "Saccānurakkhī punarāvajassu”.",
  "ja513:8.1": "Mutto ca so porisādassa hatthā,",
  "ja513:8.2": "Gantvā sakaṃ mandiraṃ kāmakāmī;",
  "ja513:8.3": "Taṃ saṅgaraṃ brāhmaṇassappadāya,",
  "ja513:8.4": "Āmantayī puttamalīnasattaṃ.",
  "ja513:9.1": "“Ajjeva rajjaṃ abhisiñcayassu,",
  "ja513:9.2": "Dhammaṃ cara sesu paresu cāpi;",
  "ja513:9.3": "Adhammakāro ca te māhu raṭṭhe,",
  "ja513:9.4": "Gacchāmahaṃ porisādassa ñatte”.",
  "ja513:10.1": "“Kiṃ kamma kubbaṃ tava deva pāva,",
  "ja513:10.2": "Nārādhayī taṃ tadicchāmi sotuṃ;",
  "ja513:10.3": "Yamajja rajjamhi udassaye tuvaṃ,",
  "ja513:10.4": "Rajjampi niccheyyaṃ tayā vināhaṃ”.",
  "ja513:11.1": "“Na kammunā vā vacasā va tāta,",
  "ja513:11.2": "Aparādhitohaṃ tuviyaṃ sarāmi;",
  "ja513:11.3": "Sandhiñca katvā purisādakena,",
  "ja513:11.4": "Saccānurakkhī punāhaṃ gamissaṃ”.",
  "ja513:12.1": "“Ahaṃ gamissāmi idheva hohi,",
  "ja513:12.2": "Natthi tato jīvato vippamokkho;",
  "ja513:12.3": "Sace tuvaṃ gacchasiyeva rāja,",
  "ja513:12.4": "Ahampi gacchāmi ubho na homa”.",
  "ja513:13.1": "“Addhā hi tāta satānesa dhammo,",
  "ja513:13.2": "Maraṇā ca me dukkhataraṃ tadassa;",
  "ja513:13.3": "Kammāsapādo taṃ yadā pacitvā,",
  "ja513:13.4": "Pasayha khāde bhidā rukkhasūle”.",
  "ja513:14.1": "“Pāṇena te pāṇamahaṃ nimissaṃ,",
  "ja513:14.2": "Mā tvaṃ agā porisādassa ñatte;",
  "ja513:14.3": "Etañca te pāṇamahaṃ nimissaṃ,",
  "ja513:14.4": "Tasmā mataṃ jīvitassa vaṇṇemi”.",
  "ja513:15.1": "Tato have dhitimā rājaputto,",
  "ja513:15.2": "Vanditvā mātu ca pitu ca pāde;",
  "ja513:15.3": "Dukhinissa mātā nipatā pathabyā,",
  "ja513:15.4": "Pitāssa paggayha bhujāni kandati.",
  "ja513:16.1": "Taṃ gacchantaṃ tāva pitā viditvā,",
  "ja513:16.2": "Parammukho vandati pañjalīko;",
  "ja513:16.3": "“Somo ca rājā varuṇo ca rājā,",
  "ja513:16.4": "Pajāpatī candimā sūriyo ca;",
  "ja513:16.5": "Etehi gutto purisādakamhā,",
  "ja513:16.6": "Anuññāto sotthi paccehi tāta”.",
  "ja513:17.1": "“Yaṃ daṇḍakirañño gatassa mātā,",
  "ja513:17.2": "Rāmassakāsi sotthānaṃ suguttā;",
  "ja513:17.3": "Taṃ te ahaṃ sotthānaṃ karomi,",
  "ja513:17.4": "Etena saccena sarantu devā;",
  "ja513:17.5": "Anuññāto sotthi paccehi putta”.",
  "ja513:18.1": "“Āvī raho vāpi manopadosaṃ,",
  "ja513:18.2": "Nāhaṃ sare jātu malīnasatte;",
  "ja513:18.3": "Etena saccena sarantu devā,",
  "ja513:18.4": "Anuññāto sotthi paccehi bhātika”.",
  "ja513:19.1": "“Yasmā ca me anadhimanosi sāmi,",
  "ja513:19.2": "Na cāpi me manasā appiyosi;",
  "ja513:19.3": "Etena saccena sarantu devā,",
  "ja513:19.4": "Anuññāto sotthi paccehi sāmi”.",
  "ja513:20.1": "“Brahā ujū cārumukho kutosi,",
  "ja513:20.2": "Na maṃ pajānāsi vane vasantaṃ;",
  "ja513:20.3": "Luddaṃ maṃ ñatvā ‘purisādako’ti,",
  "ja513:20.4": "Ko sotthimājānamidhāvajeyya”.",
  "ja513:21.1": "“Jānāmi ludda purisādako tvaṃ,",
  "ja513:21.2": "Na taṃ na jānāmi vane vasantaṃ;",
  "ja513:21.3": "Ahañca puttosmi jayaddisassa,",
  "ja513:21.4": "Mamajja khāda pituno pamokkhā”.",
  "ja513:22.1": "“Jānāmi puttoti jayaddisassa,",
  "ja513:22.2": "Tathā hi vo mukhavaṇṇo ubhinnaṃ;",
  "ja513:22.3": "Sudukkaraññeva kataṃ tavedaṃ,",
  "ja513:22.4": "Yo mattumicche pituno pamokkhā”.",
  "ja513:23.1": "“Na dukkaraṃ kiñci mahettha maññe,",
  "ja513:23.2": "Yo mattumicche pituno pamokkhā;",
  "ja513:23.3": "Mātu ca hetu paraloka gantvā,",
  "ja513:23.4": "Sukhena saggena ca sampayutto”.",
  "ja513:24.1": "“Ahañca kho attano pāpakiriyaṃ,",
  "ja513:24.2": "Āvī raho vāpi sare na jātu;",
  "ja513:24.3": "Saṅkhātajātīmaraṇohamasmi,",
  "ja513:24.4": "Yatheva me idha tathā parattha.",
  "ja513:25.1": "Khādajja maṃ dāni mahānubhāva,",
  "ja513:25.2": "Karassu kiccāni imaṃ sarīraṃ;",
  "ja513:25.3": "Rukkhassa vā te papatāmi aggā,",
  "ja513:25.4": "Chādayamāno mayhaṃ tvamadesi maṃsaṃ”.",
  "ja513:26.1": "“Idañca te ruccati rājaputta,",
  "ja513:26.2": "Cajesi pāṇaṃ pituno pamokkhā;",
  "ja513:26.3": "Tasmā hi so tvaṃ taramānarūpo,",
  "ja513:26.4": "Sambhañja kaṭṭhāni jalehi aggiṃ”.",
  "ja513:27.1": "Tato have dhitimā rājaputto,",
  "ja513:27.2": "Dāruṃ samāhatvā mahantamaggiṃ;",
  "ja513:27.3": "Sandīpayitvā paṭivedayittha,",
  "ja513:27.4": "“Ādīpito dāni mahāyamaggi”.",
  "ja513:28.1": "“Khādajja maṃ dāni pasayhakāri,",
  "ja513:28.2": "Kiṃ maṃ muhuṃ pekkhasi haṭṭhalomo;",
  "ja513:28.3": "Tathā tathā tuyhamahaṃ karomi,",
  "ja513:28.4": "Yathā yathā maṃ chādayamāno adesi”.",
  "ja513:29.1": "“Ko tādisaṃ arahati khāditāye,",
  "ja513:29.2": "Dhamme ṭhitaṃ saccavādiṃ vadaññuṃ;",
  "ja513:29.3": "Muddhāpi tassa viphaleyya sattadhā,",
  "ja513:29.4": "Yo tādisaṃ saccavādiṃ adeyya”.",
  "ja513:30.1": "“Idañhi so brāhmaṇaṃ maññamāno,",
  "ja513:30.2": "Saso avāsesi sake sarīre;",
  "ja513:30.3": "Teneva so candimā devaputto,",
  "ja513:30.4": "Sasatthuto kāmaduhajja yakkha”.",
  "ja513:31.1": "“Cando yathā rāhumukhā pamutto,",
  "ja513:31.2": "Virocate pannaraseva bhāṇumā;",
  "ja513:31.3": "Evaṃ tuvaṃ porisādā pamutto,",
  "ja513:31.4": "Viroca kapile mahānubhāva;",
  "ja513:31.5": "Āmodayaṃ pitaraṃ mātarañca,",
  "ja513:31.6": "Sabbo ca te nandatu ñātipakkho”.",
  "ja513:32.1": "Tato have dhitimā rājaputto,",
  "ja513:32.2": "Katañjalī pariyāya porisādaṃ;",
  "ja513:32.3": "Anuññāto sotthi sukhī arogo,",
  "ja513:32.4": "Paccāgamā kapilamalīnasatto.",
  "ja513:33.1": "Taṃ negamā jānapadā ca sabbe,",
  "ja513:33.2": "Hatthārohā rathikā pattikā ca;",
  "ja513:33.3": "Namassamānā pañjalikā upāgamuṃ,",
  "ja513:33.4": "“Namatthu te dukkarakārakosī”ti.",
  "ja513:34.1": "Jayaddisajātakaṃ tatiyaṃ."
}