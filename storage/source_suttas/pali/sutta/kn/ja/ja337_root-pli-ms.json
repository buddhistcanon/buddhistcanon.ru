{
  "ja337:0.1": "Jātaka",
  "ja337:0.2": "Catukkanipāta",
  "ja337:0.3": "Kokilavagga",
  "ja337:0.4": "7. Pīṭhajātaka",
  "ja337:1.1": "“Na te pīṭhamadāyimhā,",
  "ja337:1.2": "na pānaṃ napi bhojanaṃ;",
  "ja337:1.3": "Brahmacāri khamassu me,",
  "ja337:1.4": "etaṃ passāmi accayaṃ”.",
  "ja337:2.1": "“Nevābhisajjāmi na cāpi kuppe,",
  "ja337:2.2": "Na cāpi me appiyamāsi kiñci;",
  "ja337:2.3": "Athopi me āsi manovitakko,",
  "ja337:2.4": "Etādiso nūna kulassa dhammo”.",
  "ja337:3.1": "“Esasmākaṃ kule dhammo,",
  "ja337:3.2": "pitupitāmaho sadā;",
  "ja337:3.3": "Āsanaṃ udakaṃ pajjaṃ,",
  "ja337:3.4": "sabbetaṃ nipadāmase.",
  "ja337:4.1": "Esasmākaṃ kule dhammo,",
  "ja337:4.2": "pitupitāmaho sadā;",
  "ja337:4.3": "Sakkaccaṃ upatiṭṭhāma,",
  "ja337:4.4": "uttamaṃ viya ñātakan”ti.",
  "ja337:5.1": "Pīṭhajātakaṃ sattamaṃ."
}