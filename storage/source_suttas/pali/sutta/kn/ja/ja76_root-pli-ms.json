{
  "ja76:0.1": "Jātaka",
  "ja76:0.2": "Ekakanipāta",
  "ja76:0.3": "Varuṇavagga",
  "ja76:0.4": "6. Asaṃkiyajātaka",
  "ja76:1.1": "“Asaṅkiyomhi gāmamhi,",
  "ja76:1.2": "araññe natthi me bhayaṃ;",
  "ja76:1.3": "Ujuṃ maggaṃ samārūḷho,",
  "ja76:1.4": "mettāya karuṇāya cā”ti.",
  "ja76:2.1": "Asaṃkiyajātakaṃ chaṭṭhaṃ."
}