{
  "ja496:0.1": "Jātaka",
  "ja496:0.2": "Pakiṇṇakanipāta",
  "ja496:0.3": "Sālikedāravagga",
  "ja496:0.4": "13. Bhikkhāparamparajātaka",
  "ja496:1.1": "“Sukhumālarūpaṃ disvā,",
  "ja496:1.2": "raṭṭhā vivanamāgataṃ;",
  "ja496:1.3": "Kūṭāgāravarūpetaṃ,",
  "ja496:1.4": "mahāsayanamupāsitaṃ.",
  "ja496:2.1": "Tassa te pemakenāhaṃ,",
  "ja496:2.2": "adāsiṃ vaḍḍhamodanaṃ;",
  "ja496:2.3": "Sālīnaṃ vicitaṃ bhattaṃ,",
  "ja496:2.4": "suciṃ maṃsūpasecanaṃ.",
  "ja496:3.1": "Taṃ tvaṃ bhattaṃ paṭiggayha,",
  "ja496:3.2": "brāhmaṇassa adāsayi;",
  "ja496:3.3": "Attānaṃ anasitvāna,",
  "ja496:3.4": "koyaṃ dhammo namatthu te”.",
  "ja496:4.1": "“Ācariyo brāhmaṇo mayhaṃ,",
  "ja496:4.2": "kiccākiccesu byāvaṭo;",
  "ja496:4.3": "Garu ca āmantanīyo ca,",
  "ja496:4.4": "dātumarahāmi bhojanaṃ”.",
  "ja496:5.1": "“Brāhmaṇaṃ dāni pucchāmi,",
  "ja496:5.2": "gotamaṃ rājapūjitaṃ;",
  "ja496:5.3": "Rājā te bhattaṃ pādāsi,",
  "ja496:5.4": "suciṃ maṃsūpasecanaṃ.",
  "ja496:6.1": "Taṃ tvaṃ bhattaṃ paṭiggayha,",
  "ja496:6.2": "isissa bhojanaṃ adā;",
  "ja496:6.3": "Akhettaññūsi dānassa,",
  "ja496:6.4": "koyaṃ dhammo namatthu te”.",
  "ja496:7.1": "“Bharāmi puttadāre ca,",
  "ja496:7.2": "gharesu gadhito ahaṃ;",
  "ja496:7.3": "Bhuñje mānusake kāme,",
  "ja496:7.4": "anusāsāmi rājino.",
  "ja496:8.1": "Āraññikassa isino,",
  "ja496:8.2": "cirarattaṃ tapassino;",
  "ja496:8.3": "Vuḍḍhassa bhāvitattassa,",
  "ja496:8.4": "dātumarahāmi bhojanaṃ”.",
  "ja496:9.1": "“Isiñca dāni pucchāmi,",
  "ja496:9.2": "kisaṃ dhamanisanthataṃ;",
  "ja496:9.3": "Parūḷhakacchanakhalomaṃ,",
  "ja496:9.4": "paṅkadantaṃ rajassiraṃ.",
  "ja496:10.1": "Eko araññe viharasi,",
  "ja496:10.2": "nāvakaṅkhasi jīvitaṃ;",
  "ja496:10.3": "Bhikkhu kena tayā seyyo,",
  "ja496:10.4": "yassa tvaṃ bhojanaṃ adā”.",
  "ja496:11.1": "“Khaṇantālukalambāni,",
  "ja496:11.2": "bilālitakkalāni ca;",
  "ja496:11.3": "Dhunaṃ sāmākanīvāraṃ,",
  "ja496:11.4": "saṃghāriyaṃ pasāriyaṃ.",
  "ja496:12.1": "Sākaṃ bhisaṃ madhuṃ maṃsaṃ,",
  "ja496:12.2": "Badarāmalakāni ca;",
  "ja496:12.3": "Tāni āharitvā bhuñjāmi,",
  "ja496:12.4": "Atthi me so pariggaho.",
  "ja496:13.1": "Pacanto apacantassa,",
  "ja496:13.2": "amamassa sakiñcano;",
  "ja496:13.3": "Anādānassa sādāno,",
  "ja496:13.4": "dātumarahāmi bhojanaṃ”.",
  "ja496:14.1": "“Bhikkhuñca dāni pucchāmi,",
  "ja496:14.2": "tuṇhīmāsīna subbataṃ;",
  "ja496:14.3": "Isi te bhattaṃ pādāsi,",
  "ja496:14.4": "suciṃ maṃsūpasecanaṃ.",
  "ja496:15.1": "Taṃ tvaṃ bhattaṃ paṭiggayha,",
  "ja496:15.2": "tuṇhī bhuñjasi ekako;",
  "ja496:15.3": "Nāññaṃ kañci nimantesi,",
  "ja496:15.4": "koyaṃ dhammo namatthu te”.",
  "ja496:16.1": "“Na pacāmi na pācemi,",
  "ja496:16.2": "na chindāmi na chedaye;",
  "ja496:16.3": "Taṃ maṃ akiñcanaṃ ñatvā,",
  "ja496:16.4": "sabbapāpehi ārataṃ.",
  "ja496:17.1": "Vāmena bhikkhamādāya,",
  "ja496:17.2": "dakkhiṇena kamaṇḍaluṃ;",
  "ja496:17.3": "Isi me bhattaṃ pādāsi,",
  "ja496:17.4": "suciṃ maṃsūpasecanaṃ.",
  "ja496:18.1": "Ete hi dātumarahanti,",
  "ja496:18.2": "samamā sapariggahā;",
  "ja496:18.3": "Paccanīkamahaṃ maññe,",
  "ja496:18.4": "yo dātāraṃ nimantaye”.",
  "ja496:19.1": "“Atthāya vata me ajja,",
  "ja496:19.2": "idhāgacchi rathesabho;",
  "ja496:19.3": "Sohaṃ ajja pajānāmi,",
  "ja496:19.4": "yattha dinnaṃ mahapphalaṃ.",
  "ja496:20.1": "Raṭṭhesu giddhā rājāno,",
  "ja496:20.2": "Kiccākiccesu brāhmaṇā;",
  "ja496:20.3": "Isī mūlaphale giddhā,",
  "ja496:20.4": "Vippamuttā ca bhikkhavo”ti. (2219)",
  "ja496:21.1": "Bhikkhāparamparajātakaṃ terasamaṃ.",
  "ja496:22.1": "Pakiṇṇakanipātaṃ niṭṭhitaṃ.",
  "ja496:23.1": "Tassuddānaṃ",
  "ja496:24.1": "Suva kinnara mukka kharājinaso,",
  "ja496:24.2": "Bhisajāta mahesi kapotavaro;",
  "ja496:24.3": "Atha mora satacchaka vāṇijako,",
  "ja496:24.4": "Atha rāja sabrāhmaṇa bhikkhaparanti."
}