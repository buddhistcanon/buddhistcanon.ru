{
  "ja402:0.1": "Jātaka",
  "ja402:0.2": "Sattakanipāta",
  "ja402:0.3": "Kukkuvagga",
  "ja402:0.4": "7. Sattubhastajātaka",
  "ja402:1.1": "“Vibbhantacitto kupitindriyosi,",
  "ja402:1.2": "Nettehi te vārigaṇā savanti;",
  "ja402:1.3": "Kiṃ te naṭṭhaṃ kiṃ pana patthayāno,",
  "ja402:1.4": "Idhāgamā brahme tadiṅgha brūhi”.",
  "ja402:2.1": "“Miyyetha bhariyā vajato mamajja,",
  "ja402:2.2": "Agacchato maraṇamāha yakkho;",
  "ja402:2.3": "Etena dukkhena pavedhitosmi,",
  "ja402:2.4": "Akkhāhi me senaka etamatthaṃ”.",
  "ja402:3.1": "“Bahūni ṭhānāni vicintayitvā,",
  "ja402:3.2": "Yamettha vakkhāmi tadeva saccaṃ;",
  "ja402:3.3": "Maññāmi te brāhmaṇa sattubhastaṃ,",
  "ja402:3.4": "Ajānato kaṇhasappo paviṭṭho.",
  "ja402:4.1": "Ādāya daṇḍaṃ parisumbha bhastaṃ,",
  "ja402:4.2": "Passeḷamūgaṃ uragaṃ dujivhaṃ;",
  "ja402:4.3": "Chindajja kaṅkhaṃ vicikicchitāni,",
  "ja402:4.4": "Bhujaṅgamaṃ passa pamuñca bhastaṃ”.",
  "ja402:5.1": "Saṃviggarūpo parisāya majjhe,",
  "ja402:5.2": "So brāhmaṇo sattubhastaṃ pamuñci;",
  "ja402:5.3": "Atha nikkhami urago uggatejo,",
  "ja402:5.4": "Āsīviso sappo phaṇaṃ karitvā.",
  "ja402:6.1": "“Suladdhalābhā janakassa rañño,",
  "ja402:6.2": "Yo passatī senakaṃ sādhupaññaṃ;",
  "ja402:6.3": "Vivaṭṭachaddo nusi sabbadassī,",
  "ja402:6.4": "Ñāṇaṃ nu te brāhmaṇa bhiṃsarūpaṃ”.",
  "ja402:7.1": "“Imāni me sattasatāni atthi,",
  "ja402:7.2": "Gaṇhāhi sabbāni dadāmi tuyhaṃ;",
  "ja402:7.3": "Tayā hi me jīvitamajja laddhaṃ,",
  "ja402:7.4": "Athopi bhariyāya makāsi sotthiṃ”.",
  "ja402:8.1": "“Na paṇḍitā vetanamādiyanti,",
  "ja402:8.2": "Citrāhi gāthāhi subhāsitāhi;",
  "ja402:8.3": "Itopi te brahme dadantu vittaṃ,",
  "ja402:8.4": "Ādāya tvaṃ gaccha sakaṃ niketan”ti.",
  "ja402:9.1": "Sattubhastajātakaṃ sattamaṃ."
}