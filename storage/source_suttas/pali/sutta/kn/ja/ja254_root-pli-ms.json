{
  "ja254:0.1": "Jātaka",
  "ja254:0.2": "Tikanipāta",
  "ja254:0.3": "Saṅkappavagga",
  "ja254:0.4": "4. Kuṇḍakakucchisindhavajātaka",
  "ja254:1.1": "“Bhutvā tiṇaparighāsaṃ,",
  "ja254:1.2": "bhutvā ācāmakuṇḍakaṃ;",
  "ja254:1.3": "Etaṃ te bhojanaṃ āsi,",
  "ja254:1.4": "kasmā dāni na bhuñjasi”.",
  "ja254:2.1": "“Yattha posaṃ na jānanti,",
  "ja254:2.2": "jātiyā vinayena vā;",
  "ja254:2.3": "Bahuṃ tattha mahābrahme,",
  "ja254:2.4": "api ācāmakuṇḍakaṃ.",
  "ja254:3.1": "Tvañca kho maṃ pajānāsi,",
  "ja254:3.2": "yādisāyaṃ hayuttamo;",
  "ja254:3.3": "Jānanto jānamāgamma,",
  "ja254:3.4": "na te bhakkhāmi kuṇḍakan”ti.",
  "ja254:4.1": "Kuṇḍakakucchisindhavajātakaṃ catutthaṃ."
}