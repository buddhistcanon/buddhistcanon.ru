{
  "ja230:0.1": "Jātaka",
  "ja230:0.2": "Dukanipāta",
  "ja230:0.3": "Kāsāvavagga",
  "ja230:0.4": "10. Dutiyapalāyitajātaka",
  "ja230:1.1": "“Dhajamaparimitaṃ anantapāraṃ,",
  "ja230:1.2": "Duppasahaṃ dhaṅkehi sāgaraṃva;",
  "ja230:1.3": "Girimiva anilena duppasayho,",
  "ja230:1.4": "Duppasaho ahamajjatādisena”.",
  "ja230:2.1": "“Mā bāliyaṃ vilapi na hissa tādisaṃ,",
  "ja230:2.2": "Viḍayhase na hi labhase nisedhakaṃ;",
  "ja230:2.3": "Āsajjasi gajamiva ekacārinaṃ,",
  "ja230:2.4": "Yo taṃ padā naḷamiva pothayissatī”ti.",
  "ja230:3.1": "Dutiyapalāyitajātakaṃ dasamaṃ.",
  "ja230:3.2": "Kāsāvavaggo aṭṭhamo.",
  "ja230:4.1": "Tassuddānaṃ",
  "ja230:5.1": "Varavatthavaco dumakhīṇaphalaṃ,",
  "ja230:5.2": "Caturodhammavaraṃ purisuttama;",
  "ja230:5.3": "Dhaṅkamagadhā ca tayogirināma,",
  "ja230:5.4": "Gajaggavaro dhajavarena dasāti."
}