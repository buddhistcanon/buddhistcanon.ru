{
  "ja117:0.1": "Jātaka",
  "ja117:0.2": "Ekakanipāta",
  "ja117:0.3": "Haṃcivagga",
  "ja117:0.4": "7. Tittirajātaka",
  "ja117:1.1": "“Accuggatātilapatā,",
  "ja117:1.2": "ativelaṃ pabhāsitā;",
  "ja117:1.3": "Vācā hanati dummedhaṃ,",
  "ja117:1.4": "tittiraṃvātivassitan”ti.",
  "ja117:2.1": "Tittirajātakaṃ sattamaṃ."
}