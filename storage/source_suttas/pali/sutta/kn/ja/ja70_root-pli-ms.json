{
  "ja70:0.1": "Jātaka",
  "ja70:0.2": "Ekakanipāta",
  "ja70:0.3": "Itthivagga",
  "ja70:0.4": "10. Kuddālajātaka",
  "ja70:1.1": "“Na taṃ jitaṃ sādhu jitaṃ,",
  "ja70:1.2": "yaṃ jitaṃ avajīyati;",
  "ja70:1.3": "Taṃ kho jitaṃ sādhu jitaṃ,",
  "ja70:1.4": "yaṃ jitaṃ nāvajīyatī”ti.",
  "ja70:2.1": "Kuddālajātakaṃ dasamaṃ.",
  "ja70:2.2": "Itthivaggo sattamo.",
  "ja70:3.1": "Tassuddānaṃ",
  "ja70:4.1": "Sikhīsabbaghasopi ca vīṇavaro,",
  "ja70:4.2": "Pisuṇā mittabhedikā nandī nadī;",
  "ja70:4.3": "Mudulakkhaṇa sodariyā ca mano,",
  "ja70:4.4": "Visa sādhujitena bhavanti dasāti."
}