{
  "ja369:0.1": "Jātaka",
  "ja369:0.2": "Pañcakanipāta",
  "ja369:0.3": "Vaṇṇārohavagga",
  "ja369:0.4": "9. Mittavindakajātaka",
  "ja369:1.1": "“Kyāhaṃ devānamakaraṃ,",
  "ja369:1.2": "kiṃ pāpaṃ pakataṃ mayā;",
  "ja369:1.3": "Yaṃ me sirasmiṃ ohacca,",
  "ja369:1.4": "cakkaṃ bhamati matthake”.",
  "ja369:2.1": "“Atikkamma ramaṇakaṃ,",
  "ja369:2.2": "sadāmattañca dūbhakaṃ;",
  "ja369:2.3": "Brahmattarañca pāsādaṃ,",
  "ja369:2.4": "kenatthena idhāgato”.",
  "ja369:3.1": "“Ito bahutarā bhogā,",
  "ja369:3.2": "atra maññe bhavissare;",
  "ja369:3.3": "Iti etāya saññāya,",
  "ja369:3.4": "passa maṃ byasanaṃ gataṃ”.",
  "ja369:4.1": "“Catubbhi aṭṭhajjhagamā,",
  "ja369:4.2": "aṭṭhāhipi ca soḷasa;",
  "ja369:4.3": "Soḷasāhi ca bāttiṃsa,",
  "ja369:4.4": "atricchaṃ cakkamāsado;",
  "ja369:4.5": "Icchāhatassa posassa,",
  "ja369:4.6": "cakkaṃ bhamati matthake.",
  "ja369:5.1": "Uparivisālā duppūrā,",
  "ja369:5.2": "icchā visaṭagāminī;",
  "ja369:5.3": "Ye ca taṃ anugijjhanti,",
  "ja369:5.4": "te honti cakkadhārino”ti.",
  "ja369:6.1": "Mittavindakajātakaṃ navamaṃ."
}