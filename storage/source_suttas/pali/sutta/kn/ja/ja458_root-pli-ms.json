{
  "ja458:0.1": "Jātaka",
  "ja458:0.2": "Ekādasakanipāta",
  "ja458:0.3": "Mātuposakavagga",
  "ja458:0.4": "4. Udayajātaka",
  "ja458:1.1": "“Ekā nisinnā suci saññatūrū,",
  "ja458:1.2": "Pāsādamāruyha aninditaṅgī;",
  "ja458:1.3": "Yācāmi taṃ kinnaranettacakkhu,",
  "ja458:1.4": "Imekarattiṃ ubhayo vasema”.",
  "ja458:2.1": "“Okiṇṇantaraparikhaṃ,",
  "ja458:2.2": "daḷhamaṭṭālakoṭṭhakaṃ;",
  "ja458:2.3": "Rakkhitaṃ khaggahatthehi,",
  "ja458:2.4": "duppavesamidaṃ puraṃ.",
  "ja458:3.1": "Daharassa yuvino cāpi,",
  "ja458:3.2": "āgamo ca na vijjati;",
  "ja458:3.3": "Atha kena nu vaṇṇena,",
  "ja458:3.4": "saṅgamaṃ icchase mayā”.",
  "ja458:4.1": "“Yakkhohamasmi kalyāṇi,",
  "ja458:4.2": "Āgatosmi tavantike;",
  "ja458:4.3": "Tvaṃ maṃ nandaya bhaddante,",
  "ja458:4.4": "Puṇṇakaṃsaṃ dadāmi te”.",
  "ja458:5.1": "“Devaṃ va yakkhaṃ atha vā manussaṃ,",
  "ja458:5.2": "Na patthaye udayamaticca aññaṃ;",
  "ja458:5.3": "Gaccheva tvaṃ yakkha mahānubhāva,",
  "ja458:5.4": "Mā cassu gantvā punarāvajittha”.",
  "ja458:6.1": "“Yā sā rati uttamā kāmabhoginaṃ,",
  "ja458:6.2": "Yaṃ hetu sattā visamaṃ caranti;",
  "ja458:6.3": "Mā taṃ ratiṃ jīyi tuvaṃ sucimhite,",
  "ja458:6.4": "Dadāmi te rūpiyaṃ kaṃsapūraṃ”.",
  "ja458:7.1": "“Nāriṃ naro nijjhapayaṃ dhanena,",
  "ja458:7.2": "Ukkaṃsatī yattha karoti chandaṃ;",
  "ja458:7.3": "Vipaccanīko tava devadhammo,",
  "ja458:7.4": "Paccakkhato thokatarena esi”.",
  "ja458:8.1": "“Āyu ca vaṇṇo ca manussaloke,",
  "ja458:8.2": "Nihīyati manujānaṃ sugatte;",
  "ja458:8.3": "Teneva vaṇṇena dhanampi tuyhaṃ,",
  "ja458:8.4": "Nihīyati jiṇṇatarāsi ajja.",
  "ja458:9.1": "Evaṃ me pekkhamānassa,",
  "ja458:9.2": "rājaputti yasassini;",
  "ja458:9.3": "Hāyateva tava vaṇṇo,",
  "ja458:9.4": "ahorattānamaccaye”.",
  "ja458:10.1": "“Imināva tvaṃ vayasā,",
  "ja458:10.2": "rājaputti sumedhase;",
  "ja458:10.3": "Brahmacariyaṃ careyyāsi,",
  "ja458:10.4": "bhiyyo vaṇṇavatī siyā”.",
  "ja458:11.1": "“Devā na jīranti yathā manussā,",
  "ja458:11.2": "Gattesu tesaṃ valiyo na honti;",
  "ja458:11.3": "Pucchāmi taṃ yakkha mahānubhāva,",
  "ja458:11.4": "Kathaṃ nu devāna sarīradeho”.",
  "ja458:12.1": "“Devā na jīranti yathā manussā,",
  "ja458:12.2": "Gattesu tesaṃ valiyo na honti;",
  "ja458:12.3": "Suve suve bhiyyatarova tesaṃ,",
  "ja458:12.4": "Dibbo ca vaṇṇo vipulā ca bhogā”.",
  "ja458:13.1": "“Kiṃsūdha bhītā janatā anekā,",
  "ja458:13.2": "Maggo ca nekāyatanaṃ pavutto;",
  "ja458:13.3": "Pucchāmi taṃ yakkha mahānubhāva,",
  "ja458:13.4": "Katthaṭṭhito paralokaṃ na bhāye”.",
  "ja458:14.1": "“Vācaṃ manañca paṇidhāya sammā,",
  "ja458:14.2": "Kāyena pāpāni akubbamāno;",
  "ja458:14.3": "Bahunnapānaṃ gharamāvasanto,",
  "ja458:14.4": "Saddho mudū saṃvibhāgī vadaññū;",
  "ja458:14.5": "Saṅgāhako sakhilo saṇhavāco,",
  "ja458:14.6": "Etthaṭṭhito paralokaṃ na bhāye”.",
  "ja458:15.1": "“Anusāsasi maṃ yakkha,",
  "ja458:15.2": "yathā mātā yathā pitā;",
  "ja458:15.3": "Uḷāravaṇṇaṃ pucchāmi,",
  "ja458:15.4": "ko nu tvamasi subrahā”.",
  "ja458:16.1": "“Udayohamasmi kalyāṇi,",
  "ja458:16.2": "saṅgarattā idhāgato;",
  "ja458:16.3": "Āmanta kho taṃ gacchāmi,",
  "ja458:16.4": "muttosmi tava saṅgarā”.",
  "ja458:17.1": "“Sace kho tvaṃ udayosi,",
  "ja458:17.2": "saṅgarattā idhāgato;",
  "ja458:17.3": "Anusāsa maṃ rājaputta,",
  "ja458:17.4": "yathāssa puna saṅgamo”.",
  "ja458:18.1": "“Atipatati vayo khaṇo tatheva,",
  "ja458:18.2": "Ṭhānaṃ natthi dhuvaṃ cavanti sattā;",
  "ja458:18.3": "Parijiyyati addhuvaṃ sarīraṃ,",
  "ja458:18.4": "Udaye mā pamāda carassu dhammaṃ.",
  "ja458:19.1": "Kasiṇā pathavī dhanassa pūrā,",
  "ja458:19.2": "Ekasseva siyā anaññadheyyā;",
  "ja458:19.3": "Tañcāpi jahati avītarāgo,",
  "ja458:19.4": "Udaye mā pamāda carassu dhammaṃ.",
  "ja458:20.1": "Mātā ca pitā ca bhātaro ca,",
  "ja458:20.2": "Bhariyā yāpi dhanena hoti kītā;",
  "ja458:20.3": "Te cāpi jahanti aññamaññaṃ,",
  "ja458:20.4": "Udaye mā pamāda carassu dhammaṃ.",
  "ja458:21.1": "Kāyo parabhojananti ñatvā,",
  "ja458:21.2": "Saṃsāre sugatiñca duggatiñca;",
  "ja458:21.3": "Ittaravāsoti jāniyāna,",
  "ja458:21.4": "Udaye mā pamāda carassu dhammaṃ”.",
  "ja458:22.1": "“Sādhu bhāsatiyaṃ yakkho,",
  "ja458:22.2": "appaṃ maccāna jīvitaṃ;",
  "ja458:22.3": "Kasirañca parittañca,",
  "ja458:22.4": "tañca dukkhena saṃyutaṃ;",
  "ja458:22.5": "Sāhaṃ ekā pabbajissāmi,",
  "ja458:22.6": "hitvā kāsiṃ surundhanan”ti.",
  "ja458:23.1": "Udayajātakaṃ catutthaṃ."
}