{
  "ja59:0.1": "Jātaka",
  "ja59:0.2": "Ekakanipāta",
  "ja59:0.3": "Āsīsavagga",
  "ja59:0.4": "9. Bherivādakajātaka",
  "ja59:1.1": "“Dhame dhame nātidhame,",
  "ja59:1.2": "atidhantañhi pāpakaṃ;",
  "ja59:1.3": "Dhantena hi sataṃ laddhaṃ,",
  "ja59:1.4": "atidhantena nāsitan”ti.",
  "ja59:2.1": "Bherivādakajātakaṃ navamaṃ."
}