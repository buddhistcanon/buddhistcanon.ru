{
  "ja416:0.1": "Jātaka",
  "ja416:0.2": "Sattakanipāta",
  "ja416:0.3": "Gandhāravagga",
  "ja416:0.4": "11. Parantapajātaka",
  "ja416:1.1": "“Āgamissati me pāpaṃ,",
  "ja416:1.2": "āgamissati me bhayaṃ;",
  "ja416:1.3": "Tadā hi calitā sākhā,",
  "ja416:1.4": "manussena migena vā”.",
  "ja416:2.1": "“Bhīruyā nūna me kāmo,",
  "ja416:2.2": "avidūre vasantiyā;",
  "ja416:2.3": "Karissati kisaṃ paṇḍuṃ,",
  "ja416:2.4": "sāva sākhā parantapaṃ.",
  "ja416:3.1": "Socayissati maṃ kantā,",
  "ja416:3.2": "gāme vasamaninditā;",
  "ja416:3.3": "Karissati kisaṃ paṇḍuṃ,",
  "ja416:3.4": "sāva sākhā parantapaṃ.",
  "ja416:4.1": "Tayā maṃ asitāpaṅgi,",
  "ja416:4.2": "sitāni bhaṇitāni ca;",
  "ja416:4.3": "Kisaṃ paṇḍuṃ karissanti,",
  "ja416:4.4": "sāva sākhā parantapaṃ”.",
  "ja416:5.1": "“Agamā nūna so saddo,",
  "ja416:5.2": "asaṃsi nūna so tava;",
  "ja416:5.3": "Akkhātaṃ nūna taṃ tena,",
  "ja416:5.4": "yo taṃ sākhamakampayi.",
  "ja416:6.1": "Idaṃ kho taṃ samāgamma,",
  "ja416:6.2": "mama bālassa cintitaṃ;",
  "ja416:6.3": "Tadā hi calitā sākhā,",
  "ja416:6.4": "manussena migena vā”.",
  "ja416:7.1": "“Tatheva tvaṃ avedesi,",
  "ja416:7.2": "Avañci pitaraṃ mama;",
  "ja416:7.3": "Hantvā sākhāhi chādento,",
  "ja416:7.4": "Āgamissati me bhayan”ti. (1140)",
  "ja416:8.1": "Parantapajātakaṃ ekādasamaṃ.",
  "ja416:8.2": "Gandhāravaggo dutiyo.",
  "ja416:9.1": "Tassuddānaṃ",
  "ja416:10.1": "Varagāma mahākapi bhaggava ca,",
  "ja416:10.2": "Daḷhadhamma sakuñjara kesavaro;",
  "ja416:10.3": "Urago vidhuro puna jāgarataṃ,",
  "ja416:10.4": "Atha kosalādhipa parantapa cāti.",
  "ja416:11.1": "Atha vagguddānaṃ",
  "ja416:12.1": "Atha sattanipātamhi,",
  "ja416:12.2": "vaggaṃ me bhaṇato suṇa;",
  "ja416:12.3": "Kukku ca puna gandhāro,",
  "ja416:12.4": "dveva vuttā mahesināti.",
  "ja416:13.1": "Sattakanipātaṃ niṭṭhitaṃ."
}