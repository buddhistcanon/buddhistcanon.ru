{
  "ja243:0.1": "Jātaka",
  "ja243:0.2": "Dukanipāta",
  "ja243:0.3": "Siṅgālavagga",
  "ja243:0.4": "3. Guttilajātaka",
  "ja243:1.1": "“Sattatantiṃ sumadhuraṃ,",
  "ja243:1.2": "rāmaṇeyyaṃ avācayiṃ;",
  "ja243:1.3": "So maṃ raṅgamhi avheti,",
  "ja243:1.4": "saraṇaṃ me hohi kosiya”.",
  "ja243:2.1": "“Ahaṃ taṃ saraṇaṃ samma,",
  "ja243:2.2": "Ahamācariyapūjako;",
  "ja243:2.3": "Na taṃ jayissati sisso,",
  "ja243:2.4": "Sissamācariya jessasī”ti.",
  "ja243:3.1": "Guttilajātakaṃ tatiyaṃ."
}