{
  "ja415:0.1": "Jātaka",
  "ja415:0.2": "Sattakanipāta",
  "ja415:0.3": "Gandhāravagga",
  "ja415:0.4": "10. Kummāsapiṇḍijātaka",
  "ja415:1.1": "“Na kiratthi anomadassisu,",
  "ja415:1.2": "Pāricariyā buddhesu appikā;",
  "ja415:1.3": "Sukkhāya aloṇikāya ca,",
  "ja415:1.4": "Passaphalaṃ kummāsapiṇḍiyā.",
  "ja415:2.1": "Hatthigavassā cime bahū,",
  "ja415:2.2": "Dhanadhaññaṃ pathavī ca kevalā;",
  "ja415:2.3": "Nāriyo cimā accharūpamā,",
  "ja415:2.4": "Passa phalaṃ kummāsapiṇḍiyā”.",
  "ja415:3.1": "“Abhikkhaṇaṃ rājakuñjara,",
  "ja415:3.2": "Gāthā bhāsasi kosalādhipa;",
  "ja415:3.3": "Pucchāmi taṃ raṭṭhavaḍḍhana,",
  "ja415:3.4": "Bāḷhaṃ pītimano pabhāsasi”.",
  "ja415:4.1": "“Imasmiññeva nagare,",
  "ja415:4.2": "kule aññatare ahuṃ;",
  "ja415:4.3": "Parakammakaro āsiṃ,",
  "ja415:4.4": "bhatako sīlasaṃvuto.",
  "ja415:5.1": "Kammāya nikkhamantohaṃ,",
  "ja415:5.2": "caturo samaṇeddasaṃ;",
  "ja415:5.3": "Ācārasīlasampanne,",
  "ja415:5.4": "sītibhūte anāsave.",
  "ja415:6.1": "Tesu cittaṃ pasādetvā,",
  "ja415:6.2": "nisīdetvā paṇṇasanthate;",
  "ja415:6.3": "Adaṃ buddhāna kummāsaṃ,",
  "ja415:6.4": "pasanno sehi pāṇibhi.",
  "ja415:7.1": "Tassa kammassa kusalassa,",
  "ja415:7.2": "idaṃ me edisaṃ phalaṃ;",
  "ja415:7.3": "Anubhomi idaṃ rajjaṃ,",
  "ja415:7.4": "phītaṃ dharaṇimuttamaṃ”.",
  "ja415:8.1": "“Dadaṃ bhuñja mā ca pamādo,",
  "ja415:8.2": "Cakkaṃ vattaya kosalādhipa;",
  "ja415:8.3": "Mā rāja adhammiko ahu,",
  "ja415:8.4": "Dhammaṃ pālaya kosalādhipa”.",
  "ja415:9.1": "“Sohaṃ tadeva punappunaṃ,",
  "ja415:9.2": "Vaṭumaṃ ācarissāmi sobhane;",
  "ja415:9.3": "Ariyācaritaṃ sukosale,",
  "ja415:9.4": "Arahanto me manāpāva passituṃ.",
  "ja415:10.1": "Devī viya accharūpamā,",
  "ja415:10.2": "Majjhe nārigaṇassa sobhasi;",
  "ja415:10.3": "Kiṃ kammamakāsi bhaddakaṃ,",
  "ja415:10.4": "Kenāsi vaṇṇavatī sukosale”.",
  "ja415:11.1": "“Ambaṭṭhakulassa khattiya,",
  "ja415:11.2": "Dāsyāhaṃ parapesiyā ahuṃ;",
  "ja415:11.3": "Saññatā ca dhammajīvinī,",
  "ja415:11.4": "Sīlavatī ca apāpadassanā.",
  "ja415:12.1": "Uddhaṭabhattaṃ ahaṃ tadā,",
  "ja415:12.2": "Caramānassa adāsi bhikkhuno;",
  "ja415:12.3": "Vittā sumanā sayaṃ ahaṃ,",
  "ja415:12.4": "Tassa kammassa phalaṃ mamedisan”ti.",
  "ja415:13.1": "Kummāsapiṇḍijātakaṃ dasamaṃ."
}