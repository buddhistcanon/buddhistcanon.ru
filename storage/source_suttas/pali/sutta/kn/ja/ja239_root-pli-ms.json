{
  "ja239:0.1": "Jātaka",
  "ja239:0.2": "Dukanipāta",
  "ja239:0.3": "Upāhanavagga",
  "ja239:0.4": "9. Haritamaṇḍūkajātaka",
  "ja239:1.1": "“Āsīvisampi maṃ santaṃ,",
  "ja239:1.2": "Paviṭṭhaṃ kumināmukhaṃ;",
  "ja239:1.3": "Ruccate haritāmātā,",
  "ja239:1.4": "Yaṃ maṃ khādanti macchakā”.",
  "ja239:2.1": "“Vilumpateva puriso,",
  "ja239:2.2": "yāvassa upakappati;",
  "ja239:2.3": "Yadā caññe vilumpanti,",
  "ja239:2.4": "so vilutto vilumpatī”ti.",
  "ja239:3.1": "Haritamaṇḍūkajātakaṃ navamaṃ."
}