{
  "ja476:0.1": "Jātaka",
  "ja476:0.2": "Terasakanipāta",
  "ja476:0.3": "Ambavagga",
  "ja476:0.4": "3. Javanahaṃsajātaka",
  "ja476:1.1": "“Idheva haṃsa nipata,",
  "ja476:1.2": "piyaṃ me tava dassanaṃ;",
  "ja476:1.3": "Issarosi anuppatto,",
  "ja476:1.4": "yamidhatthi pavedaya”.",
  "ja476:2.1": "“Savanena ekassa piyā bhavanti,",
  "ja476:2.2": "Disvā panekassa viyeti chando;",
  "ja476:2.3": "Disvā ca sutvā ca piyā bhavanti,",
  "ja476:2.4": "Kaccinnu me pīyasi dassanena.",
  "ja476:3.1": "Savanena piyo mesi,",
  "ja476:3.2": "bhiyyo cāgamma dassanaṃ;",
  "ja476:3.3": "Evaṃ piyadassano me,",
  "ja476:3.4": "vasa haṃsa mamantike”.",
  "ja476:4.1": "“Vaseyyāma tavāgāre,",
  "ja476:4.2": "niccaṃ sakkatapūjitā;",
  "ja476:4.3": "Matto ca ekadā vajje,",
  "ja476:4.4": "‘haṃsarājaṃ pacantu me’”.",
  "ja476:5.1": "“Dhiratthu taṃ majjapānaṃ,",
  "ja476:5.2": "yaṃ me piyataraṃ tayā;",
  "ja476:5.3": "Na cāpi majjaṃ pissāmi,",
  "ja476:5.4": "yāva me vacchasī ghare”.",
  "ja476:6.1": "“Suvijānaṃ siṅgālānaṃ,",
  "ja476:6.2": "sakuṇānañca vassitaṃ;",
  "ja476:6.3": "Manussavassitaṃ rāja,",
  "ja476:6.4": "dubbijānataraṃ tato.",
  "ja476:7.1": "Api ce maññatī poso,",
  "ja476:7.2": "ñāti mitto sakhāti vā;",
  "ja476:7.3": "Yo pubbe sumano hutvā,",
  "ja476:7.4": "pacchā sampajjate diso.",
  "ja476:8.1": "Yasmiṃ mano nivisati,",
  "ja476:8.2": "avidūre sahāpi so;",
  "ja476:8.3": "Santikepi hi so dūre,",
  "ja476:8.4": "yasmiṃ nāvisate mano.",
  "ja476:9.1": "Antopi so hoti pasannacitto,",
  "ja476:9.2": "Pāraṃ samuddassa pasannacitto;",
  "ja476:9.3": "Antopi so hoti paduṭṭhacitto,",
  "ja476:9.4": "Pāraṃ samuddassa paduṭṭhacitto.",
  "ja476:10.1": "Saṃvasantā vivasanti,",
  "ja476:10.2": "ye disā te rathesabha;",
  "ja476:10.3": "Ārā santo saṃvasanti,",
  "ja476:10.4": "manasā raṭṭhavaḍḍhana.",
  "ja476:11.1": "Aticiraṃ nivāsena,",
  "ja476:11.2": "piyo bhavati appiyo;",
  "ja476:11.3": "Āmanta kho taṃ gacchāma,",
  "ja476:11.4": "purā te homa appiyā”.",
  "ja476:12.1": "“Evañce yācamānānaṃ,",
  "ja476:12.2": "Añjaliṃ nāvabujjhasi;",
  "ja476:12.3": "Paricārakānaṃ sataṃ,",
  "ja476:12.4": "Vacanaṃ na karosi no;",
  "ja476:12.5": "Evaṃ taṃ abhiyācāma,",
  "ja476:12.6": "Puna kayirāsi pariyāyaṃ”.",
  "ja476:13.1": "“Evañce no viharataṃ,",
  "ja476:13.2": "antarāyo na hessati;",
  "ja476:13.3": "Tuyhañcāpi mahārāja,",
  "ja476:13.4": "mayhañca raṭṭhavaḍḍhana;",
  "ja476:13.5": "Appeva nāma passemu,",
  "ja476:13.6": "ahorattānamaccaye”ti.",
  "ja476:14.1": "Javanahaṃsajātakaṃ tatiyaṃ."
}