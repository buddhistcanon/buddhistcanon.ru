{
  "ja250:0.1": "Jātaka",
  "ja250:0.2": "Dukanipāta",
  "ja250:0.3": "Siṅgālavagga",
  "ja250:0.4": "10. Kapijātaka",
  "ja250:1.1": "“Ayaṃ isī upasamasaṃyame rato,",
  "ja250:1.2": "Sa tiṭṭhati sisirabhayena aṭṭito;",
  "ja250:1.3": "Handa ayaṃ pavisatumaṃ agārakaṃ,",
  "ja250:1.4": "Vinetu sītaṃ darathañca kevalaṃ”.",
  "ja250:2.1": "“Nāyaṃ isī upasamasaṃyame rato,",
  "ja250:2.2": "Kapī ayaṃ dumavarasākhagocaro;",
  "ja250:2.3": "So dūsako rosako cāpi jammo,",
  "ja250:2.4": "Sacevajemampi dūseyyagāran”ti. (351)",
  "ja250:3.1": "Kapijātakaṃ dasamaṃ.",
  "ja250:3.2": "Siṅgālavaggo dasamo.",
  "ja250:4.1": "Tassuddānaṃ",
  "ja250:5.1": "Atha rājā siṅgālavaro sunakho,",
  "ja250:5.2": "Tathā kosiya icchati kālaghaso;",
  "ja250:5.3": "Atha dānavaroṭṭhapi sārathinā,",
  "ja250:5.4": "Punambavanañca sisirakapi dasāti.",
  "ja250:6.1": "Atha vagguddānaṃ",
  "ja250:7.1": "Daḷhañca vaggaṃ aparena santhavaṃ,",
  "ja250:7.2": "Kalyāṇavaggāsadiso ca rūhakaṃ;",
  "ja250:7.3": "Nataṃdaḷha bīraṇathambhakaṃ puna,",
  "ja250:7.4": "Kāsāvupāhana siṅgālakena dasāti.",
  "ja250:8.1": "Dukanipātaṃ niṭṭhitaṃ."
}