{
  "ja334:0.1": "Jātaka",
  "ja334:0.2": "Catukkanipāta",
  "ja334:0.3": "Kokilavagga",
  "ja334:0.4": "4. Rājovādajātaka",
  "ja334:1.1": "“Gavañce taramānānaṃ,",
  "ja334:1.2": "jimhaṃ gacchati puṅgavo;",
  "ja334:1.3": "Sabbā tā jimhaṃ gacchanti,",
  "ja334:1.4": "nette jimhaṃ gate sati.",
  "ja334:2.1": "Evameva manussesu,",
  "ja334:2.2": "yo hoti seṭṭhasammato;",
  "ja334:2.3": "So ce adhammaṃ carati,",
  "ja334:2.4": "pageva itarā pajā;",
  "ja334:2.5": "Sabbaṃ raṭṭhaṃ dukhaṃ seti,",
  "ja334:2.6": "rājā ce hoti adhammiko.",
  "ja334:3.1": "Gavañce taramānānaṃ,",
  "ja334:3.2": "ujuṃ gacchati puṅgavo;",
  "ja334:3.3": "Sabbā gāvī ujuṃ yanti,",
  "ja334:3.4": "nette ujuṃ gate sati.",
  "ja334:4.1": "Evameva manussesu,",
  "ja334:4.2": "yo hoti seṭṭhasammato;",
  "ja334:4.3": "So sace dhammaṃ carati,",
  "ja334:4.4": "pageva itarā pajā;",
  "ja334:4.5": "Sabbaṃ raṭṭhaṃ sukhaṃ seti,",
  "ja334:4.6": "rājā ce hoti dhammiko”ti.",
  "ja334:5.1": "Rājovādajātakaṃ catutthaṃ."
}