{
  "ja374:0.1": "Jātaka",
  "ja374:0.2": "Pañcakanipāta",
  "ja374:0.3": "Aḍḍhavagga",
  "ja374:0.4": "4. Cūḷadhanuggahajātaka",
  "ja374:1.1": "“Sabbaṃ bhaṇḍaṃ samādāya,",
  "ja374:1.2": "pāraṃ tiṇṇosi brāhmaṇa;",
  "ja374:1.3": "Paccāgaccha lahuṃ khippaṃ,",
  "ja374:1.4": "mampi tārehi dānito”.",
  "ja374:2.1": "“Asanthutaṃ maṃ cirasanthutena,",
  "ja374:2.2": "Nimīni bhotī addhuvaṃ dhuvena;",
  "ja374:2.3": "Mayāpi bhotī nimineyya aññaṃ,",
  "ja374:2.4": "Ito ahaṃ dūrataraṃ gamissaṃ”.",
  "ja374:3.1": "“Kāyaṃ eḷagalāgumbe,",
  "ja374:3.2": "karoti ahuhāsiyaṃ;",
  "ja374:3.3": "Nayīdha naccagītaṃ vā,",
  "ja374:3.4": "tāḷaṃ vā susamāhitaṃ;",
  "ja374:3.5": "Anamhikāle susoṇi,",
  "ja374:3.6": "kiṃ nu jagghasi sobhane”.",
  "ja374:4.1": "“Siṅgāla bāla dummedha,",
  "ja374:4.2": "appapaññosi jambuka;",
  "ja374:4.3": "Jīno macchañca pesiñca,",
  "ja374:4.4": "kapaṇo viya jhāyasi”.",
  "ja374:5.1": "“Sudassaṃ vajjamaññesaṃ,",
  "ja374:5.2": "attano pana duddasaṃ;",
  "ja374:5.3": "Jīnā patiñca jārañca,",
  "ja374:5.4": "maññe tvaññeva jhāyasi”.",
  "ja374:6.1": "“Evametaṃ migarāja,",
  "ja374:6.2": "yathā bhāsasi jambuka;",
  "ja374:6.3": "Sā nūnāhaṃ ito gantvā,",
  "ja374:6.4": "bhattu hessaṃ vasānugā”.",
  "ja374:7.1": "“Yo hare mattikaṃ thālaṃ,",
  "ja374:7.2": "kaṃsathālampi so hare;",
  "ja374:7.3": "Katañceva tayā pāpaṃ,",
  "ja374:7.4": "punapevaṃ karissasī”ti.",
  "ja374:8.1": "Cūḷadhanuggahajātakaṃ catutthaṃ."
}