{
  "ja122:0.1": "Jātaka",
  "ja122:0.2": "Ekakanipāta",
  "ja122:0.3": "Kusanāḷivagga",
  "ja122:0.4": "2. Dummedhajātaka",
  "ja122:1.1": "“Yasaṃ laddhāna dummedho,",
  "ja122:1.2": "anatthaṃ carati attano;",
  "ja122:1.3": "Attano ca paresañca,",
  "ja122:1.4": "hiṃsāya paṭipajjatī”ti.",
  "ja122:2.1": "Dummedhajātakaṃ dutiyaṃ."
}