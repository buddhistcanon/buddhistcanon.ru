{
  "ja13:0.1": "Jātaka",
  "ja13:0.2": "Ekakanipāta",
  "ja13:0.3": "Sīlavagga",
  "ja13:0.4": "3. Kaṇḍijātaka",
  "ja13:1.1": "“Dhiratthu kaṇḍinaṃ sallaṃ,",
  "ja13:1.2": "purisaṃ gāḷhavedhinaṃ;",
  "ja13:1.3": "Dhiratthu taṃ janapadaṃ,",
  "ja13:1.4": "yatthitthī pariṇāyikā;",
  "ja13:1.5": "Te cāpi dhikkitā sattā,",
  "ja13:1.6": "ye itthīnaṃ vasaṅgatā”ti.",
  "ja13:2.1": "Kaṇḍijātakaṃ tatiyaṃ."
}