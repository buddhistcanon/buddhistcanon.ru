{
  "ja469:0.1": "Jātaka",
  "ja469:0.2": "Dvādasakanipāta",
  "ja469:0.3": "Cūḷakuṇālavagga",
  "ja469:0.4": "6. Mahākaṇhajātaka",
  "ja469:1.1": "“Kaṇho kaṇho ca ghoro ca,",
  "ja469:1.2": "sukkadāṭho pabhāsavā;",
  "ja469:1.3": "Baddho pañcahi rajjūhi,",
  "ja469:1.4": "kiṃ ravi sunakho tava”.",
  "ja469:2.1": "“Nāyaṃ migānamatthāya,",
  "ja469:2.2": "usīnaka bhavissati;",
  "ja469:2.3": "Manussānaṃ anayo hutvā,",
  "ja469:2.4": "tadā kaṇho pamokkhati”.",
  "ja469:3.1": "“Pattahatthā samaṇakā,",
  "ja469:3.2": "muṇḍā saṅghāṭipārutā;",
  "ja469:3.3": "Naṅgalehi kasissanti,",
  "ja469:3.4": "tadā kaṇho pamokkhati.",
  "ja469:4.1": "Tapassiniyo pabbajitā,",
  "ja469:4.2": "muṇḍā saṅghāṭipārutā;",
  "ja469:4.3": "Yadā loke gamissanti,",
  "ja469:4.4": "tadā kaṇho pamokkhati.",
  "ja469:5.1": "Dīghottaroṭṭhā jaṭilā,",
  "ja469:5.2": "paṅkadantā rajassirā;",
  "ja469:5.3": "Iṇaṃ codāya gacchanti,",
  "ja469:5.4": "tadā kaṇho pamokkhati.",
  "ja469:6.1": "Adhicca vede sāvittiṃ,",
  "ja469:6.2": "yaññatantañca brāhmaṇā;",
  "ja469:6.3": "Bhatikāya yajissanti,",
  "ja469:6.4": "tadā kaṇho pamokkhati.",
  "ja469:7.1": "Mātaraṃ pitarañcāpi,",
  "ja469:7.2": "jiṇṇakaṃ gatayobbanaṃ;",
  "ja469:7.3": "Pahū santo na bharanti,",
  "ja469:7.4": "tadā kaṇho pamokkhati.",
  "ja469:8.1": "Mātaraṃ pitarañcāpi,",
  "ja469:8.2": "jiṇṇakaṃ gatayobbanaṃ;",
  "ja469:8.3": "Bālā tumheti vakkhanti,",
  "ja469:8.4": "tadā kaṇho pamokkhati.",
  "ja469:9.1": "Ācariyabhariyaṃ sakhiṃ,",
  "ja469:9.2": "mātulāniṃ pitucchakiṃ;",
  "ja469:9.3": "Yadā loke gamissanti,",
  "ja469:9.4": "tadā kaṇho pamokkhati.",
  "ja469:10.1": "Asicammaṃ gahetvāna,",
  "ja469:10.2": "khaggaṃ paggayha brāhmaṇā;",
  "ja469:10.3": "Panthaghātaṃ karissanti,",
  "ja469:10.4": "tadā kaṇho pamokkhati.",
  "ja469:11.1": "Sukkacchavī vedhaverā,",
  "ja469:11.2": "thūlabāhū apātubhā;",
  "ja469:11.3": "Mittabhedaṃ karissanti,",
  "ja469:11.4": "tadā kaṇho pamokkhati.",
  "ja469:12.1": "Māyāvino nekatikā,",
  "ja469:12.2": "asappurisacintakā;",
  "ja469:12.3": "Yadā loke bhavissanti,",
  "ja469:12.4": "tadā kaṇho pamokkhatī”ti.",
  "ja469:13.1": "Mahākaṇhajātakaṃ chaṭṭhaṃ."
}