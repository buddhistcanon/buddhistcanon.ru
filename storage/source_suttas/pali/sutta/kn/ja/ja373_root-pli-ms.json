{
  "ja373:0.1": "Jātaka",
  "ja373:0.2": "Pañcakanipāta",
  "ja373:0.3": "Aḍḍhavagga",
  "ja373:0.4": "3. Mūsikajātaka",
  "ja373:1.1": "“Kuhiṃ gatā kattha gatā,",
  "ja373:1.2": "iti lālappatī jano;",
  "ja373:1.3": "Ahameveko jānāmi,",
  "ja373:1.4": "udapāne mūsikā hatā.",
  "ja373:2.1": "Yañcetaṃ iti cīti ca,",
  "ja373:2.2": "gadrabhova nivattasi;",
  "ja373:2.3": "Udapāne mūsikaṃ hantvā,",
  "ja373:2.4": "yavaṃ bhakkhetumicchasi.",
  "ja373:3.1": "Daharo cāsi dummedha,",
  "ja373:3.2": "paṭhamuppattiko susu;",
  "ja373:3.3": "Dīghañcetaṃ samāsajja,",
  "ja373:3.4": "na te dassāmi jīvitaṃ.",
  "ja373:4.1": "Nāntalikkhabhavanena,",
  "ja373:4.2": "nāṅgaputtapinena vā;",
  "ja373:4.3": "Puttena hi patthayito,",
  "ja373:4.4": "silokehi pamocito.",
  "ja373:5.1": "Sabbaṃ sutamadhīyetha,",
  "ja373:5.2": "hīnamukkaṭṭhamajjhimaṃ;",
  "ja373:5.3": "Sabbassa atthaṃ jāneyya,",
  "ja373:5.4": "na ca sabbaṃ payojaye;",
  "ja373:5.5": "Hoti tādisako kālo,",
  "ja373:5.6": "yattha atthāvahaṃ sutan”ti.",
  "ja373:6.1": "Mūsikajātakaṃ tatiyaṃ."
}