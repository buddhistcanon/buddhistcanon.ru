{
  "ja317:0.1": "Jātaka",
  "ja317:0.2": "Catukkanipāta",
  "ja317:0.3": "Pucimandavagga",
  "ja317:0.4": "7. Matarodanajātaka",
  "ja317:1.1": "“Mataṃ mataṃ evaṃ rodatha,",
  "ja317:1.2": "Na hi taṃ rodatha yo marissati;",
  "ja317:1.3": "Sabbepi sarīradhārino,",
  "ja317:1.4": "Anupubbena jahanti jīvitaṃ.",
  "ja317:2.1": "Devamanussā catuppadā,",
  "ja317:2.2": "Pakkhigaṇā uragā ca bhogino;",
  "ja317:2.3": "Samhi sarīre anissarā,",
  "ja317:2.4": "Ramamānāva jahanti jīvitaṃ.",
  "ja317:3.1": "Evaṃ calitaṃ asaṇṭhitaṃ,",
  "ja317:3.2": "Sukhadukkhaṃ manujesvapekkhiya;",
  "ja317:3.3": "Kanditaruditaṃ niratthakaṃ,",
  "ja317:3.4": "Kiṃ vo sokagaṇābhikīrare.",
  "ja317:4.1": "Dhuttā ca soṇḍā akatā,",
  "ja317:4.2": "bālā sūrā ayogino;",
  "ja317:4.3": "Dhīraṃ maññanti bāloti,",
  "ja317:4.4": "ye dhammassa akovidā”ti.",
  "ja317:5.1": "Matarodanajātakaṃ sattamaṃ."
}