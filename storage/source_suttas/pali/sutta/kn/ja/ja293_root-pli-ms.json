{
  "ja293:0.1": "Jātaka",
  "ja293:0.2": "Tikanipāta",
  "ja293:0.3": "Kumbhavagga",
  "ja293:0.4": "3. Kāyanibbindajātaka",
  "ja293:1.1": "“Phuṭṭhassa me aññatarena byādhinā,",
  "ja293:1.2": "Rogena bāḷhaṃ dukhitassa ruppato;",
  "ja293:1.3": "Parisussati khippamidaṃ kaḷevaraṃ,",
  "ja293:1.4": "Pupphaṃ yathā paṃsuni ātape kataṃ.",
  "ja293:2.1": "Ajaññaṃ jaññasaṅkhātaṃ,",
  "ja293:2.2": "asuciṃ sucisammataṃ;",
  "ja293:2.3": "Nānākuṇapaparipūraṃ,",
  "ja293:2.4": "jaññarūpaṃ apassato.",
  "ja293:3.1": "Dhiratthumaṃ āturaṃ pūtikāyaṃ,",
  "ja293:3.2": "Jegucchiyaṃ assuciṃ byādhidhammaṃ;",
  "ja293:3.3": "Yatthappamattā adhimucchitā pajā,",
  "ja293:3.4": "Hāpenti maggaṃ sugatūpapattiyā”ti.",
  "ja293:4.1": "Kāyanibbindajātakaṃ tatiyaṃ."
}