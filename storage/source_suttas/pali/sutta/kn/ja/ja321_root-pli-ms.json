{
  "ja321:0.1": "Jātaka",
  "ja321:0.2": "Catukkanipāta",
  "ja321:0.3": "Kuṭidūsakavagga",
  "ja321:0.4": "1. Kuṭidūsakajātaka",
  "ja321:1.1": "“Manussasseva te sīsaṃ,",
  "ja321:1.2": "hatthapādā ca vānara;",
  "ja321:1.3": "Atha kena nu vaṇṇena,",
  "ja321:1.4": "agāraṃ te na vijjati”.",
  "ja321:2.1": "“Manussasseva me sīsaṃ,",
  "ja321:2.2": "hatthapādā ca siṅgila;",
  "ja321:2.3": "Yāhu seṭṭhā manussesu,",
  "ja321:2.4": "sā me paññā na vijjati”.",
  "ja321:3.1": "“Anavaṭṭhitacittassa,",
  "ja321:3.2": "lahucittassa dubbhino;",
  "ja321:3.3": "Niccaṃ addhuvasīlassa,",
  "ja321:3.4": "sukhabhāvo na vijjati.",
  "ja321:4.1": "So karassu ānubhāvaṃ,",
  "ja321:4.2": "vītivattassu sīliyaṃ;",
  "ja321:4.3": "Sītavātaparittāṇaṃ,",
  "ja321:4.4": "karassu kuṭavaṃ kapī”ti.",
  "ja321:5.1": "Kuṭidūsakajātakaṃ paṭhamaṃ."
}