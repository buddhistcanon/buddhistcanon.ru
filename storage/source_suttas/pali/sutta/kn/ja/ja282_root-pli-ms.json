{
  "ja282:0.1": "Jātaka",
  "ja282:0.2": "Tikanipāta",
  "ja282:0.3": "Abbhantaravagga",
  "ja282:0.4": "2. Seyyajātaka",
  "ja282:1.1": "“Seyyaṃso seyyaso hoti,",
  "ja282:1.2": "yo seyyamupasevati;",
  "ja282:1.3": "Ekena sandhiṃ katvāna,",
  "ja282:1.4": "sataṃ vajjhe amocayiṃ.",
  "ja282:2.1": "Tasmā sabbena lokena,",
  "ja282:2.2": "sandhiṃ katvāna ekato;",
  "ja282:2.3": "Pecca saggaṃ nigaccheyya,",
  "ja282:2.4": "idaṃ suṇātha kāsiyā”.",
  "ja282:3.1": "Idaṃ vatvā mahārājā,",
  "ja282:3.2": "Kaṃso bārāṇasiggaho;",
  "ja282:3.3": "Dhanuṃ kaṇḍañca nikkhippa,",
  "ja282:3.4": "Saṃyamaṃ ajjhupāgamīti.",
  "ja282:4.1": "Seyyajātakaṃ dutiyaṃ."
}