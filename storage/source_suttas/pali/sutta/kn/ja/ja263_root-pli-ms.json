{
  "ja263:0.1": "Jātaka",
  "ja263:0.2": "Tikanipāta",
  "ja263:0.3": "Padumavagga",
  "ja263:0.4": "3. Cūḷapalobhanajātaka",
  "ja263:1.1": "“Abhijjamāne vārismiṃ,",
  "ja263:1.2": "sayaṃ āgamma iddhiyā;",
  "ja263:1.3": "Missībhāvitthiyā gantvā,",
  "ja263:1.4": "saṃsīdasi mahaṇṇave.",
  "ja263:2.1": "Āvaṭṭanī mahāmāyā,",
  "ja263:2.2": "brahmacariyavikopanā;",
  "ja263:2.3": "Sīdanti naṃ viditvāna,",
  "ja263:2.4": "ārakā parivajjaye.",
  "ja263:3.1": "Yaṃ etā upasevanti,",
  "ja263:3.2": "chandasā vā dhanena vā;",
  "ja263:3.3": "Jātavedova saṃ ṭhānaṃ,",
  "ja263:3.4": "khippaṃ anudahanti nan”ti.",
  "ja263:4.1": "Cūḷapalobhanajātakaṃ tatiyaṃ."
}