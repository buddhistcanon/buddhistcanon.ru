{
  "ja461:0.1": "Jātaka",
  "ja461:0.2": "Ekādasakanipāta",
  "ja461:0.3": "Mātuposakavagga",
  "ja461:0.4": "7. Dasarathajātaka",
  "ja461:1.1": "“Etha lakkhaṇa sītā ca,",
  "ja461:1.2": "ubho otarathodakaṃ;",
  "ja461:1.3": "Evāyaṃ bharato āha,",
  "ja461:1.4": "‘rājā dasaratho mato’”.",
  "ja461:2.1": "“Kena rāmappabhāvena,",
  "ja461:2.2": "socitabbaṃ na socasi;",
  "ja461:2.3": "Pitaraṃ kālakataṃ sutvā,",
  "ja461:2.4": "na taṃ pasahate dukhaṃ”.",
  "ja461:3.1": "“Yaṃ na sakkā nipāletuṃ,",
  "ja461:3.2": "posena lapataṃ bahuṃ;",
  "ja461:3.3": "Sa kissa viññū medhāvī,",
  "ja461:3.4": "attānamupatāpaye.",
  "ja461:4.1": "Daharā ca hi vuddhā ca,",
  "ja461:4.2": "ye bālā ye ca paṇḍitā;",
  "ja461:4.3": "Aḍḍhā ceva daliddā ca,",
  "ja461:4.4": "sabbe maccuparāyaṇā.",
  "ja461:5.1": "Phalānamiva pakkānaṃ,",
  "ja461:5.2": "niccaṃ patanato bhayaṃ;",
  "ja461:5.3": "Evaṃ jātāna maccānaṃ,",
  "ja461:5.4": "nicca maraṇato bhayaṃ.",
  "ja461:6.1": "Sāyameke na dissanti,",
  "ja461:6.2": "pāto diṭṭhā bahujjanā;",
  "ja461:6.3": "Pāto eke na dissanti,",
  "ja461:6.4": "sāyaṃ diṭṭhā bahujjanā.",
  "ja461:7.1": "Paridevayamāno ce,",
  "ja461:7.2": "kiñcidatthaṃ udabbahe;",
  "ja461:7.3": "Sammūḷho hiṃsamattānaṃ,",
  "ja461:7.4": "kayirā taṃ vicakkhaṇo.",
  "ja461:8.1": "Kiso vivaṇṇo bhavati,",
  "ja461:8.2": "hiṃsamattānamattano;",
  "ja461:8.3": "Na tena petā pālenti,",
  "ja461:8.4": "niratthā paridevanā.",
  "ja461:9.1": "Yathā saraṇamādittaṃ,",
  "ja461:9.2": "vārinā parinibbaye;",
  "ja461:9.3": "Evampi dhīro sutavā,",
  "ja461:9.4": "medhāvī paṇḍito naro;",
  "ja461:9.5": "Khippamuppatitaṃ sokaṃ,",
  "ja461:9.6": "vāto tūlaṃva dhaṃsaye.",
  "ja461:10.1": "Macco ekova acceti,",
  "ja461:10.2": "ekova jāyate kule;",
  "ja461:10.3": "Saṃyogaparamā tveva,",
  "ja461:10.4": "sambhogā sabbapāṇinaṃ.",
  "ja461:11.1": "Tasmā hi dhīrassa bahussutassa,",
  "ja461:11.2": "Sampassato lokamimaṃ parañca;",
  "ja461:11.3": "Aññāya dhammaṃ hadayaṃ manañca,",
  "ja461:11.4": "Sokā mahantāpi na tāpayanti.",
  "ja461:12.1": "Sohaṃ dassañca bhokkhañca,",
  "ja461:12.2": "bharissāmi ca ñātake;",
  "ja461:12.3": "Sesañca pālayissāmi,",
  "ja461:12.4": "kiccametaṃ vijānato.",
  "ja461:13.1": "Dasa vassasahassāni,",
  "ja461:13.2": "saṭṭhi vassasatāni ca;",
  "ja461:13.3": "Kambugīvo mahābāhu,",
  "ja461:13.4": "rāmo rajjamakārayī”ti.",
  "ja461:14.1": "Dasarathajātakaṃ sattamaṃ."
}