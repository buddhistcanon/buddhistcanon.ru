{
  "ja462:0.1": "Jātaka",
  "ja462:0.2": "Ekādasakanipāta",
  "ja462:0.3": "Mātuposakavagga",
  "ja462:0.4": "8. Saṃvarajātaka",
  "ja462:1.1": "“Jānanto no mahārāja,",
  "ja462:1.2": "tava sīlaṃ janādhipo;",
  "ja462:1.3": "Ime kumāre pūjento,",
  "ja462:1.4": "na taṃ kenaci maññatha.",
  "ja462:2.1": "Tiṭṭhante no mahārāje,",
  "ja462:2.2": "adu deve divaṅgate;",
  "ja462:2.3": "Ñātī taṃ samanuññiṃsu,",
  "ja462:2.4": "sampassaṃ atthamattano.",
  "ja462:3.1": "Kena saṃvara vattena,",
  "ja462:3.2": "sañjāte abhitiṭṭhasi;",
  "ja462:3.3": "Kena taṃ nātivattanti,",
  "ja462:3.4": "ñātisaṅghā samāgatā”.",
  "ja462:4.1": "“Na rājaputta usūyāmi,",
  "ja462:4.2": "samaṇānaṃ mahesinaṃ;",
  "ja462:4.3": "Sakkaccaṃ te namassāmi,",
  "ja462:4.4": "pāde vandāmi tādinaṃ.",
  "ja462:5.1": "Te maṃ dhammaguṇe yuttaṃ,",
  "ja462:5.2": "sussūsamanusūyakaṃ;",
  "ja462:5.3": "Samaṇā manusāsanti,",
  "ja462:5.4": "isī dhammaguṇe ratā.",
  "ja462:6.1": "Tesāhaṃ vacanaṃ sutvā,",
  "ja462:6.2": "samaṇānaṃ mahesinaṃ;",
  "ja462:6.3": "Na kiñci atimaññāmi,",
  "ja462:6.4": "dhamme me nirato mano.",
  "ja462:7.1": "Hatthārohā anīkaṭṭhā,",
  "ja462:7.2": "rathikā pattikārakā;",
  "ja462:7.3": "Tesaṃ nappaṭibandhāmi,",
  "ja462:7.4": "niviṭṭhaṃ bhattavetanaṃ.",
  "ja462:8.1": "Mahāmattā ca me atthi,",
  "ja462:8.2": "mantino paricārakā;",
  "ja462:8.3": "Bārāṇasiṃ voharanti,",
  "ja462:8.4": "bahumaṃsasurodakaṃ.",
  "ja462:9.1": "Athopi vāṇijā phītā,",
  "ja462:9.2": "nānāraṭṭhehi āgatā;",
  "ja462:9.3": "Tesu me vihitā rakkhā,",
  "ja462:9.4": "evaṃ jānāhuposatha”.",
  "ja462:10.1": "“Dhammena kira ñātīnaṃ,",
  "ja462:10.2": "rajjaṃ kārehi saṃvara;",
  "ja462:10.3": "Medhāvī paṇḍito cāsi,",
  "ja462:10.4": "athopi ñātinaṃ hito.",
  "ja462:11.1": "Taṃ taṃ ñātiparibyūḷhaṃ,",
  "ja462:11.2": "nānāratanamocitaṃ;",
  "ja462:11.3": "Amittā nappasahanti,",
  "ja462:11.4": "indaṃva asurādhipo”ti.",
  "ja462:12.1": "Saṃvarajātakaṃ aṭṭhamaṃ."
}