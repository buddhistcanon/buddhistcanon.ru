{
  "ja297:0.1": "Jātaka",
  "ja297:0.2": "Tikanipāta",
  "ja297:0.3": "Kumbhavagga",
  "ja297:0.4": "7. Kāmavilāpajātaka",
  "ja297:1.1": "“Ucce sakuṇa ḍemāna,",
  "ja297:1.2": "pattayāna vihaṅgama;",
  "ja297:1.3": "Vajjāsi kho tvaṃ vāmūruṃ,",
  "ja297:1.4": "ciraṃ kho sā karissati.",
  "ja297:2.1": "Idaṃ kho sā na jānāti,",
  "ja297:2.2": "asiṃ sattiñca oḍḍitaṃ;",
  "ja297:2.3": "Sā caṇḍī kāhati kodhaṃ,",
  "ja297:2.4": "taṃ me tapati no idaṃ.",
  "ja297:3.1": "Esa uppalasannāho,",
  "ja297:3.2": "nikkhañcussīsakohitaṃ;",
  "ja297:3.3": "Kāsikañca muduṃ vatthaṃ,",
  "ja297:3.4": "tappetu dhanikā piyā”ti.",
  "ja297:4.1": "Kāmavilāpajātakaṃ sattamaṃ."
}