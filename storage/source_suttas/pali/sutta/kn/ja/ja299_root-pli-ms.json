{
  "ja299:0.1": "Jātaka",
  "ja299:0.2": "Tikanipāta",
  "ja299:0.3": "Kumbhavagga",
  "ja299:0.4": "9. Komāraputtajātaka",
  "ja299:1.1": "“Pure tuvaṃ sīlavataṃ sakāse,",
  "ja299:1.2": "Okkantikaṃ kīḷasi assamamhi;",
  "ja299:1.3": "Karohare makkaṭiyāni makkaṭa,",
  "ja299:1.4": "Na taṃ mayaṃ sīlavataṃ ramāma”.",
  "ja299:2.1": "“Sutā hi mayhaṃ paramā visuddhi,",
  "ja299:2.2": "Komāraputtassa bahussutassa;",
  "ja299:2.3": "Mā dāni maṃ maññi tuvaṃ yathā pure,",
  "ja299:2.4": "Jhānānuyutto viharāmi āvuso”.",
  "ja299:3.1": "“Sacepi selasmi vapeyya bījaṃ,",
  "ja299:3.2": "Devo ca vasse na hi taṃ virūḷhe;",
  "ja299:3.3": "Sutā hi te sā paramā visuddhi,",
  "ja299:3.4": "Ārā tuvaṃ makkaṭa jhānabhūmiyā”ti.",
  "ja299:4.1": "Komāraputtajātakaṃ navamaṃ."
}