{
  "ja432:0.1": "Jātaka",
  "ja432:0.2": "Navakanipāta",
  "ja432:0.3": "Gijjhavagga",
  "ja432:0.4": "6. Padakusalamāṇavajātaka",
  "ja432:1.1": "“Bahussutaṃ cittakathiṃ,",
  "ja432:1.2": "gaṅgā vahati pāṭaliṃ;",
  "ja432:1.3": "Vuyhamānaka bhaddante,",
  "ja432:1.4": "ekaṃ me dehi gāthakaṃ”.",
  "ja432:2.1": "“Yena siñcanti dukkhitaṃ,",
  "ja432:2.2": "yena siñcanti āturaṃ;",
  "ja432:2.3": "Tassa majjhe marissāmi,",
  "ja432:2.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:3.1": "“Yattha bījāni rūhanti,",
  "ja432:3.2": "sattā yattha patiṭṭhitā;",
  "ja432:3.3": "Sā me sīsaṃ nipīḷeti,",
  "ja432:3.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:4.1": "“Yena bhattāni paccanti,",
  "ja432:4.2": "sītaṃ yena vihaññati;",
  "ja432:4.3": "So maṃ ḍahati gattāni,",
  "ja432:4.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:5.1": "“Yena bhuttena yāpenti,",
  "ja432:5.2": "puthū brāhmaṇakhattiyā;",
  "ja432:5.3": "So maṃ bhutto byāpādeti,",
  "ja432:5.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:6.1": "“Gimhānaṃ pacchime māse,",
  "ja432:6.2": "vātamicchanti paṇḍitā;",
  "ja432:6.3": "So maṃ bhañjati gattāni,",
  "ja432:6.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:7.1": "“Yaṃ nissitā jagatiruhaṃ,",
  "ja432:7.2": "svāyaṃ aggiṃ pamuñcati;",
  "ja432:7.3": "Disā bhajatha vakkaṅgā,",
  "ja432:7.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:8.1": "“Yamānayiṃ somanassaṃ,",
  "ja432:8.2": "māliniṃ candanussadaṃ;",
  "ja432:8.3": "Sā maṃ gharā nicchubhati,",
  "ja432:8.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:9.1": "“Yena jātena nandissaṃ,",
  "ja432:9.2": "yassa ca bhavamicchisaṃ;",
  "ja432:9.3": "So maṃ gharā nicchubhati,",
  "ja432:9.4": "jātaṃ saraṇato bhayaṃ”.",
  "ja432:10.1": "“Suṇantu me jānapadā,",
  "ja432:10.2": "negamā ca samāgatā;",
  "ja432:10.3": "Yatodakaṃ tadādittaṃ,",
  "ja432:10.4": "yato khemaṃ tato bhayaṃ.",
  "ja432:11.1": "Rājā vilumpate raṭṭhaṃ,",
  "ja432:11.2": "brāhmaṇo ca purohito;",
  "ja432:11.3": "Attaguttā viharatha,",
  "ja432:11.4": "jātaṃ saraṇato bhayan”ti.",
  "ja432:12.1": "Padakusalamāṇavajātakaṃ chaṭṭhaṃ."
}