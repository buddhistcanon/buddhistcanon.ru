{
  "ja364:0.1": "Jātaka",
  "ja364:0.2": "Pañcakanipāta",
  "ja364:0.3": "Vaṇṇārohavagga",
  "ja364:0.4": "4. Khajjopanakajātaka",
  "ja364:1.1": "“Ko nu santamhi pajjote,",
  "ja364:1.2": "aggipariyesanaṃ caraṃ;",
  "ja364:1.3": "Addakkhi ratti khajjotaṃ,",
  "ja364:1.4": "jātavedaṃ amaññatha.",
  "ja364:2.1": "Svassa gomayacuṇṇāni,",
  "ja364:2.2": "abhimatthaṃ tiṇāni ca;",
  "ja364:2.3": "Viparītāya saññāya,",
  "ja364:2.4": "nāsakkhi pajjaletave.",
  "ja364:3.1": "Evampi anupāyena,",
  "ja364:3.2": "atthaṃ na labhate migo;",
  "ja364:3.3": "Visāṇato gavaṃ dohaṃ,",
  "ja364:3.4": "yattha khīraṃ na vindati.",
  "ja364:4.1": "Vividhehi upāyehi,",
  "ja364:4.2": "atthaṃ papponti māṇavā;",
  "ja364:4.3": "Niggahena amittānaṃ,",
  "ja364:4.4": "mittānaṃ paggahena ca.",
  "ja364:5.1": "Senāmokkhapalābhena,",
  "ja364:5.2": "vallabhānaṃ nayena ca;",
  "ja364:5.3": "Jagatiṃ jagatipālā,",
  "ja364:5.4": "āvasanti vasundharan”ti.",
  "ja364:6.1": "Khajjopanakajātakaṃ catutthaṃ."
}