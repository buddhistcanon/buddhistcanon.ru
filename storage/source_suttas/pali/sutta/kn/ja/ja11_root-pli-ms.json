{
  "ja11:0.1": "Jātaka",
  "ja11:0.2": "Ekakanipāta",
  "ja11:0.3": "Sīlavagga",
  "ja11:0.4": "1. Lakkhaṇamigajātaka",
  "ja11:1.1": "“Hoti sīlavataṃ attho,",
  "ja11:1.2": "paṭisanthāravuttinaṃ;",
  "ja11:1.3": "Lakkhaṇaṃ passa āyantaṃ,",
  "ja11:1.4": "ñātisaṅghapurakkhataṃ;",
  "ja11:1.5": "Atha passasimaṃ kāḷaṃ,",
  "ja11:1.6": "suvihīnaṃva ñātibhī”ti.",
  "ja11:2.1": "Lakkhaṇamigajātakaṃ paṭhamaṃ."
}