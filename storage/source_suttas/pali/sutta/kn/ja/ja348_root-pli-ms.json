{
  "ja348:0.1": "Jātaka",
  "ja348:0.2": "Catukkanipāta",
  "ja348:0.3": "Cūḷakuṇālavagga",
  "ja348:0.4": "8. Araññajātaka",
  "ja348:1.1": "“Araññā gāmamāgamma,",
  "ja348:1.2": "kiṃsīlaṃ kiṃvataṃ ahaṃ;",
  "ja348:1.3": "Purisaṃ tāta seveyyaṃ,",
  "ja348:1.4": "taṃ me akkhāhi pucchito”.",
  "ja348:2.1": "“Yo taṃ vissāsaye tāta,",
  "ja348:2.2": "vissāsañca khameyya te;",
  "ja348:2.3": "Sussūsī ca titikkhī ca,",
  "ja348:2.4": "taṃ bhajehi ito gato.",
  "ja348:3.1": "Yassa kāyena vācāya,",
  "ja348:3.2": "manasā natthi dukkaṭaṃ;",
  "ja348:3.3": "Urasīva patiṭṭhāya,",
  "ja348:3.4": "taṃ bhajehi ito gato.",
  "ja348:4.1": "Haliddirāgaṃ kapicittaṃ,",
  "ja348:4.2": "purisaṃ rāgavirāginaṃ;",
  "ja348:4.3": "Tādisaṃ tāta mā sevi,",
  "ja348:4.4": "nimmanussampi ce siyā”ti.",
  "ja348:5.1": "Araññajātakaṃ aṭṭhamaṃ."
}