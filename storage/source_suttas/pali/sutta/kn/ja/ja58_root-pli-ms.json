{
  "ja58:0.1": "Jātaka",
  "ja58:0.2": "Ekakanipāta",
  "ja58:0.3": "Āsīsavagga",
  "ja58:0.4": "8. Tayodhammajātaka",
  "ja58:1.1": "“Yassa ete tayo dhammā,",
  "ja58:1.2": "vānarinda yathā tava;",
  "ja58:1.3": "Dakkhiyaṃ sūriyaṃ paññā,",
  "ja58:1.4": "diṭṭhaṃ so ativattatī”ti.",
  "ja58:2.1": "Tayodhammajātakaṃ aṭṭhamaṃ."
}