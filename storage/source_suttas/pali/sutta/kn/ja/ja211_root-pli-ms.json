{
  "ja211:0.1": "Jātaka",
  "ja211:0.2": "Dukanipāta",
  "ja211:0.3": "Bīraṇathambhavagga",
  "ja211:0.4": "1. Somadattajātaka",
  "ja211:1.1": "“Akāsi yoggaṃ dhuvamappamatto,",
  "ja211:1.2": "Saṃvaccharaṃ bīraṇathambhakasmiṃ;",
  "ja211:1.3": "Byākāsi saññaṃ parisaṃ vigayha,",
  "ja211:1.4": "Na niyyamo tāyati appapaññaṃ”.",
  "ja211:2.1": "“Dvayaṃ yācanako tāta,",
  "ja211:2.2": "somadatta nigacchati;",
  "ja211:2.3": "Alābhaṃ dhanalābhaṃ vā,",
  "ja211:2.4": "evaṃ dhammā hi yācanā”ti.",
  "ja211:3.1": "Somadattajātakaṃ paṭhamaṃ."
}