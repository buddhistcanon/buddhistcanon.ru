{
  "ja307:0.1": "Jātaka",
  "ja307:0.2": "Catukkanipāta",
  "ja307:0.3": "Kāliṅgavagga",
  "ja307:0.4": "7. Palāsajātaka",
  "ja307:1.1": "“Acetanaṃ brāhmaṇa assuṇantaṃ,",
  "ja307:1.2": "Jāno ajānantamimaṃ palāsaṃ;",
  "ja307:1.3": "Āraddhaviriyo dhuvaṃ appamatto,",
  "ja307:1.4": "Sukhaseyyaṃ pucchasi kissa hetu”.",
  "ja307:2.1": "“Dūre suto ceva brahā ca rukkho,",
  "ja307:2.2": "Dese ṭhito bhūtanivāsarūpo;",
  "ja307:2.3": "Tasmā namassāmi imaṃ palāsaṃ,",
  "ja307:2.4": "Ye cettha bhūtā te dhanassa hetu”.",
  "ja307:3.1": "“So te karissāmi yathānubhāvaṃ,",
  "ja307:3.2": "Kataññutaṃ brāhmaṇa pekkhamāno;",
  "ja307:3.3": "Kathañhi āgamma sataṃ sakāse,",
  "ja307:3.4": "Moghāni te assu pariphanditāni.",
  "ja307:4.1": "Yo tindukarukkhassa paro pilakkho,",
  "ja307:4.2": "Parivārito pubbayañño uḷāro;",
  "ja307:4.3": "Tassesa mūlasmiṃ nidhi nikhāto,",
  "ja307:4.4": "Adāyādo gaccha taṃ uddharāhī”ti.",
  "ja307:5.1": "Palāsajātakaṃ sattamaṃ."
}