{
  "ja312:0.1": "Jātaka",
  "ja312:0.2": "Catukkanipāta",
  "ja312:0.3": "Pucimandavagga",
  "ja312:0.4": "2. Kassapamandiyajātaka",
  "ja312:1.1": "“Api kassapa mandiyā,",
  "ja312:1.2": "yuvā sapati hanti vā;",
  "ja312:1.3": "Sabbaṃ taṃ khamate dhīro,",
  "ja312:1.4": "paṇḍito taṃ titikkhati.",
  "ja312:2.1": "Sacepi santo vivadanti,",
  "ja312:2.2": "khippaṃ sandhīyare puna;",
  "ja312:2.3": "Bālā pattāva bhijjanti,",
  "ja312:2.4": "na te samathamajjhagū.",
  "ja312:3.1": "Ete bhiyyo samāyanti,",
  "ja312:3.2": "sandhi tesaṃ na jīrati;",
  "ja312:3.3": "Yo cādhipannaṃ jānāti,",
  "ja312:3.4": "yo ca jānāti desanaṃ.",
  "ja312:4.1": "Eso hi uttaritaro,",
  "ja312:4.2": "Bhāravaho dhuraddharo;",
  "ja312:4.3": "Yo paresādhipannānaṃ,",
  "ja312:4.4": "Sayaṃ sandhātumarahatī”ti.",
  "ja312:5.1": "Kassapamandiyajātakaṃ dutiyaṃ."
}