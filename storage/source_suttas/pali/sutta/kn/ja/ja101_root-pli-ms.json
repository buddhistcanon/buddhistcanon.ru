{
  "ja101:0.1": "Jātaka",
  "ja101:0.2": "Ekakanipāta",
  "ja101:0.3": "Parosatavagga",
  "ja101:0.4": "1. Parosatajātaka",
  "ja101:1.1": "“Parosatañcepi samāgatānaṃ,",
  "ja101:1.2": "Jhāyeyyuṃ te vassasataṃ apaññā;",
  "ja101:1.3": "Ekova seyyo puriso sapañño,",
  "ja101:1.4": "Yo bhāsitassa vijānāti atthan”ti.",
  "ja101:2.1": "Parosatajātakaṃ paṭhamaṃ."
}