{
  "ja134:0.1": "Jātaka",
  "ja134:0.2": "Ekakanipāta",
  "ja134:0.3": "Asampadānavagga",
  "ja134:0.4": "4. Jhānasodhanajātaka",
  "ja134:1.1": "“Ye saññino tepi duggatā,",
  "ja134:1.2": "Yepi asaññino tepi duggatā;",
  "ja134:1.3": "Etaṃ ubhayaṃ vivajjaya,",
  "ja134:1.4": "Taṃ samāpattisukhaṃ anaṅgaṇan”ti.",
  "ja134:2.1": "Jhānasodhanajātakaṃ catutthaṃ."
}