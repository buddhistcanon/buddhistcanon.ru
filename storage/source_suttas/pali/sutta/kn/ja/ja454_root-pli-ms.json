{
  "ja454:0.1": "Jātaka",
  "ja454:0.2": "Dasakanipāta",
  "ja454:0.3": "Catudvāravagga",
  "ja454:0.4": "16. Ghaṭapaṇḍitajātaka",
  "ja454:1.1": "“Uṭṭhehi kaṇha kiṃ sesi,",
  "ja454:1.2": "Ko attho supanena te;",
  "ja454:1.3": "Yopi tuyhaṃ sako bhātā,",
  "ja454:1.4": "Hadayaṃ cakkhu ca dakkhiṇaṃ;",
  "ja454:1.5": "Tassa vātā balīyanti,",
  "ja454:1.6": "Ghaṭo jappati kesava”.",
  "ja454:2.1": "“Tassa taṃ vacanaṃ sutvā,",
  "ja454:2.2": "rohiṇeyyassa kesavo;",
  "ja454:2.3": "Taramānarūpo vuṭṭhāsi,",
  "ja454:2.4": "bhātusokena aṭṭito”.",
  "ja454:3.1": "“Kiṃ nu ummattarūpova,",
  "ja454:3.2": "kevalaṃ dvārakaṃ imaṃ;",
  "ja454:3.3": "Saso sasoti lapasi,",
  "ja454:3.4": "ko nu te sasamāhari”.",
  "ja454:4.1": "“Sovaṇṇamayaṃ maṇīmayaṃ,",
  "ja454:4.2": "Lohamayaṃ atha rūpiyāmayaṃ;",
  "ja454:4.3": "Saṅkhasilāpavāḷamayaṃ,",
  "ja454:4.4": "Kārayissāmi te sasaṃ”.",
  "ja454:5.1": "“Santi aññepi sasakā,",
  "ja454:5.2": "araññe vanagocarā;",
  "ja454:5.3": "Tepi te ānayissāmi,",
  "ja454:5.4": "kīdisaṃ sasamicchasi”.",
  "ja454:6.1": "“Na cāhamete icchāmi,",
  "ja454:6.2": "ye sasā pathavissitā;",
  "ja454:6.3": "Candato sasamicchāmi,",
  "ja454:6.4": "taṃ me ohara kesava”.",
  "ja454:7.1": "“So nūna madhuraṃ ñāti,",
  "ja454:7.2": "jīvitaṃ vijahissasi;",
  "ja454:7.3": "Apatthiyaṃ yo patthayasi,",
  "ja454:7.4": "candato sasamicchasi”.",
  "ja454:8.1": "“Evañce kaṇha jānāsi,",
  "ja454:8.2": "yadaññamanusāsasi;",
  "ja454:8.3": "Kasmā pure mataṃ puttaṃ,",
  "ja454:8.4": "ajjāpi manusocasi”.",
  "ja454:9.1": "“Yaṃ na labbhā manussena,",
  "ja454:9.2": "amanussena vā puna;",
  "ja454:9.3": "Jāto me mā marī putto,",
  "ja454:9.4": "kuto labbhā alabbhiyaṃ.",
  "ja454:10.1": "Na mantā mūlabhesajjā,",
  "ja454:10.2": "osadhehi dhanena vā;",
  "ja454:10.3": "Sakkā ānayituṃ kaṇha,",
  "ja454:10.4": "yaṃ petamanusocasi”.",
  "ja454:11.1": "“Yassa etādisā assu,",
  "ja454:11.2": "amaccā purisapaṇḍitā;",
  "ja454:11.3": "Yathā nijjhāpaye ajja,",
  "ja454:11.4": "ghaṭo purisapaṇḍito.",
  "ja454:12.1": "Ādittaṃ vata maṃ santaṃ,",
  "ja454:12.2": "ghatasittaṃva pāvakaṃ;",
  "ja454:12.3": "Vārinā viya osiñcaṃ,",
  "ja454:12.4": "sabbaṃ nibbāpaye daraṃ.",
  "ja454:13.1": "Abbahī vata me sallaṃ,",
  "ja454:13.2": "yamāsi hadayassitaṃ;",
  "ja454:13.3": "Yo me sokaparetassa,",
  "ja454:13.4": "puttasokaṃ apānudi.",
  "ja454:14.1": "Sohaṃ abbūḷhasallosmi,",
  "ja454:14.2": "vītasoko anāvilo;",
  "ja454:14.3": "Na socāmi na rodāmi,",
  "ja454:14.4": "tava sutvāna māṇava”.",
  "ja454:15.1": "“Evaṃ karonti sappaññā,",
  "ja454:15.2": "Ye honti anukampakā;",
  "ja454:15.3": "Nivattayanti sokamhā,",
  "ja454:15.4": "Ghaṭo jeṭṭhaṃva bhātaran”ti. (1527)",
  "ja454:16.1": "Ghaṭapaṇḍitajātakaṃ soḷasamaṃ.",
  "ja454:17.1": "Dasakanipātaṃ niṭṭhitaṃ.",
  "ja454:18.1": "Tassuddānaṃ",
  "ja454:19.1": "Daḷha kaṇha dhanañjaya saṅkhavaro,",
  "ja454:19.2": "Raja sattaha kassa ca takkalinā;",
  "ja454:19.3": "Dhammaṃ kukkuṭa kuṇḍali bhojanadā,",
  "ja454:19.4": "Cakkavāka subhūrisa sotthi ghaṭoti."
}