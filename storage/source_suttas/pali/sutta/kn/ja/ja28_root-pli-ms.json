{
  "ja28:0.1": "Jātaka",
  "ja28:0.2": "Ekakanipāta",
  "ja28:0.3": "Kuruṅgavagga",
  "ja28:0.4": "8. Nandivisālajātaka",
  "ja28:1.1": "“Manuññameva bhāseyya,",
  "ja28:1.2": "nāmanuññaṃ kudācanaṃ;",
  "ja28:1.3": "Manuññaṃ bhāsamānassa,",
  "ja28:1.4": "garuṃ bhāraṃ udaddhari;",
  "ja28:1.5": "Dhanañca naṃ alābhesi,",
  "ja28:1.6": "tena cattamano ahū”ti.",
  "ja28:2.1": "Nandivisālajātakaṃ aṭṭhamaṃ."
}