{
  "ja365:0.1": "Jātaka",
  "ja365:0.2": "Pañcakanipāta",
  "ja365:0.3": "Vaṇṇārohavagga",
  "ja365:0.4": "5. Ahituṇḍikajātaka",
  "ja365:1.1": "“Dhuttomhi samma sumukha,",
  "ja365:1.2": "jūte akkhaparājito;",
  "ja365:1.3": "Harehi ambapakkāni,",
  "ja365:1.4": "vīriyaṃ te bhakkhayāmase”.",
  "ja365:2.1": "“Alikaṃ vata maṃ samma,",
  "ja365:2.2": "abhūtena pasaṃsasi;",
  "ja365:2.3": "Ko te suto vā diṭṭho vā,",
  "ja365:2.4": "sumukho nāma makkaṭo.",
  "ja365:3.1": "Ajjāpi me taṃ manasi,",
  "ja365:3.2": "yaṃ maṃ tvaṃ ahituṇḍika;",
  "ja365:3.3": "Dhaññāpaṇaṃ pavisitvā,",
  "ja365:3.4": "matto chātaṃ hanāsi maṃ.",
  "ja365:4.1": "Tāhaṃ saraṃ dukkhaseyyaṃ,",
  "ja365:4.2": "api rajjampi kāraye;",
  "ja365:4.3": "Nevāhaṃ yācito dajjaṃ,",
  "ja365:4.4": "tathā hi bhayatajjito.",
  "ja365:5.1": "Yañca jaññā kule jātaṃ,",
  "ja365:5.2": "Gabbhe tittaṃ amacchariṃ;",
  "ja365:5.3": "Tena sakhiñca mittañca,",
  "ja365:5.4": "Dhīro sandhātumarahatī”ti.",
  "ja365:6.1": "Ahituṇḍikajātakaṃ pañcamaṃ."
}