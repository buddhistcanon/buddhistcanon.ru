{
  "ja261:0.1": "Jātaka",
  "ja261:0.2": "Tikanipāta",
  "ja261:0.3": "Padumavagga",
  "ja261:0.4": "1. Padumajātaka",
  "ja261:1.1": "“Yathā kesā ca massū ca,",
  "ja261:1.2": "chinnaṃ chinnaṃ virūhati;",
  "ja261:1.3": "Evaṃ rūhatu te nāsā,",
  "ja261:1.4": "padumaṃ dehi yācito”.",
  "ja261:2.1": "“Yathā sāradikaṃ bījaṃ,",
  "ja261:2.2": "khette vuttaṃ virūhati;",
  "ja261:2.3": "Evaṃ rūhatu te nāsā,",
  "ja261:2.4": "padumaṃ dehi yācito”.",
  "ja261:3.1": "“Ubhopi palapantete,",
  "ja261:3.2": "api padmāni dassati;",
  "ja261:3.3": "Vajjuṃ vā te na vā vajjuṃ,",
  "ja261:3.4": "natthi nāsāya rūhanā;",
  "ja261:3.5": "Dehi samma padumāni,",
  "ja261:3.6": "ahaṃ yācāmi yācito”ti.",
  "ja261:4.1": "Padumajātakaṃ paṭhamaṃ."
}