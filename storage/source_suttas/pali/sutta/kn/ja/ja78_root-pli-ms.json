{
  "ja78:0.1": "Jātaka",
  "ja78:0.2": "Ekakanipāta",
  "ja78:0.3": "Varuṇavagga",
  "ja78:0.4": "8. Illisajātaka",
  "ja78:1.1": "“Ubho khañjā ubho kuṇī,",
  "ja78:1.2": "ubho visamacakkhukā;",
  "ja78:1.3": "Ubhinnaṃ piḷakā jātā,",
  "ja78:1.4": "nāhaṃ passāmi illisan”ti.",
  "ja78:2.1": "Illisajātakaṃ aṭṭhamaṃ."
}