{
  "ja385:0.1": "Jātaka",
  "ja385:0.2": "Chakkanipāta",
  "ja385:0.3": "Avāriyavagga",
  "ja385:0.4": "10. Nandiyamigarājajātaka",
  "ja385:1.1": "“Sace brāhmaṇa gacchesi,",
  "ja385:1.2": "sākete ajjunaṃ vanaṃ;",
  "ja385:1.3": "Vajjāsi nandiyaṃ nāma,",
  "ja385:1.4": "puttaṃ asmākamorasaṃ;",
  "ja385:1.5": "Mātā pitā ca te vuddhā,",
  "ja385:1.6": "te taṃ icchanti passituṃ”.",
  "ja385:2.1": "“Bhuttā mayā nivāpāni,",
  "ja385:2.2": "Rājino pānabhojanaṃ;",
  "ja385:2.3": "Taṃ rājapiṇḍaṃ avabhottuṃ,",
  "ja385:2.4": "Nāhaṃ brāhmaṇa mussahe”.",
  "ja385:3.1": "“Odahissāmahaṃ passaṃ,",
  "ja385:3.2": "khurappānissa rājino;",
  "ja385:3.3": "Tadāhaṃ sukhito mutto,",
  "ja385:3.4": "api passeyya mātaraṃ.",
  "ja385:4.1": "Migarājā pure āsiṃ,",
  "ja385:4.2": "kosalassa niketane;",
  "ja385:4.3": "Nandiyo nāma nāmena,",
  "ja385:4.4": "abhirūpo catuppado.",
  "ja385:5.1": "Taṃ maṃ vadhitumāgacchi,",
  "ja385:5.2": "dāyasmiṃ ajjune vane;",
  "ja385:5.3": "Dhanuṃ advejjhaṃ katvāna,",
  "ja385:5.4": "usuṃ sannayha kosalo.",
  "ja385:6.1": "Tassāhaṃ odahiṃ passaṃ,",
  "ja385:6.2": "khurappānissa rājino;",
  "ja385:6.3": "Tadāhaṃ sukhito mutto,",
  "ja385:6.4": "mātaraṃ daṭṭhumāgato”ti.",
  "ja385:7.1": "Nandiyamigarājajātakaṃ dasamaṃ.",
  "ja385:7.2": "Avāriyavaggo paṭhamo.",
  "ja385:8.1": "Tassuddānaṃ",
  "ja385:9.1": "Atha kujjharathesabha ketuvaro,",
  "ja385:9.2": "Sadarīmukha neru latā ca puna;",
  "ja385:9.3": "Apananda sirī ca sucittavaro,",
  "ja385:9.4": "Atha dhammika nandimigena dasāti."
}