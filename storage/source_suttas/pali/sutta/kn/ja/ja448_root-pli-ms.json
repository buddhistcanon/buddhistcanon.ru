{
  "ja448:0.1": "Jātaka",
  "ja448:0.2": "Dasakanipāta",
  "ja448:0.3": "Catudvāravagga",
  "ja448:0.4": "10. Kukkuṭajātaka",
  "ja448:1.1": "“Nāsmase katapāpamhi,",
  "ja448:1.2": "nāsmase alikavādine;",
  "ja448:1.3": "Nāsmase attatthapaññamhi,",
  "ja448:1.4": "atisantepi nāsmase.",
  "ja448:2.1": "Bhavanti heke purisā,",
  "ja448:2.2": "gopipāsikajātikā;",
  "ja448:2.3": "Ghasanti maññe mittāni,",
  "ja448:2.4": "vācāya na ca kammunā.",
  "ja448:3.1": "Sukkhañjalipaggahitā,",
  "ja448:3.2": "vācāya paliguṇṭhitā;",
  "ja448:3.3": "Manussapheggū nāsīde,",
  "ja448:3.4": "yasmiṃ natthi kataññutā.",
  "ja448:4.1": "Na hi aññaññacittānaṃ,",
  "ja448:4.2": "itthīnaṃ purisāna vā;",
  "ja448:4.3": "Nānāvikatvā saṃsaggaṃ,",
  "ja448:4.4": "tādisampi ca nāsmase.",
  "ja448:5.1": "Anariyakammamokkantaṃ,",
  "ja448:5.2": "athetaṃ sabbaghātinaṃ;",
  "ja448:5.3": "Nisitaṃva paṭicchannaṃ,",
  "ja448:5.4": "tādisampi ca nāsmase.",
  "ja448:6.1": "Mittarūpenidhekacce,",
  "ja448:6.2": "sākhalyena acetasā;",
  "ja448:6.3": "Vividhehi upāyanti,",
  "ja448:6.4": "tādisampi ca nāsmase.",
  "ja448:7.1": "Āmisaṃ vā dhanaṃ vāpi,",
  "ja448:7.2": "yattha passati tādiso;",
  "ja448:7.3": "Dubbhiṃ karoti dummedho,",
  "ja448:7.4": "tañca hantvāna gacchati”.",
  "ja448:8.1": "“Mittarūpena bahavo,",
  "ja448:8.2": "channā sevanti sattavo;",
  "ja448:8.3": "Jahe kāpurise hete,",
  "ja448:8.4": "kukkuṭo viya senakaṃ.",
  "ja448:9.1": "Yo ca uppatitaṃ atthaṃ,",
  "ja448:9.2": "na khippamanubujjhati;",
  "ja448:9.3": "Amittavasamanveti,",
  "ja448:9.4": "pacchā ca anutappati.",
  "ja448:10.1": "Yo ca uppatitaṃ atthaṃ,",
  "ja448:10.2": "khippameva nibodhati;",
  "ja448:10.3": "Muccate sattusambādhā,",
  "ja448:10.4": "kukkuṭo viya senakā.",
  "ja448:11.1": "Taṃ tādisaṃ kūṭamivoḍḍitaṃ vane,",
  "ja448:11.2": "Adhammikaṃ niccavidhaṃsakārinaṃ;",
  "ja448:11.3": "Ārā vivajjeyya naro vicakkhaṇo,",
  "ja448:11.4": "Senaṃ yathā kukkuṭo vaṃsakānane”ti.",
  "ja448:12.1": "Kukkuṭajātakaṃ dasamaṃ."
}