{
  "ja302:0.1": "Jātaka",
  "ja302:0.2": "Catukkanipāta",
  "ja302:0.3": "Kāliṅgavagga",
  "ja302:0.4": "2. Mahāassārohajātaka",
  "ja302:1.1": "“Adeyyesu dadaṃ dānaṃ,",
  "ja302:1.2": "deyyesu nappavecchati;",
  "ja302:1.3": "Āpāsu byasanaṃ patto,",
  "ja302:1.4": "sahāyaṃ nādhigacchati.",
  "ja302:2.1": "Nādeyyesu dadaṃ dānaṃ,",
  "ja302:2.2": "deyyesu yo pavecchati;",
  "ja302:2.3": "Āpāsu byasanaṃ patto,",
  "ja302:2.4": "sahāyamadhigacchati.",
  "ja302:3.1": "Saññogasambhogavisesadassanaṃ,",
  "ja302:3.2": "Anariyadhammesu saṭhesu nassati;",
  "ja302:3.3": "Katañca ariyesu ca ajjavesu,",
  "ja302:3.4": "Mahapphalaṃ hoti aṇumpi tādisu.",
  "ja302:4.1": "Yo pubbe katakalyāṇo,",
  "ja302:4.2": "Akā loke sudukkaraṃ;",
  "ja302:4.3": "Pacchā kayirā na vā kayirā,",
  "ja302:4.4": "Accantaṃ pūjanāraho”ti.",
  "ja302:5.1": "Mahāassārohajātakaṃ dutiyaṃ."
}