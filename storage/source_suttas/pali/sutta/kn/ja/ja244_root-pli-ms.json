{
  "ja244:0.1": "Jātaka",
  "ja244:0.2": "Dukanipāta",
  "ja244:0.3": "Siṅgālavagga",
  "ja244:0.4": "4. Vigaticchajātaka",
  "ja244:1.1": "“Yaṃ passati na taṃ icchati,",
  "ja244:1.2": "Yañca na passati taṃ kiricchati;",
  "ja244:1.3": "Maññāmi ciraṃ carissati,",
  "ja244:1.4": "Na hi taṃ lacchati yaṃ sa icchati.",
  "ja244:2.1": "Yaṃ labhati na tena tussati,",
  "ja244:2.2": "Yañca pattheti laddhaṃ hīḷeti;",
  "ja244:2.3": "Icchā hi anantagocarā,",
  "ja244:2.4": "Vigaticchāna namo karomase”ti.",
  "ja244:3.1": "Vigaticchajātakaṃ catutthaṃ."
}