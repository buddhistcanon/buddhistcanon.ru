{
  "ja257:0.1": "Jātaka",
  "ja257:0.2": "Tikanipāta",
  "ja257:0.3": "Saṅkappavagga",
  "ja257:0.4": "7. Gāmaṇicandajātaka",
  "ja257:1.1": "“Nāyaṃ gharānaṃ kusalo,",
  "ja257:1.2": "lolo ayaṃ valīmukho;",
  "ja257:1.3": "Kataṃ kataṃ kho dūseyya,",
  "ja257:1.4": "evaṃdhammamidaṃ kulaṃ.",
  "ja257:2.1": "Nayidaṃ cittavato lomaṃ,",
  "ja257:2.2": "nāyaṃ assāsiko migo;",
  "ja257:2.3": "Siṭṭhaṃ me janasandhena,",
  "ja257:2.4": "nāyaṃ kiñci vijānati.",
  "ja257:3.1": "Na mātaraṃ pitaraṃ vā,",
  "ja257:3.2": "bhātaraṃ bhaginiṃ sakaṃ;",
  "ja257:3.3": "Bhareyya tādiso poso,",
  "ja257:3.4": "siṭṭhaṃ dasarathena me”ti.",
  "ja257:4.1": "Gāmaṇicanda jātakaṃ sattamaṃ."
}