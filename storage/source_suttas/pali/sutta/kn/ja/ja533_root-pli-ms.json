{
  "ja533:0.1": "Jātaka",
  "ja533:0.2": "Asītinipāta",
  "ja533:0.3": "Cūḷahaṃsavagga",
  "ja533:0.4": "1 Cūḷahaṃsajātaka",
  "ja533:1.1": "“Sumukha anupacinantā,",
  "ja533:1.2": "pakkamanti vihaṅgamā;",
  "ja533:1.3": "Gaccha tuvampi mā kaṅkhi,",
  "ja533:1.4": "natthi baddhe sahāyatā”.",
  "ja533:2.1": "“Gacche vāhaṃ na vā gacche,",
  "ja533:2.2": "na tena amaro siyaṃ;",
  "ja533:2.3": "Sukhitaṃ taṃ upāsitvā,",
  "ja533:2.4": "dukkhitaṃ taṃ kathaṃ jahe.",
  "ja533:3.1": "Maraṇaṃ vā tayā saddhiṃ,",
  "ja533:3.2": "jīvitaṃ vā tayā vinā;",
  "ja533:3.3": "Tadeva maraṇaṃ seyyo,",
  "ja533:3.4": "yañce jīve tayā vinā.",
  "ja533:4.1": "Nesa dhammo mahārāja,",
  "ja533:4.2": "yaṃ taṃ evaṃ gataṃ jahe;",
  "ja533:4.3": "Yā gati tuyhaṃ sā mayhaṃ,",
  "ja533:4.4": "ruccate vihagādhipa”.",
  "ja533:5.1": "“Kā nu pāsena baddhassa,",
  "ja533:5.2": "gati aññā mahānasā;",
  "ja533:5.3": "Sā kathaṃ cetayānassa,",
  "ja533:5.4": "muttassa tava ruccati.",
  "ja533:6.1": "Kaṃ vā tvaṃ passase atthaṃ,",
  "ja533:6.2": "mama tuyhañca pakkhima;",
  "ja533:6.3": "Ñātīnaṃ vāvasiṭṭhānaṃ,",
  "ja533:6.4": "ubhinnaṃ jīvitakkhaye.",
  "ja533:7.1": "Yaṃ na kañcanadepiñcha,",
  "ja533:7.2": "Andhena tamasā gataṃ;",
  "ja533:7.3": "Tādise sañcajaṃ pāṇaṃ,",
  "ja533:7.4": "Kamatthamabhijotaye”.",
  "ja533:8.1": "“Kathaṃ nu patataṃ seṭṭha,",
  "ja533:8.2": "Dhamme atthaṃ na bujjhasi;",
  "ja533:8.3": "Dhammo apacito santo,",
  "ja533:8.4": "Atthaṃ dasseti pāṇinaṃ.",
  "ja533:9.1": "Sohaṃ dhammaṃ apekkhāno,",
  "ja533:9.2": "dhammā catthaṃ samuṭṭhitaṃ;",
  "ja533:9.3": "Bhattiñca tayi sampassaṃ,",
  "ja533:9.4": "nāvakaṅkhāmi jīvitaṃ.",
  "ja533:10.1": "Addhā eso sataṃ dhammo,",
  "ja533:10.2": "yo mitto mittamāpade;",
  "ja533:10.3": "Na caje jīvitassāpi,",
  "ja533:10.4": "hetudhammamanussaraṃ”.",
  "ja533:11.1": "“Svāyaṃ dhammo ca te ciṇṇo,",
  "ja533:11.2": "bhatti ca viditā mayi;",
  "ja533:11.3": "Kāmaṃ karassu mayhetaṃ,",
  "ja533:11.4": "gacchevānumato mayā.",
  "ja533:12.1": "Api tvevaṃ gate kāle,",
  "ja533:12.2": "yaṃ khaṇḍaṃ ñātinaṃ mayā;",
  "ja533:12.3": "Tayā taṃ buddhisampannaṃ,",
  "ja533:12.4": "assa paramasaṃvutaṃ”.",
  "ja533:13.1": "Iccevaṃ mantayantānaṃ,",
  "ja533:13.2": "Ariyānaṃ ariyavuttinaṃ;",
  "ja533:13.3": "Paccadissatha nesādo,",
  "ja533:13.4": "Āturānamivantako.",
  "ja533:14.1": "Te sattumabhisañcikkha,",
  "ja533:14.2": "dīgharattaṃ hitā dijā;",
  "ja533:14.3": "Tuṇhīmāsittha ubhayo,",
  "ja533:14.4": "na sañcalesumāsanā.",
  "ja533:15.1": "Dhataraṭṭhe ca disvāna,",
  "ja533:15.2": "samuḍḍente tato tato;",
  "ja533:15.3": "Abhikkamatha vegena,",
  "ja533:15.4": "dijasattu dijādhipe.",
  "ja533:16.1": "So ca vegenabhikkamma,",
  "ja533:16.2": "āsajja parame dije;",
  "ja533:16.3": "Paccakamittha nesādo,",
  "ja533:16.4": "baddhā iti vicintayaṃ.",
  "ja533:17.1": "Ekaṃva baddhamāsīnaṃ,",
  "ja533:17.2": "abaddhañca punāparaṃ;",
  "ja533:17.3": "Āsajja baddhamāsīnaṃ,",
  "ja533:17.4": "pekkhamānamadīnavaṃ.",
  "ja533:18.1": "Tato so vimatoyeva,",
  "ja533:18.2": "paṇḍare ajjhabhāsatha;",
  "ja533:18.3": "Pavaḍḍhakāye āsīne,",
  "ja533:18.4": "dijasaṅghagaṇādhipe.",
  "ja533:19.1": "“Yannu pāsena mahatā,",
  "ja533:19.2": "baddho na kurute disaṃ;",
  "ja533:19.3": "Atha kasmā abaddho tvaṃ,",
  "ja533:19.4": "balī pakkhi na gacchasi.",
  "ja533:20.1": "Kiṃ nu tyāyaṃ dijo hoti,",
  "ja533:20.2": "mutto baddhaṃ upāsasi;",
  "ja533:20.3": "Ohāya sakuṇā yanti,",
  "ja533:20.4": "kiṃ eko avahīyasi”.",
  "ja533:21.1": "“Rājā me so dijāmitta,",
  "ja533:21.2": "sakhā pāṇasamo ca me;",
  "ja533:21.3": "Neva naṃ vijahissāmi,",
  "ja533:21.4": "yāva kālassa pariyāyaṃ”.",
  "ja533:22.1": "“Kathaṃ panāyaṃ vihaṅgo,",
  "ja533:22.2": "nāddasa pāsamoḍḍitaṃ;",
  "ja533:22.3": "Padañhetaṃ mahantānaṃ,",
  "ja533:22.4": "boddhumarahanti āpadaṃ”.",
  "ja533:23.1": "“Yadā parābhavo hoti,",
  "ja533:23.2": "poso jīvitasaṅkhaye;",
  "ja533:23.3": "Atha jālañca pāsañca,",
  "ja533:23.4": "āsajjāpi na bujjhati.",
  "ja533:24.1": "Api tveva mahāpañña,",
  "ja533:24.2": "pāsā bahuvidhā tatā;",
  "ja533:24.3": "Guyhamāsajja bajjhanti,",
  "ja533:24.4": "athevaṃ jīvitakkhaye”.",
  "ja533:25.1": "“Api nāyaṃ tayā saddhiṃ,",
  "ja533:25.2": "saṃvāsassa sukhudrayo;",
  "ja533:25.3": "Api no anumaññāsi,",
  "ja533:25.4": "api no jīvitaṃ dade”.",
  "ja533:26.1": "“Na ceva me tvaṃ baddhosi,",
  "ja533:26.2": "napi icchāmi te vadhaṃ;",
  "ja533:26.3": "Kāmaṃ khippamito gantvā,",
  "ja533:26.4": "jīva tvaṃ anigho ciraṃ”.",
  "ja533:27.1": "“Nevāhametamicchāmi,",
  "ja533:27.2": "aññatretassa jīvitā;",
  "ja533:27.3": "Sace ekena tuṭṭhosi,",
  "ja533:27.4": "muñcetaṃ mañca bhakkhaya.",
  "ja533:28.1": "Ārohapariṇāhena,",
  "ja533:28.2": "tulyāsmā vayasā ubho;",
  "ja533:28.3": "Na te lābhena jīvatthi,",
  "ja533:28.4": "etena niminā tuvaṃ.",
  "ja533:29.1": "Tadiṅgha samapekkhassu,",
  "ja533:29.2": "hotu giddhi tavamhasu;",
  "ja533:29.3": "Maṃ pubbe bandha pāsena,",
  "ja533:29.4": "pacchā muñca dijādhipaṃ.",
  "ja533:30.1": "Tāvadeva ca te lābho,",
  "ja533:30.2": "katāssa yācanāya ca;",
  "ja533:30.3": "Mitti ca dhataraṭṭhehi,",
  "ja533:30.4": "yāvajīvāya te siyā”.",
  "ja533:31.1": "“Passantu no mahāsaṃghā,",
  "ja533:31.2": "tayā muttaṃ ito gataṃ;",
  "ja533:31.3": "Mittāmaccā ca bhaccā ca,",
  "ja533:31.4": "puttadārā ca bandhavā.",
  "ja533:32.1": "Na ca te tādisā mittā,",
  "ja533:32.2": "bahūnaṃ idha vijjati;",
  "ja533:32.3": "Yathā tvaṃ dhataraṭṭhassa,",
  "ja533:32.4": "pāṇasādhāraṇo sakhā.",
  "ja533:33.1": "So te sahāyaṃ muñcāmi,",
  "ja533:33.2": "hotu rājā tavānugo;",
  "ja533:33.3": "Kāmaṃ khippamito gantvā,",
  "ja533:33.4": "ñātimajjhe virocatha”.",
  "ja533:34.1": "“So patīto pamuttena,",
  "ja533:34.2": "bhattunā bhattugāravo;",
  "ja533:34.3": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:34.4": "vācaṃ kaṇṇasukhaṃ bhaṇaṃ.",
  "ja533:35.1": "Evaṃ luddaka nandassu,",
  "ja533:35.2": "saha sabbehi ñātibhi;",
  "ja533:35.3": "Yathāhamajja nandāmi,",
  "ja533:35.4": "muttaṃ disvā dijādhipaṃ”.",
  "ja533:36.1": "“Ehi taṃ anusikkhāmi,",
  "ja533:36.2": "Yathā tvamapi lacchase;",
  "ja533:36.3": "Lābhaṃ tavāyaṃ dhataraṭṭho,",
  "ja533:36.4": "Pāpaṃ kiñci na dakkhati.",
  "ja533:37.1": "Khippamantepuraṃ netvā,",
  "ja533:37.2": "rañño dassehi no ubho;",
  "ja533:37.3": "Abaddhe pakatibhūte,",
  "ja533:37.4": "kāje ubhayato ṭhite.",
  "ja533:38.1": "Dhataraṭṭhā mahārāja,",
  "ja533:38.2": "haṃsādhipatino ime;",
  "ja533:38.3": "Ayañhi rājā haṃsānaṃ,",
  "ja533:38.4": "ayaṃ senāpatītaro.",
  "ja533:39.1": "Asaṃsayaṃ imaṃ disvā,",
  "ja533:39.2": "haṃsarājaṃ narādhipo;",
  "ja533:39.3": "Patīto sumano vitto,",
  "ja533:39.4": "bahuṃ dassati te dhanaṃ”.",
  "ja533:40.1": "Tassa taṃ vacanaṃ sutvā,",
  "ja533:40.2": "kammunā upapādayi;",
  "ja533:40.3": "Khippamantepuraṃ gantvā,",
  "ja533:40.4": "rañño haṃse adassayi;",
  "ja533:40.5": "Abaddhe pakatibhūte,",
  "ja533:40.6": "kāje ubhayato ṭhite.",
  "ja533:41.1": "“Dhataraṭṭhā mahārāja,",
  "ja533:41.2": "haṃsādhipatino ime;",
  "ja533:41.3": "Ayañhi rājā haṃsānaṃ,",
  "ja533:41.4": "ayaṃ senāpatītaro”.",
  "ja533:42.1": "“Kathaṃ panime vihaṅgā,",
  "ja533:42.2": "tava hatthattamāgatā;",
  "ja533:42.3": "Kathaṃ luddo mahantānaṃ,",
  "ja533:42.4": "issare idha ajjhagā”.",
  "ja533:43.1": "“Vihitā santime pāsā,",
  "ja533:43.2": "pallalesu janādhipa;",
  "ja533:43.3": "Yaṃ yadāyatanaṃ maññe,",
  "ja533:43.4": "dijānaṃ pāṇarodhanaṃ.",
  "ja533:44.1": "Tādisaṃ pāsamāsajja,",
  "ja533:44.2": "haṃsarājā abajjhatha;",
  "ja533:44.3": "Taṃ abaddho upāsīno,",
  "ja533:44.4": "mamāyaṃ ajjhabhāsatha.",
  "ja533:45.1": "Sudukkaraṃ anariyehi,",
  "ja533:45.2": "dahate bhāvamuttamaṃ;",
  "ja533:45.3": "Bhatturatthe parakkanto,",
  "ja533:45.4": "dhammayutto vihaṅgamo.",
  "ja533:46.1": "Attanāyaṃ cajitvāna,",
  "ja533:46.2": "jīvitaṃ jīvitāraho;",
  "ja533:46.3": "Anutthunanto āsīno,",
  "ja533:46.4": "bhattu yācittha jīvitaṃ.",
  "ja533:47.1": "Tassa taṃ vacanaṃ sutvā,",
  "ja533:47.2": "pasādamahamajjhagā;",
  "ja533:47.3": "Tato naṃ pāmuciṃ pāsā,",
  "ja533:47.4": "anuññāsiṃ sukhena ca.",
  "ja533:48.1": "So patīto pamuttena,",
  "ja533:48.2": "bhattunā bhattugāravo;",
  "ja533:48.3": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:48.4": "vācaṃ kaṇṇasukhaṃ bhaṇaṃ.",
  "ja533:49.1": "‘Evaṃ luddaka nandassu,",
  "ja533:49.2": "saha sabbehi ñātibhi;",
  "ja533:49.3": "Yathāhamajja nandāmi,",
  "ja533:49.4": "muttaṃ disvā dijādhipaṃ.",
  "ja533:50.1": "Ehi taṃ anusikkhāmi,",
  "ja533:50.2": "yathā tvamapi lacchase;",
  "ja533:50.3": "Lābhaṃ tavāyaṃ dhataraṭṭho,",
  "ja533:50.4": "pāpaṃ kiñci na dakkhati.",
  "ja533:51.1": "Khippamantepuraṃ netvā,",
  "ja533:51.2": "rañño dassehi no ubho;",
  "ja533:51.3": "Abaddhe pakatibhūte,",
  "ja533:51.4": "kāje ubhayato ṭhite.",
  "ja533:52.1": "“Dhataraṭṭhā mahārāja,",
  "ja533:52.2": "haṃsādhipatino ime;",
  "ja533:52.3": "Ayañhi rājā haṃsānaṃ,",
  "ja533:52.4": "ayaṃ senāpatītaro”.",
  "ja533:53.1": "Asaṃsayaṃ imaṃ disvā,",
  "ja533:53.2": "haṃsarājaṃ narādhipo;",
  "ja533:53.3": "Patīto sumano vitto,",
  "ja533:53.4": "bahuṃ dassati te dhanaṃ’.",
  "ja533:54.1": "Evametassa vacanā,",
  "ja533:54.2": "ānītāme ubho mayā;",
  "ja533:54.3": "Ettheva hi ime āsuṃ,",
  "ja533:54.4": "ubho anumatā mayā.",
  "ja533:55.1": "Soyaṃ evaṃ gato pakkhī,",
  "ja533:55.2": "dijo paramadhammiko;",
  "ja533:55.3": "Mādisassa hi luddassa,",
  "ja533:55.4": "janayeyyātha maddavaṃ.",
  "ja533:56.1": "Upāyanañca te deva,",
  "ja533:56.2": "nāññaṃ passāmi edisaṃ;",
  "ja533:56.3": "Sabbasākuṇikāgāme,",
  "ja533:56.4": "taṃ passa manujādhipa”.",
  "ja533:57.1": "Disvā nisinnaṃ rājānaṃ,",
  "ja533:57.2": "pīṭhe sovaṇṇaye subhe;",
  "ja533:57.3": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:57.4": "vācaṃ kaṇṇasukhaṃ bhaṇaṃ.",
  "ja533:58.1": "“Kaccinnu bhoto kusalaṃ,",
  "ja533:58.2": "kacci bhoto anāmayaṃ;",
  "ja533:58.3": "Kacci raṭṭhamidaṃ phītaṃ,",
  "ja533:58.4": "dhammena manusāsasi”.",
  "ja533:59.1": "“Kusalañceva me haṃsa,",
  "ja533:59.2": "Atho haṃsa anāmayaṃ;",
  "ja533:59.3": "Atho raṭṭhamidaṃ phītaṃ,",
  "ja533:59.4": "Dhammena manusāsahaṃ”.",
  "ja533:60.1": "“Kacci bhoto amaccesu,",
  "ja533:60.2": "doso koci na vijjati;",
  "ja533:60.3": "Kacci ca te tavatthesu,",
  "ja533:60.4": "nāvakaṅkhanti jīvitaṃ”.",
  "ja533:61.1": "“Athopi me amaccesu,",
  "ja533:61.2": "doso koci na vijjati;",
  "ja533:61.3": "Athopi te mamatthesu,",
  "ja533:61.4": "nāvakaṅkhanti jīvitaṃ”.",
  "ja533:62.1": "“Kacci te sādisī bhariyā,",
  "ja533:62.2": "assavā piyabhāṇinī;",
  "ja533:62.3": "Puttarūpayasūpetā,",
  "ja533:62.4": "tava chandavasānugā”.",
  "ja533:63.1": "“Atho me sādisī bhariyā,",
  "ja533:63.2": "assavā piyabhāṇinī;",
  "ja533:63.3": "Puttarūpayasūpetā,",
  "ja533:63.4": "mama chandavasānugā.",
  "ja533:64.1": "Bhavantaṃ kacci nu mahā-",
  "ja533:64.2": "sattuhatthattataṃ gato;",
  "ja533:64.3": "Dukkhamāpajji vipulaṃ,",
  "ja533:64.4": "tasmiṃ paṭhamamāpade.",
  "ja533:65.1": "Kacci yantāpatitvāna,",
  "ja533:65.2": "daṇḍena samapothayi;",
  "ja533:65.3": "Evametesaṃ jammānaṃ,",
  "ja533:65.4": "pātikaṃ bhavati tāvade”.",
  "ja533:66.1": "“Khemamāsi mahārāja,",
  "ja533:66.2": "evamāpadiyā sati;",
  "ja533:66.3": "Na cāyaṃ kiñci rasmāsu,",
  "ja533:66.4": "sattūva samapajjatha.",
  "ja533:67.1": "Paccagamittha nesādo,",
  "ja533:67.2": "pubbeva ajjhabhāsatha;",
  "ja533:67.3": "Tadāyaṃ sumukhoyeva,",
  "ja533:67.4": "paṇḍito paccabhāsatha.",
  "ja533:68.1": "Tassa taṃ vacanaṃ sutvā,",
  "ja533:68.2": "pasādamayamajjhagā;",
  "ja533:68.3": "Tato maṃ pāmucī pāsā,",
  "ja533:68.4": "anuññāsi sukhena ca.",
  "ja533:69.1": "Idañca sumukheneva,",
  "ja533:69.2": "etadatthāya cintitaṃ;",
  "ja533:69.3": "Bhoto sakāsegamanaṃ,",
  "ja533:69.4": "etassa dhanamicchatā”.",
  "ja533:70.1": "“Svāgatañcevidaṃ bhavataṃ,",
  "ja533:70.2": "patīto casmi dassanā;",
  "ja533:70.3": "Eso cāpi bahuṃ vittaṃ,",
  "ja533:70.4": "labhataṃ yāvadicchati”.",
  "ja533:71.1": "Santappayitvā nesādaṃ,",
  "ja533:71.2": "bhogehi manujādhipo;",
  "ja533:71.3": "Ajjhabhāsatha vakkaṅgaṃ,",
  "ja533:71.4": "vācaṃ kaṇṇasukhaṃ bhaṇaṃ.",
  "ja533:72.1": "“Yaṃ khalu dhammamādhīnaṃ,",
  "ja533:72.2": "vaso vattati kiñcanaṃ;",
  "ja533:72.3": "Sabbatthissariyaṃ tava,",
  "ja533:72.4": "taṃ pasāsa yadicchatha.",
  "ja533:73.1": "Dānatthaṃ upabhottuṃ vā,",
  "ja533:73.2": "yaṃ caññaṃ upakappati;",
  "ja533:73.3": "Etaṃ dadāmi vo vittaṃ,",
  "ja533:73.4": "issariyaṃ vissajāmi vo”.",
  "ja533:74.1": "“Yathā ca myāyaṃ sumukho,",
  "ja533:74.2": "ajjhabhāseyya paṇḍito;",
  "ja533:74.3": "Kāmasā buddhisampanno,",
  "ja533:74.4": "taṃ myāssa paramappiyaṃ”.",
  "ja533:75.1": "“Ahaṃ khalu mahārāja,",
  "ja533:75.2": "nāgarājārivantaraṃ;",
  "ja533:75.3": "Paṭivattuṃ na sakkomi,",
  "ja533:75.4": "na me so vinayo siyā.",
  "ja533:76.1": "Amhākañceva so seṭṭho,",
  "ja533:76.2": "tvañca uttamasattavo;",
  "ja533:76.3": "Bhūmipālo manussindo,",
  "ja533:76.4": "pūjā bahūhi hetuhi.",
  "ja533:77.1": "Tesaṃ ubhinnaṃ bhaṇataṃ,",
  "ja533:77.2": "vattamāne vinicchaye;",
  "ja533:77.3": "Nantaraṃ paṭivattabbaṃ,",
  "ja533:77.4": "pessena manujādhipa”.",
  "ja533:78.1": "“Dhammena kira nesādo,",
  "ja533:78.2": "paṇḍito aṇḍajo iti;",
  "ja533:78.3": "Na heva akatattassa,",
  "ja533:78.4": "nayo etādiso siyā.",
  "ja533:79.1": "Evaṃ aggapakatimā,",
  "ja533:79.2": "evaṃ uttamasattavo;",
  "ja533:79.3": "Yāvatatthi mayā diṭṭhā,",
  "ja533:79.4": "nāññaṃ passāmi edisaṃ.",
  "ja533:80.1": "Tuṭṭhosmi vo pakatiyā,",
  "ja533:80.2": "vākyena madhurena ca;",
  "ja533:80.3": "Eso cāpi mamacchando,",
  "ja533:80.4": "ciraṃ passeyya vo ubho”.",
  "ja533:81.1": "“Yaṃ kiccaṃ parame mitte,",
  "ja533:81.2": "katamasmāsu taṃ tayā;",
  "ja533:81.3": "Pattā nissaṃsayaṃ tyāmhā,",
  "ja533:81.4": "bhattirasmāsu yā tava.",
  "ja533:82.1": "Aduñca nūna sumahā,",
  "ja533:82.2": "ñātisaṅghassa mantaraṃ;",
  "ja533:82.3": "Adassanena asmākaṃ,",
  "ja533:82.4": "dukkhaṃ bahūsu pakkhisu.",
  "ja533:83.1": "Tesaṃ sokavighātāya,",
  "ja533:83.2": "Tayā anumatā mayaṃ;",
  "ja533:83.3": "Taṃ padakkhiṇato katvā,",
  "ja533:83.4": "Ñātiṃ passemurindama.",
  "ja533:84.1": "Addhāhaṃ vipulaṃ pītiṃ,",
  "ja533:84.2": "bhavataṃ vindāmi dassanā;",
  "ja533:84.3": "Eso cāpi mahā attho,",
  "ja533:84.4": "ñātivissāsanā siyā”.",
  "ja533:85.1": "Idaṃ vatvā dhataraṭṭho,",
  "ja533:85.2": "haṃsarājā narādhipaṃ;",
  "ja533:85.3": "Uttamaṃ javamanvāya,",
  "ja533:85.4": "ñātisaṅghaṃ upāgamuṃ.",
  "ja533:86.1": "Te aroge anuppatte,",
  "ja533:86.2": "disvāna parame dije;",
  "ja533:86.3": "Kekāti makaruṃ haṃsā,",
  "ja533:86.4": "puthusaddo ajāyatha.",
  "ja533:87.1": "Te patītā pamuttena,",
  "ja533:87.2": "bhattunā bhattugāravā;",
  "ja533:87.3": "Samantā parikiriṃsu,",
  "ja533:87.4": "aṇḍajā laddhapaccayā.",
  "ja533:88.1": "Evaṃ mittavataṃ atthā,",
  "ja533:88.2": "sabbe honti padakkhiṇā;",
  "ja533:88.3": "Haṃsā yathā dhataraṭṭhā,",
  "ja533:88.4": "ñātisaṅghaṃ upāgamunti.",
  "ja533:89.1": "Cūḷahaṃsajātakaṃ paṭhamaṃ."
}