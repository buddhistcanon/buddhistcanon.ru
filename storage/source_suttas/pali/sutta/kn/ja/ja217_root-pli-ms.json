{
  "ja217:0.1": "Jātaka",
  "ja217:0.2": "Dukanipāta",
  "ja217:0.3": "Bīraṇathambhavagga",
  "ja217:0.4": "7. Seggujātaka",
  "ja217:1.1": "“Sabbo loko attamano ahosi,",
  "ja217:1.2": "Akovidā gāmadhammassa seggu;",
  "ja217:1.3": "Komāri ko nāma tavajja dhammo,",
  "ja217:1.4": "Yaṃ tvaṃ gahitā pavane parodasi”.",
  "ja217:2.1": "“Yo dukkhaphuṭṭhāya bhaveyya tāṇaṃ,",
  "ja217:2.2": "So me pitā dubbhi vane karoti;",
  "ja217:2.3": "Sā kassa kandāmi vanassa majjhe,",
  "ja217:2.4": "Yo tāyitā so sahasaṃ karotī”ti.",
  "ja217:3.1": "Seggujātakaṃ sattamaṃ."
}