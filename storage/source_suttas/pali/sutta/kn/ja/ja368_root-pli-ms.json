{
  "ja368:0.1": "Jātaka",
  "ja368:0.2": "Pañcakanipāta",
  "ja368:0.3": "Vaṇṇārohavagga",
  "ja368:0.4": "8. Tacasārajātaka",
  "ja368:1.1": "“Amittahatthatthagatā,",
  "ja368:1.2": "tacasārasamappitā;",
  "ja368:1.3": "Pasannamukhavaṇṇāttha,",
  "ja368:1.4": "kasmā tumhe na socatha”.",
  "ja368:2.1": "“Na socanāya paridevanāya,",
  "ja368:2.2": "Atthova labbho api appakopi;",
  "ja368:2.3": "Socantamenaṃ dukhitaṃ viditvā,",
  "ja368:2.4": "Paccatthikā attamanā bhavanti.",
  "ja368:3.1": "Yato ca kho paṇḍito āpadāsu,",
  "ja368:3.2": "Na vedhatī atthavinicchayaññū;",
  "ja368:3.3": "Paccatthikāssa dukhitā bhavanti,",
  "ja368:3.4": "Disvā mukhaṃ avikāraṃ purāṇaṃ.",
  "ja368:4.1": "Jappena mantena subhāsitena,",
  "ja368:4.2": "Anuppadānena paveṇiyā vā;",
  "ja368:4.3": "Yathā yathā yattha labhetha atthaṃ,",
  "ja368:4.4": "Tathā tathā tattha parakkameyya.",
  "ja368:5.1": "Yato ca jāneyya alabbhaneyyo,",
  "ja368:5.2": "Mayā va aññena vā esa attho;",
  "ja368:5.3": "Asocamāno adhivāsayeyya,",
  "ja368:5.4": "Kammaṃ daḷhaṃ kinti karomi dānī”ti.",
  "ja368:6.1": "Tacasārajātakaṃ aṭṭhamaṃ."
}