{
  "ja40:0.1": "Jātaka",
  "ja40:0.2": "Ekakanipāta",
  "ja40:0.3": "Kulāvakavagga",
  "ja40:0.4": "10. Khadiraṅgārajātaka",
  "ja40:1.1": "“Kāmaṃ patāmi nirayaṃ,",
  "ja40:1.2": "uddhampādo avaṃsiro;",
  "ja40:1.3": "Nānariyaṃ karissāmi,",
  "ja40:1.4": "handa piṇḍaṃ paṭiggahā”ti.",
  "ja40:2.1": "Khadiraṅgārajātakaṃ dasamaṃ.",
  "ja40:2.2": "Kulāvakavaggo catuttho.",
  "ja40:3.1": "Tassuddānaṃ",
  "ja40:4.1": "Sirimātali dhītara pakkhivaro,",
  "ja40:4.2": "Ratiyāgato mātāpitā ca puna;",
  "ja40:4.3": "Jagatīruha vuḍḍha sukakkaṭako,",
  "ja40:4.4": "Tathā nandakapiṇḍavarena dasāti."
}