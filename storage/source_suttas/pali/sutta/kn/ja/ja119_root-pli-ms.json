{
  "ja119:0.1": "Jātaka",
  "ja119:0.2": "Ekakanipāta",
  "ja119:0.3": "Haṃcivagga",
  "ja119:0.4": "9. Akālarāvijātaka",
  "ja119:1.1": "“Amātāpitarasaṃvaddho,",
  "ja119:1.2": "anācerakule vasaṃ;",
  "ja119:1.3": "Nāyaṃ kālaṃ akālaṃ vā,",
  "ja119:1.4": "abhijānāti kukkuṭo”ti.",
  "ja119:2.1": "Akālarāvijātakaṃ navamaṃ."
}