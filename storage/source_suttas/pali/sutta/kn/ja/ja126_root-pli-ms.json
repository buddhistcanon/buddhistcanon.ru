{
  "ja126:0.1": "Jātaka",
  "ja126:0.2": "Ekakanipāta",
  "ja126:0.3": "Kusanāḷivagga",
  "ja126:0.4": "6. Asilakkhaṇajātaka",
  "ja126:1.1": "“Tadevekassa kalyāṇaṃ,",
  "ja126:1.2": "tadevekassa pāpakaṃ;",
  "ja126:1.3": "Tasmā sabbaṃ na kalyāṇaṃ,",
  "ja126:1.4": "sabbaṃ vāpi na pāpakan”ti.",
  "ja126:2.1": "Asilakkhaṇajātakaṃ chaṭṭhaṃ."
}