{
  "ja91:0.1": "Jātaka",
  "ja91:0.2": "Ekakanipāta",
  "ja91:0.3": "Littavagga",
  "ja91:0.4": "1. Littajātaka",
  "ja91:1.1": "“Littaṃ paramena tejasā,",
  "ja91:1.2": "Gilamakkhaṃ puriso na bujjhati;",
  "ja91:1.3": "Gila re gila pāpadhuttaka,",
  "ja91:1.4": "Pacchā te kaṭukaṃ bhavissatī”ti.",
  "ja91:2.1": "Littajātakaṃ paṭhamaṃ."
}