{
  "ja437:0.1": "Jātaka",
  "ja437:0.2": "Navakanipāta",
  "ja437:0.3": "Gijjhavagga",
  "ja437:0.4": "11. Pūtimaṃsajātaka",
  "ja437:1.1": "“Na kho me ruccati āḷi,",
  "ja437:1.2": "pūtimaṃsassa pekkhanā;",
  "ja437:1.3": "Etādisā sakhārasmā,",
  "ja437:1.4": "ārakā parivajjaye”.",
  "ja437:2.1": "“Ummattikā ayaṃ veṇī,",
  "ja437:2.2": "vaṇṇeti patino sakhiṃ;",
  "ja437:2.3": "Pajjhāyi paṭigacchantiṃ,",
  "ja437:2.4": "āgataṃ meṇḍamātaraṃ”.",
  "ja437:3.1": "“Tvaṃ khosi samma ummatto,",
  "ja437:3.2": "dummedho avicakkhaṇo;",
  "ja437:3.3": "Yo tvaṃ matālayaṃ katvā,",
  "ja437:3.4": "akālena vipekkhasi”.",
  "ja437:4.1": "“Na akāle vipekkheyya,",
  "ja437:4.2": "kāle pekkheyya paṇḍito;",
  "ja437:4.3": "Pūtimaṃsova pajjhāyi,",
  "ja437:4.4": "yo akāle vipekkhati”.",
  "ja437:5.1": "“Piyaṃ kho āḷi me hotu,",
  "ja437:5.2": "puṇṇapattaṃ dadāhi me;",
  "ja437:5.3": "Pati sañjīvito mayhaṃ,",
  "ja437:5.4": "eyyāsi piyapucchikā”.",
  "ja437:6.1": "“Piyaṃ kho āḷi te hotu,",
  "ja437:6.2": "puṇṇapattaṃ dadāmi te;",
  "ja437:6.3": "Mahatā parivārena,",
  "ja437:6.4": "essaṃ kayirāhi bhojanaṃ”.",
  "ja437:7.1": "“Kīdiso tuyhaṃ parivāro,",
  "ja437:7.2": "yesaṃ kāhāmi bhojanaṃ;",
  "ja437:7.3": "Kiṃnāmakā ca te sabbe,",
  "ja437:7.4": "taṃ me akkhāhi pucchitā”.",
  "ja437:8.1": "“Māliyo caturakkho ca,",
  "ja437:8.2": "piṅgiyo atha jambuko;",
  "ja437:8.3": "Ediso mayhaṃ parivāro,",
  "ja437:8.4": "tesaṃ kayirāhi bhojanaṃ”.",
  "ja437:9.1": "“Nikkhantāya agārasmā,",
  "ja437:9.2": "bhaṇḍakampi vinassati;",
  "ja437:9.3": "Ārogyaṃ āḷino vajjaṃ,",
  "ja437:9.4": "idheva vasa māgamā”ti.",
  "ja437:10.1": "Pūtimaṃsajātakaṃ ekādasamaṃ."
}