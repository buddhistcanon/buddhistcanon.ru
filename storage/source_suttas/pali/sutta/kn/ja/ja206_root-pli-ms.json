{
  "ja206:0.1": "Jātaka",
  "ja206:0.2": "Dukanipāta",
  "ja206:0.3": "Nataṃdaḷhavagga",
  "ja206:0.4": "6. Kuruṅgamigajātaka",
  "ja206:1.1": "“Iṅgha vaṭṭamayaṃ pāsaṃ,",
  "ja206:1.2": "chinda dantehi kacchapa;",
  "ja206:1.3": "Ahaṃ tathā karissāmi,",
  "ja206:1.4": "yathā nehiti luddako”.",
  "ja206:2.1": "“Kacchapo pāvisī vāriṃ,",
  "ja206:2.2": "kuruṅgo pāvisī vanaṃ;",
  "ja206:2.3": "Satapatto dumaggamhā,",
  "ja206:2.4": "dūre putte apānayī”ti.",
  "ja206:3.1": "Kuruṅgamigajātakaṃ chaṭṭhaṃ."
}