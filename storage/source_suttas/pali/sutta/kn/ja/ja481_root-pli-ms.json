{
  "ja481:0.1": "Jātaka",
  "ja481:0.2": "Terasakanipāta",
  "ja481:0.3": "Ambavagga",
  "ja481:0.4": "8. Takkāriyajātaka",
  "ja481:1.1": "“Ahameva dubbhāsitaṃ bhāsi bālo,",
  "ja481:1.2": "Bhekovaraññe ahimavhāyamāno;",
  "ja481:1.3": "Takkāriye sobbhamimaṃ patāmi,",
  "ja481:1.4": "Na kireva sādhu ativelabhāṇī”.",
  "ja481:2.1": "“Pappoti macco ativelabhāṇī,",
  "ja481:2.2": "Bandhaṃ vadhaṃ sokapariddavañca;",
  "ja481:2.3": "Attānameva garahāsi ettha,",
  "ja481:2.4": "Ācera yaṃ taṃ nikhaṇanti sobbhe”.",
  "ja481:3.1": "“Kimevahaṃ tuṇḍilamanupucchiṃ,",
  "ja481:3.2": "Kareyya saṃ bhātaraṃ kāḷikāyaṃ;",
  "ja481:3.3": "Naggovahaṃ vatthayugañca jīno,",
  "ja481:3.4": "Ayampi attho bahutādisova”.",
  "ja481:4.1": "“Yo yujjhamānānamayujjhamāno,",
  "ja481:4.2": "Meṇḍantaraṃ accupatī kuliṅgo;",
  "ja481:4.3": "So piṃsito meṇḍasirehi tattha,",
  "ja481:4.4": "Ayampi attho bahutādisova.",
  "ja481:5.1": "Caturo janā potthakamaggahesuṃ,",
  "ja481:5.2": "Ekañca posaṃ anurakkhamānā;",
  "ja481:5.3": "Sabbeva te bhinnasirā sayiṃsu,",
  "ja481:5.4": "Ayampi attho bahutādisova.",
  "ja481:6.1": "Ajā yathā veḷugumbasmiṃ baddhā,",
  "ja481:6.2": "Avakkhipantī asimajjhagacchi;",
  "ja481:6.3": "Teneva tassā galakāvakantaṃ,",
  "ja481:6.4": "Ayampi attho bahutādisova”.",
  "ja481:7.1": "“Ime na devā na gandhabbaputtā,",
  "ja481:7.2": "Migā ime atthavasaṃ gatā me;",
  "ja481:7.3": "Ekañca naṃ sāyamāse pacantu,",
  "ja481:7.4": "Ekaṃ punappātarāse pacantu”.",
  "ja481:8.1": "“Sataṃ sahassāni dubhāsitāni,",
  "ja481:8.2": "Kalampi nāgghanti subhāsitassa;",
  "ja481:8.3": "Dubbhāsitaṃ saṅkamāno kileso,",
  "ja481:8.4": "Tasmā tuṇhī kimpurisā na bālyā”.",
  "ja481:9.1": "“Yā mesā byāhāsi pamuñcathetaṃ,",
  "ja481:9.2": "Giriñca naṃ himavantaṃ nayantu;",
  "ja481:9.3": "Imañca kho dentu mahānasāya,",
  "ja481:9.4": "Pātova naṃ pātarāse pacantu”.",
  "ja481:10.1": "“Pajjunnanāthā pasavo,",
  "ja481:10.2": "pasunāthā ayaṃ pajā;",
  "ja481:10.3": "Tvaṃ nāthosi mahārāja,",
  "ja481:10.4": "nāthohaṃ bhariyāya me;",
  "ja481:10.5": "Dvinnamaññataraṃ ñatvā,",
  "ja481:10.6": "mutto gaccheyya pabbataṃ.",
  "ja481:11.1": "Na ve nindā suparivajjayetha,",
  "ja481:11.2": "Nānā janā sevitabbā janinda;",
  "ja481:11.3": "Yeneva eko labhate pasaṃsaṃ,",
  "ja481:11.4": "Teneva añño labhate ninditāraṃ.",
  "ja481:12.1": "Sabbo loko paricitto aticitto,",
  "ja481:12.2": "Sabbo loko cittavā samhi citte;",
  "ja481:12.3": "Paccekacittā puthu sabbasattā,",
  "ja481:12.4": "Kassīdha cittassa vasena vatte”.",
  "ja481:13.1": "“Tuṇhī ahū kimpuriso sabhariyo,",
  "ja481:13.2": "Yo dāni byāhāsi bhayassa bhīto;",
  "ja481:13.3": "So dāni mutto sukhito arogo,",
  "ja481:13.4": "Vācākirevatthavatī narānan”ti.",
  "ja481:14.1": "Takkāriyajātakaṃ aṭṭhamaṃ."
}