{
  "ja200:0.1": "Jātaka",
  "ja200:0.2": "Dukanipāta",
  "ja200:0.3": "Ruhakavagga",
  "ja200:0.4": "10. Sādhusīlajātaka",
  "ja200:1.1": "“Sarīradabyaṃ vuḍḍhabyaṃ,",
  "ja200:1.2": "sojaccaṃ sādhusīliyaṃ;",
  "ja200:1.3": "Brāhmaṇaṃ teva pucchāma,",
  "ja200:1.4": "kaṃ nu tesaṃ vanimhase”.",
  "ja200:2.1": "“Attho atthi sarīrasmiṃ,",
  "ja200:2.2": "vuḍḍhabyassa namo kare;",
  "ja200:2.3": "Attho atthi sujātasmiṃ,",
  "ja200:2.4": "sīlaṃ asmāka ruccatī”ti.",
  "ja200:3.1": "Sādhusīlajātakaṃ dasamaṃ.",
  "ja200:3.2": "Ruhakavaggo pañcamo.",
  "ja200:4.1": "Tassuddānaṃ",
  "ja200:5.1": "Apiruhaka rūpavatī musalo,",
  "ja200:5.2": "Pavasanti sapañcamapokkharaṇī;",
  "ja200:5.3": "Atha muttimavāṇija umhayate,",
  "ja200:5.4": "Ciraāgata koṭṭha sarīra dasāti."
}