{
  "ja335:0.1": "Jātaka",
  "ja335:0.2": "Catukkanipāta",
  "ja335:0.3": "Kokilavagga",
  "ja335:0.4": "5. Jambukajātaka",
  "ja335:1.1": "“Brahā pavaḍḍhakāyo so,",
  "ja335:1.2": "dīghadāṭho ca jambuka;",
  "ja335:1.3": "Na tvaṃ tattha kule jāto,",
  "ja335:1.4": "yattha gaṇhanti kuñjaraṃ”.",
  "ja335:2.1": "“Asīho sīhamānena,",
  "ja335:2.2": "yo attānaṃ vikubbati;",
  "ja335:2.3": "Kotthūva gajamāsajja,",
  "ja335:2.4": "seti bhūmyā anutthunaṃ.",
  "ja335:3.1": "Yasassino uttamapuggalassa,",
  "ja335:3.2": "Sañjātakhandhassa mahabbalassa;",
  "ja335:3.3": "Asamekkhiya thāmabalūpapattiṃ,",
  "ja335:3.4": "Sa seti nāgena hatoyaṃ jambuko.",
  "ja335:4.1": "Yo cīdha kammaṃ kurute pamāya,",
  "ja335:4.2": "Thāmabbalaṃ attani saṃviditvā;",
  "ja335:4.3": "Jappena mantena subhāsitena,",
  "ja335:4.4": "Parikkhavā so vipulaṃ jinātī”ti.",
  "ja335:5.1": "Jambukajātakaṃ pañcamaṃ."
}