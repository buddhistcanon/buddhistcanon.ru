{
  "ja144:0.1": "Jātaka",
  "ja144:0.2": "Ekakanipāta",
  "ja144:0.3": "Kakaṇṭakavagga",
  "ja144:0.4": "4. Naṅguṭṭhajātaka",
  "ja144:1.1": "“Bahumpetaṃ asabbhi jātaveda,",
  "ja144:1.2": "Yaṃ taṃ vāladhinābhipūjayāma;",
  "ja144:1.3": "Maṃsārahassa natthajja maṃsaṃ,",
  "ja144:1.4": "Naṅguṭṭhampi bhavaṃ paṭiggahātū”ti.",
  "ja144:2.1": "Naṅguṭṭhajātakaṃ catutthaṃ."
}