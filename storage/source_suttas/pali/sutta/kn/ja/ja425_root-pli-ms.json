{
  "ja425:0.1": "Jātaka",
  "ja425:0.2": "Aṭṭhakanipāta",
  "ja425:0.3": "Kaccānivagga",
  "ja425:0.4": "9. Aṭṭhānajātaka",
  "ja425:1.1": "“Gaṅgā kumudinī santā,",
  "ja425:1.2": "saṅkhavaṇṇā ca kokilā;",
  "ja425:1.3": "Jambu tālaphalaṃ dajjā,",
  "ja425:1.4": "atha nūna tadā siyā.",
  "ja425:2.1": "Yadā kacchapalomānaṃ,",
  "ja425:2.2": "pāvāro tividho siyā;",
  "ja425:2.3": "Hemantikaṃ pāvuraṇaṃ,",
  "ja425:2.4": "atha nūna tadā siyā.",
  "ja425:3.1": "Yadā makasapādānaṃ,",
  "ja425:3.2": "aṭṭālo sukato siyā;",
  "ja425:3.3": "Daḷho ca avikampī ca,",
  "ja425:3.4": "atha nūna tadā siyā.",
  "ja425:4.1": "Yadā sasavisāṇānaṃ,",
  "ja425:4.2": "nisseṇī sukatā siyā;",
  "ja425:4.3": "Saggassārohaṇatthāya,",
  "ja425:4.4": "atha nūna tadā siyā.",
  "ja425:5.1": "Yadā nisseṇimāruyha,",
  "ja425:5.2": "candaṃ khādeyyu mūsikā;",
  "ja425:5.3": "Rāhuñca paripāteyyuṃ,",
  "ja425:5.4": "atha nūna tadā siyā.",
  "ja425:6.1": "Yadā surāghaṭaṃ pitvā,",
  "ja425:6.2": "makkhikā gaṇacāriṇī;",
  "ja425:6.3": "Aṅgāre vāsaṃ kappeyyuṃ,",
  "ja425:6.4": "atha nūna tadā siyā.",
  "ja425:7.1": "Yadā bimboṭṭhasampanno,",
  "ja425:7.2": "gadrabho sumukho siyā;",
  "ja425:7.3": "Kusalo naccagītassa,",
  "ja425:7.4": "atha nūna tadā siyā.",
  "ja425:8.1": "Yadā kākā ulūkā ca,",
  "ja425:8.2": "mantayeyyuṃ rahogatā;",
  "ja425:8.3": "Aññamaññaṃ pihayeyyuṃ,",
  "ja425:8.4": "atha nūna tadā siyā.",
  "ja425:9.1": "Yadā muḷāla pattānaṃ,",
  "ja425:9.2": "chattaṃ thirataraṃ siyā;",
  "ja425:9.3": "Vassassa paṭighātāya,",
  "ja425:9.4": "atha nūna tadā siyā.",
  "ja425:10.1": "Yadā kulako sakuṇo,",
  "ja425:10.2": "pabbataṃ gandhamādanaṃ;",
  "ja425:10.3": "Tuṇḍenādāya gaccheyya,",
  "ja425:10.4": "atha nūna tadā siyā.",
  "ja425:11.1": "Yadā sāmuddikaṃ nāvaṃ,",
  "ja425:11.2": "sayantaṃ savaṭākaraṃ;",
  "ja425:11.3": "Ceṭo ādāya gaccheyya,",
  "ja425:11.4": "atha nūna tadā siyā”ti.",
  "ja425:12.1": "Aṭṭhānajātakaṃ navamaṃ."
}