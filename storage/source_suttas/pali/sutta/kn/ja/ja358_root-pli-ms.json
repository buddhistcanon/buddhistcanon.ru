{
  "ja358:0.1": "Jātaka",
  "ja358:0.2": "Pañcakanipāta",
  "ja358:0.3": "Maṇikuṇḍalavagga",
  "ja358:0.4": "8. Cūḷadhammapālajātaka",
  "ja358:1.1": "“Ahameva dūsiyā bhūnahatā,",
  "ja358:1.2": "Rañño mahāpatāpassa;",
  "ja358:1.3": "Etaṃ muñcatu dhammapālaṃ,",
  "ja358:1.4": "Hatthe me deva chedehi.",
  "ja358:2.1": "Ahameva dūsiyā bhūnahatā,",
  "ja358:2.2": "Rañño mahāpatāpassa;",
  "ja358:2.3": "Etaṃ muñcatu dhammapālaṃ,",
  "ja358:2.4": "Pāde me deva chedehi.",
  "ja358:3.1": "Ahameva dūsiyā bhūnahatā,",
  "ja358:3.2": "Rañño mahāpatāpassa;",
  "ja358:3.3": "Etaṃ muñcatu dhammapālaṃ,",
  "ja358:3.4": "Sīsaṃ me deva chedehi.",
  "ja358:4.1": "Na hi nūnimassa rañño,",
  "ja358:4.2": "Mittāmaccā ca vijjare suhadā;",
  "ja358:4.3": "Ye na vadanti rājānaṃ,",
  "ja358:4.4": "Mā ghātayi orasaṃ puttaṃ.",
  "ja358:5.1": "Na hi nūnimassa rañño,",
  "ja358:5.2": "Ñātī mittā ca vijjare suhadā;",
  "ja358:5.3": "Ye na vadanti rājānaṃ,",
  "ja358:5.4": "Mā ghātayi atrajaṃ puttaṃ.",
  "ja358:6.1": "Candanasārānulittā,",
  "ja358:6.2": "Bāhā chijjanti dhammapālassa;",
  "ja358:6.3": "Dāyādassa pathabyā,",
  "ja358:6.4": "Pāṇā me deva rujjhantī”ti.",
  "ja358:7.1": "Cūḷadhammapālajātakaṃ aṭṭhamaṃ."
}