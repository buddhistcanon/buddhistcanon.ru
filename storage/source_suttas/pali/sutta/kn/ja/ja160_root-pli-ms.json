{
  "ja160:0.1": "Jātaka",
  "ja160:0.2": "Dukanipāta",
  "ja160:0.3": "Daḷhavagga",
  "ja160:0.4": "10. Vinīlajātaka",
  "ja160:1.1": "“Evameva nūna rājānaṃ,",
  "ja160:1.2": "vedehaṃ mithilaggahaṃ;",
  "ja160:1.3": "Assā vahanti ājaññā,",
  "ja160:1.4": "yathā haṃsā vinīlakaṃ”.",
  "ja160:2.1": "“Vinīla duggaṃ bhajasi,",
  "ja160:2.2": "abhūmiṃ tāta sevasi;",
  "ja160:2.3": "Gāmantakāni sevassu,",
  "ja160:2.4": "etaṃ mātālayaṃ tavāti.",
  "ja160:3.1": "Vinīlajātakaṃ dasamaṃ.",
  "ja160:3.2": "Daḷhavaggo paṭhamo.",
  "ja160:4.1": "Tassuddānaṃ",
  "ja160:5.1": "Varaballika daddara sūkarako,",
  "ja160:5.2": "Uraguttama pañcamabhaggavaro;",
  "ja160:5.3": "Mahatīcamu yāva siṅgālavaro,",
  "ja160:5.4": "Suhanuttama mora vinīlaṃ dasā”ti."
}