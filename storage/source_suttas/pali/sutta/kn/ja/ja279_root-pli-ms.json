{
  "ja279:0.1": "Jātaka",
  "ja279:0.2": "Tikanipāta",
  "ja279:0.3": "Udapānavagga",
  "ja279:0.4": "9. Satapattajātaka",
  "ja279:1.1": "“Yathā māṇavako panthe,",
  "ja279:1.2": "siṅgāliṃ vanagocariṃ;",
  "ja279:1.3": "Atthakāmaṃ pavedentiṃ,",
  "ja279:1.4": "anatthakāmāti maññati;",
  "ja279:1.5": "Anatthakāmaṃ satapattaṃ,",
  "ja279:1.6": "atthakāmoti maññati.",
  "ja279:2.1": "Evameva idhekacco,",
  "ja279:2.2": "puggalo hoti tādiso;",
  "ja279:2.3": "Hitehi vacanaṃ vutto,",
  "ja279:2.4": "paṭigaṇhāti vāmato.",
  "ja279:3.1": "Ye ca kho naṃ pasaṃsanti,",
  "ja279:3.2": "bhayā ukkaṃsayanti vā;",
  "ja279:3.3": "Tañhi so maññate mittaṃ,",
  "ja279:3.4": "satapattaṃva māṇavo”ti.",
  "ja279:4.1": "Satapattajātakaṃ navamaṃ."
}