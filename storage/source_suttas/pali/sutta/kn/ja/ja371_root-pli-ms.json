{
  "ja371:0.1": "Jātaka",
  "ja371:0.2": "Pañcakanipāta",
  "ja371:0.3": "Aḍḍhavagga",
  "ja371:0.4": "1. Dīghītikosalajātaka",
  "ja371:1.1": "“Evaṃbhūtassa te rāja,",
  "ja371:1.2": "āgatassa vase mama;",
  "ja371:1.3": "Atthi nu koci pariyāyo,",
  "ja371:1.4": "yo taṃ dukkhā pamocaye”.",
  "ja371:2.1": "“Evaṃbhūtassa me tāta,",
  "ja371:2.2": "āgatassa vase tava;",
  "ja371:2.3": "Natthi no koci pariyāyo,",
  "ja371:2.4": "yo maṃ dukkhā pamocaye”.",
  "ja371:3.1": "“Nāññaṃ sucaritaṃ rāja,",
  "ja371:3.2": "nāññaṃ rāja subhāsitaṃ;",
  "ja371:3.3": "Tāyate maraṇakāle,",
  "ja371:3.4": "evamevitaraṃ dhanaṃ.",
  "ja371:4.1": "Akkocchi maṃ avadhi maṃ,",
  "ja371:4.2": "ajini maṃ ahāsi me;",
  "ja371:4.3": "Ye ca taṃ upanayhanti,",
  "ja371:4.4": "veraṃ tesaṃ na sammati.",
  "ja371:5.1": "Akkocchi maṃ avadhi maṃ,",
  "ja371:5.2": "ajini maṃ ahāsi me;",
  "ja371:5.3": "Ye ca taṃ nupanayhanti,",
  "ja371:5.4": "veraṃ tesūpasammati.",
  "ja371:6.1": "Na hi verena verāni,",
  "ja371:6.2": "sammantīdha kudācanaṃ;",
  "ja371:6.3": "Averena ca sammanti,",
  "ja371:6.4": "esa dhammo sanantano”ti.",
  "ja371:7.1": "Dīghītikosalajātakaṃ paṭhamaṃ."
}