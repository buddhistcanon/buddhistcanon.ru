{
  "ja353:0.1": "Jātaka",
  "ja353:0.2": "Pañcakanipāta",
  "ja353:0.3": "Maṇikuṇḍalavagga",
  "ja353:0.4": "3. Venasākhajātaka",
  "ja353:1.1": "“Nayidaṃ niccaṃ bhavitabbaṃ brahmadatta,",
  "ja353:1.2": "Khemaṃ subhikkhaṃ sukhatā ca kāye;",
  "ja353:1.3": "Atthaccaye mā ahu sampamūḷho,",
  "ja353:1.4": "Bhinnaplavo sāgarasseva majjhe.",
  "ja353:2.1": "Yāni karoti puriso,",
  "ja353:2.2": "tāni attani passati;",
  "ja353:2.3": "Kalyāṇakārī kalyāṇaṃ,",
  "ja353:2.4": "pāpakārī ca pāpakaṃ;",
  "ja353:2.5": "Yādisaṃ vapate bījaṃ,",
  "ja353:2.6": "tādisaṃ harate phalaṃ”.",
  "ja353:3.1": "“Idaṃ tadācariyavaco,",
  "ja353:3.2": "pārāsariyo yadabravi;",
  "ja353:3.3": "Mā su tvaṃ akari pāpaṃ,",
  "ja353:3.4": "yaṃ tvaṃ pacchā kataṃ tape.",
  "ja353:4.1": "Ayameva so piṅgiya venasākho,",
  "ja353:4.2": "Yamhi ghātayiṃ khattiyānaṃ sahassaṃ;",
  "ja353:4.3": "Alaṅkate candanasārānulitte,",
  "ja353:4.4": "Tameva dukkhaṃ paccāgataṃ mamaṃ.",
  "ja353:5.1": "Sāmā ca kho candanalittagattā,",
  "ja353:5.2": "Laṭṭhīva sobhañjanakassa uggatā;",
  "ja353:5.3": "Adisvā kālaṃ karissāmi ubbariṃ,",
  "ja353:5.4": "Taṃ me ito dukkhataraṃ bhavissatī”ti.",
  "ja353:6.1": "Venasākhajātakaṃ tatiyaṃ."
}