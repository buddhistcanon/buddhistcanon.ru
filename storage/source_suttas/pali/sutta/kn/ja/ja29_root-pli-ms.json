{
  "ja29:0.1": "Jātaka",
  "ja29:0.2": "Ekakanipāta",
  "ja29:0.3": "Kuruṅgavagga",
  "ja29:0.4": "9. Kaṇhajātaka",
  "ja29:1.1": "“Yato yato garu dhuraṃ,",
  "ja29:1.2": "yato gambhīravattanī;",
  "ja29:1.3": "Tadāssu kaṇhaṃ yuñjanti,",
  "ja29:1.4": "svāssu taṃ vahate dhuran”ti.",
  "ja29:2.1": "Kaṇhajātakaṃ navamaṃ."
}