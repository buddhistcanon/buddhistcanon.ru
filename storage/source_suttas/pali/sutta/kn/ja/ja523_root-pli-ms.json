{
  "ja523:0.1": "Jātaka",
  "ja523:0.2": "Cattālīsanipāta",
  "ja523:0.3": "Tesakuṇavagga",
  "ja523:0.4": "3. Alambusājātaka",
  "ja523:1.1": "Atha bravi brahā indo,",
  "ja523:1.2": "vatrabhū jayataṃ pitā;",
  "ja523:1.3": "Devakaññaṃ parābhetvā,",
  "ja523:1.4": "sudhammāyaṃ alambusaṃ.",
  "ja523:2.1": "“Misse devā taṃ yācanti,",
  "ja523:2.2": "tāvatiṃsā saindakā;",
  "ja523:2.3": "Isippalobhane gaccha,",
  "ja523:2.4": "isisiṅgaṃ alambuse.",
  "ja523:3.1": "Purāyaṃ amhe acceti,",
  "ja523:3.2": "vattavā brahmacariyavā;",
  "ja523:3.3": "Nibbānābhirato vuddho,",
  "ja523:3.4": "tassa maggāni āvara”.",
  "ja523:4.1": "“Devarāja kimeva tvaṃ,",
  "ja523:4.2": "mameva tuvaṃ sikkhasi;",
  "ja523:4.3": "Isippalobhane gaccha,",
  "ja523:4.4": "santi aññāpi accharā.",
  "ja523:5.1": "Mādisiyo pavarā ceva,",
  "ja523:5.2": "asoke nandane vane;",
  "ja523:5.3": "Tāsampi hotu pariyāyo,",
  "ja523:5.4": "tāpi yantu palobhanā”.",
  "ja523:6.1": "“Addhā hi saccaṃ bhaṇasi,",
  "ja523:6.2": "santi aññāpi accharā;",
  "ja523:6.3": "Tādisiyo pavarā ceva,",
  "ja523:6.4": "asoke nandane vane.",
  "ja523:7.1": "Na tā evaṃ pajānanti,",
  "ja523:7.2": "pāricariyaṃ pumaṃ gatā;",
  "ja523:7.3": "Yādisaṃ tvaṃ pajānāsi,",
  "ja523:7.4": "nāri sabbaṅgasobhane.",
  "ja523:8.1": "Tvameva gaccha kalyāṇi,",
  "ja523:8.2": "itthīnaṃ pavarā casi;",
  "ja523:8.3": "Taveva vaṇṇarūpena,",
  "ja523:8.4": "savasamānayissasi”.",
  "ja523:9.1": "“Na vāhaṃ na gamissāmi,",
  "ja523:9.2": "devarājena pesitā;",
  "ja523:9.3": "Vibhemi cetaṃ āsāduṃ,",
  "ja523:9.4": "uggatejo hi brāhmaṇo.",
  "ja523:10.1": "Aneke nirayaṃ pattā,",
  "ja523:10.2": "isimāsādiyā janā;",
  "ja523:10.3": "Āpannā mohasaṃsāraṃ,",
  "ja523:10.4": "tasmā lomāni haṃsaye”.",
  "ja523:11.1": "Idaṃ vatvāna pakkāmi,",
  "ja523:11.2": "accharā kāmavaṇṇinī;",
  "ja523:11.3": "Missā missitumicchantī,",
  "ja523:11.4": "isisiṅgaṃ alambusā.",
  "ja523:12.1": "Sā ca taṃ vanamogayha,",
  "ja523:12.2": "isisiṅgena rakkhitaṃ;",
  "ja523:12.3": "Bimbajālakasañchannaṃ,",
  "ja523:12.4": "samantā addhayojanaṃ.",
  "ja523:13.1": "Pātova pātarāsamhi,",
  "ja523:13.2": "udaṇhasamayaṃ pati;",
  "ja523:13.3": "Aggiṭṭhaṃ parimajjantaṃ,",
  "ja523:13.4": "isisiṅgaṃ upāgami.",
  "ja523:14.1": "“Kā nu vijjurivābhāsi,",
  "ja523:14.2": "osadhī viya tārakā;",
  "ja523:14.3": "Vicittahatthābharaṇā,",
  "ja523:14.4": "āmuttamaṇikuṇḍalā.",
  "ja523:15.1": "Ādiccavaṇṇasaṅkāsā,",
  "ja523:15.2": "hemacandanagandhinī;",
  "ja523:15.3": "Saññatūrū mahāmāyā,",
  "ja523:15.4": "kumārī cārudassanā.",
  "ja523:16.1": "Vilaggā mudukā suddhā,",
  "ja523:16.2": "pādā te suppatiṭṭhitā;",
  "ja523:16.3": "Gamanā kāmanīyā te,",
  "ja523:16.4": "harantiyeva me mano.",
  "ja523:17.1": "Anupubbāva te ūrū,",
  "ja523:17.2": "nāganāsasamūpamā;",
  "ja523:17.3": "Vimaṭṭhā tuyhaṃ sussoṇī,",
  "ja523:17.4": "akkhassa phalakaṃ yathā.",
  "ja523:18.1": "Uppalasseva kiñjakkhā,",
  "ja523:18.2": "nābhi te sādhu saṇṭhitā;",
  "ja523:18.3": "Pūrā kaṇhañjanasseva,",
  "ja523:18.4": "dūrato paṭidissati.",
  "ja523:19.1": "Duvidhā jātā urajā,",
  "ja523:19.2": "avaṇṭā sādhupaccudā;",
  "ja523:19.3": "Payodharā apatitā,",
  "ja523:19.4": "aḍḍhalābusamā thanā.",
  "ja523:20.1": "Dīghā kambutalābhāsā,",
  "ja523:20.2": "gīvā eṇeyyakā yathā;",
  "ja523:20.3": "Paṇḍarāvaraṇā vaggu,",
  "ja523:20.4": "catutthamanasannibhā.",
  "ja523:21.1": "Uddhaggā ca adhaggā ca,",
  "ja523:21.2": "dumaggaparimajjitā;",
  "ja523:21.3": "Duvijā nelasambhūtā,",
  "ja523:21.4": "dantā tava sudassanā.",
  "ja523:22.1": "Apaṇḍarā lohitantā,",
  "ja523:22.2": "jiñjūkaphalasannibhā;",
  "ja523:22.3": "Āyatā ca visālā ca,",
  "ja523:22.4": "nettā tava sudassanā.",
  "ja523:23.1": "Nātidīghā susammaṭṭhā,",
  "ja523:23.2": "kanakabyā samocitā;",
  "ja523:23.3": "Uttamaṅgaruhā tuyhaṃ,",
  "ja523:23.4": "kesā candanagandhikā.",
  "ja523:24.1": "Yāvatā kasigorakkhā,",
  "ja523:24.2": "vāṇijānaṃ ca yā gati;",
  "ja523:24.3": "Isīnañca parakkantaṃ,",
  "ja523:24.4": "saññatānaṃ tapassinaṃ.",
  "ja523:25.1": "Na te samasamaṃ passe,",
  "ja523:25.2": "asmiṃ pathavimaṇḍale;",
  "ja523:25.3": "Ko vā tvaṃ kassa vā putto,",
  "ja523:25.4": "kathaṃ jānemu taṃ mayaṃ”.",
  "ja523:26.1": "“Na pañhakālo bhaddante,",
  "ja523:26.2": "kassapevaṃ gate sati;",
  "ja523:26.3": "Ehi samma ramissāma,",
  "ja523:26.4": "ubho asmākamassame;",
  "ja523:26.5": "Ehi taṃ upagūhissaṃ,",
  "ja523:26.6": "ratīnaṃ kusalo bhava”.",
  "ja523:27.1": "Idaṃ vatvāna pakkāmi,",
  "ja523:27.2": "accharā kāmavaṇṇinī;",
  "ja523:27.3": "Missā missitumicchantī,",
  "ja523:27.4": "isisiṅgaṃ alambusā.",
  "ja523:28.1": "So ca vegena nikkhamma,",
  "ja523:28.2": "Chetvā dandhaparakkamaṃ;",
  "ja523:28.3": "Tamuttamāsu veṇīsu,",
  "ja523:28.4": "Ajjhappatto parāmasi.",
  "ja523:29.1": "Tamudāvatta kalyāṇī,",
  "ja523:29.2": "palissaji susobhanā;",
  "ja523:29.3": "Cavitamhi brahmacariyā,",
  "ja523:29.4": "yathā taṃ atha tositā.",
  "ja523:30.1": "Manasā agamā indaṃ,",
  "ja523:30.2": "vasantaṃ nandane vane;",
  "ja523:30.3": "Tassā saṅkappamaññāya,",
  "ja523:30.4": "maghavā devakuñjaro.",
  "ja523:31.1": "Pallaṅkaṃ pahiṇī khippaṃ,",
  "ja523:31.2": "sovaṇṇaṃ sopavāhanaṃ;",
  "ja523:31.3": "Sauttaracchadapaññāsaṃ,",
  "ja523:31.4": "sahassapaṭiyatthataṃ.",
  "ja523:32.1": "Tamenaṃ tattha dhāresi,",
  "ja523:32.2": "ure katvāna sobhanā;",
  "ja523:32.3": "Yathā ekamuhuttaṃva,",
  "ja523:32.4": "tīṇi vassāni dhārayi.",
  "ja523:33.1": "Vimado tīhi vassehi,",
  "ja523:33.2": "pabujjhitvāna brāhmaṇo;",
  "ja523:33.3": "Addasāsi haritarukkhe,",
  "ja523:33.4": "samantā aggiyāyanaṃ.",
  "ja523:34.1": "Navapattavanaṃ phullaṃ,",
  "ja523:34.2": "kokilaggaṇaghositaṃ;",
  "ja523:34.3": "Samantā paviloketvā,",
  "ja523:34.4": "rudaṃ assūni vattayi.",
  "ja523:35.1": "“Na juhe na jape mante,",
  "ja523:35.2": "aggihuttaṃ pahāpitaṃ;",
  "ja523:35.3": "Ko nu me pāricariyāya,",
  "ja523:35.4": "pubbe cittaṃ palobhayi.",
  "ja523:36.1": "Araññe me viharato,",
  "ja523:36.2": "yo me tejā ha sambhutaṃ;",
  "ja523:36.3": "Nānāratnaparipūraṃ,",
  "ja523:36.4": "nāvaṃva gaṇhi aṇṇave”.",
  "ja523:37.1": "“Ahaṃ te pāricariyāya,",
  "ja523:37.2": "devarājena pesitā;",
  "ja523:37.3": "Avadhiṃ cittaṃ cittena,",
  "ja523:37.4": "pamādo tvaṃ na bujjhasi”.",
  "ja523:38.1": "“Imāni kira maṃ tāto,",
  "ja523:38.2": "kassapo anusāsati;",
  "ja523:38.3": "Kamalāsadisitthiyo,",
  "ja523:38.4": "tāyo bujjhesi māṇava.",
  "ja523:39.1": "Uregaṇḍāyo bujjhesi,",
  "ja523:39.2": "tāyo bujjhesi māṇava;",
  "ja523:39.3": "Iccānusāsi maṃ tāto,",
  "ja523:39.4": "yathā maṃ anukampako.",
  "ja523:40.1": "Tassāhaṃ vacanaṃ nākaṃ,",
  "ja523:40.2": "pitu vuddhassa sāsanaṃ;",
  "ja523:40.3": "Araññe nimmanussamhi,",
  "ja523:40.4": "svajja jhāyāmi ekako.",
  "ja523:41.1": "Sohaṃ tathā karissāmi,",
  "ja523:41.2": "dhiratthu jīvitena me;",
  "ja523:41.3": "Puna vā tādiso hessaṃ,",
  "ja523:41.4": "maraṇaṃ me bhavissati”.",
  "ja523:42.1": "Tassa tejaṃ vīriyañca,",
  "ja523:42.2": "Dhitiṃ ñatvā avaṭṭhitaṃ;",
  "ja523:42.3": "Sirasā aggahī pāde,",
  "ja523:42.4": "Isisiṅgaṃ alambusā.",
  "ja523:43.1": "“Mā me kujjha mahāvīra,",
  "ja523:43.2": "mā me kujjha mahāise;",
  "ja523:43.3": "Mahā attho mayā ciṇṇo,",
  "ja523:43.4": "tidasānaṃ yasassinaṃ;",
  "ja523:43.5": "Tayā saṅkampitaṃ āsi,",
  "ja523:43.6": "sabbaṃ devapuraṃ tadā”.",
  "ja523:44.1": "“Tāvatiṃsā ca ye devā,",
  "ja523:44.2": "tidasānañca vāsavo;",
  "ja523:44.3": "Tvañca bhadde sukhī hohi,",
  "ja523:44.4": "gaccha kaññe yathāsukhaṃ”.",
  "ja523:45.1": "Tassa pāde gahetvāna,",
  "ja523:45.2": "katvā ca naṃ padakkhiṇaṃ;",
  "ja523:45.3": "Añjaliṃ paggahetvāna,",
  "ja523:45.4": "tamhā ṭhānā apakkami.",
  "ja523:46.1": "Yo ca tassāsi pallaṅko,",
  "ja523:46.2": "sovaṇṇo sopavāhano;",
  "ja523:46.3": "Sauttaracchadapaññāso,",
  "ja523:46.4": "sahassapaṭiyatthato;",
  "ja523:46.5": "Tameva pallaṅkamāruyha,",
  "ja523:46.6": "agā devāna santike.",
  "ja523:47.1": "Tamokkamiva āyantiṃ,",
  "ja523:47.2": "jalantiṃ vijjutaṃ yathā;",
  "ja523:47.3": "Patīto sumano vitto,",
  "ja523:47.4": "devindo adadā varaṃ.",
  "ja523:48.1": "“Varañce me ado sakka,",
  "ja523:48.2": "sabbabhūtānamissara;",
  "ja523:48.3": "Nisippalobhikā gacche,",
  "ja523:48.4": "etaṃ sakka varaṃ vare”ti.",
  "ja523:49.1": "Alambusājātakaṃ tatiyaṃ."
}