{
  "ja180:0.1": "Jātaka",
  "ja180:0.2": "Dukanipāta",
  "ja180:0.3": "Kalyāṇavagga",
  "ja180:0.4": "10. Duddadajātaka",
  "ja180:1.1": "“Duddadaṃ dadamānānaṃ,",
  "ja180:1.2": "dukkaraṃ kamma kubbataṃ;",
  "ja180:1.3": "Asanto nānukubbanti,",
  "ja180:1.4": "sataṃ dhammo durannayo”.",
  "ja180:2.1": "“Tasmā satañca asataṃ,",
  "ja180:2.2": "nānā hoti ito gati;",
  "ja180:2.3": "Asanto nirayaṃ yanti,",
  "ja180:2.4": "santo saggaparāyaṇāti.",
  "ja180:3.1": "Duddadajātakaṃ dasamaṃ.",
  "ja180:3.2": "Kalyāṇavaggo tatiyo.",
  "ja180:4.1": "Tassuddānaṃ",
  "ja180:5.1": "Susamaññamigādhibhū māṇavako,",
  "ja180:5.2": "Vāripahūtarūpādiccupaṭṭhānā;",
  "ja180:5.3": "Sakaḷāyasatindukapaṅka puna,",
  "ja180:5.4": "Satadhamma sududdadakena dasā”ti."
}