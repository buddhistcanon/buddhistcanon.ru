{
  "ja147:0.1": "Jātaka",
  "ja147:0.2": "Ekakanipāta",
  "ja147:0.3": "Kakaṇṭakavagga",
  "ja147:0.4": "7. Puppharattajātaka",
  "ja147:1.1": "“Nayidaṃ dukkhaṃ aduṃ dukkhaṃ,",
  "ja147:1.2": "Yaṃ maṃ tudati vāyaso;",
  "ja147:1.3": "Yaṃ sāmā puppharattena,",
  "ja147:1.4": "Kattikaṃ nānubhossatī”ti.",
  "ja147:2.1": "Puppharattajātakaṃ sattamaṃ."
}