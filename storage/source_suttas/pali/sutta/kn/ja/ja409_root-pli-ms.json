{
  "ja409:0.1": "Jātaka",
  "ja409:0.2": "Sattakanipāta",
  "ja409:0.3": "Gandhāravagga",
  "ja409:0.4": "4. Daḷhadhammajātaka",
  "ja409:1.1": "“Ahañce daḷhadhammassa,",
  "ja409:1.2": "vahantī nābhirādhayiṃ;",
  "ja409:1.3": "Dharantī urasi sallaṃ,",
  "ja409:1.4": "yuddhe vikkantacārinī.",
  "ja409:2.1": "Nūna rājā na jānāti,",
  "ja409:2.2": "mama vikkamaporisaṃ;",
  "ja409:2.3": "Saṅgāme sukatantāni,",
  "ja409:2.4": "dūtavippahitāni ca.",
  "ja409:3.1": "Sā nūnāhaṃ marissāmi,",
  "ja409:3.2": "abandhu aparāyinī;",
  "ja409:3.3": "Tadā hi kumbhakārassa,",
  "ja409:3.4": "dinnā chakaṇahārikā”.",
  "ja409:4.1": "“Yāvatāsīsatī poso,",
  "ja409:4.2": "tāvadeva pavīṇati;",
  "ja409:4.3": "Atthāpāye jahanti naṃ,",
  "ja409:4.4": "oṭṭhibyādhiṃva khattiyo.",
  "ja409:5.1": "Yo pubbe katakalyāṇo,",
  "ja409:5.2": "katattho nāvabujjhati;",
  "ja409:5.3": "Atthā tassa palujjanti,",
  "ja409:5.4": "ye honti abhipatthitā.",
  "ja409:6.1": "Yo pubbe katakalyāṇo,",
  "ja409:6.2": "katattho manubujjhati;",
  "ja409:6.3": "Atthā tassa pavaḍḍhanti,",
  "ja409:6.4": "ye honti abhipatthitā.",
  "ja409:7.1": "Taṃ vo vadāmi bhaddante,",
  "ja409:7.2": "yāvantettha samāgatā;",
  "ja409:7.3": "Sabbe kataññuno hotha,",
  "ja409:7.4": "ciraṃ saggamhi ṭhassathā”ti.",
  "ja409:8.1": "Daḷhadhammajātakaṃ catutthaṃ."
}