{
  "ja395:0.1": "Jātaka",
  "ja395:0.2": "Chakkanipāta",
  "ja395:0.3": "Kharaputtavagga",
  "ja395:0.4": "10. Pārāvatajātaka",
  "ja395:1.1": "“Cirassaṃ vata passāmi,",
  "ja395:1.2": "sahāyaṃ maṇidhārinaṃ;",
  "ja395:1.3": "Sukatā massukuttiyā,",
  "ja395:1.4": "sobhate vata me sakhā”.",
  "ja395:2.1": "“Parūḷhakacchanakhalomo,",
  "ja395:2.2": "ahaṃ kammesu byāvaṭo;",
  "ja395:2.3": "Cirassaṃ nhāpitaṃ laddhā,",
  "ja395:2.4": "lomaṃ taṃ ajja hārayiṃ”.",
  "ja395:3.1": "“Yannu lomaṃ ahāresi,",
  "ja395:3.2": "dullabhaṃ laddha kappakaṃ;",
  "ja395:3.3": "Atha kiñcarahi te samma,",
  "ja395:3.4": "kaṇṭhe kiṇikiṇāyati”.",
  "ja395:4.1": "“Manussasukhumālānaṃ,",
  "ja395:4.2": "maṇi kaṇṭhesu lambati;",
  "ja395:4.3": "Tesāhaṃ anusikkhāmi,",
  "ja395:4.4": "mā tvaṃ maññi davā kataṃ.",
  "ja395:5.1": "Sacepimaṃ pihayasi,",
  "ja395:5.2": "massukuttiṃ sukāritaṃ;",
  "ja395:5.3": "Kārayissāmi te samma,",
  "ja395:5.4": "maṇiñcāpi dadāmi te”.",
  "ja395:6.1": "“Tvaññeva maṇinā channo,",
  "ja395:6.2": "Sukatāya ca massuyā;",
  "ja395:6.3": "Āmanta kho taṃ gacchāmi,",
  "ja395:6.4": "Piyaṃ me tavadassanan”ti. (980)",
  "ja395:7.1": "Pārāvatajātakaṃ dasamaṃ.",
  "ja395:7.2": "Kharaputtavaggo dutiyo.",
  "ja395:8.1": "Tassuddānaṃ",
  "ja395:9.1": "Atha passa sasūci ca tuṇḍilako,",
  "ja395:9.2": "Miga mayhakapañcamapakkhivaro;",
  "ja395:9.3": "Atha pañjali vārija mejjha puna,",
  "ja395:9.4": "Atha vaṭṭa kapotavarena dasāti.",
  "ja395:10.1": "Atha vagguddānaṃ",
  "ja395:11.1": "Atha vaggaṃ pakittissaṃ,",
  "ja395:11.2": "chanipātaṃ varuttame;",
  "ja395:11.3": "Avāriyā ca kharo ca,",
  "ja395:11.4": "dve ca vuttā subyañjanāti.",
  "ja395:12.1": "Chakkanipātaṃ niṭṭhitaṃ."
}