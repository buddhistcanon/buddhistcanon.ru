{
  "ja512:0.1": "Jātaka",
  "ja512:0.2": "Tiṃsanipāta",
  "ja512:0.3": "Kiṃchandavagga",
  "ja512:0.4": "2. Kumbhajātaka",
  "ja512:1.1": "“Ko pāturāsī tidivā nabhamhi,",
  "ja512:1.2": "Obhāsayaṃ saṃvariṃ candimāva;",
  "ja512:1.3": "Gattehi te rasmiyo niccharanti,",
  "ja512:1.4": "Sateratā vijjurivantalikkhe.",
  "ja512:2.1": "So chinnavātaṃ kamasī aghamhi,",
  "ja512:2.2": "Vehāyasaṃ gacchasi tiṭṭhasī ca;",
  "ja512:2.3": "Iddhī nu te vatthukatā subhāvitā,",
  "ja512:2.4": "Anaddhagūnaṃ api devatānaṃ.",
  "ja512:3.1": "Vehāyasaṃ gammamāgamma tiṭṭhasi,",
  "ja512:3.2": "Kumbhaṃ kiṇāthāti yametamatthaṃ;",
  "ja512:3.3": "Ko vā tuvaṃ kissa vā tāya kumbho,",
  "ja512:3.4": "Akkhāhi me brāhmaṇa etamatthaṃ”.",
  "ja512:4.1": "“Na sappikumbho napi telakumbho,",
  "ja512:4.2": "Na phāṇitassa na madhussa kumbho;",
  "ja512:4.3": "Kumbhassa vajjāni anappakāni,",
  "ja512:4.4": "Dose bahū kumbhagate suṇātha.",
  "ja512:5.1": "Gaḷeyya yaṃ pitvā pate papātaṃ,",
  "ja512:5.2": "Sobbhaṃ guhaṃ candaniyoḷigallaṃ;",
  "ja512:5.3": "Bahumpi bhuñjeyya abhojaneyyaṃ,",
  "ja512:5.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:6.1": "Yaṃ pitva cittasmimanesamāno,",
  "ja512:6.2": "Āhiṇḍatī goriva bhakkhasādī;",
  "ja512:6.3": "Anāthamāno upagāyati naccati ca,",
  "ja512:6.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:7.1": "Yaṃ ve pivitvā acelova naggo,",
  "ja512:7.2": "Careyya gāme visikhantarāni;",
  "ja512:7.3": "Sammūḷhacitto ativelasāyī,",
  "ja512:7.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:8.1": "Yaṃ pitva uṭṭhāya pavedhamāno,",
  "ja512:8.2": "Sīsañca bāhuñca pacālayanto;",
  "ja512:8.3": "So naccatī dārukaṭallakova,",
  "ja512:8.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:9.1": "Yaṃ ve pivitvā aggidaḍḍhā sayanti,",
  "ja512:9.2": "Atho sigālehipi khāditāse;",
  "ja512:9.3": "Bandhaṃ vadhaṃ bhogajāniñcupenti,",
  "ja512:9.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:10.1": "Yaṃ pitva bhāseyya abhāsaneyyaṃ,",
  "ja512:10.2": "Sabhāyamāsīno apetavattho;",
  "ja512:10.3": "Sammakkhito vantagato byasanno,",
  "ja512:10.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:11.1": "Yaṃ ve pivitvā ukkaṭṭho āvilakkho,",
  "ja512:11.2": "Mameva sabbā pathavīti maññe;",
  "ja512:11.3": "Na me samo cāturantopi rājā,",
  "ja512:11.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:12.1": "Mānātimānā kalahāni pesuṇī,",
  "ja512:12.2": "Dubbaṇṇinī naggayinī palāyinī;",
  "ja512:12.3": "Corāna dhuttāna gatī niketo,",
  "ja512:12.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:13.1": "Iddhāni phītāni kulāni assu,",
  "ja512:13.2": "Anekasāhassadhanāni loke;",
  "ja512:13.3": "Ucchinnadāyajjakatānimāya,",
  "ja512:13.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:14.1": "Dhaññaṃ dhanaṃ rajataṃ jātarūpaṃ,",
  "ja512:14.2": "Khettaṃ gavaṃ yattha vināsayanti;",
  "ja512:14.3": "Ucchedanī vittavataṃ kulānaṃ,",
  "ja512:14.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:15.1": "Yaṃ ve pitvā dittarūpova poso,",
  "ja512:15.2": "Akkosati mātaraṃ pitarañca;",
  "ja512:15.3": "Sassumpi gaṇheyya athopi suṇhaṃ,",
  "ja512:15.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:16.1": "Yaṃ ve pitvā dittarūpāva nārī,",
  "ja512:16.2": "Akkosatī sasuraṃ sāmikañca;",
  "ja512:16.3": "Dāsampi gaṇhe paricārakampi,",
  "ja512:16.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:17.1": "Yaṃ ve pivitvāna haneyya poso,",
  "ja512:17.2": "Dhamme ṭhitaṃ samaṇaṃ brāhmaṇaṃ vā;",
  "ja512:17.3": "Gacche apāyampi tatonidānaṃ,",
  "ja512:17.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:18.1": "Yaṃ ve pivitvā duccaritaṃ caranti,",
  "ja512:18.2": "Kāyena vācāya ca cetasā ca;",
  "ja512:18.3": "Nirayaṃ vajanti duccaritaṃ caritvā,",
  "ja512:18.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:19.1": "Yaṃ yācamānā na labhanti pubbe,",
  "ja512:19.2": "Bahuṃ hiraññampi pariccajantā;",
  "ja512:19.3": "So taṃ pivitvā alikaṃ bhaṇāti,",
  "ja512:19.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:20.1": "Yaṃ ve pitvā pesane pesiyanto,",
  "ja512:20.2": "Accāyike karaṇīyamhi jāte;",
  "ja512:20.3": "Atthampi so nappajānāti vutto,",
  "ja512:20.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:21.1": "Hirīmanāpi ahirīkabhāvaṃ,",
  "ja512:21.2": "Pātuṃ karonti madanāya mattā;",
  "ja512:21.3": "Dhīrāpi santā bahukaṃ bhaṇanti,",
  "ja512:21.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:22.1": "Yaṃ ve pitvā ekathūpā sayanti,",
  "ja512:22.2": "Anāsakā thaṇḍiladukkhaseyyaṃ;",
  "ja512:22.3": "Dubbaṇṇiyaṃ āyasakyañcupenti,",
  "ja512:22.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:23.1": "Yaṃ ve pitvā pattakhandhā sayanti,",
  "ja512:23.2": "Gāvo kuṭahatāva na hi vāruṇiyā;",
  "ja512:23.3": "Vego narena susahoriva,",
  "ja512:23.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:24.1": "Yaṃ manussā vivajjenti,",
  "ja512:24.2": "sappaṃ ghoravisamiva;",
  "ja512:24.3": "Taṃ loke visasamānaṃ,",
  "ja512:24.4": "ko naro pātumarahati.",
  "ja512:25.1": "Yaṃ ve pitvā andhakaveṇḍaputtā,",
  "ja512:25.2": "Samuddatīre paricārayantā;",
  "ja512:25.3": "Upakkamuṃ musalehaññamaññaṃ,",
  "ja512:25.4": "Tassā puṇṇaṃ kumbhamimaṃ kiṇātha.",
  "ja512:26.1": "Yaṃ ve pitvā pubbadevā pamattā,",
  "ja512:26.2": "Tidivā cutā sassatiyā samāya;",
  "ja512:26.3": "Taṃ tādisaṃ majjamimaṃ niratthakaṃ,",
  "ja512:26.4": "Jānaṃ mahārāja kathaṃ piveyya.",
  "ja512:27.1": "Nayimasmiṃ kumbhasmiṃ dadhi vā madhu vā,",
  "ja512:27.2": "Evaṃ abhiññāya kiṇāhi rāja;",
  "ja512:27.3": "Evañhimaṃ kumbhagatā mayā te,",
  "ja512:27.4": "Akkhātarūpaṃ tava sabbamitta”.",
  "ja512:28.1": "“Na me pitā vā athavāpi mātā,",
  "ja512:28.2": "Etādisā yādisako tuvaṃsi;",
  "ja512:28.3": "Hitānukampī paramatthakāmo,",
  "ja512:28.4": "Sohaṃ karissaṃ vacanaṃ tavajja.",
  "ja512:29.1": "Dadāmi te gāmavarāni pañca,",
  "ja512:29.2": "Dāsīsataṃ satta gavaṃsatāni;",
  "ja512:29.3": "Ājaññayutte ca rathe dasa ime,",
  "ja512:29.4": "Ācariyo hosi mamatthakāmo”.",
  "ja512:30.1": "“Taveva dāsīsatamatthu rāja,",
  "ja512:30.2": "Gāmā ca gāvo ca taveva hontu;",
  "ja512:30.3": "Ājaññayuttā ca rathā taveva,",
  "ja512:30.4": "Sakkohamasmī tidasānamindo.",
  "ja512:31.1": "Maṃsodanaṃ sappipāyāsaṃ bhuñja,",
  "ja512:31.2": "Khādassu ca tvaṃ madhumāsapūve;",
  "ja512:31.3": "Evaṃ tuvaṃ dhammarato janinda,",
  "ja512:31.4": "Anindito saggamupehi ṭhānan”ti.",
  "ja512:32.1": "Kumbhajātakaṃ dutiyaṃ."
}