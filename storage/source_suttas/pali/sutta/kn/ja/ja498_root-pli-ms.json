{
  "ja498:0.1": "Jātaka",
  "ja498:0.2": "Vīsatinipāta",
  "ja498:0.3": "Mātaṅgavagga",
  "ja498:0.4": "2. Cittasambhūtajātaka",
  "ja498:1.1": "“Sabbaṃ narānaṃ saphalaṃ suciṇṇaṃ,",
  "ja498:1.2": "Na kammunā kiñcana moghamatthi;",
  "ja498:1.3": "Passāmi sambhūtaṃ mahānubhāvaṃ,",
  "ja498:1.4": "Sakammunā puññaphalūpapannaṃ.",
  "ja498:2.1": "Sabbaṃ narānaṃ saphalaṃ suciṇṇaṃ,",
  "ja498:2.2": "Na kammunā kiñcana moghamatthi;",
  "ja498:2.3": "Kaccinnu cittassapi evamevaṃ,",
  "ja498:2.4": "Iddho mano tassa yathāpi mayhaṃ”.",
  "ja498:3.1": "“Sabbaṃ narānaṃ saphalaṃ suciṇṇaṃ,",
  "ja498:3.2": "Na kammunā kiñcana moghamatthi;",
  "ja498:3.3": "Cittampi jānāhi tatheva deva,",
  "ja498:3.4": "Iddho mano tassa yathāpi tuyhaṃ”.",
  "ja498:4.1": "“Bhavaṃ nu citto sutamaññato te,",
  "ja498:4.2": "Udāhu te koci naṃ etadakkhā;",
  "ja498:4.3": "Gāthā sugītā na mamatthi kaṅkhā,",
  "ja498:4.4": "Dadāmi te gāmavaraṃ satañca”.",
  "ja498:5.1": "“Na cāhaṃ citto sutamaññato me,",
  "ja498:5.2": "Isī ca me etamatthaṃ asaṃsi;",
  "ja498:5.3": "‘Gantvāna rañño paṭigāhi gāthaṃ,",
  "ja498:5.4": "Api te varaṃ attamano dadeyya’”.",
  "ja498:6.1": "“Yojentu ve rājarathe,",
  "ja498:6.2": "sukate cittasibbane;",
  "ja498:6.3": "Kacchaṃ nāgānaṃ bandhatha,",
  "ja498:6.4": "gīveyyaṃ paṭimuñcatha.",
  "ja498:7.1": "Āhaññantu bherimudiṅgasaṅkhe,",
  "ja498:7.2": "Sīghāni yānāni ca yojayantu;",
  "ja498:7.3": "Ajjevahaṃ assamaṃ taṃ gamissaṃ,",
  "ja498:7.4": "Yattheva dakkhissamisiṃ nisinnaṃ”.",
  "ja498:8.1": "“Suladdhalābho vata me ahosi,",
  "ja498:8.2": "Gāthā sugītā parisāya majjhe;",
  "ja498:8.3": "Svāhaṃ isiṃ sīlavatūpapannaṃ,",
  "ja498:8.4": "Disvā patīto sumanohamasmi”.",
  "ja498:9.1": "“Āsanaṃ udakaṃ pajjaṃ,",
  "ja498:9.2": "paṭiggaṇhātu no bhavaṃ;",
  "ja498:9.3": "Agghe bhavantaṃ pucchāma,",
  "ja498:9.4": "agghaṃ kurutu no bhavaṃ”.",
  "ja498:10.1": "“Rammañca te āvasathaṃ karontu,",
  "ja498:10.2": "Nārīgaṇehi paricārayassu;",
  "ja498:10.3": "Karohi okāsamanuggahāya,",
  "ja498:10.4": "Ubhopimaṃ issariyaṃ karoma”.",
  "ja498:11.1": "“Disvā phalaṃ duccaritassa rāja,",
  "ja498:11.2": "Atho suciṇṇassa mahāvipākaṃ;",
  "ja498:11.3": "Attānameva paṭisaṃyamissaṃ,",
  "ja498:11.4": "Na patthaye putta pasuṃ dhanaṃ vā.",
  "ja498:12.1": "Dasevimā vassadasā,",
  "ja498:12.2": "maccānaṃ idha jīvitaṃ;",
  "ja498:12.3": "Apattaññeva taṃ odhiṃ,",
  "ja498:12.4": "naḷo chinnova sussati.",
  "ja498:13.1": "Tattha kā nandi kā khiḍḍā,",
  "ja498:13.2": "kā ratī kā dhanesanā;",
  "ja498:13.3": "Kiṃ me puttehi dārehi,",
  "ja498:13.4": "rāja muttosmi bandhanā.",
  "ja498:14.1": "Sohaṃ evaṃ pajānāmi,",
  "ja498:14.2": "maccu me nappamajjati;",
  "ja498:14.3": "Antakenādhipannassa,",
  "ja498:14.4": "kā ratī kā dhanesanā.",
  "ja498:15.1": "Jāti narānaṃ adhamā janinda,",
  "ja498:15.2": "Caṇḍālayoni dvipadākaniṭṭhā;",
  "ja498:15.3": "Sakehi kammehi supāpakehi,",
  "ja498:15.4": "Caṇḍālagabbhe avasimha pubbe.",
  "ja498:16.1": "Caṇḍālāhumha avantīsu,",
  "ja498:16.2": "migā nerañjaraṃ pati;",
  "ja498:16.3": "Ukkusā nammadātīre,",
  "ja498:16.4": "tyajja brāhmaṇakhattiyā.",
  "ja498:17.1": "Upanīyati jīvitamappamāyu,",
  "ja498:17.2": "Jarūpanītassa na santi tāṇā;",
  "ja498:17.3": "Karohi pañcāla mameta vākyaṃ,",
  "ja498:17.4": "Mākāsi kammāni dukkhudrayāni.",
  "ja498:18.1": "Upanīyati jīvitamappamāyu,",
  "ja498:18.2": "Jarūpanītassa na santi tāṇā;",
  "ja498:18.3": "Karohi pañcāla mameta vākyaṃ,",
  "ja498:18.4": "Mākāsi kammāni dukkhapphalāni.",
  "ja498:19.1": "Upanīyati jīvitamappamāyu,",
  "ja498:19.2": "Jarūpanītassa na santi tāṇā;",
  "ja498:19.3": "Karohi pañcāla mameta vākyaṃ,",
  "ja498:19.4": "Mākāsi kammāni rajassirāni.",
  "ja498:20.1": "Upanīyati jīvitamappamāyu,",
  "ja498:20.2": "Vaṇṇaṃ jarā hanti narassa jiyyato;",
  "ja498:20.3": "Karohi pañcāla mameta vākyaṃ,",
  "ja498:20.4": "Mākāsi kammaṃ nirayūpapattiyā”.",
  "ja498:21.1": "“Addhā hi saccaṃ vacanaṃ tavetaṃ,",
  "ja498:21.2": "Yathā isī bhāsasi evametaṃ;",
  "ja498:21.3": "Kāmā ca me santi anapparūpā,",
  "ja498:21.4": "Te duccajā mādisakena bhikkhu.",
  "ja498:22.1": "Nāgo yathā paṅkamajjhe byasanno,",
  "ja498:22.2": "Passaṃ thalaṃ nābhisambhoti gantuṃ;",
  "ja498:22.3": "Evampahaṃ kāmapaṅke byasanno,",
  "ja498:22.4": "Na bhikkhuno maggamanubbajāmi.",
  "ja498:23.1": "Yathāpi mātā ca pitā ca puttaṃ,",
  "ja498:23.2": "Anusāsare kinti sukhī bhaveyya;",
  "ja498:23.3": "Evampi maṃ tvaṃ anusāsa bhante,",
  "ja498:23.4": "Yathā ciraṃ pecca sukhī bhaveyyaṃ”.",
  "ja498:24.1": "“No ce tuvaṃ ussahase janinda,",
  "ja498:24.2": "Kāme ime mānusake pahātuṃ;",
  "ja498:24.3": "Dhammiṃ baliṃ paṭṭhapayassu rāja,",
  "ja498:24.4": "Adhammakāro tava māhu raṭṭhe.",
  "ja498:25.1": "Dūtā vidhāvantu disā catasso,",
  "ja498:25.2": "Nimantakā samaṇabrāhmaṇānaṃ;",
  "ja498:25.3": "Te annapānena upaṭṭhahassu,",
  "ja498:25.4": "Vatthena senāsanapaccayena ca.",
  "ja498:26.1": "Annena pānena pasannacitto,",
  "ja498:26.2": "Santappaya samaṇabrāhmaṇe ca;",
  "ja498:26.3": "Datvā ca bhutvā ca yathānubhāvaṃ,",
  "ja498:26.4": "Anindito saggamupehi ṭhānaṃ.",
  "ja498:27.1": "Sace ca taṃ rāja mado saheyya,",
  "ja498:27.2": "Nārīgaṇehi paricārayantaṃ;",
  "ja498:27.3": "Imameva gāthaṃ manasī karohi,",
  "ja498:27.4": "Bhāsesi cenaṃ parisāya majjhe.",
  "ja498:28.1": "Abbhokāsasayo jantu,",
  "ja498:28.2": "vajantyā khīrapāyito;",
  "ja498:28.3": "Parikiṇṇo suvānehi,",
  "ja498:28.4": "svājja rājāti vuccatī”ti.",
  "ja498:29.1": "Cittasambhūtajātakaṃ dutiyaṃ."
}