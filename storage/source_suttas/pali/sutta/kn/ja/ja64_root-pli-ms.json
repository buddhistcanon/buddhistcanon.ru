{
  "ja64:0.1": "Jātaka",
  "ja64:0.2": "Ekakanipāta",
  "ja64:0.3": "Itthivagga",
  "ja64:0.4": "4. Durājānajātaka",
  "ja64:1.1": "Mā su nandi icchati maṃ,",
  "ja64:1.2": "mā su soci na micchati;",
  "ja64:1.3": "Thīnaṃ bhāvo durājāno,",
  "ja64:1.4": "macchassevodake gatan”ti.",
  "ja64:2.1": "Durājānajātakaṃ catutthaṃ."
}