{
  "cp27:0.1": "Cariyāpiṭaka",
  "cp27:0.2": "Yudhañjayavagga",
  "cp27:0.3": "Saccapāramī 7",
  "cp27:0.4": "Kapirājacariya",
  "cp27:1.1": "“Yadā ahaṃ kapi āsiṃ,",
  "cp27:1.2": "nadīkūle darīsaye;",
  "cp27:1.3": "Pīḷito susumārena,",
  "cp27:1.4": "gamanaṃ na labhāmahaṃ.",
  "cp27:2.1": "Yamhokāse ahaṃ ṭhatvā,",
  "cp27:2.2": "orā pāraṃ patāmahaṃ;",
  "cp27:2.3": "Tatthacchi sattu vadhako,",
  "cp27:2.4": "kumbhīlo luddadassano.",
  "cp27:3.1": "So maṃ asaṃsi ‘ehī’ti,",
  "cp27:3.2": "‘ahampemī’ti taṃ vatiṃ;",
  "cp27:3.3": "Tassa matthakamakkamma,",
  "cp27:3.4": "parakūle patiṭṭhahiṃ.",
  "cp27:4.1": "Na tassa alikaṃ bhaṇitaṃ,",
  "cp27:4.2": "yathā vācaṃ akāsahaṃ;",
  "cp27:4.3": "Saccena me samo natthi,",
  "cp27:4.4": "esā me saccapāramī”ti.",
  "cp27:4.5": "Kapirājacariyaṃ sattamaṃ."
}