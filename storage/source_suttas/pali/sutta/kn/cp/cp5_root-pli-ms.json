{
  "cp5:0.1": "Cariyāpiṭaka",
  "cp5:0.2": "Akittivagga",
  "cp5:0.3": "Dānapāramī 5",
  "cp5:0.4": "Mahāgovindacariya",
  "cp5:1.1": "“Punāparaṃ yadā homi,",
  "cp5:1.2": "sattarājapurohito;",
  "cp5:1.3": "Pūjito naradevehi,",
  "cp5:1.4": "mahāgovindabrāhmaṇo.",
  "cp5:2.1": "Tadāhaṃ sattarajjesu,",
  "cp5:2.2": "Yaṃ me āsi upāyanaṃ;",
  "cp5:2.3": "Tena demi mahādānaṃ,",
  "cp5:2.4": "Akkhobbhaṃ sāgarūpamaṃ.",
  "cp5:3.1": "Na me dessaṃ dhanaṃ dhaññaṃ,",
  "cp5:3.2": "Napi natthi nicayo mayi;",
  "cp5:3.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
  "cp5:3.4": "Tasmā demi varaṃ dhanan”ti.",
  "cp5:4.1": "Mahāgovindacariyaṃ pañcamaṃ."
}