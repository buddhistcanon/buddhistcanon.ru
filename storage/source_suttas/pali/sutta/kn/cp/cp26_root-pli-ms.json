{
  "cp26:0.1": "Cariyāpiṭaka",
  "cp26:0.2": "Yudhañjayavagga",
  "cp26:0.3": "Adiṭṭhānapāramī 1",
  "cp26:0.4": "Temiyacariya",
  "cp26:1.1": "“Punāparaṃ yadā homi,",
  "cp26:1.2": "kāsirājassa atrajo;",
  "cp26:1.3": "Mūgapakkhoti nāmena,",
  "cp26:1.4": "temiyoti vadanti maṃ.",
  "cp26:2.1": "Soḷasitthisahassānaṃ,",
  "cp26:2.2": "na vijjati pumo tadā;",
  "cp26:2.3": "Ahorattānaṃ accayena,",
  "cp26:2.4": "nibbatto ahamekako.",
  "cp26:3.1": "Kicchā laddhaṃ piyaṃ puttaṃ,",
  "cp26:3.2": "abhijātaṃ jutindharaṃ;",
  "cp26:3.3": "Setacchattaṃ dhārayitvāna,",
  "cp26:3.4": "sayane poseti maṃ pitā.",
  "cp26:4.1": "Niddāyamāno sayanavare,",
  "cp26:4.2": "Pabujjhitvānahaṃ tadā;",
  "cp26:4.3": "Addasaṃ paṇḍaraṃ chattaṃ,",
  "cp26:4.4": "Yenāhaṃ nirayaṃ gato.",
  "cp26:5.1": "Saha diṭṭhassa me chattaṃ,",
  "cp26:5.2": "tāso uppajji bheravo;",
  "cp26:5.3": "Vinicchayaṃ samāpanno,",
  "cp26:5.4": "‘kathāhaṃ imaṃ muñcissaṃ’.",
  "cp26:6.1": "Pubbasālohitā mayhaṃ,",
  "cp26:6.2": "devatā atthakāminī;",
  "cp26:6.3": "Sā maṃ disvāna dukkhitaṃ,",
  "cp26:6.4": "tīsu ṭhānesu yojayi.",
  "cp26:7.1": "‘Mā paṇḍiccayaṃ vibhāvaya,",
  "cp26:7.2": "Bālamato bhava sabbapāṇinaṃ;",
  "cp26:7.3": "Sabbo taṃ jano ocināyatu,",
  "cp26:7.4": "Evaṃ tava attho bhavissati’.",
  "cp26:8.1": "Evaṃ vuttāyahaṃ tassā,",
  "cp26:8.2": "idaṃ vacanamabraviṃ;",
  "cp26:8.3": "‘Karomi te taṃ vacanaṃ,",
  "cp26:8.4": "yaṃ tvaṃ bhaṇasi devate;",
  "cp26:8.5": "Atthakāmāsi me amma,",
  "cp26:8.6": "hitakāmāsi devate’.",
  "cp26:9.1": "Tassāhaṃ vacanaṃ sutvā,",
  "cp26:9.2": "sāgareva thalaṃ labhiṃ;",
  "cp26:9.3": "Haṭṭho saṃviggamānaso,",
  "cp26:9.4": "tayo aṅge adhiṭṭhahiṃ.",
  "cp26:10.1": "Mūgo ahosiṃ badhiro,",
  "cp26:10.2": "pakkho gativivajjito;",
  "cp26:10.3": "Ete aṅge adhiṭṭhāya,",
  "cp26:10.4": "vassāni soḷasaṃ vasiṃ.",
  "cp26:11.1": "Tato me hatthapāde ca,",
  "cp26:11.2": "jivhaṃ sotañca maddiya;",
  "cp26:11.3": "Anūnataṃ me passitvā,",
  "cp26:11.4": "‘kāḷakaṇṇī’ti nindisuṃ.",
  "cp26:12.1": "Tato jānapadā sabbe,",
  "cp26:12.2": "senāpatipurohitā;",
  "cp26:12.3": "Sabbe ekamanā hutvā,",
  "cp26:12.4": "chaḍḍanaṃ anumodisuṃ.",
  "cp26:13.1": "Sohaṃ tesaṃ matiṃ sutvā,",
  "cp26:13.2": "haṭṭho saṃviggamānaso;",
  "cp26:13.3": "‘Yassatthāya tapociṇṇo,",
  "cp26:13.4": "so me attho samijjhatha’.",
  "cp26:14.1": "Nhāpetvā anulimpitvā,",
  "cp26:14.2": "veṭhetvā rājaveṭhanaṃ;",
  "cp26:14.3": "Chattena abhisiñcitvā,",
  "cp26:14.4": "kāresuṃ puraṃ padakkhiṇaṃ.",
  "cp26:15.1": "Sattāhaṃ dhārayitvāna,",
  "cp26:15.2": "uggate ravimaṇḍale;",
  "cp26:15.3": "Rathena maṃ nīharitvā,",
  "cp26:15.4": "sārathī vanamupāgami.",
  "cp26:16.1": "Ekokāse rathaṃ katvā,",
  "cp26:16.2": "sajjassaṃ hatthamuccito;",
  "cp26:16.3": "Sārathī khaṇatī kāsuṃ,",
  "cp26:16.4": "nikhātuṃ pathaviyā mamaṃ.",
  "cp26:17.1": "Adhiṭṭhitamadhiṭṭhānaṃ,",
  "cp26:17.2": "tajjento vividhakāraṇā;",
  "cp26:17.3": "Na bhindiṃ tamadhiṭṭhānaṃ,",
  "cp26:17.4": "bodhiyāyeva kāraṇā.",
  "cp26:18.1": "Mātāpitā na me dessā,",
  "cp26:18.2": "attā me na ca dessiyo;",
  "cp26:18.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
  "cp26:18.4": "tasmā vatamadhiṭṭhahiṃ.",
  "cp26:19.1": "Ete aṅge adhiṭṭhāya,",
  "cp26:19.2": "Vassāni soḷasaṃ vasiṃ;",
  "cp26:19.3": "Adhiṭṭhānena me samo natthi,",
  "cp26:19.4": "Esā me adhiṭṭhānapāramī”ti.",
  "cp26:20.1": "Temiyacariyaṃ chaṭṭhaṃ."
}