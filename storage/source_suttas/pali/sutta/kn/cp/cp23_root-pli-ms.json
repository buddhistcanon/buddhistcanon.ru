{
  "cp23:0.1": "Cariyāpiṭaka",
  "cp23:0.2": "Yudhañjayavagga",
  "cp23:0.3": "Nekkhammapāramī 3",
  "cp23:0.4": "Ayogharacariya",
  "cp23:1.1": "“Punāparaṃ yadā homi,",
  "cp23:1.2": "kāsirājassa atrajo;",
  "cp23:1.3": "Ayogharamhi saṃvaḍḍho,",
  "cp23:1.4": "nāmenāsi ayogharo.",
  "cp23:2.1": "‘Dukkhena jīvito laddho,",
  "cp23:2.2": "saṃpīḷe patiposito;",
  "cp23:2.3": "Ajjeva putta paṭipajja,",
  "cp23:2.4": "kevalaṃ vasudhaṃ imaṃ’.",
  "cp23:3.1": "Saraṭṭhakaṃ sanigamaṃ,",
  "cp23:3.2": "sajanaṃ vanditva khattiyaṃ;",
  "cp23:3.3": "Añjaliṃ paggahetvāna,",
  "cp23:3.4": "idaṃ vacanamabraviṃ.",
  "cp23:4.1": "‘Ye keci mahiyā sattā,",
  "cp23:4.2": "hīnamukkaṭṭhamajjhimā;",
  "cp23:4.3": "Nirārakkhā sake gehe,",
  "cp23:4.4": "vaḍḍhanti sakañātibhi.",
  "cp23:5.1": "Idaṃ loke uttariyaṃ,",
  "cp23:5.2": "saṃpīḷe mama posanaṃ;",
  "cp23:5.3": "Ayogharamhi saṃvaḍḍho,",
  "cp23:5.4": "appabhe candasūriye.",
  "cp23:6.1": "Pūtikuṇapasampuṇṇā,",
  "cp23:6.2": "muccitvā mātukucchito;",
  "cp23:6.3": "Tato ghoratare dukkhe,",
  "cp23:6.4": "puna pakkhittayoghare.",
  "cp23:7.1": "Yadihaṃ tādisaṃ patvā,",
  "cp23:7.2": "dukkhaṃ paramadāruṇaṃ;",
  "cp23:7.3": "Rajjesu yadi rajjāmi,",
  "cp23:7.4": "pāpānaṃ uttamo siyaṃ.",
  "cp23:8.1": "Ukkaṇṭhitomhi kāyena,",
  "cp23:8.2": "rajjenamhi anatthiko;",
  "cp23:8.3": "Nibbutiṃ pariyesissaṃ,",
  "cp23:8.4": "yattha maṃ maccu na maddiye’.",
  "cp23:9.1": "Evāhaṃ cintayitvāna,",
  "cp23:9.2": "viravante mahājane;",
  "cp23:9.3": "Nāgova bandhanaṃ chetvā,",
  "cp23:9.4": "pāvisiṃ kānanaṃ vanaṃ.",
  "cp23:10.1": "Mātāpitā na me dessā,",
  "cp23:10.2": "napi me dessaṃ mahāyasaṃ;",
  "cp23:10.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
  "cp23:10.4": "tasmā rajjaṃ pariccajin”ti.",
  "cp23:11.1": "Ayogharacariyaṃ tatiyaṃ."
}