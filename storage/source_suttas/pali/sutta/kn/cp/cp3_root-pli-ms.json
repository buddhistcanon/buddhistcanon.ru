{
  "cp3:0.1": "Cariyāpiṭaka",
  "cp3:0.2": "Akittivagga",
  "cp3:0.3": "Dānapāramī 3",
  "cp3:0.4": "Kururājacariya",
  "cp3:1.1": "“Punāparaṃ yadā homi,",
  "cp3:1.2": "indapatthe puruttame;",
  "cp3:1.3": "Rājā dhanañcayo nāma,",
  "cp3:1.4": "kusale dasahupāgato.",
  "cp3:2.1": "Kaliṅgaraṭṭhavisayā,",
  "cp3:2.2": "brāhmaṇā upagañchu maṃ;",
  "cp3:2.3": "Āyācuṃ maṃ hatthināgaṃ,",
  "cp3:2.4": "dhaññaṃ maṅgalasammataṃ.",
  "cp3:3.1": "‘Avuṭṭhiko janapado,",
  "cp3:3.2": "dubbhikkho chātako mahā;",
  "cp3:3.3": "Dadāhi pavaraṃ nāgaṃ,",
  "cp3:3.4": "nīlaṃ añjanasavhayaṃ’.",
  "cp3:4.1": "‘Na me yācakamanuppatte,",
  "cp3:4.2": "paṭikkhepo anucchavo;",
  "cp3:4.3": "Mā me bhijji samādānaṃ,",
  "cp3:4.4": "dassāmi vipulaṃ gajaṃ’.",
  "cp3:5.1": "Nāgaṃ gahetvā soṇḍāya,",
  "cp3:5.2": "bhiṅgāre ratanāmaye;",
  "cp3:5.3": "Jalaṃ hatthe ākiritvā,",
  "cp3:5.4": "brāhmaṇānaṃ adaṃ gajaṃ.",
  "cp3:6.1": "Tassa nāge padinnamhi,",
  "cp3:6.2": "amaccā etadabravuṃ;",
  "cp3:6.3": "‘Kiṃ nu tuyhaṃ varaṃ nāgaṃ,",
  "cp3:6.4": "yācakānaṃ padassasi.",
  "cp3:7.1": "Dhaññaṃ maṅgalasampannaṃ,",
  "cp3:7.2": "saṅgāmavijayuttamaṃ;",
  "cp3:7.3": "Tasmiṃ nāge padinnamhi,",
  "cp3:7.4": "kiṃ te rajjaṃ karissati’.",
  "cp3:8.1": "‘Rajjampi me dade sabbaṃ,",
  "cp3:8.2": "sarīraṃ dajjamattano;",
  "cp3:8.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
  "cp3:8.4": "tasmā nāgaṃ adāsahan’”ti.",
  "cp3:9.1": "Kururājacariyaṃ tatiyaṃ."
}