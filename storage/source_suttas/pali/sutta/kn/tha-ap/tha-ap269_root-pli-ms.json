{
  "tha-ap269:0.1": "Therāpadāna",
  "tha-ap269:0.2": "Padumukkhipavagga",
  "tha-ap269:0.3": "7. Javahaṃsakattheraapadāna",
  "tha-ap269:1.1": "“Candabhāgānadītīre,",
  "tha-ap269:1.2": "Āsiṃ vanacaro tadā;",
  "tha-ap269:1.3": "Siddhatthaṃ addasaṃ buddhaṃ,",
  "tha-ap269:1.4": "Gacchantaṃ anilañjase.",
  "tha-ap269:2.1": "Añjaliṃ paggahetvāna,",
  "tha-ap269:2.2": "ullokento mahāmuniṃ;",
  "tha-ap269:2.3": "Sakaṃ cittaṃ pasādetvā,",
  "tha-ap269:2.4": "avandiṃ nāyakaṃ ahaṃ.",
  "tha-ap269:3.1": "Catunnavutito kappe,",
  "tha-ap269:3.2": "yamavandiṃ narāsabhaṃ;",
  "tha-ap269:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap269:3.4": "vandanāya idaṃ phalaṃ.",
  "tha-ap269:4.1": "Paṭisambhidā catasso,",
  "tha-ap269:4.2": "…pe…",
  "tha-ap269:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap269:5.1": "Itthaṃ sudaṃ āyasmā javahaṃsako thero imā gāthāyo abhāsitthāti.",
  "tha-ap269:6.1": "Javahaṃsakattherassāpadānaṃ sattamaṃ."
}