{
  "tha-ap38:0.1": "Therāpadāna",
  "tha-ap38:0.2": "Kuṇḍadhānavagga",
  "tha-ap38:0.3": "6 Adhimuttattheraapadāna",
  "tha-ap38:1.1": "“Nibbute lokanāthamhi,",
  "tha-ap38:1.2": "atthadassīnaruttame;",
  "tha-ap38:1.3": "Upaṭṭhahiṃ bhikkhusaṃghaṃ,",
  "tha-ap38:1.4": "vippasannena cetasā.",
  "tha-ap38:2.1": "Nimantetvā bhikkhusaṃghaṃ,",
  "tha-ap38:2.2": "Ujubhūtaṃ samāhitaṃ;",
  "tha-ap38:2.3": "Ucchunā maṇḍapaṃ katvā,",
  "tha-ap38:2.4": "Bhojesiṃ saṃghamuttamaṃ.",
  "tha-ap38:3.1": "Yaṃ yaṃ yonupapajjāmi,",
  "tha-ap38:3.2": "devattaṃ atha mānusaṃ;",
  "tha-ap38:3.3": "Sabbe satte abhibhomi,",
  "tha-ap38:3.4": "puññakammassidaṃ phalaṃ.",
  "tha-ap38:4.1": "Aṭṭhārase kappasate,",
  "tha-ap38:4.2": "yaṃ dānamadadiṃ tadā;",
  "tha-ap38:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap38:4.4": "ucchudānassidaṃ phalaṃ.",
  "tha-ap38:5.1": "Paṭisambhidā catasso,",
  "tha-ap38:5.2": "…pe…",
  "tha-ap38:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap38:6.1": "Itthaṃ sudaṃ āyasmā adhimutto thero imā gāthāyo abhāsitthāti.",
  "tha-ap38:7.1": "Adhimuttattherassāpadānaṃ chaṭṭhaṃ."
}