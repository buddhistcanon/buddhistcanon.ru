{
  "tha-ap284:0.1": "Therāpadāna",
  "tha-ap284:0.2": "Paṇṇadāyakavagga",
  "tha-ap284:0.3": "2. Phaladāyakattheraapadāna",
  "tha-ap284:1.1": "“Sinerusamasantoso,",
  "tha-ap284:1.2": "dharaṇīsamasādiso;",
  "tha-ap284:1.3": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap284:1.4": "bhikkhāya mamupaṭṭhito.",
  "tha-ap284:2.1": "Harītakaṃ āmalakaṃ,",
  "tha-ap284:2.2": "ambajambuvibhītakaṃ;",
  "tha-ap284:2.3": "Kolaṃ bhallātakaṃ billaṃ,",
  "tha-ap284:2.4": "phārusakaphalāni ca.",
  "tha-ap284:3.1": "Siddhatthassa mahesissa,",
  "tha-ap284:3.2": "sabbalokānukampino;",
  "tha-ap284:3.3": "Tañca sabbaṃ mayā dinnaṃ,",
  "tha-ap284:3.4": "vippasannena cetasā.",
  "tha-ap284:4.1": "Catunnavutito kappe,",
  "tha-ap284:4.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap284:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap284:4.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap284:5.1": "Sattapaññāsito kappe,",
  "tha-ap284:5.2": "ekajjho nāma khattiyo;",
  "tha-ap284:5.3": "Sattaratanasampanno,",
  "tha-ap284:5.4": "cakkavattī mahabbalo.",
  "tha-ap284:6.1": "Paṭisambhidā catasso,",
  "tha-ap284:6.2": "…pe…",
  "tha-ap284:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap284:7.1": "Itthaṃ sudaṃ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap284:8.1": "Phaladāyakattherassāpadānaṃ dutiyaṃ."
}