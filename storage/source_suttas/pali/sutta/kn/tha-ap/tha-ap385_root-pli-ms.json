{
  "tha-ap385:0.1": "Therāpadāna",
  "tha-ap385:0.2": "Avaṭaphalavagga",
  "tha-ap385:0.3": "3. Udumbaraphaladāyakattheraapadāna",
  "tha-ap385:1.1": "“Vinatānadiyā tīre,",
  "tha-ap385:1.2": "vihāsi purisuttamo;",
  "tha-ap385:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap385:1.4": "ekaggaṃ susamāhitaṃ.",
  "tha-ap385:2.1": "Tasmiṃ pasannamānaso,",
  "tha-ap385:2.2": "kilesamaladhovane;",
  "tha-ap385:2.3": "Udumbaraphalaṃ gayha,",
  "tha-ap385:2.4": "buddhaseṭṭhassadāsahaṃ.",
  "tha-ap385:3.1": "Ekanavutito kappe,",
  "tha-ap385:3.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap385:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap385:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap385:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap385:4.2": "…pe…",
  "tha-ap385:4.3": "viharāmi anāsavo.",
  "tha-ap385:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap385:5.2": "…pe…",
  "tha-ap385:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap385:6.1": "Paṭisambhidā catasso,",
  "tha-ap385:6.2": "…pe…",
  "tha-ap385:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap385:7.1": "Itthaṃ sudaṃ āyasmā udumbaraphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap385:8.1": "Udumbaraphaladāyakattherassāpadānaṃ tatiyaṃ."
}