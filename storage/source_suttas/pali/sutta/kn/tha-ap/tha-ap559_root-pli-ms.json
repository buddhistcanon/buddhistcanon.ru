{
  "tha-ap559:0.1": "Therāpadāna",
  "tha-ap559:0.2": "Yasavagga",
  "tha-ap559:0.3": "7. Aparauttarattheraapadāna",
  "tha-ap559:1.1": "“Nibbute lokanāthamhi,",
  "tha-ap559:1.2": "siddhatthe lokanāyake;",
  "tha-ap559:1.3": "Mama ñātī samānetvā,",
  "tha-ap559:1.4": "dhātupūjaṃ akāsahaṃ.",
  "tha-ap559:2.1": "Catunnavutito kappe,",
  "tha-ap559:2.2": "yaṃ dhātumabhipūjayiṃ;",
  "tha-ap559:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap559:2.4": "dhātupūjāyidaṃ phalaṃ.",
  "tha-ap559:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap559:3.2": "…pe…",
  "tha-ap559:3.3": "viharāmi anāsavo.",
  "tha-ap559:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap559:4.2": "…pe…",
  "tha-ap559:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap559:5.1": "Paṭisambhidā catasso,",
  "tha-ap559:5.2": "…pe…",
  "tha-ap559:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap559:6.1": "Itthaṃ sudaṃ āyasmā aparauttaratthero imā gāthāyo abhāsitthāti.",
  "tha-ap559:7.1": "Aparassa uttarattherassāpadānaṃ sattamaṃ."
}