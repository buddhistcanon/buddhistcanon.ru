{
  "tha-ap388:0.1": "Therāpadāna",
  "tha-ap388:0.2": "Avaṭaphalavagga",
  "tha-ap388:0.3": "6. Valliphaladāyakattheraapadāna",
  "tha-ap388:1.1": "“Sabbe janā samāgamma,",
  "tha-ap388:1.2": "agamiṃsu vanaṃ tadā;",
  "tha-ap388:1.3": "Phalamanvesamānā te,",
  "tha-ap388:1.4": "alabhiṃsu phalaṃ tadā.",
  "tha-ap388:2.1": "Tatthaddasāsiṃ sambuddhaṃ,",
  "tha-ap388:2.2": "sayambhuṃ aparājitaṃ;",
  "tha-ap388:2.3": "Pasannacitto sumano,",
  "tha-ap388:2.4": "valliphalamadāsahaṃ.",
  "tha-ap388:3.1": "Ekattiṃse ito kappe,",
  "tha-ap388:3.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap388:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap388:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap388:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap388:4.2": "…pe…",
  "tha-ap388:4.3": "viharāmi anāsavo.",
  "tha-ap388:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap388:5.2": "…pe…",
  "tha-ap388:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap388:6.1": "Paṭisambhidā catasso,",
  "tha-ap388:6.2": "…pe…",
  "tha-ap388:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap388:7.1": "Itthaṃ sudaṃ āyasmā valliphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap388:8.1": "Valliphaladāyakattherassāpadānaṃ chaṭṭhaṃ."
}