{
  "tha-ap193:0.1": "Therāpadāna",
  "tha-ap193:0.2": "Tamālapupphiyavagga",
  "tha-ap193:0.3": "1. Tamālapupphiyattheraapadāna",
  "tha-ap193:1.1": "“Cullāsītisahassāni,",
  "tha-ap193:1.2": "thambhā sovaṇṇayā ahū;",
  "tha-ap193:1.3": "Devalaṭṭhipaṭibhāgaṃ,",
  "tha-ap193:1.4": "vimānaṃ me sunimmitaṃ.",
  "tha-ap193:2.1": "Tamālapupphaṃ paggayha,",
  "tha-ap193:2.2": "vippasannena cetasā;",
  "tha-ap193:2.3": "Buddhassa abhiropayiṃ,",
  "tha-ap193:2.4": "sikhino lokabandhuno.",
  "tha-ap193:3.1": "Ekattiṃse ito kappe,",
  "tha-ap193:3.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap193:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap193:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap193:4.1": "Ito vīsatime kappe,",
  "tha-ap193:4.2": "candatittoti ekako;",
  "tha-ap193:4.3": "Sattaratanasampanno,",
  "tha-ap193:4.4": "cakkavattī mahabbalo.",
  "tha-ap193:5.1": "Paṭisambhidā catasso,",
  "tha-ap193:5.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap193:5.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap193:5.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap193:6.1": "Itthaṃ sudaṃ āyasmā tamālapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap193:7.1": "Tamālapupphiyattherassāpadānaṃ paṭhamaṃ."
}