{
  "tha-ap207:0.1": "Therāpadāna",
  "tha-ap207:0.2": "Kaṇikārapupphiyavagga",
  "tha-ap207:0.3": "5. Nigguṇḍipupphiyattheraapadāna",
  "tha-ap207:1.1": "“Vipassissa bhagavato,",
  "tha-ap207:1.2": "āsimārāmiko ahaṃ;",
  "tha-ap207:1.3": "Nigguṇḍipupphaṃ paggayha,",
  "tha-ap207:1.4": "buddhassa abhiropayiṃ.",
  "tha-ap207:2.1": "Ekanavutito kappe,",
  "tha-ap207:2.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap207:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap207:2.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap207:3.1": "Pañcavīse ito kappe,",
  "tha-ap207:3.2": "eko āsiṃ janādhipo;",
  "tha-ap207:3.3": "Mahāpatāpanāmena,",
  "tha-ap207:3.4": "cakkavattī mahabbalo.",
  "tha-ap207:4.1": "Paṭisambhidā catasso,",
  "tha-ap207:4.2": "…pe…",
  "tha-ap207:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap207:5.1": "Itthaṃ sudaṃ āyasmā nigguṇḍipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap207:6.1": "Nigguṇḍipupphiyattherassāpadānaṃ pañcamaṃ."
}