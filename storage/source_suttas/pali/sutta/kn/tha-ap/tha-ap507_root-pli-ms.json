{
  "tha-ap507:0.1": "Therāpadāna",
  "tha-ap507:0.2": "Kaṇikāravagga",
  "tha-ap507:0.3": "5 Pādaphaliyattheraapadāna",
  "tha-ap507:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap507:1.2": "Āhutīnaṃ paṭiggahaṃ;",
  "tha-ap507:1.3": "Rathiyaṃ paṭipajjantaṃ,",
  "tha-ap507:1.4": "Pādaphalaṃ adāsahaṃ.",
  "tha-ap507:2.1": "Ekanavutito kappe,",
  "tha-ap507:2.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap507:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap507:2.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap507:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap507:3.2": "…pe…",
  "tha-ap507:3.3": "viharāmi anāsavo.",
  "tha-ap507:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap507:4.2": "…pe…",
  "tha-ap507:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap507:5.1": "Paṭisambhidā catasso,",
  "tha-ap507:5.2": "…pe…",
  "tha-ap507:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap507:6.1": "Itthaṃ sudaṃ āyasmā pādaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap507:7.1": "Pādaphaliyattherassāpadānaṃ pañcamaṃ."
}