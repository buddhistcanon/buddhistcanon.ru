{
  "tha-ap384:0.1": "Therāpadāna",
  "tha-ap384:0.2": "Avaṭaphalavagga",
  "tha-ap384:0.3": "2. Labujadāyakattheraapadāna",
  "tha-ap384:1.1": "“Nagare bandhumatiyā,",
  "tha-ap384:1.2": "āsiṃ ārāmiko tadā;",
  "tha-ap384:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap384:1.4": "gacchantaṃ anilañjase.",
  "tha-ap384:2.1": "Labujassa phalaṃ gayha,",
  "tha-ap384:2.2": "buddhaseṭṭhassadāsahaṃ;",
  "tha-ap384:2.3": "Ākāse ṭhitako santo,",
  "tha-ap384:2.4": "paṭigaṇhi mahāyaso.",
  "tha-ap384:3.1": "Vittisañjananaṃ mayhaṃ,",
  "tha-ap384:3.2": "diṭṭhadhammasukhāvahaṃ;",
  "tha-ap384:3.3": "Phalaṃ buddhassa datvāna,",
  "tha-ap384:3.4": "vippasannena cetasā.",
  "tha-ap384:4.1": "Adhigacchiṃ tadā pītiṃ,",
  "tha-ap384:4.2": "vipulañca sukhuttamaṃ;",
  "tha-ap384:4.3": "Uppajjateva ratanaṃ,",
  "tha-ap384:4.4": "nibbattassa tahiṃ tahiṃ.",
  "tha-ap384:5.1": "Ekanavutito kappe,",
  "tha-ap384:5.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap384:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap384:5.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap384:6.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap384:6.2": "…pe…",
  "tha-ap384:6.3": "viharāmi anāsavo.",
  "tha-ap384:7.1": "Svāgataṃ vata me āsi,",
  "tha-ap384:7.2": "…pe…",
  "tha-ap384:7.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap384:8.1": "Paṭisambhidā catasso,",
  "tha-ap384:8.2": "…pe…",
  "tha-ap384:8.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap384:9.1": "Itthaṃ sudaṃ āyasmā labujadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap384:10.1": "Labujadāyakattherassāpadānaṃ dutiyaṃ."
}