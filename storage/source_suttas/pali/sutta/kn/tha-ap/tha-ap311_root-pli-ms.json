{
  "tha-ap311:0.1": "Therāpadāna",
  "tha-ap311:0.2": "Padumakesaravagga",
  "tha-ap311:0.3": "9 Buddhasaññakattheraapadāna",
  "tha-ap311:1.1": "“Udentaṃ sataraṃsiṃva,",
  "tha-ap311:1.2": "pītaraṃsiṃva bhāṇumaṃ;",
  "tha-ap311:1.3": "Vanantaragataṃ santaṃ,",
  "tha-ap311:1.4": "lokajeṭṭhaṃ narāsabhaṃ.",
  "tha-ap311:2.1": "Addasaṃ supinantena,",
  "tha-ap311:2.2": "siddhatthaṃ lokanāyakaṃ;",
  "tha-ap311:2.3": "Tattha cittaṃ pasādetvā,",
  "tha-ap311:2.4": "sugatiṃ upapajjahaṃ.",
  "tha-ap311:3.1": "Catunnavutito kappe,",
  "tha-ap311:3.2": "yaṃ saññamalabhiṃ tadā;",
  "tha-ap311:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap311:3.4": "buddhasaññāyidaṃ phalaṃ.",
  "tha-ap311:4.1": "Paṭisambhidā catasso,",
  "tha-ap311:4.2": "…pe…",
  "tha-ap311:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap311:5.1": "Itthaṃ sudaṃ āyasmā buddhasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap311:6.1": "Buddhasaññakattherassāpadānaṃ navamaṃ."
}