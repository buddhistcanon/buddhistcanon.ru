{
  "tha-ap325:0.1": "Therāpadāna",
  "tha-ap325:0.2": "Umāpupphiyavagga",
  "tha-ap325:0.3": "3. Hāsajanakattheraapadāna",
  "tha-ap325:1.1": "“Dumagge paṃsukūlakaṃ,",
  "tha-ap325:1.2": "laggaṃ disvāna satthuno;",
  "tha-ap325:1.3": "Añjaliṃ paggahetvāna,",
  "tha-ap325:1.4": "bhiyyo uccāritaṃ mayā.",
  "tha-ap325:2.1": "Dūrato pana disvāna,",
  "tha-ap325:2.2": "hāso me udapajjatha;",
  "tha-ap325:2.3": "Añjaliṃ paggahetvāna,",
  "tha-ap325:2.4": "bhiyyo cittaṃ pasādayiṃ.",
  "tha-ap325:3.1": "Ekanavutito kappe,",
  "tha-ap325:3.2": "yaṃ saññamalabhiṃ tadā;",
  "tha-ap325:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap325:3.4": "buddhasaññāyidaṃ phalaṃ.",
  "tha-ap325:4.1": "Paṭisambhidā catasso,",
  "tha-ap325:4.2": "…pe…",
  "tha-ap325:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap325:5.1": "Itthaṃ sudaṃ āyasmā hāsajanako thero imā gāthāyo abhāsitthāti.",
  "tha-ap325:6.1": "Hāsajanakattherassāpadānaṃ tatiyaṃ."
}