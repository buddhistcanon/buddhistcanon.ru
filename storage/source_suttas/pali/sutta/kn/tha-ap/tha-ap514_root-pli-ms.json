{
  "tha-ap514:0.1": "Therāpadāna",
  "tha-ap514:0.2": "Phaladāyakavagga",
  "tha-ap514:0.3": "2 Kapitthaphaladāyakattheraapadāna",
  "tha-ap514:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap514:1.2": "Āhutīnaṃ paṭiggahaṃ;",
  "tha-ap514:1.3": "Rathiyaṃ paṭipajjantaṃ,",
  "tha-ap514:1.4": "Kapitthaṃ adadiṃ phalaṃ.",
  "tha-ap514:2.1": "Ekanavutito kappe,",
  "tha-ap514:2.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap514:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap514:2.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap514:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap514:3.2": "…pe…",
  "tha-ap514:3.3": "viharāmi anāsavo.",
  "tha-ap514:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap514:4.2": "…pe…",
  "tha-ap514:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap514:5.1": "Paṭisambhidā catasso,",
  "tha-ap514:5.2": "…pe…",
  "tha-ap514:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap514:6.1": "Itthaṃ sudaṃ āyasmā kapitthaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap514:7.1": "Kapitthaphaladāyakattherassāpadānaṃ dutiyaṃ."
}