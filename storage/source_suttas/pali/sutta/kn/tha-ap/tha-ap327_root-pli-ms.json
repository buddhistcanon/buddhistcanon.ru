{
  "tha-ap327:0.1": "Therāpadāna",
  "tha-ap327:0.2": "Umāpupphiyavagga",
  "tha-ap327:0.3": "5. Nimittasaññakattheraapadāna",
  "tha-ap327:1.1": "“Candabhāgānadītīre,",
  "tha-ap327:1.2": "vasāmi assame ahaṃ;",
  "tha-ap327:1.3": "Suvaṇṇamigamaddakkhiṃ,",
  "tha-ap327:1.4": "carantaṃ vipine ahaṃ.",
  "tha-ap327:2.1": "Mige cittaṃ pasādetvā,",
  "tha-ap327:2.2": "lokajeṭṭhaṃ anussariṃ;",
  "tha-ap327:2.3": "Tena cittappasādena,",
  "tha-ap327:2.4": "aññe buddhe anussariṃ.",
  "tha-ap327:3.1": "Abbhatītā ca ye buddhā,",
  "tha-ap327:3.2": "vattamānā anāgatā;",
  "tha-ap327:3.3": "Evamevaṃ virocanti,",
  "tha-ap327:3.4": "migarājāva te tayo.",
  "tha-ap327:4.1": "Catunnavutito kappe,",
  "tha-ap327:4.2": "yaṃ saññamalabhiṃ tadā;",
  "tha-ap327:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap327:4.4": "buddhasaññāyidaṃ phalaṃ.",
  "tha-ap327:5.1": "Sattavīse ito kappe,",
  "tha-ap327:5.2": "eko āsiṃ mahīpati;",
  "tha-ap327:5.3": "Araññasattho nāmena,",
  "tha-ap327:5.4": "cakkavattī mahabbalo.",
  "tha-ap327:6.1": "Paṭisambhidā catasso,",
  "tha-ap327:6.2": "…pe…",
  "tha-ap327:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap327:7.1": "Itthaṃ sudaṃ āyasmā nimittasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap327:8.1": "Nimittasaññakattherassāpadānaṃ pañcamaṃ."
}