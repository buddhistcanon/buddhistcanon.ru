{
  "tha-ap527:0.1": "Therāpadāna",
  "tha-ap527:0.2": "Tiṇadāyakavagga",
  "tha-ap527:0.3": "5. Supaṭadāyakattheraapadāna",
  "tha-ap527:1.1": "“Divāvihārā nikkhanto,",
  "tha-ap527:1.2": "vipassī lokanāyako;",
  "tha-ap527:1.3": "Lahuṃ supaṭakaṃ datvā,",
  "tha-ap527:1.4": "kappaṃ saggamhi modahaṃ.",
  "tha-ap527:2.1": "Ekanavutito kappe,",
  "tha-ap527:2.2": "supaṭakamadāsahaṃ;",
  "tha-ap527:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap527:2.4": "supaṭassa idaṃ phalaṃ.",
  "tha-ap527:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap527:3.2": "…pe…",
  "tha-ap527:3.3": "viharāmi anāsavo.",
  "tha-ap527:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap527:4.2": "…pe…",
  "tha-ap527:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap527:5.1": "Paṭisambhidā catasso,",
  "tha-ap527:5.2": "…pe…",
  "tha-ap527:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap527:6.1": "Itthaṃ sudaṃ āyasmā supaṭadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap527:7.1": "Supaṭadāyakattherassāpadānaṃ pañcamaṃ."
}