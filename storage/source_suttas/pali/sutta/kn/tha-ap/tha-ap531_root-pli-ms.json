{
  "tha-ap531:0.1": "Therāpadāna",
  "tha-ap531:0.2": "Tiṇadāyakavagga",
  "tha-ap531:0.3": "9. Āmaṇḍaphaladāyakattheraapadāna",
  "tha-ap531:1.1": "“Padumuttaro nāma jino,",
  "tha-ap531:1.2": "sabbadhammāna pāragū;",
  "tha-ap531:1.3": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap531:1.4": "caṅkamī lokanāyako.",
  "tha-ap531:2.1": "Khāribhāraṃ gahetvāna,",
  "tha-ap531:2.2": "āharanto phalaṃ tadā;",
  "tha-ap531:2.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap531:2.4": "caṅkamantaṃ mahāmuniṃ.",
  "tha-ap531:3.1": "Pasannacitto sumano,",
  "tha-ap531:3.2": "sire katvāna añjaliṃ;",
  "tha-ap531:3.3": "Sambuddhaṃ abhivādetvā,",
  "tha-ap531:3.4": "āmaṇḍamadadiṃ phalaṃ.",
  "tha-ap531:4.1": "Satasahassito kappe,",
  "tha-ap531:4.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap531:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap531:4.4": "āmaṇḍassa idaṃ phalaṃ.",
  "tha-ap531:5.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap531:5.2": "…pe…",
  "tha-ap531:5.3": "viharāmi anāsavo.",
  "tha-ap531:6.1": "Svāgataṃ vata me āsi,",
  "tha-ap531:6.2": "…pe…",
  "tha-ap531:6.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap531:7.1": "Paṭisambhidā catasso,",
  "tha-ap531:7.2": "…pe…",
  "tha-ap531:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap531:8.1": "Itthaṃ sudaṃ āyasmā āmaṇḍaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap531:9.1": "Āmaṇḍaphaladāyakattherassāpadānaṃ navamaṃ."
}