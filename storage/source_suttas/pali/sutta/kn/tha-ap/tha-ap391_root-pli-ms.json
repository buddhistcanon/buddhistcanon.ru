{
  "tha-ap391:0.1": "Therāpadāna",
  "tha-ap391:0.2": "Avaṭaphalavagga",
  "tha-ap391:0.3": "9 Soṇakoṭivīsattheraapadāna",
  "tha-ap391:1.1": "“Vipassino pāvacane,",
  "tha-ap391:1.2": "ekaṃ leṇaṃ mayā kataṃ;",
  "tha-ap391:1.3": "Cātuddisassa saṃghassa,",
  "tha-ap391:1.4": "bandhumārājadhāniyā.",
  "tha-ap391:2.1": "Dussehi bhūmiṃ leṇassa,",
  "tha-ap391:2.2": "santharitvā pariccajiṃ;",
  "tha-ap391:2.3": "Udaggacitto sumano,",
  "tha-ap391:2.4": "akāsiṃ paṇidhiṃ tadā.",
  "tha-ap391:3.1": "Ārādhayeyyaṃ sambuddhaṃ,",
  "tha-ap391:3.2": "pabbajjañca labheyyahaṃ;",
  "tha-ap391:3.3": "Anuttarañca nibbānaṃ,",
  "tha-ap391:3.4": "phuseyyaṃ santimuttamaṃ.",
  "tha-ap391:4.1": "Teneva sukkamūlena,",
  "tha-ap391:4.2": "kappe navuti saṃsariṃ;",
  "tha-ap391:4.3": "Devabhūto manusso ca,",
  "tha-ap391:4.4": "katapuñño virocahaṃ.",
  "tha-ap391:5.1": "Tato kammāvasesena,",
  "tha-ap391:5.2": "idha pacchimake bhave;",
  "tha-ap391:5.3": "Campāyaṃ aggaseṭṭhissa,",
  "tha-ap391:5.4": "jātomhi ekaputtako.",
  "tha-ap391:6.1": "Jātamattassa me sutvā,",
  "tha-ap391:6.2": "pitu chando ayaṃ ahu;",
  "tha-ap391:6.3": "Dadāmahaṃ kumārassa,",
  "tha-ap391:6.4": "vīsakoṭī anūnakā.",
  "tha-ap391:7.1": "Caturaṅgulā ca me lomā,",
  "tha-ap391:7.2": "jātā pādatale ubho;",
  "tha-ap391:7.3": "Sukhumā mudusamphassā,",
  "tha-ap391:7.4": "tūlāpicusamā subhā.",
  "tha-ap391:8.1": "Atītā navuti kappā,",
  "tha-ap391:8.2": "ayaṃ eko ca uttari;",
  "tha-ap391:8.3": "Nābhijānāmi nikkhitte,",
  "tha-ap391:8.4": "pāde bhūmyā asanthate.",
  "tha-ap391:9.1": "Ārādhito me sambuddho,",
  "tha-ap391:9.2": "pabbajiṃ anagāriyaṃ;",
  "tha-ap391:9.3": "Arahattañca me pattaṃ,",
  "tha-ap391:9.4": "sītibhūtomhi nibbuto.",
  "tha-ap391:10.1": "Aggo āraddhavīriyānaṃ,",
  "tha-ap391:10.2": "niddiṭṭho sabbadassinā;",
  "tha-ap391:10.3": "Khīṇāsavomhi arahā,",
  "tha-ap391:10.4": "chaḷabhiñño mahiddhiko.",
  "tha-ap391:11.1": "Ekanavutito kappe,",
  "tha-ap391:11.2": "yaṃ dānamadadiṃ tadā;",
  "tha-ap391:11.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap391:11.4": "leṇadānassidaṃ phalaṃ.",
  "tha-ap391:12.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap391:12.2": "…pe…",
  "tha-ap391:12.3": "viharāmi anāsavo.",
  "tha-ap391:13.1": "Svāgataṃ vata me āsi,",
  "tha-ap391:13.2": "…pe…",
  "tha-ap391:13.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap391:14.1": "Paṭisambhidā catasso,",
  "tha-ap391:14.2": "…pe…",
  "tha-ap391:14.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap391:15.1": "Thero koṭivīso soṇo,",
  "tha-ap391:15.2": "bhikkhusaṃghassa aggato;",
  "tha-ap391:15.3": "Pañhaṃ puṭṭho viyākāsi,",
  "tha-ap391:15.4": "anotatte mahāsareti.",
  "tha-ap391:16.1": "Itthaṃ sudaṃ āyasmā soṇo koṭivīso thero imā gāthāyo abhāsitthāti.",
  "tha-ap391:17.1": "Soṇakoṭivīsattherassāpadānaṃ navamaṃ."
}