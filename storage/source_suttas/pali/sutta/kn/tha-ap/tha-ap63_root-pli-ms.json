{
  "tha-ap63:0.1": "Therāpadāna",
  "tha-ap63:0.2": "Sakacintaniyavagga",
  "tha-ap63:0.3": "1 Sakacintaniyattheraapadāna",
  "tha-ap63:1.1": "“Pavanaṃ kānanaṃ disvā,",
  "tha-ap63:1.2": "appasaddamanāvilaṃ;",
  "tha-ap63:1.3": "Isīnaṃ anuciṇṇaṃva,",
  "tha-ap63:1.4": "āhutīnaṃ paṭiggahaṃ.",
  "tha-ap63:2.1": "Thūpaṃ katvāna pulinaṃ,",
  "tha-ap63:2.2": "nānāpupphaṃ samokiriṃ;",
  "tha-ap63:2.3": "Sammukhā viya sambuddhaṃ,",
  "tha-ap63:2.4": "nimmitaṃ abhivandahaṃ.",
  "tha-ap63:3.1": "Sattaratanasampanno,",
  "tha-ap63:3.2": "rājā raṭṭhamhi issaro;",
  "tha-ap63:3.3": "Sakakammābhiraddhohaṃ,",
  "tha-ap63:3.4": "pupphapūjāyidaṃ phalaṃ.",
  "tha-ap63:4.1": "Ekanavutito kappe,",
  "tha-ap63:4.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap63:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap63:4.4": "pupphapūjāyidaṃ phalaṃ.",
  "tha-ap63:5.1": "Asītikappenantayaso,",
  "tha-ap63:5.2": "cakkavattī ahosahaṃ;",
  "tha-ap63:5.3": "Sattaratanasampanno,",
  "tha-ap63:5.4": "catudīpamhi issaro.",
  "tha-ap63:6.1": "Paṭisambhidā catasso,",
  "tha-ap63:6.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap63:6.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap63:6.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap63:7.1": "Itthaṃ sudaṃ āyasmā sakacintaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap63:8.1": "Sakacintaniyattherassāpadānaṃ paṭhamaṃ."
}