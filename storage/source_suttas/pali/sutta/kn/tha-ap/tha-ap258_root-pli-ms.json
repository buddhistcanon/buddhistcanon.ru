{
  "tha-ap258:0.1": "Therāpadāna",
  "tha-ap258:0.2": "Thomakavagga",
  "tha-ap258:0.3": "6. Upāhanadāyakattheraapadāna",
  "tha-ap258:1.1": "“Ahosiṃ candano nāma,",
  "tha-ap258:1.2": "sambuddhassatrajo tadā;",
  "tha-ap258:1.3": "Ekopāhano mayā dinno,",
  "tha-ap258:1.4": "bodhiṃ sampajja me tuvaṃ.",
  "tha-ap258:2.1": "Ekanavutito kappe,",
  "tha-ap258:2.2": "yaṃ pānadhiṃ dadiṃ tadā;",
  "tha-ap258:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap258:2.4": "upāhanassidaṃ phalaṃ.",
  "tha-ap258:3.1": "Paṭisambhidā catasso,",
  "tha-ap258:3.2": "…pe…",
  "tha-ap258:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap258:4.1": "Itthaṃ sudaṃ āyasmā upāhanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap258:5.1": "Upāhanadāyakattherassāpadānaṃ chaṭṭhaṃ."
}