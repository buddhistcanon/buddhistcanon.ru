{
  "tha-ap263:0.1": "Therāpadāna",
  "tha-ap263:0.2": "Padumukkhipavagga",
  "tha-ap263:0.3": "1. Ākāsukkhipiyattheraapadāna",
  "tha-ap263:1.1": "“Suvaṇṇavaṇṇaṃ siddhatthaṃ,",
  "tha-ap263:1.2": "Gacchantaṃ antarāpaṇe;",
  "tha-ap263:1.3": "Jalajagge duve gayha,",
  "tha-ap263:1.4": "Upāgacchiṃ narāsabhaṃ.",
  "tha-ap263:2.1": "Ekañca pupphaṃ pādesu,",
  "tha-ap263:2.2": "buddhaseṭṭhassa nikkhipiṃ;",
  "tha-ap263:2.3": "Ekañca pupphaṃ paggayha,",
  "tha-ap263:2.4": "ākāse ukkhipiṃ ahaṃ.",
  "tha-ap263:3.1": "Catunnavutito kappe,",
  "tha-ap263:3.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap263:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap263:3.4": "pupphadānassidaṃ phalaṃ.",
  "tha-ap263:4.1": "Ito chattiṃsakappamhi,",
  "tha-ap263:4.2": "eko āsiṃ mahīpati;",
  "tha-ap263:4.3": "Antalikkhakaro nāma,",
  "tha-ap263:4.4": "cakkavattī mahabbalo.",
  "tha-ap263:5.1": "Paṭisambhidā catasso,",
  "tha-ap263:5.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap263:5.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap263:5.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap263:6.1": "Itthaṃ sudaṃ āyasmā ākāsukkhipiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap263:7.1": "Ākāsukkhipiyattherassāpadānaṃ paṭhamaṃ."
}