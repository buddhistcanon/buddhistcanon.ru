{
  "tha-ap513:0.1": "Therāpadāna",
  "tha-ap513:0.2": "Phaladāyakavagga",
  "tha-ap513:0.3": "1 Kurañciyaphaladāyakattheraapadāna",
  "tha-ap513:1.1": "“Migaluddo pure āsiṃ,",
  "tha-ap513:1.2": "vipine vicaraṃ ahaṃ;",
  "tha-ap513:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap513:1.4": "sabbadhammāna pāraguṃ.",
  "tha-ap513:2.1": "Kurañciyaphalaṃ gayha,",
  "tha-ap513:2.2": "buddhaseṭṭhassadāsahaṃ;",
  "tha-ap513:2.3": "Puññakkhettassa tādino,",
  "tha-ap513:2.4": "pasanno sehi pāṇibhi.",
  "tha-ap513:3.1": "Ekatiṃse ito kappe,",
  "tha-ap513:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap513:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap513:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap513:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap513:4.2": "bhavā sabbe samūhatā;",
  "tha-ap513:4.3": "Nāgova bandhanaṃ chetvā,",
  "tha-ap513:4.4": "viharāmi anāsavo.",
  "tha-ap513:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap513:5.2": "buddhaseṭṭhassa santike;",
  "tha-ap513:5.3": "Tisso vijjā anuppattā,",
  "tha-ap513:5.4": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap513:6.1": "Paṭisambhidā catasso,",
  "tha-ap513:6.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap513:6.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap513:6.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap513:7.1": "Itthaṃ sudaṃ āyasmā kurañciyaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap513:8.1": "Kurañciyaphaladāyakattherassāpadānaṃ paṭhamaṃ."
}