{
  "tha-ap477:0.1": "Therāpadāna",
  "tha-ap477:0.2": "Naḷamālivagga",
  "tha-ap477:0.3": "5. Kummāsadāyakattheraapadāna",
  "tha-ap477:1.1": "“Esanāya carantassa,",
  "tha-ap477:1.2": "vipassissa mahesino;",
  "tha-ap477:1.3": "Rittakaṃ pattaṃ disvāna,",
  "tha-ap477:1.4": "kummāsaṃ pūrayiṃ ahaṃ.",
  "tha-ap477:2.1": "Ekanavutito kappe,",
  "tha-ap477:2.2": "yaṃ bhikkhaṃ adadiṃ tadā;",
  "tha-ap477:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap477:2.4": "kummāsassa idaṃ phalaṃ.",
  "tha-ap477:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap477:3.2": "…pe…",
  "tha-ap477:3.3": "viharāmi anāsavo.",
  "tha-ap477:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap477:4.2": "…pe…",
  "tha-ap477:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap477:5.1": "Paṭisambhidā catasso,",
  "tha-ap477:5.2": "…pe…",
  "tha-ap477:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap477:6.1": "Itthaṃ sudaṃ āyasmā kummāsadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap477:7.1": "Kummāsadāyakattherassāpadānaṃ pañcamaṃ."
}