{
  "tha-ap483:0.1": "Therāpadāna",
  "tha-ap483:0.2": "Paṃsukūlavagga",
  "tha-ap483:0.3": "1. Paṃsukūlasaññakattheraapadāna",
  "tha-ap483:1.1": "“Tisso nāmāsi bhagavā,",
  "tha-ap483:1.2": "sayambhū aggapuggalo;",
  "tha-ap483:1.3": "Paṃsukūlaṃ ṭhapetvāna,",
  "tha-ap483:1.4": "vihāraṃ pāvisī jino.",
  "tha-ap483:2.1": "Vinataṃ dhanumādāya,",
  "tha-ap483:2.2": "bhakkhatthāya cariṃ ahaṃ;",
  "tha-ap483:2.3": "Maṇḍalaggaṃ gahetvāna,",
  "tha-ap483:2.4": "kānanaṃ pāvisiṃ ahaṃ.",
  "tha-ap483:3.1": "Tatthaddasaṃ paṃsukūlaṃ,",
  "tha-ap483:3.2": "dumagge laggitaṃ tadā;",
  "tha-ap483:3.3": "Cāpaṃ tattheva nikkhippa,",
  "tha-ap483:3.4": "sire katvāna añjaliṃ.",
  "tha-ap483:4.1": "Pasannacitto sumano,",
  "tha-ap483:4.2": "vipulāya ca pītiyā;",
  "tha-ap483:4.3": "Buddhaseṭṭhaṃ saritvāna,",
  "tha-ap483:4.4": "paṃsukūlaṃ avandahaṃ.",
  "tha-ap483:5.1": "Dvenavute ito kappe,",
  "tha-ap483:5.2": "paṃsukūlamavandahaṃ;",
  "tha-ap483:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap483:5.4": "vandanāya idaṃ phalaṃ.",
  "tha-ap483:6.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap483:6.2": "bhavā sabbe samūhatā;",
  "tha-ap483:6.3": "Nāgova bandhanaṃ chetvā,",
  "tha-ap483:6.4": "viharāmi anāsavo.",
  "tha-ap483:7.1": "Svāgataṃ vata me āsi,",
  "tha-ap483:7.2": "mama buddhassa santike;",
  "tha-ap483:7.3": "Tisso vijjā anuppattā,",
  "tha-ap483:7.4": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap483:8.1": "Paṭisambhidā catasso,",
  "tha-ap483:8.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap483:8.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap483:8.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap483:9.1": "Itthaṃ sudaṃ āyasmā paṃsukūlasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap483:10.1": "Paṃsukūlasaññakattherassāpadānaṃ paṭhamaṃ."
}