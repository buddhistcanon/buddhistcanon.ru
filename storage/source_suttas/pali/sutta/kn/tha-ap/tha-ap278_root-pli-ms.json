{
  "tha-ap278:0.1": "Therāpadāna",
  "tha-ap278:0.2": "Suvaṇṇabibbohanavagga",
  "tha-ap278:0.3": "6 Potthakadāyakattheraapadāna",
  "tha-ap278:1.1": "“Satthāraṃ dhammamārabbha,",
  "tha-ap278:1.2": "saṃghañcāpi mahesinaṃ;",
  "tha-ap278:1.3": "Potthadānaṃ mayā dinnaṃ,",
  "tha-ap278:1.4": "dakkhiṇeyye anuttare.",
  "tha-ap278:2.1": "Ekanavutito kappe,",
  "tha-ap278:2.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap278:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap278:2.4": "potthadānassidaṃ phalaṃ.",
  "tha-ap278:3.1": "Paṭisambhidā catasso,",
  "tha-ap278:3.2": "…pe…",
  "tha-ap278:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap278:4.1": "Itthaṃ sudaṃ āyasmā potthakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap278:5.1": "Potthakadāyakattherassāpadānaṃ chaṭṭhaṃ."
}