{
  "tha-ap445:0.1": "Therāpadāna",
  "tha-ap445:0.2": "Vibhītakavagga",
  "tha-ap445:0.3": "3 Billiyattheraapadāna",
  "tha-ap445:1.1": "“Candabhāgānadītīre,",
  "tha-ap445:1.2": "assamo sukato mama;",
  "tha-ap445:1.3": "Billarukkhehi ākiṇṇo,",
  "tha-ap445:1.4": "nānādumanisevito.",
  "tha-ap445:2.1": "Sugandhaṃ beluvaṃ disvā,",
  "tha-ap445:2.2": "buddhaseṭṭhamanussariṃ;",
  "tha-ap445:2.3": "Khāribhāraṃ pūrayitvā,",
  "tha-ap445:2.4": "tuṭṭho saṃviggamānaso.",
  "tha-ap445:3.1": "Kakusandhaṃ upāgamma,",
  "tha-ap445:3.2": "billapakkamadāsahaṃ;",
  "tha-ap445:3.3": "Puññakkhettassa vīrassa,",
  "tha-ap445:3.4": "vippasannena cetasā.",
  "tha-ap445:4.1": "Imasmiṃyeva kappasmiṃ,",
  "tha-ap445:4.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap445:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap445:4.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap445:5.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap445:5.2": "…pe…",
  "tha-ap445:5.3": "viharāmi anāsavo.",
  "tha-ap445:6.1": "Svāgataṃ vata me āsi,",
  "tha-ap445:6.2": "…pe…",
  "tha-ap445:6.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap445:7.1": "Paṭisambhidā catasso,",
  "tha-ap445:7.2": "…pe…",
  "tha-ap445:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap445:8.1": "Itthaṃ sudaṃ āyasmā billiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap445:9.1": "Billiyattherassāpadānaṃ tatiyaṃ."
}