{
  "tha-ap469:0.1": "Therāpadāna",
  "tha-ap469:0.2": "Sālakusumiyavagga",
  "tha-ap469:0.3": "7 Labujaphaladāyakattheraapadāna",
  "tha-ap469:1.1": "“Nagare bandhumatiyā,",
  "tha-ap469:1.2": "ārāmiko ahaṃ tadā;",
  "tha-ap469:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap469:1.4": "gacchantaṃ anilañjase.",
  "tha-ap469:2.1": "Labujaṃ phalamādāya,",
  "tha-ap469:2.2": "buddhaseṭṭhassadāsahaṃ;",
  "tha-ap469:2.3": "Ākāseva ṭhito santo,",
  "tha-ap469:2.4": "paṭiggaṇhi mahāyaso.",
  "tha-ap469:3.1": "Vittisañjanano mayhaṃ,",
  "tha-ap469:3.2": "diṭṭhadhammasukhāvaho;",
  "tha-ap469:3.3": "Phalaṃ buddhassa datvāna,",
  "tha-ap469:3.4": "vippasannena cetasā.",
  "tha-ap469:4.1": "Adhigañchiṃ tadā pītiṃ,",
  "tha-ap469:4.2": "vipulaṃ sukhamuttamaṃ;",
  "tha-ap469:4.3": "Uppajjateva ratanaṃ,",
  "tha-ap469:4.4": "nibbattassa tahiṃ tahiṃ.",
  "tha-ap469:5.1": "Ekanavutito kappe,",
  "tha-ap469:5.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap469:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap469:5.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap469:6.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap469:6.2": "…pe…",
  "tha-ap469:6.3": "viharāmi anāsavo.",
  "tha-ap469:7.1": "Svāgataṃ vata me āsi,",
  "tha-ap469:7.2": "…pe…",
  "tha-ap469:7.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap469:8.1": "Paṭisambhidā catasso,",
  "tha-ap469:8.2": "…pe…",
  "tha-ap469:8.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap469:9.1": "Itthaṃ sudaṃ āyasmā labujaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap469:10.1": "Labujaphaladāyakattherassāpadānaṃ sattamaṃ."
}