{
  "tha-ap168:0.1": "Therāpadāna",
  "tha-ap168:0.2": "Supāricariyavagga",
  "tha-ap168:0.3": "6. Sappidāyakattheraapadāna",
  "tha-ap168:1.1": "“Phusso nāmāsi bhagavā,",
  "tha-ap168:1.2": "āhutīnaṃ paṭiggaho;",
  "tha-ap168:1.3": "Gacchate vīthiyaṃ vīro,",
  "tha-ap168:1.4": "nibbāpento mahājanaṃ.",
  "tha-ap168:2.1": "Anupubbena bhagavā,",
  "tha-ap168:2.2": "āgacchi mama santikaṃ;",
  "tha-ap168:2.3": "Tato taṃ pattaṃ paggayha,",
  "tha-ap168:2.4": "sappitelamadāsahaṃ.",
  "tha-ap168:3.1": "Dvenavute ito kappe,",
  "tha-ap168:3.2": "yaṃ sappimadadiṃ tadā;",
  "tha-ap168:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap168:3.4": "sappidānassidaṃ phalaṃ.",
  "tha-ap168:4.1": "Chappaññāse ito kappe,",
  "tha-ap168:4.2": "eko āsi samodako;",
  "tha-ap168:4.3": "Sattaratanasampanno,",
  "tha-ap168:4.4": "cakkavattī mahabbalo.",
  "tha-ap168:5.1": "Paṭisambhidā catasso,",
  "tha-ap168:5.2": "…pe…",
  "tha-ap168:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap168:6.1": "Itthaṃ sudaṃ āyasmā sappidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap168:7.1": "Sappidāyakattherassāpadānaṃ chaṭṭhaṃ."
}