{
  "tha-ap389:0.1": "Therāpadāna",
  "tha-ap389:0.2": "Avaṭaphalavagga",
  "tha-ap389:0.3": "7. Kadaliphaladāyakattheraapadāna",
  "tha-ap389:1.1": "“Kaṇikāraṃva jalitaṃ,",
  "tha-ap389:1.2": "puṇṇamāyeva candimaṃ;",
  "tha-ap389:1.3": "Jalantaṃ dīparukkhaṃva,",
  "tha-ap389:1.4": "addasaṃ lokanāyakaṃ.",
  "tha-ap389:2.1": "Kadaliphalaṃ paggayha,",
  "tha-ap389:2.2": "adāsiṃ satthuno ahaṃ;",
  "tha-ap389:2.3": "Pasannacitto sumano,",
  "tha-ap389:2.4": "vanditvāna apakkamiṃ.",
  "tha-ap389:3.1": "Ekattiṃse ito kappe,",
  "tha-ap389:3.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap389:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap389:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap389:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap389:4.2": "…pe…",
  "tha-ap389:4.3": "viharāmi anāsavo.",
  "tha-ap389:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap389:5.2": "…pe…",
  "tha-ap389:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap389:6.1": "Paṭisambhidā catasso,",
  "tha-ap389:6.2": "…pe…",
  "tha-ap389:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap389:7.1": "Itthaṃ sudaṃ āyasmā kadaliphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap389:8.1": "Kadaliphaladāyakattherassāpadānaṃ sattamaṃ."
}