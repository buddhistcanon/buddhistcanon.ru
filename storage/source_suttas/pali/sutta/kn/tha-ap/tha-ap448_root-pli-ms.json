{
  "tha-ap448:0.1": "Therāpadāna",
  "tha-ap448:0.2": "Vibhītakavagga",
  "tha-ap448:0.3": "6. Ambāṭakiyattheraapadāna",
  "tha-ap448:1.1": "“Supupphitaṃ sālavanaṃ,",
  "tha-ap448:1.2": "ogayha vessabhū muni;",
  "tha-ap448:1.3": "Nisīdi giriduggesu,",
  "tha-ap448:1.4": "abhijātova kesarī.",
  "tha-ap448:2.1": "Pasannacitto sumano,",
  "tha-ap448:2.2": "ambāṭakamapūjayiṃ;",
  "tha-ap448:2.3": "Puññakkhettaṃ anuttaraṃ,",
  "tha-ap448:2.4": "pasanno sehi pāṇibhi.",
  "tha-ap448:3.1": "Ekatiṃse ito kappe,",
  "tha-ap448:3.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap448:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap448:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap448:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap448:4.2": "…pe…",
  "tha-ap448:4.3": "viharāmi anāsavo.",
  "tha-ap448:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap448:5.2": "…pe…",
  "tha-ap448:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap448:6.1": "Paṭisambhidā catasso,",
  "tha-ap448:6.2": "…pe…",
  "tha-ap448:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap448:7.1": "Itthaṃ sudaṃ āyasmā ambāṭakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap448:8.1": "Ambāṭakiyattherassāpadānaṃ chaṭṭhaṃ."
}