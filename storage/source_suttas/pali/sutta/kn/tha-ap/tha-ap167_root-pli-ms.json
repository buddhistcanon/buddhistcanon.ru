{
  "tha-ap167:0.1": "Therāpadāna",
  "tha-ap167:0.2": "Supāricariyavagga",
  "tha-ap167:0.3": "5 Kaṇikārachattiyattheraapadāna",
  "tha-ap167:1.1": "“Vessabhū nāma sambuddho,",
  "tha-ap167:1.2": "lokajeṭṭho narāsabho;",
  "tha-ap167:1.3": "Divāvihārāya muni,",
  "tha-ap167:1.4": "ogāhayi mahāvanaṃ.",
  "tha-ap167:2.1": "Kaṇikāraṃ ocinitvā,",
  "tha-ap167:2.2": "chattaṃ katvānahaṃ tadā;",
  "tha-ap167:2.3": "Pupphacchadanaṃ katvāna,",
  "tha-ap167:2.4": "buddhassa abhiropayiṃ.",
  "tha-ap167:3.1": "Ekattiṃse ito kappe,",
  "tha-ap167:3.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap167:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap167:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap167:4.1": "Ito vīsatikappamhi,",
  "tha-ap167:4.2": "soṇṇābhā aṭṭha khattiyā;",
  "tha-ap167:4.3": "Sattaratanasampannā,",
  "tha-ap167:4.4": "cakkavattī mahabbalā.",
  "tha-ap167:5.1": "Paṭisambhidā catasso,",
  "tha-ap167:5.2": "…pe…",
  "tha-ap167:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap167:6.1": "Itthaṃ sudaṃ āyasmā kaṇikārachattiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap167:7.1": "Kaṇikārachattiyattherassāpadānaṃ pañcamaṃ."
}