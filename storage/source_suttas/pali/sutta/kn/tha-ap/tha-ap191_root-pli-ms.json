{
  "tha-ap191:0.1": "Therāpadāna",
  "tha-ap191:0.2": "Kuṭajapupphiyavagga",
  "tha-ap191:0.3": "9. Ekacāriyattheraapadāna",
  "tha-ap191:1.1": "“Tāvatiṃsesu devesu,",
  "tha-ap191:1.2": "mahāghoso tadā ahu;",
  "tha-ap191:1.3": "Buddho ca loke nibbāti,",
  "tha-ap191:1.4": "mayañcamha sarāgino.",
  "tha-ap191:2.1": "Tesaṃ saṃvegajātānaṃ,",
  "tha-ap191:2.2": "sokasallasamaṅginaṃ;",
  "tha-ap191:2.3": "Sabalena upatthaddho,",
  "tha-ap191:2.4": "agamaṃ buddhasantikaṃ.",
  "tha-ap191:3.1": "Mandāravaṃ gahetvāna,",
  "tha-ap191:3.2": "saṅgīti abhinimmitaṃ;",
  "tha-ap191:3.3": "Parinibbutakālamhi,",
  "tha-ap191:3.4": "buddhassa abhiropayiṃ.",
  "tha-ap191:4.1": "Sabbe devānumodiṃsu,",
  "tha-ap191:4.2": "accharāyo ca me tadā;",
  "tha-ap191:4.3": "Kappānaṃ satasahassaṃ,",
  "tha-ap191:4.4": "duggatiṃ nupapajjahaṃ.",
  "tha-ap191:5.1": "Saṭṭhikappasahassamhi,",
  "tha-ap191:5.2": "ito soḷasa te janā;",
  "tha-ap191:5.3": "Mahāmallajanā nāma,",
  "tha-ap191:5.4": "cakkavattī mahabbalā.",
  "tha-ap191:6.1": "Paṭisambhidā catasso,",
  "tha-ap191:6.2": "…pe…",
  "tha-ap191:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap191:7.1": "Itthaṃ sudaṃ āyasmā ekacāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap191:8.1": "Ekacāriyattherassāpadānaṃ navamaṃ."
}