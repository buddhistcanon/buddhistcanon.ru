{
  "tha-ap356:0.1": "Therāpadāna",
  "tha-ap356:0.2": "Saddasaññakavagga",
  "tha-ap356:0.3": "4 Sakosakakoraṇḍadāyakattheraapadāna",
  "tha-ap356:1.1": "“Akkantañca padaṃ disvā,",
  "tha-ap356:1.2": "sikhino lokabandhuno;",
  "tha-ap356:1.3": "Ekaṃsaṃ ajinaṃ katvā,",
  "tha-ap356:1.4": "padaseṭṭhaṃ avandahaṃ.",
  "tha-ap356:2.1": "Koraṇḍaṃ pupphitaṃ disvā,",
  "tha-ap356:2.2": "pādapaṃ dharaṇīruhaṃ;",
  "tha-ap356:2.3": "Sakosakaṃ gahetvāna,",
  "tha-ap356:2.4": "padacakkaṃ apūjayiṃ.",
  "tha-ap356:3.1": "Ekattiṃse ito kappe,",
  "tha-ap356:3.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap356:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap356:3.4": "padapūjāyidaṃ phalaṃ.",
  "tha-ap356:4.1": "Paṭisambhidā catasso,",
  "tha-ap356:4.2": "…pe…",
  "tha-ap356:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap356:5.1": "Itthaṃ sudaṃ āyasmā sakosakakoraṇḍadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap356:6.1": "Sakosakakoraṇḍadāyakattherassāpadānaṃ catutthaṃ."
}