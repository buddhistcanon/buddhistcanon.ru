{
  "tha-ap511:0.1": "Therāpadāna",
  "tha-ap511:0.2": "Kaṇikāravagga",
  "tha-ap511:0.3": "9 Tālaphaladāyakattheraapadāna",
  "tha-ap511:1.1": "“Sataraṃsī nāma bhagavā,",
  "tha-ap511:1.2": "sayambhū aparājito;",
  "tha-ap511:1.3": "Vivekā vuṭṭhahitvāna,",
  "tha-ap511:1.4": "gocarāyābhinikkhami.",
  "tha-ap511:2.1": "Phalahattho ahaṃ disvā,",
  "tha-ap511:2.2": "upagacchiṃ narāsabhaṃ;",
  "tha-ap511:2.3": "Pasannacitto sumano,",
  "tha-ap511:2.4": "tālaphalaṃ adāsahaṃ.",
  "tha-ap511:3.1": "Catunnavutito kappe,",
  "tha-ap511:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap511:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap511:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap511:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap511:4.2": "…pe…",
  "tha-ap511:4.3": "viharāmi anāsavo.",
  "tha-ap511:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap511:5.2": "…pe…",
  "tha-ap511:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap511:6.1": "Paṭisambhidā catasso,",
  "tha-ap511:6.2": "…pe…",
  "tha-ap511:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap511:7.1": "Itthaṃ sudaṃ āyasmā tālaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap511:8.1": "Tālaphaladāyakattherassāpadānaṃ navamaṃ."
}