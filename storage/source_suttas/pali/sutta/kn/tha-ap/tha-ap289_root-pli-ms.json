{
  "tha-ap289:0.1": "Therāpadāna",
  "tha-ap289:0.2": "Paṇṇadāyakavagga",
  "tha-ap289:0.3": "7. Apadāniyattheraapadāna",
  "tha-ap289:1.1": "“Apadānaṃ sugatānaṃ,",
  "tha-ap289:1.2": "kittayiṃhaṃ mahesinaṃ;",
  "tha-ap289:1.3": "Pāde ca sirasā vandiṃ,",
  "tha-ap289:1.4": "pasanno sehi pāṇibhi.",
  "tha-ap289:2.1": "Dvenavute ito kappe,",
  "tha-ap289:2.2": "apadānaṃ pakittayiṃ;",
  "tha-ap289:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap289:2.4": "kittanāya idaṃ phalaṃ.",
  "tha-ap289:3.1": "Paṭisambhidā catasso,",
  "tha-ap289:3.2": "…pe…",
  "tha-ap289:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap289:4.1": "Itthaṃ sudaṃ āyasmā apadāniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap289:5.1": "Apadāniyattherassāpadānaṃ sattamaṃ."
}