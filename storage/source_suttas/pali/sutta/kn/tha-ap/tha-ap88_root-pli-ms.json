{
  "tha-ap88:0.1": "Therāpadāna",
  "tha-ap88:0.2": "Timiravagga",
  "tha-ap88:0.3": "6 Dutiyaraṃsisaññakattheraapadāna",
  "tha-ap88:1.1": "“Pabbate himavantamhi,",
  "tha-ap88:1.2": "vākacīradharo ahaṃ;",
  "tha-ap88:1.3": "Caṅkamañca samārūḷho,",
  "tha-ap88:1.4": "nisīdiṃ pācināmukho.",
  "tha-ap88:2.1": "Pabbate sugataṃ disvā,",
  "tha-ap88:2.2": "phussaṃ jhānarataṃ tadā;",
  "tha-ap88:2.3": "Añjaliṃ paggahetvāna,",
  "tha-ap88:2.4": "raṃsyā cittaṃ pasādayiṃ.",
  "tha-ap88:3.1": "Dvenavute ito kappe,",
  "tha-ap88:3.2": "yaṃ saññamalabhiṃ tadā;",
  "tha-ap88:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap88:3.4": "raṃsisaññāyidaṃ phalaṃ.",
  "tha-ap88:4.1": "Paṭisambhidā catasso,",
  "tha-ap88:4.2": "…pe…",
  "tha-ap88:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap88:5.1": "Itthaṃ sudaṃ āyasmā raṃsisaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap88:6.1": "Dutiyaraṃsisaññakattherassāpadānaṃ chaṭṭhaṃ."
}