{
  "tha-ap237:0.1": "Therāpadāna",
  "tha-ap237:0.2": "Udakāsanavagga",
  "tha-ap237:0.3": "5 Vedikārakattheraapadāna",
  "tha-ap237:1.1": "“Vipassino bhagavato,",
  "tha-ap237:1.2": "bodhiyā pādaputtame;",
  "tha-ap237:1.3": "Pasannacitto sumano,",
  "tha-ap237:1.4": "kāresiṃ vedikaṃ ahaṃ.",
  "tha-ap237:2.1": "Ekanavutito kappe,",
  "tha-ap237:2.2": "kāresiṃ vedikaṃ ahaṃ;",
  "tha-ap237:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap237:2.4": "vedikāya idaṃ phalaṃ.",
  "tha-ap237:3.1": "Ito ekādase kappe,",
  "tha-ap237:3.2": "ahosiṃ sūriyassamo;",
  "tha-ap237:3.3": "Sattaratanasampanno,",
  "tha-ap237:3.4": "cakkavattī mahabbalo.",
  "tha-ap237:4.1": "Paṭisambhidā catasso,",
  "tha-ap237:4.2": "…pe…",
  "tha-ap237:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap237:5.1": "Itthaṃ sudaṃ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap237:6.1": "Vedikārakattherassāpadānaṃ pañcamaṃ."
}