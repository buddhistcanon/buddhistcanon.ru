{
  "tha-ap386:0.1": "Therāpadāna",
  "tha-ap386:0.2": "Avaṭaphalavagga",
  "tha-ap386:0.3": "4 Pilakkhaphaladāyakattheraapadāna",
  "tha-ap386:1.1": "“Vanantare buddhaṃ disvā,",
  "tha-ap386:1.2": "atthadassiṃ mahāyasaṃ;",
  "tha-ap386:1.3": "Pasannacitto sumano,",
  "tha-ap386:1.4": "pilakkhassādadiṃ phalaṃ.",
  "tha-ap386:2.1": "Aṭṭhārase kappasate,",
  "tha-ap386:2.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap386:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap386:2.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap386:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap386:3.2": "…pe…",
  "tha-ap386:3.3": "viharāmi anāsavo.",
  "tha-ap386:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap386:4.2": "…pe…",
  "tha-ap386:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap386:5.1": "Paṭisambhidā catasso,",
  "tha-ap386:5.2": "…pe…",
  "tha-ap386:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap386:6.1": "Itthaṃ sudaṃ āyasmā pilakkhaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap386:7.1": "Pilakkhaphaladāyakattherassāpadānaṃ catutthaṃ."
}