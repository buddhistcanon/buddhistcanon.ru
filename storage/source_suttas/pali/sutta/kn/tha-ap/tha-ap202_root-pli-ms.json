{
  "tha-ap202:0.1": "Therāpadāna",
  "tha-ap202:0.2": "Tamālapupphiyavagga",
  "tha-ap202:0.3": "10 Yūthikapupphiyattheraapadāna",
  "tha-ap202:1.1": "“Padumuttaro nāma jino,",
  "tha-ap202:1.2": "āhutīnaṃ paṭiggaho;",
  "tha-ap202:1.3": "Pavanā nikkhamitvāna,",
  "tha-ap202:1.4": "vihāraṃ yāti cakkhumā.",
  "tha-ap202:2.1": "Ubho hatthehi paggayha,",
  "tha-ap202:2.2": "yūthikaṃ pupphamuttamaṃ;",
  "tha-ap202:2.3": "Buddhassa abhiropayiṃ,",
  "tha-ap202:2.4": "mettacittassa tādino.",
  "tha-ap202:3.1": "Tena cittappasādena,",
  "tha-ap202:3.2": "anubhotvāna sampadā;",
  "tha-ap202:3.3": "Kappānaṃ satasahassaṃ,",
  "tha-ap202:3.4": "duggatiṃ nupapajjahaṃ.",
  "tha-ap202:4.1": "Ito paññāsakappesu,",
  "tha-ap202:4.2": "eko āsiṃ janādhipo;",
  "tha-ap202:4.3": "Samittanandano nāma,",
  "tha-ap202:4.4": "cakkavattī mahabbalo.",
  "tha-ap202:5.1": "Paṭisambhidā catasso,",
  "tha-ap202:5.2": "…pe…",
  "tha-ap202:5.3": "kataṃ buddhassa sāsanaṃ”. (2265)",
  "tha-ap202:6.1": "Itthaṃ sudaṃ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap202:7.1": "Yūthikapupphiyattherassāpadānaṃ dasamaṃ.",
  "tha-ap202:7.2": "Tamālapupphiyavaggo vīsatimo.",
  "tha-ap202:8.1": "Tassuddānaṃ",
  "tha-ap202:9.1": "Tamālatiṇasanthāro,",
  "tha-ap202:9.2": "khaṇḍaphulli asokiyo;",
  "tha-ap202:9.3": "Aṅkolakī kisalayo,",
  "tha-ap202:9.4": "tinduko nelapupphiyo;",
  "tha-ap202:9.5": "Kiṃkaṇiko yūthiko ca,",
  "tha-ap202:9.6": "gāthā paññāsa aṭṭha cāti.",
  "tha-ap202:10.1": "Atha vagguddānaṃ",
  "tha-ap202:11.1": "Bhikkhādāyī parivāro,",
  "tha-ap202:11.2": "sereyyo sobhito tathā;",
  "tha-ap202:11.3": "Chattañca bandhujīvī ca,",
  "tha-ap202:11.4": "supāricariyopi ca.",
  "tha-ap202:12.1": "Kumudo kuṭajo ceva,",
  "tha-ap202:12.2": "tamāli dasamo kato;",
  "tha-ap202:12.3": "Chasatāni ca gāthāni,",
  "tha-ap202:12.4": "chasaṭṭhi ca tatuttari.",
  "tha-ap202:13.1": "Bhikkhāvaggadasakaṃ.",
  "tha-ap202:14.1": "Dutiyasatakaṃ samattaṃ."
}