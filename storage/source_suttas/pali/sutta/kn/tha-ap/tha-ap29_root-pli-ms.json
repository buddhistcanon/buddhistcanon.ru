{
  "tha-ap29:0.1": "Therāpadāna",
  "tha-ap29:0.2": "Subhūtivagga",
  "tha-ap29:0.3": "7 Pulinapūjakattheraapadāna",
  "tha-ap29:1.1": "“Vipassissa bhagavato,",
  "tha-ap29:1.2": "bodhiyā pādaputtame;",
  "tha-ap29:1.3": "Purāṇapulinaṃ hitvā,",
  "tha-ap29:1.4": "suddhapulinamākiriṃ.",
  "tha-ap29:2.1": "Ekanavutito kappe,",
  "tha-ap29:2.2": "yaṃ pulinamadāsahaṃ;",
  "tha-ap29:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap29:2.4": "pulinadānassidaṃ phalaṃ.",
  "tha-ap29:3.1": "Tiṃsatime ito kappe,",
  "tha-ap29:3.2": "rājā āsiṃ janādhibhū;",
  "tha-ap29:3.3": "Mahāpulinanāmena,",
  "tha-ap29:3.4": "cakkavattī mahabbalo.",
  "tha-ap29:4.1": "Paṭisambhidā catasso,",
  "tha-ap29:4.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap29:4.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap29:4.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap29:5.1": "Itthaṃ sudaṃ āyasmā pulinapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap29:6.1": "Pulinapūjakattherassāpadānaṃ sattamaṃ."
}