{
  "tha-ap89:0.1": "Therāpadāna",
  "tha-ap89:0.2": "Timiravagga",
  "tha-ap89:0.3": "7. Phaladāyakattheraapadāna",
  "tha-ap89:1.1": "“Pabbate himavantamhi,",
  "tha-ap89:1.2": "kharājinadharo ahaṃ;",
  "tha-ap89:1.3": "Phussaṃ jinavaraṃ disvā,",
  "tha-ap89:1.4": "phalahattho phalaṃ adaṃ.",
  "tha-ap89:2.1": "Yamahaṃ phalamadāsiṃ,",
  "tha-ap89:2.2": "vippasannena cetasā;",
  "tha-ap89:2.3": "Bhave nibbattamānamhi,",
  "tha-ap89:2.4": "phalaṃ nibbattate mama.",
  "tha-ap89:3.1": "Dvenavute ito kappe,",
  "tha-ap89:3.2": "yaṃ phalaṃ adadiṃ ahaṃ;",
  "tha-ap89:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap89:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap89:4.1": "Paṭisambhidā catasso,",
  "tha-ap89:4.2": "…pe…",
  "tha-ap89:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap89:5.1": "Itthaṃ sudaṃ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap89:6.1": "Phaladāyakattherassāpadānaṃ sattamaṃ."
}