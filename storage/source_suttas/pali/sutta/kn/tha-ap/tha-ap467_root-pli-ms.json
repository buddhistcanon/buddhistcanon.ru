{
  "tha-ap467:0.1": "Therāpadāna",
  "tha-ap467:0.2": "Sālakusumiyavagga",
  "tha-ap467:0.3": "5. Sumanatālavaṇṭiyattheraapadāna",
  "tha-ap467:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap467:1.2": "tālavaṇṭamadāsahaṃ;",
  "tha-ap467:1.3": "Sumanehi paṭicchannaṃ,",
  "tha-ap467:1.4": "dhārayāmi mahāyasaṃ.",
  "tha-ap467:2.1": "Catunnavutito kappe,",
  "tha-ap467:2.2": "tālavaṇṭamadāsahaṃ;",
  "tha-ap467:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap467:2.4": "tālavaṇṭassidaṃ phalaṃ.",
  "tha-ap467:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap467:3.2": "…pe…",
  "tha-ap467:3.3": "viharāmi anāsavo.",
  "tha-ap467:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap467:4.2": "…pe…",
  "tha-ap467:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap467:5.1": "Paṭisambhidā catasso,",
  "tha-ap467:5.2": "…pe…",
  "tha-ap467:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap467:6.1": "Itthaṃ sudaṃ āyasmā sumanatālavaṇṭiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap467:7.1": "Sumanatālavaṇṭiyattherassāpadānaṃ pañcamaṃ."
}