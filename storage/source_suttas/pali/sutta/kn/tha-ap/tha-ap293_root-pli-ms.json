{
  "tha-ap293:0.1": "Therāpadāna",
  "tha-ap293:0.2": "Citakapūjakavagga",
  "tha-ap293:0.3": "1. Citakapūjakattheraapadāna",
  "tha-ap293:1.1": "“Ajito nāma nāmena,",
  "tha-ap293:1.2": "ahosiṃ brāhmaṇo tadā;",
  "tha-ap293:1.3": "Āhutiṃ yiṭṭhukāmohaṃ,",
  "tha-ap293:1.4": "nānāpupphaṃ samānayiṃ.",
  "tha-ap293:2.1": "Jalantaṃ citakaṃ disvā,",
  "tha-ap293:2.2": "sikhino lokabandhuno;",
  "tha-ap293:2.3": "Tañca pupphaṃ samānetvā,",
  "tha-ap293:2.4": "citake okiriṃ ahaṃ.",
  "tha-ap293:3.1": "Ekattiṃse ito kappe,",
  "tha-ap293:3.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap293:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap293:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap293:4.1": "Sattavīse ito kappe,",
  "tha-ap293:4.2": "sattāsuṃ manujādhipā;",
  "tha-ap293:4.3": "Supajjalitanāmā te,",
  "tha-ap293:4.4": "cakkavattī mahabbalā.",
  "tha-ap293:5.1": "Paṭisambhidā catasso,",
  "tha-ap293:5.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap293:5.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap293:5.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap293:6.1": "Itthaṃ sudaṃ āyasmā citakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap293:7.1": "Citakapūjakattherassāpadānaṃ paṭhamaṃ."
}