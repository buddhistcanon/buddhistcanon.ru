{
  "tha-ap533:0.1": "Therāpadāna",
  "tha-ap533:0.2": "Kaccāyanavagga",
  "tha-ap533:0.3": "1 Mahākaccāyanattheraapadāna",
  "tha-ap533:1.1": "“Padumuttaro nāma jino,",
  "tha-ap533:1.2": "anejo ajitaṃ jayo;",
  "tha-ap533:1.3": "Satasahasse kappānaṃ,",
  "tha-ap533:1.4": "ito uppajji nāyako.",
  "tha-ap533:2.1": "Vīro kamalapattakkho,",
  "tha-ap533:2.2": "sasaṅkavimalānano;",
  "tha-ap533:2.3": "Kanakācalasaṅkāso,",
  "tha-ap533:2.4": "ravidittisamappabho.",
  "tha-ap533:3.1": "Sattanettamanohārī,",
  "tha-ap533:3.2": "varalakkhaṇabhūsito;",
  "tha-ap533:3.3": "Sabbavākyapathātīto,",
  "tha-ap533:3.4": "manujāmarasakkato.",
  "tha-ap533:4.1": "Sambuddho bodhayaṃ satte,",
  "tha-ap533:4.2": "vāgīso madhurassaro;",
  "tha-ap533:4.3": "Karuṇānibandhasantāno,",
  "tha-ap533:4.4": "parisāsu visārado.",
  "tha-ap533:5.1": "Deseti madhuraṃ dhammaṃ,",
  "tha-ap533:5.2": "catusaccūpasaṃhitaṃ;",
  "tha-ap533:5.3": "Nimugge mohapaṅkamhi,",
  "tha-ap533:5.4": "samuddharati pāṇine.",
  "tha-ap533:6.1": "Tadā ekacaro hutvā,",
  "tha-ap533:6.2": "tāpaso himavālayo;",
  "tha-ap533:6.3": "Nabhasā mānusaṃ lokaṃ,",
  "tha-ap533:6.4": "gacchanto jinamaddasaṃ.",
  "tha-ap533:7.1": "Upecca santikaṃ tassa,",
  "tha-ap533:7.2": "assosiṃ dhammadesanaṃ;",
  "tha-ap533:7.3": "Vaṇṇayantassa vīrassa,",
  "tha-ap533:7.4": "sāvakassa mahāguṇaṃ.",
  "tha-ap533:8.1": "‘Saṃkhittena mayā vuttaṃ,",
  "tha-ap533:8.2": "vitthārena pakāsayaṃ;",
  "tha-ap533:8.3": "Parisaṃ mañca toseti,",
  "tha-ap533:8.4": "yathā kaccāyano ayaṃ.",
  "tha-ap533:9.1": "Nāhaṃ evamidhekaccaṃ,",
  "tha-ap533:9.2": "aññaṃ passāmi sāvakaṃ;",
  "tha-ap533:9.3": "Tasmātadagge esaggo,",
  "tha-ap533:9.4": "evaṃ dhāretha bhikkhavo’.",
  "tha-ap533:10.1": "Tadāhaṃ vimhito hutvā,",
  "tha-ap533:10.2": "sutvā vākyaṃ manoramaṃ;",
  "tha-ap533:10.3": "Himavantaṃ gamitvāna,",
  "tha-ap533:10.4": "āhitvā pupphasañcayaṃ.",
  "tha-ap533:11.1": "Pūjetvā lokasaraṇaṃ,",
  "tha-ap533:11.2": "taṃ ṭhānamabhipatthayiṃ;",
  "tha-ap533:11.3": "Tadā mamāsayaṃ ñatvā,",
  "tha-ap533:11.4": "byākāsi sa raṇañjaho.",
  "tha-ap533:12.1": "‘Passathetaṃ isivaraṃ,",
  "tha-ap533:12.2": "niddhantakanakattacaṃ;",
  "tha-ap533:12.3": "Uddhaggalomaṃ pīṇaṃsaṃ,",
  "tha-ap533:12.4": "acalaṃ pañjaliṃ ṭhitaṃ.",
  "tha-ap533:13.1": "Hāsaṃ supuṇṇanayanaṃ,",
  "tha-ap533:13.2": "buddhavaṇṇagatāsayaṃ;",
  "tha-ap533:13.3": "Dhammajaṃ uggahadayaṃ,",
  "tha-ap533:13.4": "amatāsittasannibhaṃ’.",
  "tha-ap533:14.1": "Kaccānassa guṇaṃ sutvā,",
  "tha-ap533:14.2": "taṃ ṭhānaṃ patthayaṃ ṭhito;",
  "tha-ap533:14.3": "Anāgatamhi addhāne,",
  "tha-ap533:14.4": "gotamassa mahāmune.",
  "tha-ap533:15.1": "Tassa dhammesu dāyādo,",
  "tha-ap533:15.2": "oraso dhammanimmito;",
  "tha-ap533:15.3": "Kaccāno nāma nāmena,",
  "tha-ap533:15.4": "hessati satthu sāvako.",
  "tha-ap533:16.1": "Bahussuto mahāñāṇī,",
  "tha-ap533:16.2": "adhippāyavidū mune;",
  "tha-ap533:16.3": "Pāpuṇissati taṃ ṭhānaṃ,",
  "tha-ap533:16.4": "yathāyaṃ byākato mayā.",
  "tha-ap533:17.1": "Satasahassito kappe,",
  "tha-ap533:17.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap533:17.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap533:17.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap533:18.1": "Duve bhave saṃsarāmi,",
  "tha-ap533:18.2": "devatte atha mānuse;",
  "tha-ap533:18.3": "Aññaṃ gatiṃ na gacchāmi,",
  "tha-ap533:18.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap533:19.1": "Duve kule pajāyāmi,",
  "tha-ap533:19.2": "khattiye atha brāhmaṇe;",
  "tha-ap533:19.3": "Nīce kule na jāyāmi,",
  "tha-ap533:19.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap533:20.1": "Pacchime ca bhave dāni,",
  "tha-ap533:20.2": "jāto ujjeniyaṃ pure;",
  "tha-ap533:20.3": "Pajjotassa ca caṇḍassa,",
  "tha-ap533:20.4": "purohitadijādhino.",
  "tha-ap533:21.1": "Putto tiriṭivacchassa,",
  "tha-ap533:21.2": "nipuṇo vedapāragū;",
  "tha-ap533:21.3": "Mātā ca candimā nāma,",
  "tha-ap533:21.4": "kaccānohaṃ varattaco.",
  "tha-ap533:22.1": "Vīmaṃsanatthaṃ buddhassa,",
  "tha-ap533:22.2": "bhūmipālena pesito;",
  "tha-ap533:22.3": "Disvā mokkhapuradvāraṃ,",
  "tha-ap533:22.4": "nāyakaṃ guṇasañcayaṃ.",
  "tha-ap533:23.1": "Sutvā ca vimalaṃ vākyaṃ,",
  "tha-ap533:23.2": "gatipaṅkavisosanaṃ;",
  "tha-ap533:23.3": "Pāpuṇiṃ amataṃ santaṃ,",
  "tha-ap533:23.4": "sesehi saha sattahi.",
  "tha-ap533:24.1": "Adhippāyavidū jāto,",
  "tha-ap533:24.2": "sugatassa mahāmate;",
  "tha-ap533:24.3": "Ṭhapito etadagge ca,",
  "tha-ap533:24.4": "susamiddhamanoratho.",
  "tha-ap533:25.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap533:25.2": "bhavā sabbe samūhatā;",
  "tha-ap533:25.3": "Nāgova bandhanaṃ chetvā,",
  "tha-ap533:25.4": "viharāmi anāsavo.",
  "tha-ap533:26.1": "Svāgataṃ vata me āsi,",
  "tha-ap533:26.2": "mama buddhassa santike;",
  "tha-ap533:26.3": "Tisso vijjā anuppattā,",
  "tha-ap533:26.4": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap533:27.1": "Paṭisambhidā catasso,",
  "tha-ap533:27.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap533:27.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap533:27.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap533:28.1": "Itthaṃ sudaṃ āyasmā mahākaccāyano thero imā gāthāyo abhāsitthāti.",
  "tha-ap533:29.1": "Mahākaccāyanattherassāpadānaṃ paṭhamaṃ."
}