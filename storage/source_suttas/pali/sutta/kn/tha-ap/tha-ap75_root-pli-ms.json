{
  "tha-ap75:0.1": "Therāpadāna",
  "tha-ap75:0.2": "Nāgasamālavagga",
  "tha-ap75:0.3": "3 Buddhasaññakattheraapadāna",
  "tha-ap75:1.1": "“Dumagge paṃsukūlikaṃ,",
  "tha-ap75:1.2": "laggaṃ disvāna satthuno;",
  "tha-ap75:1.3": "Tato tamañjaliṃ katvā,",
  "tha-ap75:1.4": "paṃsukūlaṃ avandahaṃ.",
  "tha-ap75:2.1": "Dvenavute ito kappe,",
  "tha-ap75:2.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap75:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap75:2.4": "buddhasaññāyidaṃ phalaṃ.",
  "tha-ap75:3.1": "Ito catutthake kappe,",
  "tha-ap75:3.2": "dumasārosi khattiyo;",
  "tha-ap75:3.3": "Cāturanto vijitāvī,",
  "tha-ap75:3.4": "cakkavattī mahabbalo.",
  "tha-ap75:4.1": "Paṭisambhidā catasso,",
  "tha-ap75:4.2": "…pe…",
  "tha-ap75:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap75:5.1": "Itthaṃ sudaṃ āyasmā buddhasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap75:6.1": "Buddhasaññakattherassāpadānaṃ tatiyaṃ."
}