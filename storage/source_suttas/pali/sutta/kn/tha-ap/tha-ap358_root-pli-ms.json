{
  "tha-ap358:0.1": "Therāpadāna",
  "tha-ap358:0.2": "Saddasaññakavagga",
  "tha-ap358:0.3": "6. Ambayāgudāyakattheraapadāna",
  "tha-ap358:1.1": "“Sataraṃsī nāma sambuddho,",
  "tha-ap358:1.2": "sayambhū aparājito;",
  "tha-ap358:1.3": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap358:1.4": "bhikkhāya mamupāgami.",
  "tha-ap358:2.1": "Paccekabuddhaṃ disvāna,",
  "tha-ap358:2.2": "ambayāguṃ adāsahaṃ;",
  "tha-ap358:2.3": "Vippasannamanaṃ tassa,",
  "tha-ap358:2.4": "vippasannena cetasā.",
  "tha-ap358:3.1": "Catunnavutito kappe,",
  "tha-ap358:3.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap358:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap358:3.4": "ambayāguyidaṃ phalaṃ.",
  "tha-ap358:4.1": "Paṭisambhidā catasso,",
  "tha-ap358:4.2": "…pe…",
  "tha-ap358:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap358:5.1": "Itthaṃ sudaṃ āyasmā ambayāgudāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap358:6.1": "Ambayāgudāyakattherassāpadānaṃ chaṭṭhaṃ."
}