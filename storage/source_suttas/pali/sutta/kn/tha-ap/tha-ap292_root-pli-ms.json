{
  "tha-ap292:0.1": "Therāpadāna",
  "tha-ap292:0.2": "Paṇṇadāyakavagga",
  "tha-ap292:0.3": "10 Pubbaṅgamiyattheraapadāna",
  "tha-ap292:1.1": "“Cullāsītisahassāni,",
  "tha-ap292:1.2": "pabbajimha akiñcanā;",
  "tha-ap292:1.3": "Tesaṃ pubbaṅgamo āsiṃ,",
  "tha-ap292:1.4": "uttamatthassa pattiyā.",
  "tha-ap292:2.1": "Sarāgā sabhavā cete,",
  "tha-ap292:2.2": "vippasannamanāvilā;",
  "tha-ap292:2.3": "Upaṭṭhahiṃsu sakkaccaṃ,",
  "tha-ap292:2.4": "pasannā sehi pāṇibhi.",
  "tha-ap292:3.1": "Khīṇāsavā vantadosā,",
  "tha-ap292:3.2": "katakiccā anāsavā;",
  "tha-ap292:3.3": "Phariṃsu mettacittena,",
  "tha-ap292:3.4": "sayambhū aparājitā.",
  "tha-ap292:4.1": "Tesaṃ upaṭṭhahitvāna,",
  "tha-ap292:4.2": "sambuddhānaṃ patissato;",
  "tha-ap292:4.3": "Maraṇañca anuppatto,",
  "tha-ap292:4.4": "devattañca agamhase.",
  "tha-ap292:5.1": "Catunnavutito kappe,",
  "tha-ap292:5.2": "yaṃ sīlamanupālayiṃ;",
  "tha-ap292:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap292:5.4": "saññamassa idaṃ phalaṃ.",
  "tha-ap292:6.1": "Paṭisambhidā catasso,",
  "tha-ap292:6.2": "…pe…",
  "tha-ap292:6.3": "kataṃ buddhassa sāsanaṃ”. (2666)",
  "tha-ap292:7.1": "Itthaṃ sudaṃ āyasmā pubbaṅgamiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap292:8.1": "Pubbaṅgamiyattherassāpadānaṃ dasamaṃ.",
  "tha-ap292:9.1": "Paṇṇadāyakavaggo ekūnatiṃsatimo.",
  "tha-ap292:10.1": "Tassuddānaṃ",
  "tha-ap292:11.1": "Paṇṇaṃ phalaṃ paccuggamaṃ,",
  "tha-ap292:11.2": "ekapupphi ca maghavā;",
  "tha-ap292:11.3": "Upaṭṭhākāpadānañca,",
  "tha-ap292:11.4": "pabbajjā buddhupaṭṭhāko;",
  "tha-ap292:11.5": "Pubbaṅgamo ca gāthāyo,",
  "tha-ap292:11.6": "aṭṭhatālīsa kittitā."
}