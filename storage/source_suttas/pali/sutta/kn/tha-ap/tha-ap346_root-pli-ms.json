{
  "tha-ap346:0.1": "Therāpadāna",
  "tha-ap346:0.2": "Ekapadumiyavagga",
  "tha-ap346:0.3": "4 Tikiṅkaṇipūjakattheraapadāna",
  "tha-ap346:1.1": "“Himavantassāvidūre,",
  "tha-ap346:1.2": "bhūtagaṇo nāma pabbato;",
  "tha-ap346:1.3": "Tatthaddasaṃ paṃsukūlaṃ,",
  "tha-ap346:1.4": "dumaggamhi vilaggitaṃ.",
  "tha-ap346:2.1": "Tīṇi kiṅkaṇipupphāni,",
  "tha-ap346:2.2": "ocinitvānahaṃ tadā;",
  "tha-ap346:2.3": "Haṭṭho haṭṭhena cittena,",
  "tha-ap346:2.4": "paṃsukūlaṃ apūjayiṃ.",
  "tha-ap346:3.1": "Ekattiṃse ito kappe,",
  "tha-ap346:3.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap346:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap346:3.4": "tiṇṇaṃ pupphānidaṃ phalaṃ.",
  "tha-ap346:4.1": "Paṭisambhidā catasso,",
  "tha-ap346:4.2": "…pe…",
  "tha-ap346:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap346:5.1": "Itthaṃ sudaṃ āyasmā tikiṅkaṇipūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap346:6.1": "Tikiṅkaṇipūjakattherassāpadānaṃ catutthaṃ."
}