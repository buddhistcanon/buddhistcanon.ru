{
  "tha-ap59:0.1": "Therāpadāna",
  "tha-ap59:0.2": "Bījanivagga",
  "tha-ap59:0.3": "7. Pañcadīpakattheraapadāna",
  "tha-ap59:1.1": "“Padumuttarabuddhassa,",
  "tha-ap59:1.2": "sabbabhūtānukampino;",
  "tha-ap59:1.3": "Saddahitvāna saddhamme,",
  "tha-ap59:1.4": "ujudiṭṭhi ahosahaṃ.",
  "tha-ap59:2.1": "Padīpadānaṃ pādāsiṃ,",
  "tha-ap59:2.2": "parivāretvāna bodhiyaṃ;",
  "tha-ap59:2.3": "Saddahanto padīpāni,",
  "tha-ap59:2.4": "akariṃ tāvade ahaṃ.",
  "tha-ap59:3.1": "Yaṃ yaṃ yonupapajjāmi,",
  "tha-ap59:3.2": "devattaṃ atha mānusaṃ;",
  "tha-ap59:3.3": "Ākāse ukkaṃ dhārenti,",
  "tha-ap59:3.4": "dīpadānassidaṃ phalaṃ.",
  "tha-ap59:4.1": "Tirokuṭṭaṃ tiroselaṃ,",
  "tha-ap59:4.2": "samatiggayha pabbataṃ;",
  "tha-ap59:4.3": "Samantā yojanasataṃ,",
  "tha-ap59:4.4": "dassanaṃ anubhomahaṃ.",
  "tha-ap59:5.1": "Tena kammāvasesena,",
  "tha-ap59:5.2": "pattomhi āsavakkhayaṃ;",
  "tha-ap59:5.3": "Dhāremi antimaṃ dehaṃ,",
  "tha-ap59:5.4": "dvipadindassa sāsane.",
  "tha-ap59:6.1": "Catuttiṃse kappasate,",
  "tha-ap59:6.2": "satacakkhusanāmakā;",
  "tha-ap59:6.3": "Rājāhesuṃ mahātejā,",
  "tha-ap59:6.4": "cakkavattī mahabbalā.",
  "tha-ap59:7.1": "Paṭisambhidā catasso,",
  "tha-ap59:7.2": "…pe…",
  "tha-ap59:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap59:8.1": "Itthaṃ sudaṃ āyasmā pañcadīpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap59:9.1": "Pañcadīpakattherassāpadānaṃ sattamaṃ."
}