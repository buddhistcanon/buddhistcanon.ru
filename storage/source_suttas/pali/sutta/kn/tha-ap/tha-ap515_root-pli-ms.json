{
  "tha-ap515:0.1": "Therāpadāna",
  "tha-ap515:0.2": "Phaladāyakavagga",
  "tha-ap515:0.3": "3 Kosambaphaliyattheraapadāna",
  "tha-ap515:1.1": "“Kakudhaṃ vilasantaṃva,",
  "tha-ap515:1.2": "devadevaṃ narāsabhaṃ;",
  "tha-ap515:1.3": "Rathiyaṃ paṭipajjantaṃ,",
  "tha-ap515:1.4": "kosambaṃ adadiṃ tadā.",
  "tha-ap515:2.1": "Ekatiṃse ito kappe,",
  "tha-ap515:2.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap515:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap515:2.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap515:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap515:3.2": "…pe…",
  "tha-ap515:3.3": "viharāmi anāsavo.",
  "tha-ap515:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap515:4.2": "…pe…",
  "tha-ap515:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap515:5.1": "Paṭisambhidā catasso,",
  "tha-ap515:5.2": "…pe…",
  "tha-ap515:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap515:6.1": "Itthaṃ sudaṃ āyasmā kosambaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap515:7.1": "Kosambaphaliyattherassāpadānaṃ tatiyaṃ."
}