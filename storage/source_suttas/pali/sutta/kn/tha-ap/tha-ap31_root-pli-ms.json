{
  "tha-ap31:0.1": "Therāpadāna",
  "tha-ap31:0.2": "Subhūtivagga",
  "tha-ap31:0.3": "9. Ekañjalikattheraapadāna",
  "tha-ap31:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap31:1.2": "Gacchantaṃ antarāpaṇe;",
  "tha-ap31:1.3": "Vipassiṃ satthavāhaggaṃ,",
  "tha-ap31:1.4": "Naravaraṃ vināyakaṃ.",
  "tha-ap31:2.1": "Adantadamanaṃ tādiṃ,",
  "tha-ap31:2.2": "mahāvādiṃ mahāmatiṃ;",
  "tha-ap31:2.3": "Disvā pasanno sumano,",
  "tha-ap31:2.4": "ekañjalimakāsahaṃ.",
  "tha-ap31:3.1": "Ekanavutito kappe,",
  "tha-ap31:3.2": "yamañjaliṃ kariṃ tadā;",
  "tha-ap31:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap31:3.4": "añjalissa idaṃ phalaṃ.",
  "tha-ap31:4.1": "Paṭisambhidā catasso,",
  "tha-ap31:4.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap31:4.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap31:4.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap31:5.1": "Itthaṃ sudaṃ āyasmā ekañjaliko thero imā gāthāyo abhāsitthāti.",
  "tha-ap31:6.1": "Ekañjalikattherassāpadānaṃ navamaṃ."
}