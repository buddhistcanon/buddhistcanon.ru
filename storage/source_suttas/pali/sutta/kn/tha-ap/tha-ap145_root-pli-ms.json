{
  "tha-ap145:0.1": "Therāpadāna",
  "tha-ap145:0.2": "Chattavagga",
  "tha-ap145:0.3": "3. Vedikārakattheraapadāna",
  "tha-ap145:1.1": "“Nibbute lokanāthamhi,",
  "tha-ap145:1.2": "piyadassīnaruttame;",
  "tha-ap145:1.3": "Pasannacitto sumano,",
  "tha-ap145:1.4": "muttāvedimakāsahaṃ.",
  "tha-ap145:2.1": "Maṇīhi parivāretvā,",
  "tha-ap145:2.2": "akāsiṃ vedimuttamaṃ;",
  "tha-ap145:2.3": "Vedikāya mahaṃ katvā,",
  "tha-ap145:2.4": "tattha kālaṅkato ahaṃ.",
  "tha-ap145:3.1": "Yaṃ yaṃ yonupapajjāmi,",
  "tha-ap145:3.2": "devattaṃ atha mānusaṃ;",
  "tha-ap145:3.3": "Maṇī dhārenti ākāse,",
  "tha-ap145:3.4": "puññakammassidaṃ phalaṃ.",
  "tha-ap145:4.1": "Soḷaseto kappasate,",
  "tha-ap145:4.2": "maṇippabhāsanāmakā;",
  "tha-ap145:4.3": "Chattiṃsāsiṃsu rājāno,",
  "tha-ap145:4.4": "cakkavattī mahabbalā.",
  "tha-ap145:5.1": "Paṭisambhidā catasso,",
  "tha-ap145:5.2": "…pe…",
  "tha-ap145:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap145:6.1": "Itthaṃ sudaṃ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap145:7.1": "Vedikārakattherassāpadānaṃ tatiyaṃ."
}