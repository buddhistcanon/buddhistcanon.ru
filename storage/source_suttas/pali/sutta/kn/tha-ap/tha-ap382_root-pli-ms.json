{
  "tha-ap382:0.1": "Therāpadāna",
  "tha-ap382:0.2": "Bodhivandanavagga",
  "tha-ap382:0.3": "10. Kāsumāriphaladāyakattheraapadāna",
  "tha-ap382:1.1": "“Kaṇikāraṃva jotantaṃ,",
  "tha-ap382:1.2": "nisinnaṃ pabbatantare;",
  "tha-ap382:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap382:1.4": "lokajeṭṭhaṃ narāsabhaṃ.",
  "tha-ap382:2.1": "Pasannacitto sumano,",
  "tha-ap382:2.2": "sire katvāna añjaliṃ;",
  "tha-ap382:2.3": "Kāsumāriphalaṃ gayha,",
  "tha-ap382:2.4": "buddhaseṭṭhassadāsahaṃ.",
  "tha-ap382:3.1": "Ekattiṃse ito kappe,",
  "tha-ap382:3.2": "yaṃ phalamadadiṃ ahaṃ;",
  "tha-ap382:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap382:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap382:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap382:4.2": "…pe…",
  "tha-ap382:4.3": "viharāmi anāsavo.",
  "tha-ap382:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap382:5.2": "…pe…",
  "tha-ap382:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap382:6.1": "Paṭisambhidā catasso,",
  "tha-ap382:6.2": "…pe…",
  "tha-ap382:6.3": "kataṃ buddhassa sāsanaṃ”. (3271)",
  "tha-ap382:7.1": "Itthaṃ sudaṃ āyasmā kāsumāriphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap382:8.1": "Kāsumāriphaladāyakattherassāpadānaṃ dasamaṃ.",
  "tha-ap382:9.1": "Bodhivandanavaggo aṭṭhatiṃsatimo.",
  "tha-ap382:10.1": "Tassuddānaṃ",
  "tha-ap382:11.1": "Bodhi pāṭali uppalī,",
  "tha-ap382:11.2": "paṭṭi ca sattapaṇṇiyo;",
  "tha-ap382:11.3": "Gandhamuṭṭhi ca citako,",
  "tha-ap382:11.4": "tālaṃ sumanadāmako;",
  "tha-ap382:11.5": "Kāsumāriphalī ceva,",
  "tha-ap382:11.6": "gāthā ekūnasaṭṭhikā."
}