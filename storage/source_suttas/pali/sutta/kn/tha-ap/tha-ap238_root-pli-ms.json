{
  "tha-ap238:0.1": "Therāpadāna",
  "tha-ap238:0.2": "Udakāsanavagga",
  "tha-ap238:0.3": "6 Vaṇṇakārattheraapadāna",
  "tha-ap238:1.1": "“Nagare aruṇavatiyā,",
  "tha-ap238:1.2": "vaṇṇakāro ahaṃ tadā;",
  "tha-ap238:1.3": "Cetiye dussabhaṇḍāni,",
  "tha-ap238:1.4": "nānāvaṇṇaṃ rajesahaṃ.",
  "tha-ap238:2.1": "Ekattiṃse ito kappe,",
  "tha-ap238:2.2": "yaṃ vaṇṇaṃ rajayiṃ tadā;",
  "tha-ap238:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap238:2.4": "vaṇṇadānassidaṃ phalaṃ.",
  "tha-ap238:3.1": "Ito tevīsatikappe,",
  "tha-ap238:3.2": "vaṇṇasamasanāmako;",
  "tha-ap238:3.3": "Sattaratanasampanno,",
  "tha-ap238:3.4": "cakkavattī mahabbalo.",
  "tha-ap238:4.1": "Paṭisambhidā catasso,",
  "tha-ap238:4.2": "…pe…",
  "tha-ap238:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap238:5.1": "Itthaṃ sudaṃ āyasmā vaṇṇakāro thero imā gāthāyo abhāsitthāti.",
  "tha-ap238:6.1": "Vaṇṇakārattherassāpadānaṃ chaṭṭhaṃ."
}