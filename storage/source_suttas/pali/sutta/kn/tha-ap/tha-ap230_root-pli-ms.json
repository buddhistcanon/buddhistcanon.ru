{
  "tha-ap230:0.1": "Therāpadāna",
  "tha-ap230:0.2": "Ālambaṇadāyakavagga",
  "tha-ap230:0.3": "8. Miñjavaṭaṃsakiyattheraapadāna",
  "tha-ap230:1.1": "“Nibbute lokanāthamhi,",
  "tha-ap230:1.2": "sikhimhi vadataṃ vare;",
  "tha-ap230:1.3": "Vaṭaṃsakehi ākiṇṇaṃ,",
  "tha-ap230:1.4": "bodhipūjaṃ akāsahaṃ.",
  "tha-ap230:2.1": "Ekattiṃse ito kappe,",
  "tha-ap230:2.2": "yaṃ pūjamakariṃ tadā;",
  "tha-ap230:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap230:2.4": "bodhipūjāyidaṃ phalaṃ.",
  "tha-ap230:3.1": "Ito chabbīsatikappe,",
  "tha-ap230:3.2": "ahuṃ meghabbhanāmako;",
  "tha-ap230:3.3": "Sattaratanasampanno,",
  "tha-ap230:3.4": "cakkavattī mahabbalo.",
  "tha-ap230:4.1": "Paṭisambhidā catasso,",
  "tha-ap230:4.2": "…pe…",
  "tha-ap230:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap230:5.1": "Itthaṃ sudaṃ āyasmā miñjavaṭaṃsakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap230:6.1": "Miñjavaṭaṃsakiyattherassāpadānaṃ aṭṭhamaṃ."
}