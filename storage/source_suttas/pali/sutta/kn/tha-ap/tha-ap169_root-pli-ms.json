{
  "tha-ap169:0.1": "Therāpadāna",
  "tha-ap169:0.2": "Supāricariyavagga",
  "tha-ap169:0.3": "7 Yūthikapupphiyattheraapadāna",
  "tha-ap169:1.1": "“Candabhāgānadītīre,",
  "tha-ap169:1.2": "anusotaṃ vajāmahaṃ;",
  "tha-ap169:1.3": "Sayambhuṃ addasaṃ tattha,",
  "tha-ap169:1.4": "sālarājaṃva phullitaṃ.",
  "tha-ap169:2.1": "Pupphaṃ yūthikamādāya,",
  "tha-ap169:2.2": "upagacchiṃ mahāmuniṃ;",
  "tha-ap169:2.3": "Pasannacitto sumano,",
  "tha-ap169:2.4": "buddhassa abhiropayiṃ.",
  "tha-ap169:3.1": "Catunnavutito kappe,",
  "tha-ap169:3.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap169:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap169:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap169:4.1": "Sattasaṭṭhimhito kappe,",
  "tha-ap169:4.2": "eko sāmuddharo ahuṃ;",
  "tha-ap169:4.3": "Sattaratanasampanno,",
  "tha-ap169:4.4": "cakkavattī mahabbalo.",
  "tha-ap169:5.1": "Paṭisambhidā catasso,",
  "tha-ap169:5.2": "…pe…",
  "tha-ap169:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap169:6.1": "Itthaṃ sudaṃ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap169:7.1": "Yūthikapupphiyattherassāpadānaṃ sattamaṃ."
}