{
  "tha-ap141:0.1": "Therāpadāna",
  "tha-ap141:0.2": "Sobhitavagga",
  "tha-ap141:0.3": "9 Sālapupphadāyakattheraapadāna",
  "tha-ap141:1.1": "“Migarājā tadā āsiṃ,",
  "tha-ap141:1.2": "abhijāto sukesarī;",
  "tha-ap141:1.3": "Giriduggaṃ gavesanto,",
  "tha-ap141:1.4": "addasaṃ lokanāyakaṃ.",
  "tha-ap141:2.1": "Ayaṃ nu kho mahāvīro,",
  "tha-ap141:2.2": "nibbāpeti mahājanaṃ;",
  "tha-ap141:2.3": "Yannūnāhaṃ upāseyyaṃ,",
  "tha-ap141:2.4": "devadevaṃ narāsabhaṃ.",
  "tha-ap141:3.1": "Sākhaṃ sālassa bhañjitvā,",
  "tha-ap141:3.2": "sakosaṃ pupphamāhariṃ;",
  "tha-ap141:3.3": "Upagantvāna sambuddhaṃ,",
  "tha-ap141:3.4": "adāsiṃ pupphamuttamaṃ.",
  "tha-ap141:4.1": "Ekanavutito kappe,",
  "tha-ap141:4.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap141:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap141:4.4": "pupphadānassidaṃ phalaṃ.",
  "tha-ap141:5.1": "Ito ca navame kappe,",
  "tha-ap141:5.2": "virocanasanāmakā;",
  "tha-ap141:5.3": "Tayo āsiṃsu rājāno,",
  "tha-ap141:5.4": "cakkavattī mahabbalā.",
  "tha-ap141:6.1": "Paṭisambhidā catasso,",
  "tha-ap141:6.2": "…pe…",
  "tha-ap141:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap141:7.1": "Itthaṃ sudaṃ āyasmā sālapupphadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap141:8.1": "Sālapupphadāyakattherassāpadānaṃ navamaṃ."
}