{
  "tha-ap261:0.1": "Therāpadāna",
  "tha-ap261:0.2": "Thomakavagga",
  "tha-ap261:0.3": "9. Kuṭidāyakattheraapadāna",
  "tha-ap261:1.1": "“Vipinacārī sambuddho,",
  "tha-ap261:1.2": "rukkhamūle vasī tadā;",
  "tha-ap261:1.3": "Paṇṇasālaṃ karitvāna,",
  "tha-ap261:1.4": "adāsiṃ aparājite.",
  "tha-ap261:2.1": "Ekanavutito kappe,",
  "tha-ap261:2.2": "yaṃ paṇṇakuṭikaṃ adaṃ;",
  "tha-ap261:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap261:2.4": "kuṭidānassidaṃ phalaṃ.",
  "tha-ap261:3.1": "Aṭṭhavīse ito kappe,",
  "tha-ap261:3.2": "soḷasāsiṃsu rājāno;",
  "tha-ap261:3.3": "Sabbattha abhivassīti,",
  "tha-ap261:3.4": "vuccare cakkavattino.",
  "tha-ap261:4.1": "Paṭisambhidā catasso,",
  "tha-ap261:4.2": "…pe…",
  "tha-ap261:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap261:5.1": "Itthaṃ sudaṃ āyasmā kuṭidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap261:6.1": "Kuṭidāyakattherassāpadānaṃ navamaṃ."
}