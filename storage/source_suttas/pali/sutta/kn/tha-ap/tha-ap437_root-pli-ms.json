{
  "tha-ap437:0.1": "Therāpadāna",
  "tha-ap437:0.2": "Ekavihārivagga",
  "tha-ap437:0.3": "5. Ucchukhaṇḍikattheraapadāna",
  "tha-ap437:1.1": "“Nagare bandhumatiyā,",
  "tha-ap437:1.2": "dvārapālo ahosahaṃ;",
  "tha-ap437:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap437:1.4": "sabbadhammāna pāraguṃ.",
  "tha-ap437:2.1": "Ucchukhaṇḍikamādāya,",
  "tha-ap437:2.2": "buddhaseṭṭhassadāsahaṃ;",
  "tha-ap437:2.3": "Pasannacitto sumano,",
  "tha-ap437:2.4": "vipassissa mahesino.",
  "tha-ap437:3.1": "Ekanavutito kappe,",
  "tha-ap437:3.2": "yaṃ ucchumadadiṃ tadā;",
  "tha-ap437:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap437:3.4": "ucchukhaṇḍassidaṃ phalaṃ.",
  "tha-ap437:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap437:4.2": "…pe…",
  "tha-ap437:4.3": "viharāmi anāsavo.",
  "tha-ap437:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap437:5.2": "…pe…",
  "tha-ap437:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap437:6.1": "Paṭisambhidā catasso,",
  "tha-ap437:6.2": "…pe…",
  "tha-ap437:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap437:7.1": "Itthaṃ sudaṃ āyasmā ucchukhaṇḍiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap437:8.1": "Ucchukhaṇḍikattherassāpadānaṃ pañcamaṃ."
}