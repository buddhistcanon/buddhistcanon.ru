{
  "tha-ap73:0.1": "Therāpadāna",
  "tha-ap73:0.2": "Nāgasamālavagga",
  "tha-ap73:0.3": "1. Nāgasamālattheraapadāna",
  "tha-ap73:1.1": "“Āpāṭaliṃ ahaṃ pupphaṃ,",
  "tha-ap73:1.2": "ujjhitaṃ sumahāpathe;",
  "tha-ap73:1.3": "Thūpamhi abhiropesiṃ,",
  "tha-ap73:1.4": "sikhino lokabandhuno.",
  "tha-ap73:2.1": "Ekattiṃse ito kappe,",
  "tha-ap73:2.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap73:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap73:2.4": "thūpapūjāyidaṃ phalaṃ.",
  "tha-ap73:3.1": "Ito pannarase kappe,",
  "tha-ap73:3.2": "bhūmiyo nāma khattiyo;",
  "tha-ap73:3.3": "Sattaratanasampanno,",
  "tha-ap73:3.4": "cakkavattī mahabbalo.",
  "tha-ap73:4.1": "Paṭisambhidā catasso,",
  "tha-ap73:4.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap73:4.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap73:4.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap73:5.1": "Itthaṃ sudaṃ āyasmā nāgasamālo thero imā gāthāyo abhāsitthāti.",
  "tha-ap73:6.1": "Nāgasamālattherassāpadānaṃ paṭhamaṃ."
}