{
  "tha-ap151:0.1": "Therāpadāna",
  "tha-ap151:0.2": "Chattavagga",
  "tha-ap151:0.3": "9. Vaṭaṃsakiyattheraapadāna",
  "tha-ap151:1.1": "“Sumedho nāma nāmena,",
  "tha-ap151:1.2": "sayambhū aparājito;",
  "tha-ap151:1.3": "Vivekamanubrūhanto,",
  "tha-ap151:1.4": "ajjhogāhi mahāvanaṃ.",
  "tha-ap151:2.1": "Saḷalaṃ pupphitaṃ disvā,",
  "tha-ap151:2.2": "ganthitvāna vaṭaṃsakaṃ;",
  "tha-ap151:2.3": "Buddhassa abhiropesiṃ,",
  "tha-ap151:2.4": "sammukhā lokanāyakaṃ.",
  "tha-ap151:3.1": "Tiṃsakappasahassamhi,",
  "tha-ap151:3.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap151:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap151:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap151:4.1": "Ūnavīse kappasate,",
  "tha-ap151:4.2": "soḷasāsuṃ sunimmitā;",
  "tha-ap151:4.3": "Sattaratanasampannā,",
  "tha-ap151:4.4": "cakkavattī mahabbalā.",
  "tha-ap151:5.1": "Paṭisambhidā catasso,",
  "tha-ap151:5.2": "…pe…",
  "tha-ap151:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap151:6.1": "Itthaṃ sudaṃ āyasmā vaṭaṃsakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap151:7.1": "Vaṭaṃsakiyattherassāpadānaṃ navamaṃ."
}