{
  "tha-ap252:0.1": "Therāpadāna",
  "tha-ap252:0.2": "Tuvaradāyakavagga",
  "tha-ap252:0.3": "10 Uddālakadāyakattheraapadāna",
  "tha-ap252:1.1": "“Kakudho nāma nāmena,",
  "tha-ap252:1.2": "sayambhū aparājito;",
  "tha-ap252:1.3": "Pavanā nikkhamitvāna,",
  "tha-ap252:1.4": "anuppatto mahānadiṃ.",
  "tha-ap252:2.1": "Uddālakaṃ gahetvāna,",
  "tha-ap252:2.2": "sayambhussa adāsahaṃ;",
  "tha-ap252:2.3": "Saṃyatassujubhūtassa,",
  "tha-ap252:2.4": "pasannamānaso ahaṃ.",
  "tha-ap252:3.1": "Ekattiṃse ito kappe,",
  "tha-ap252:3.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap252:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap252:3.4": "pupphadānassidaṃ phalaṃ.",
  "tha-ap252:4.1": "Paṭisambhidā catasso,",
  "tha-ap252:4.2": "…pe…",
  "tha-ap252:4.3": "kataṃ buddhassa sāsanaṃ”. (2494)",
  "tha-ap252:5.1": "Itthaṃ sudaṃ āyasmā uddālakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap252:6.1": "Uddālakadāyakattherassāpadānaṃ dasamaṃ.",
  "tha-ap252:7.1": "Tuvaradāyakavaggo pañcavīsatimo.",
  "tha-ap252:8.1": "Tassuddānaṃ",
  "tha-ap252:9.1": "Tuvaranāganaḷinā,",
  "tha-ap252:9.2": "viravī kuṭidhūpako;",
  "tha-ap252:9.3": "Patto dhātusattaliyo,",
  "tha-ap252:9.4": "bimbi uddālakena ca;",
  "tha-ap252:9.5": "Sattatiṃsati gāthāyo,",
  "tha-ap252:9.6": "gaṇitāyo vibhāvibhi."
}