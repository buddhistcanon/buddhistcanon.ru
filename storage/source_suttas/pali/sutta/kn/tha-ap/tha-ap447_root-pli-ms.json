{
  "tha-ap447:0.1": "Therāpadāna",
  "tha-ap447:0.2": "Vibhītakavagga",
  "tha-ap447:0.3": "5 Uttalipupphiyattheraapadāna",
  "tha-ap447:1.1": "“Nigrodhe haritobhāse,",
  "tha-ap447:1.2": "saṃviruḷhamhi pādape;",
  "tha-ap447:1.3": "Uttalimālaṃ paggayha,",
  "tha-ap447:1.4": "bodhiyā abhiropayiṃ.",
  "tha-ap447:2.1": "Imasmiṃyeva kappamhi,",
  "tha-ap447:2.2": "yaṃ bodhimabhipūjayiṃ;",
  "tha-ap447:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap447:2.4": "bodhipūjāyidaṃ phalaṃ.",
  "tha-ap447:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap447:3.2": "…pe…",
  "tha-ap447:3.3": "viharāmi anāsavo.",
  "tha-ap447:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap447:4.2": "…pe…",
  "tha-ap447:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap447:5.1": "Paṭisambhidā catasso,",
  "tha-ap447:5.2": "…pe…",
  "tha-ap447:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap447:6.1": "Itthaṃ sudaṃ āyasmā uttalipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap447:7.1": "Uttalipupphiyattherassāpadānaṃ pañcamaṃ."
}