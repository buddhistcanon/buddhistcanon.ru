{
  "tha-ap290:0.1": "Therāpadāna",
  "tha-ap290:0.2": "Paṇṇadāyakavagga",
  "tha-ap290:0.3": "8. Sattāhapabbajitattheraapadāna",
  "tha-ap290:1.1": "“Vipassissa bhagavato,",
  "tha-ap290:1.2": "saṃgho sakkatamānito;",
  "tha-ap290:1.3": "Byasanaṃ me anuppattaṃ,",
  "tha-ap290:1.4": "ñātibhedo pure ahu.",
  "tha-ap290:2.1": "Pabbajjaṃ upagantvāna,",
  "tha-ap290:2.2": "byasanupasamāyahaṃ;",
  "tha-ap290:2.3": "Sattāhābhirato tattha,",
  "tha-ap290:2.4": "satthusāsanakamyatā.",
  "tha-ap290:3.1": "Ekanavutito kappe,",
  "tha-ap290:3.2": "yamahaṃ pabbajiṃ tadā;",
  "tha-ap290:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap290:3.4": "pabbajjāya idaṃ phalaṃ.",
  "tha-ap290:4.1": "Sattasaṭṭhimhito kappe,",
  "tha-ap290:4.2": "satta āsuṃ mahīpatī;",
  "tha-ap290:4.3": "Sunikkhamāti ñāyanti,",
  "tha-ap290:4.4": "cakkavattī mahabbalā.",
  "tha-ap290:5.1": "Paṭisambhidā catasso,",
  "tha-ap290:5.2": "…pe…",
  "tha-ap290:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap290:6.1": "Itthaṃ sudaṃ āyasmā sattāhapabbajito thero imā gāthāyo abhāsitthāti.",
  "tha-ap290:7.1": "Sattāhapabbajitattherassāpadānaṃ aṭṭhamaṃ."
}