{
  "tha-ap458:0.1": "Therāpadāna",
  "tha-ap458:0.2": "Jagatidāyakavagga",
  "tha-ap458:0.3": "6. Vanakoraṇḍiyattheraapadāna",
  "tha-ap458:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap458:1.2": "lokajeṭṭhassa tādino;",
  "tha-ap458:1.3": "Vanakoraṇḍamādāya,",
  "tha-ap458:1.4": "buddhassa abhiropayiṃ.",
  "tha-ap458:2.1": "Catunnavutito kappe,",
  "tha-ap458:2.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap458:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap458:2.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap458:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap458:3.2": "…pe…",
  "tha-ap458:3.3": "viharāmi anāsavo.",
  "tha-ap458:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap458:4.2": "…pe…",
  "tha-ap458:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap458:5.1": "Paṭisambhidā catasso,",
  "tha-ap458:5.2": "…pe…",
  "tha-ap458:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap458:6.1": "Itthaṃ sudaṃ āyasmā vanakoraṇḍiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap458:7.1": "Vanakoraṇḍiyattherassāpadānaṃ chaṭṭhaṃ.",
  "tha-ap458:7.2": "Vīsatimaṃ bhāṇavāraṃ."
}