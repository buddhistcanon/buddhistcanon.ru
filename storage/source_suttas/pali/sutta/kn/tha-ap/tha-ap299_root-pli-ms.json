{
  "tha-ap299:0.1": "Therāpadāna",
  "tha-ap299:0.2": "Citakapūjakavagga",
  "tha-ap299:0.3": "7 Desakittakattheraapadāna",
  "tha-ap299:1.1": "“Upasālakanāmohaṃ,",
  "tha-ap299:1.2": "ahosiṃ brāhmaṇo tadā;",
  "tha-ap299:1.3": "Kānanaṃ vanamogāḷhaṃ,",
  "tha-ap299:1.4": "lokajeṭṭhaṃ narāsabhaṃ.",
  "tha-ap299:2.1": "Disvāna vandiṃ pādesu,",
  "tha-ap299:2.2": "lokāhutipaṭiggahaṃ;",
  "tha-ap299:2.3": "Pasannacittaṃ maṃ ñatvā,",
  "tha-ap299:2.4": "buddho antaradhāyatha.",
  "tha-ap299:3.1": "Kānanā abhinikkhamma,",
  "tha-ap299:3.2": "buddhaseṭṭhamanussariṃ;",
  "tha-ap299:3.3": "Taṃ desaṃ kittayitvāna,",
  "tha-ap299:3.4": "kappaṃ saggamhi modahaṃ.",
  "tha-ap299:4.1": "Dvenavute ito kappe,",
  "tha-ap299:4.2": "yaṃ desamabhikittayiṃ;",
  "tha-ap299:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap299:4.4": "kittanāya idaṃ phalaṃ.",
  "tha-ap299:5.1": "Paṭisambhidā catasso,",
  "tha-ap299:5.2": "…pe…",
  "tha-ap299:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap299:6.1": "Itthaṃ sudaṃ āyasmā desakittako thero imā gāthāyo abhāsitthāti.",
  "tha-ap299:7.1": "Desakittakattherassāpadānaṃ sattamaṃ."
}