{
  "tha-ap285:0.1": "Therāpadāna",
  "tha-ap285:0.2": "Paṇṇadāyakavagga",
  "tha-ap285:0.3": "3. Paccuggamaniyattheraapadāna",
  "tha-ap285:1.1": "“Sīhaṃ yathā vanacaraṃ,",
  "tha-ap285:1.2": "nisabhājāniyaṃ yathā;",
  "tha-ap285:1.3": "Kakudhaṃ vilasantaṃva,",
  "tha-ap285:1.4": "āgacchantaṃ narāsabhaṃ.",
  "tha-ap285:2.1": "Siddhatthaṃ lokapajjotaṃ,",
  "tha-ap285:2.2": "sabbalokatikicchakaṃ;",
  "tha-ap285:2.3": "Akāsiṃ paccuggamanaṃ,",
  "tha-ap285:2.4": "vippasannena cetasā.",
  "tha-ap285:3.1": "Catunnavutito kappe,",
  "tha-ap285:3.2": "paccuggacchiṃ narāsabhaṃ;",
  "tha-ap285:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap285:3.4": "paccuggamane idaṃ phalaṃ.",
  "tha-ap285:4.1": "Sattatiṃse ito kappe,",
  "tha-ap285:4.2": "eko āsiṃ janādhipo;",
  "tha-ap285:4.3": "Saparivāroti nāmena,",
  "tha-ap285:4.4": "cakkavattī mahabbalo.",
  "tha-ap285:5.1": "Paṭisambhidā catasso,",
  "tha-ap285:5.2": "…pe…",
  "tha-ap285:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap285:6.1": "Itthaṃ sudaṃ āyasmā paccuggamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap285:7.1": "Paccuggamaniyattherassāpadānaṃ tatiyaṃ."
}