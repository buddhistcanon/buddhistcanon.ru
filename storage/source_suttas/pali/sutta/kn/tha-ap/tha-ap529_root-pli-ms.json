{
  "tha-ap529:0.1": "Therāpadāna",
  "tha-ap529:0.2": "Tiṇadāyakavagga",
  "tha-ap529:0.3": "7. Girinelapūjakattheraapadāna",
  "tha-ap529:1.1": "“Migaluddo pure āsiṃ,",
  "tha-ap529:1.2": "vipine vicaraṃ ahaṃ;",
  "tha-ap529:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap529:1.4": "sabbadhammāna pāraguṃ.",
  "tha-ap529:2.1": "Tasmiṃ mahākāruṇike,",
  "tha-ap529:2.2": "sabbasattahite rate;",
  "tha-ap529:2.3": "Pasannacitto sumano,",
  "tha-ap529:2.4": "nelapupphamapūjayiṃ.",
  "tha-ap529:3.1": "Ekatiṃse ito kappe,",
  "tha-ap529:3.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap529:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap529:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap529:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap529:4.2": "…pe…",
  "tha-ap529:4.3": "viharāmi anāsavo.",
  "tha-ap529:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap529:5.2": "…pe…",
  "tha-ap529:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap529:6.1": "Paṭisambhidā catasso,",
  "tha-ap529:6.2": "…pe…",
  "tha-ap529:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap529:7.1": "Itthaṃ sudaṃ āyasmā girinelapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap529:8.1": "Girinelapūjakattherassāpadānaṃ sattamaṃ."
}