{
  "tha-ap347:0.1": "Therāpadāna",
  "tha-ap347:0.2": "Ekapadumiyavagga",
  "tha-ap347:0.3": "5. Naḷāgārikattheraapadāna",
  "tha-ap347:1.1": "“Himavantassāvidūre,",
  "tha-ap347:1.2": "hārito nāma pabbato;",
  "tha-ap347:1.3": "Sayambhū nārado nāma,",
  "tha-ap347:1.4": "rukkhamūle vasī tadā.",
  "tha-ap347:2.1": "Naḷāgāraṃ karitvāna,",
  "tha-ap347:2.2": "tiṇena chādayiṃ ahaṃ;",
  "tha-ap347:2.3": "Caṅkamaṃ sodhayitvāna,",
  "tha-ap347:2.4": "sayambhussa adāsahaṃ.",
  "tha-ap347:3.1": "Catuddasasu kappesu,",
  "tha-ap347:3.2": "devaloke ramiṃ ahaṃ;",
  "tha-ap347:3.3": "Catusattatikkhattuñca,",
  "tha-ap347:3.4": "devarajjaṃ akārayiṃ.",
  "tha-ap347:4.1": "Catusattatikkhattuñca,",
  "tha-ap347:4.2": "cakkavattī ahosahaṃ;",
  "tha-ap347:4.3": "Padesarajjaṃ vipulaṃ,",
  "tha-ap347:4.4": "gaṇanāto asaṅkhiyaṃ.",
  "tha-ap347:5.1": "Ubbiddhaṃ bhavanaṃ mayhaṃ,",
  "tha-ap347:5.2": "Indalaṭṭhīva uggataṃ;",
  "tha-ap347:5.3": "Sahassathambhaṃ atulaṃ,",
  "tha-ap347:5.4": "Vimānaṃ sapabhassaraṃ.",
  "tha-ap347:6.1": "Dve sampattī anubhotvā,",
  "tha-ap347:6.2": "sukkamūlena codito;",
  "tha-ap347:6.3": "Gotamassa bhagavato,",
  "tha-ap347:6.4": "sāsane pabbajiṃ ahaṃ.",
  "tha-ap347:7.1": "Padhānapahitattomhi,",
  "tha-ap347:7.2": "upasanto nirūpadhi;",
  "tha-ap347:7.3": "Nāgova bandhanaṃ chetvā,",
  "tha-ap347:7.4": "viharāmi anāsavo.",
  "tha-ap347:8.1": "Paṭisambhidā catasso,",
  "tha-ap347:8.2": "…pe…",
  "tha-ap347:8.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap347:9.1": "Itthaṃ sudaṃ āyasmā naḷāgāriko thero imā gāthāyo abhāsitthāti.",
  "tha-ap347:10.1": "Naḷāgārikattherassāpadānaṃ pañcamaṃ."
}