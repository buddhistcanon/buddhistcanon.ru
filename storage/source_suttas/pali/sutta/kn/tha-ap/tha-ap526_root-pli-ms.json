{
  "tha-ap526:0.1": "Therāpadāna",
  "tha-ap526:0.2": "Tiṇadāyakavagga",
  "tha-ap526:0.3": "4. Abbhañjanadāyakattheraapadāna",
  "tha-ap526:1.1": "“Nagare bandhumatiyā,",
  "tha-ap526:1.2": "rājuyyāne vasāmahaṃ;",
  "tha-ap526:1.3": "Cammavāsī tadā āsiṃ,",
  "tha-ap526:1.4": "kamaṇḍaludharo ahaṃ.",
  "tha-ap526:2.1": "Addasaṃ vimalaṃ buddhaṃ,",
  "tha-ap526:2.2": "sayambhuṃ aparājitaṃ;",
  "tha-ap526:2.3": "Padhānaṃ pahitattaṃ taṃ,",
  "tha-ap526:2.4": "jhāyiṃ jhānarataṃ vasiṃ.",
  "tha-ap526:3.1": "Sabbakāmasamiddhiñca,",
  "tha-ap526:3.2": "oghatiṇṇamanāsavaṃ;",
  "tha-ap526:3.3": "Disvā pasanno sumano,",
  "tha-ap526:3.4": "abbhañjanamadāsahaṃ.",
  "tha-ap526:4.1": "Ekanavutito kappe,",
  "tha-ap526:4.2": "yaṃ dānamadadiṃ tadā;",
  "tha-ap526:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap526:4.4": "abbhañjanassidaṃ phalaṃ.",
  "tha-ap526:5.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap526:5.2": "…pe…",
  "tha-ap526:5.3": "viharāmi anāsavo.",
  "tha-ap526:6.1": "Svāgataṃ vata me āsi,",
  "tha-ap526:6.2": "…pe…",
  "tha-ap526:6.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap526:7.1": "Paṭisambhidā catasso,",
  "tha-ap526:7.2": "…pe…",
  "tha-ap526:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap526:8.1": "Itthaṃ sudaṃ āyasmā abbhañjanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap526:9.1": "Abbhañjanadāyakattherassāpadānaṃ catutthaṃ."
}