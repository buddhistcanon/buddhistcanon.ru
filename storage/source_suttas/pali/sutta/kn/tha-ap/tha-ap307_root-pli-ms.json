{
  "tha-ap307:0.1": "Therāpadāna",
  "tha-ap307:0.2": "Padumakesaravagga",
  "tha-ap307:0.3": "5. Phaladāyakattheraapadāna",
  "tha-ap307:1.1": "“Bhāgīrathīnadītīre,",
  "tha-ap307:1.2": "ahosi assamo tadā;",
  "tha-ap307:1.3": "Tamahaṃ assamaṃ gacchiṃ,",
  "tha-ap307:1.4": "phalahattho apekkhavā.",
  "tha-ap307:2.1": "Vipassiṃ tattha addakkhiṃ,",
  "tha-ap307:2.2": "pītaraṃsiṃva bhāṇumaṃ;",
  "tha-ap307:2.3": "Yaṃ me atthi phalaṃ sabbaṃ,",
  "tha-ap307:2.4": "adāsiṃ satthuno ahaṃ.",
  "tha-ap307:3.1": "Ekanavutito kappe,",
  "tha-ap307:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap307:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap307:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap307:4.1": "Paṭisambhidā catasso,",
  "tha-ap307:4.2": "…pe…",
  "tha-ap307:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap307:5.1": "Itthaṃ sudaṃ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap307:6.1": "Phaladāyakattherassāpadānaṃ pañcamaṃ."
}