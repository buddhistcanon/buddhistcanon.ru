{
  "tha-ap84:0.1": "Therāpadāna",
  "tha-ap84:0.2": "Timiravagga",
  "tha-ap84:0.3": "2. Gatasaññakattheraapadāna",
  "tha-ap84:1.1": "“Jātiyā sattavassohaṃ,",
  "tha-ap84:1.2": "pabbajiṃ anagāriyaṃ;",
  "tha-ap84:1.3": "Avandiṃ satthuno pāde,",
  "tha-ap84:1.4": "vippasannena cetasā.",
  "tha-ap84:2.1": "Sattanaṅgalakīpupphe,",
  "tha-ap84:2.2": "ākāse ukkhipiṃ ahaṃ;",
  "tha-ap84:2.3": "Tissaṃ buddhaṃ samuddissa,",
  "tha-ap84:2.4": "anantaguṇasāgaraṃ.",
  "tha-ap84:3.1": "Sugatānugataṃ maggaṃ,",
  "tha-ap84:3.2": "pūjetvā haṭṭhamānaso;",
  "tha-ap84:3.3": "Añjaliñca tadākāsiṃ,",
  "tha-ap84:3.4": "pasanno sehi pāṇibhi.",
  "tha-ap84:4.1": "Dvenavute ito kappe,",
  "tha-ap84:4.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap84:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap84:4.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap84:5.1": "Ito aṭṭhamake kappe,",
  "tha-ap84:5.2": "tayo aggisikhā ahu;",
  "tha-ap84:5.3": "Sattaratanasampannā,",
  "tha-ap84:5.4": "cakkavattī mahabbalā.",
  "tha-ap84:6.1": "Paṭisambhidā catasso,",
  "tha-ap84:6.2": "…pe…",
  "tha-ap84:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap84:7.1": "Itthaṃ sudaṃ āyasmā gatasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap84:8.1": "Gatasaññakattherassāpadānaṃ dutiyaṃ."
}