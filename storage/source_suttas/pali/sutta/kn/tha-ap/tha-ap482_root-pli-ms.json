{
  "tha-ap482:0.1": "Therāpadāna",
  "tha-ap482:0.2": "Naḷamālivagga",
  "tha-ap482:0.3": "10 Pulinacaṅkamiyattheraapadāna",
  "tha-ap482:1.1": "“Migaluddo pure āsiṃ,",
  "tha-ap482:1.2": "araññe kānane ahaṃ;",
  "tha-ap482:1.3": "Vātamigaṃ gavesanto,",
  "tha-ap482:1.4": "caṅkamaṃ addasaṃ ahaṃ.",
  "tha-ap482:2.1": "Ucchaṅgena pulinaṃ gayha,",
  "tha-ap482:2.2": "caṅkame okiriṃ ahaṃ;",
  "tha-ap482:2.3": "Pasannacitto sumano,",
  "tha-ap482:2.4": "sugatassa sirīmato.",
  "tha-ap482:3.1": "Ekatiṃse ito kappe,",
  "tha-ap482:3.2": "pulinaṃ okiriṃ ahaṃ;",
  "tha-ap482:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap482:3.4": "pulinassa idaṃ phalaṃ.",
  "tha-ap482:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap482:4.2": "…pe…",
  "tha-ap482:4.3": "viharāmi anāsavo.",
  "tha-ap482:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap482:5.2": "…pe…",
  "tha-ap482:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap482:6.1": "Paṭisambhidā catasso,",
  "tha-ap482:6.2": "…pe…",
  "tha-ap482:6.3": "kataṃ buddhassa sāsanaṃ”. (4938)",
  "tha-ap482:7.1": "Itthaṃ sudaṃ āyasmā pulinacaṅkamiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap482:8.1": "Pulinacaṅkamiyattherassāpadānaṃ dasamaṃ.",
  "tha-ap482:9.1": "Naḷamālivaggo aṭṭhacattālīsamo.",
  "tha-ap482:10.1": "Tassuddānaṃ",
  "tha-ap482:11.1": "Naḷamālī maṇidado,",
  "tha-ap482:11.2": "ukkāsatikabījanī;",
  "tha-ap482:11.3": "Kummāso ca kusaṭṭho ca,",
  "tha-ap482:11.4": "giripunnāgiyopi ca.",
  "tha-ap482:12.1": "Vallikāro pānadhido,",
  "tha-ap482:12.2": "atho pulinacaṅkamo;",
  "tha-ap482:12.3": "Gāthāyo pañcanavuti,",
  "tha-ap482:12.4": "gaṇitāyo vibhāvibhi."
}