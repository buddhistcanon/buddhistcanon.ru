{
  "tha-ap557:0.1": "Therāpadāna",
  "tha-ap557:0.2": "Yasavagga",
  "tha-ap557:0.3": "5. Vajjīputtattheraapadāna",
  "tha-ap557:1.1": "“Sahassaraṃsī bhagavā,",
  "tha-ap557:1.2": "sayambhū aparājito;",
  "tha-ap557:1.3": "Vivekā vuṭṭhahitvāna,",
  "tha-ap557:1.4": "gocarāyābhinikkhami.",
  "tha-ap557:2.1": "Phalahattho ahaṃ disvā,",
  "tha-ap557:2.2": "upagacchiṃ narāsabhaṃ;",
  "tha-ap557:2.3": "Pasannacitto sumano,",
  "tha-ap557:2.4": "savaṇṭaṃ adadiṃ phalaṃ.",
  "tha-ap557:3.1": "Catunnavutito kappe,",
  "tha-ap557:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap557:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap557:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap557:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap557:4.2": "…pe…",
  "tha-ap557:4.3": "viharāmi anāsavo.",
  "tha-ap557:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap557:5.2": "…pe…",
  "tha-ap557:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap557:6.1": "Paṭisambhidā catasso,",
  "tha-ap557:6.2": "…pe…",
  "tha-ap557:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap557:7.1": "Itthaṃ sudaṃ āyasmā vajjīputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap557:8.1": "Vajjīputtattherassāpadānaṃ pañcamaṃ."
}