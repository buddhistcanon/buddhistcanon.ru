{
  "tha-ap232:0.1": "Therāpadāna",
  "tha-ap232:0.2": "Ālambaṇadāyakavagga",
  "tha-ap232:0.3": "10 Ekavandaniyattheraapadāna",
  "tha-ap232:1.1": "“Usabhaṃ pavaraṃ vīraṃ,",
  "tha-ap232:1.2": "vessabhuṃ vijitāvinaṃ;",
  "tha-ap232:1.3": "Pasannacitto sumano,",
  "tha-ap232:1.4": "buddhaseṭṭhamavandahaṃ.",
  "tha-ap232:2.1": "Ekattiṃse ito kappe,",
  "tha-ap232:2.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap232:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap232:2.4": "vandanāya idaṃ phalaṃ.",
  "tha-ap232:3.1": "Catuvīsatikappamhi,",
  "tha-ap232:3.2": "vikatānandanāmako;",
  "tha-ap232:3.3": "Sattaratanasampanno,",
  "tha-ap232:3.4": "cakkavattī mahabbalo.",
  "tha-ap232:4.1": "Paṭisambhidā catasso,",
  "tha-ap232:4.2": "…pe…",
  "tha-ap232:4.3": "kataṃ buddhassa sāsanaṃ”. (2419)",
  "tha-ap232:5.1": "Itthaṃ sudaṃ āyasmā ekavandaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap232:6.1": "Ekavandaniyattherassāpadānaṃ dasamaṃ.",
  "tha-ap232:7.1": "Ālambaṇadāyakavaggo tevīsatimo.",
  "tha-ap232:8.1": "Tassuddānaṃ",
  "tha-ap232:9.1": "Ālambaṇañca ajinaṃ,",
  "tha-ap232:9.2": "maṃsadārakkhadāyako;",
  "tha-ap232:9.3": "Abyādhi aṅkolaṃ soṇṇaṃ,",
  "tha-ap232:9.4": "miñjaāveḷavandanaṃ;",
  "tha-ap232:9.5": "Pañcapaññāsa gāthāyo,",
  "tha-ap232:9.6": "gaṇitā atthadassibhi."
}