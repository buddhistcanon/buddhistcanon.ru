{
  "tha-ap506:0.1": "Therāpadāna",
  "tha-ap506:0.2": "Kaṇikāravagga",
  "tha-ap506:0.3": "4. Avaṭaphaliyattheraapadāna",
  "tha-ap506:1.1": "“Sahassaraṃsī bhagavā,",
  "tha-ap506:1.2": "sayambhū aparājito;",
  "tha-ap506:1.3": "Vivekā vuṭṭhahitvāna,",
  "tha-ap506:1.4": "gocarāyābhinikkhami.",
  "tha-ap506:2.1": "Phalahattho ahaṃ disvā,",
  "tha-ap506:2.2": "upagacchiṃ narāsabhaṃ;",
  "tha-ap506:2.3": "Pasannacitto sumano,",
  "tha-ap506:2.4": "avaṭaṃ adadiṃ phalaṃ.",
  "tha-ap506:3.1": "Catunnavutito kappe,",
  "tha-ap506:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap506:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap506:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap506:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap506:4.2": "…pe…",
  "tha-ap506:4.3": "viharāmi anāsavo.",
  "tha-ap506:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap506:5.2": "…pe…",
  "tha-ap506:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap506:6.1": "Paṭisambhidā catasso,",
  "tha-ap506:6.2": "…pe…",
  "tha-ap506:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap506:7.1": "Itthaṃ sudaṃ āyasmā avaṭaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap506:8.1": "Avaṭaphaliyattherassāpadānaṃ catutthaṃ."
}