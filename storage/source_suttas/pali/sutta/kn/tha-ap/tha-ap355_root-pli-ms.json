{
  "tha-ap355:0.1": "Therāpadāna",
  "tha-ap355:0.2": "Saddasaññakavagga",
  "tha-ap355:0.3": "3. Kiṃsukapūjakattheraapadāna",
  "tha-ap355:1.1": "“Kiṃsukaṃ pupphitaṃ disvā,",
  "tha-ap355:1.2": "paggahetvāna añjaliṃ;",
  "tha-ap355:1.3": "Buddhaṃ saritvā siddhatthaṃ,",
  "tha-ap355:1.4": "ākāse abhipūjayiṃ.",
  "tha-ap355:2.1": "Catunnavutito kappe,",
  "tha-ap355:2.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap355:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap355:2.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap355:3.1": "Paṭisambhidā catasso,",
  "tha-ap355:3.2": "…pe…",
  "tha-ap355:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap355:4.1": "Itthaṃ sudaṃ āyasmā kiṃsukapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap355:5.1": "Kiṃsukapūjakattherassāpadānaṃ tatiyaṃ."
}