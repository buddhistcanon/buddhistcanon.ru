{
  "tha-ap100:0.1": "Therāpadāna",
  "tha-ap100:0.2": "Sudhāvagga",
  "tha-ap100:0.3": "8. Senāsanadāyakattheraapadāna",
  "tha-ap100:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap100:1.2": "adāsiṃ paṇṇasantharaṃ;",
  "tha-ap100:1.3": "Samantā upahārañca,",
  "tha-ap100:1.4": "kusumaṃ okiriṃ ahaṃ.",
  "tha-ap100:2.1": "Pāsādevaṃ guṇaṃ rammaṃ,",
  "tha-ap100:2.2": "Anubhomi mahārahaṃ;",
  "tha-ap100:2.3": "Mahagghāni ca pupphāni,",
  "tha-ap100:2.4": "Sayanebhisavanti me.",
  "tha-ap100:3.1": "Sayanehaṃ tuvaṭṭāmi,",
  "tha-ap100:3.2": "vicitte pupphasanthate;",
  "tha-ap100:3.3": "Pupphavuṭṭhi ca sayane,",
  "tha-ap100:3.4": "abhivassati tāvade.",
  "tha-ap100:4.1": "Catunnavutito kappe,",
  "tha-ap100:4.2": "adāsiṃ paṇṇasantharaṃ;",
  "tha-ap100:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap100:4.4": "santharassa idaṃ phalaṃ.",
  "tha-ap100:5.1": "Tiṇasantharakā nāma,",
  "tha-ap100:5.2": "sattete cakkavattino;",
  "tha-ap100:5.3": "Ito te pañcame kappe,",
  "tha-ap100:5.4": "uppajjiṃsu janādhipā.",
  "tha-ap100:6.1": "Paṭisambhidā catasso,",
  "tha-ap100:6.2": "…pe…",
  "tha-ap100:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap100:7.1": "Itthaṃ sudaṃ āyasmā senāsanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap100:8.1": "Senāsanadāyakattherassāpadānaṃ aṭṭhamaṃ."
}