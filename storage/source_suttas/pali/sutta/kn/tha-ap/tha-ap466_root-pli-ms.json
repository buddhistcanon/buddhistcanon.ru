{
  "tha-ap466:0.1": "Therāpadāna",
  "tha-ap466:0.2": "Sālakusumiyavagga",
  "tha-ap466:0.3": "4. Setudāyakattheraapadāna",
  "tha-ap466:1.1": "“Vipassino bhagavato,",
  "tha-ap466:1.2": "caṅkamantassa sammukhā;",
  "tha-ap466:1.3": "Pasannacitto sumano,",
  "tha-ap466:1.4": "setuṃ kārāpayiṃ ahaṃ.",
  "tha-ap466:2.1": "Ekanavutito kappe,",
  "tha-ap466:2.2": "yaṃ setuṃ kārayiṃ ahaṃ;",
  "tha-ap466:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap466:2.4": "setudānassidaṃ phalaṃ.",
  "tha-ap466:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap466:3.2": "…pe…",
  "tha-ap466:3.3": "viharāmi anāsavo.",
  "tha-ap466:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap466:4.2": "…pe…",
  "tha-ap466:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap466:5.1": "Paṭisambhidā catasso,",
  "tha-ap466:5.2": "…pe…",
  "tha-ap466:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap466:6.1": "Itthaṃ sudaṃ āyasmā setudāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap466:7.1": "Setudāyakattherassāpadānaṃ catutthaṃ."
}