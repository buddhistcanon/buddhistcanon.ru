{
  "tha-ap268:0.1": "Therāpadāna",
  "tha-ap268:0.2": "Padumukkhipavagga",
  "tha-ap268:0.3": "6. Macchadāyakattheraapadāna",
  "tha-ap268:1.1": "“Candabhāgānadītīre,",
  "tha-ap268:1.2": "ukkuso āsahaṃ tadā;",
  "tha-ap268:1.3": "Mahantaṃ macchaṃ paggayha,",
  "tha-ap268:1.4": "siddhatthamunino adaṃ.",
  "tha-ap268:2.1": "Catunnavutito kappe,",
  "tha-ap268:2.2": "yaṃ macchamadadiṃ tadā;",
  "tha-ap268:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap268:2.4": "macchadānassidaṃ phalaṃ.",
  "tha-ap268:3.1": "Paṭisambhidā catasso,",
  "tha-ap268:3.2": "…pe…",
  "tha-ap268:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap268:4.1": "Itthaṃ sudaṃ āyasmā macchadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap268:5.1": "Macchadāyakattherassāpadānaṃ chaṭṭhaṃ."
}