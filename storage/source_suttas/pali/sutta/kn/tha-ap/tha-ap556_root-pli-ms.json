{
  "tha-ap556:0.1": "Therāpadāna",
  "tha-ap556:0.2": "Yasavagga",
  "tha-ap556:0.3": "4 Kimilattheraapadāna",
  "tha-ap556:1.1": "“Nibbute kakusandhamhi,",
  "tha-ap556:1.2": "brāhmaṇamhi vusīmati;",
  "tha-ap556:1.3": "Gahetvā salalaṃ mālaṃ,",
  "tha-ap556:1.4": "maṇḍapaṃ kārayiṃ ahaṃ.",
  "tha-ap556:2.1": "Tāvatiṃsaṃ gato santo,",
  "tha-ap556:2.2": "labhimha byamhamuttamaṃ;",
  "tha-ap556:2.3": "Aññe devetirocāmi,",
  "tha-ap556:2.4": "puññakammassidaṃ phalaṃ.",
  "tha-ap556:3.1": "Divā vā yadi vā rattiṃ,",
  "tha-ap556:3.2": "caṅkamanto ṭhito cahaṃ;",
  "tha-ap556:3.3": "Channo salalapupphehi,",
  "tha-ap556:3.4": "puññakammassidaṃ phalaṃ.",
  "tha-ap556:4.1": "Imasmiṃyeva kappamhi,",
  "tha-ap556:4.2": "yaṃ buddhamabhipūjayiṃ;",
  "tha-ap556:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap556:4.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap556:5.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap556:5.2": "…pe…",
  "tha-ap556:5.3": "viharāmi anāsavo.",
  "tha-ap556:6.1": "Svāgataṃ vata me āsi,",
  "tha-ap556:6.2": "…pe…",
  "tha-ap556:6.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap556:7.1": "Paṭisambhidā catasso,",
  "tha-ap556:7.2": "…pe…",
  "tha-ap556:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap556:8.1": "Itthaṃ sudaṃ āyasmā kimilo thero imā gāthāyo abhāsitthāti.",
  "tha-ap556:9.1": "Kimilattherassāpadānaṃ catutthaṃ."
}