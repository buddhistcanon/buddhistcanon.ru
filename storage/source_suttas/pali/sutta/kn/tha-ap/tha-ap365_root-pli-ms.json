{
  "tha-ap365:0.1": "Therāpadāna",
  "tha-ap365:0.2": "Mandāravapupphiyavagga",
  "tha-ap365:0.3": "3. Bhisamuḷāladāyakattheraapadāna",
  "tha-ap365:1.1": "“Phusso nāmāsi sambuddho,",
  "tha-ap365:1.2": "sabbadhammāna pāragū;",
  "tha-ap365:1.3": "Vivekakāmo sabbaññū,",
  "tha-ap365:1.4": "āgañchi mama santike.",
  "tha-ap365:2.1": "Tasmiṃ cittaṃ pasādetvā,",
  "tha-ap365:2.2": "mahākāruṇike jine;",
  "tha-ap365:2.3": "Bhisamuḷālaṃ paggayha,",
  "tha-ap365:2.4": "buddhaseṭṭhassadāsahaṃ.",
  "tha-ap365:3.1": "Dvenavute ito kappe,",
  "tha-ap365:3.2": "yaṃ bhisamadadiṃ tadā;",
  "tha-ap365:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap365:3.4": "bhisadānassidaṃ phalaṃ.",
  "tha-ap365:4.1": "Paṭisambhidā catasso,",
  "tha-ap365:4.2": "…pe…",
  "tha-ap365:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap365:5.1": "Itthaṃ sudaṃ āyasmā bhisamuḷāladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap365:6.1": "Bhisamuḷāladāyakattherassāpadānaṃ tatiyaṃ."
}