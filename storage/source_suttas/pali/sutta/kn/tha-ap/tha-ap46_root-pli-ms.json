{
  "tha-ap46:0.1": "Therāpadāna",
  "tha-ap46:0.2": "Upālivagga",
  "tha-ap46:0.3": "4. Sanniṭṭhāpakattheraapadāna",
  "tha-ap46:1.1": "“Araññe kuṭikaṃ katvā,",
  "tha-ap46:1.2": "vasāmi pabbatantare;",
  "tha-ap46:1.3": "Lābhālābhena santuṭṭho,",
  "tha-ap46:1.4": "yasena ayasena ca.",
  "tha-ap46:2.1": "Padumuttaro lokavidū,",
  "tha-ap46:2.2": "āhutīnaṃ paṭiggaho;",
  "tha-ap46:2.3": "Vasīsatasahassehi,",
  "tha-ap46:2.4": "āgacchi mama santikaṃ.",
  "tha-ap46:3.1": "Upāgataṃ mahānāgaṃ,",
  "tha-ap46:3.2": "jalajuttamanāmakaṃ;",
  "tha-ap46:3.3": "Tiṇasantharaṃ paññāpetvā,",
  "tha-ap46:3.4": "adāsiṃ satthuno ahaṃ.",
  "tha-ap46:4.1": "Pasannacitto sumano,",
  "tha-ap46:4.2": "āmaṇḍaṃ pānīyañcahaṃ;",
  "tha-ap46:4.3": "Adāsiṃ ujubhūtassa,",
  "tha-ap46:4.4": "vippasannena cetasā.",
  "tha-ap46:5.1": "Satasahassito kappe,",
  "tha-ap46:5.2": "yaṃ dānamadadiṃ tadā;",
  "tha-ap46:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap46:5.4": "āmaṇḍassa idaṃ phalaṃ.",
  "tha-ap46:6.1": "Ekatālīsakappamhi,",
  "tha-ap46:6.2": "eko āsiṃ arindamo;",
  "tha-ap46:6.3": "Sattaratanasampanno,",
  "tha-ap46:6.4": "cakkavattī mahabbalo.",
  "tha-ap46:7.1": "Paṭisambhidā catasso,",
  "tha-ap46:7.2": "…pe…",
  "tha-ap46:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap46:8.1": "Itthaṃ sudaṃ āyasmā sanniṭṭhāpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap46:9.1": "Sanniṭṭhāpakattherassāpadānaṃ catutthaṃ."
}