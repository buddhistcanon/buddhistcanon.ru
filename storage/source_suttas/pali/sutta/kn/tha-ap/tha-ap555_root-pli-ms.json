{
  "tha-ap555:0.1": "Therāpadāna",
  "tha-ap555:0.2": "Yasavagga",
  "tha-ap555:0.3": "3. Gayākassapattheraapadāna",
  "tha-ap555:1.1": "“Ajinacammavatthohaṃ,",
  "tha-ap555:1.2": "khāribhāradharo tadā;",
  "tha-ap555:1.3": "Khārikaṃ hārayitvāna,",
  "tha-ap555:1.4": "kolaṃ ahāsi assamaṃ.",
  "tha-ap555:2.1": "Bhagavā tamhi samaye,",
  "tha-ap555:2.2": "eko adutiyo jino;",
  "tha-ap555:2.3": "Mamassamaṃ upāgacchi,",
  "tha-ap555:2.4": "jotento sabbakālikaṃ.",
  "tha-ap555:3.1": "Sakaṃ cittaṃ pasādetvā,",
  "tha-ap555:3.2": "abhivādetvāna subbataṃ;",
  "tha-ap555:3.3": "Ubho hatthehi paggayha,",
  "tha-ap555:3.4": "kolaṃ buddhassadāsahaṃ.",
  "tha-ap555:4.1": "Ekatiṃse ito kappe,",
  "tha-ap555:4.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap555:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap555:4.4": "koladānassidaṃ phalaṃ.",
  "tha-ap555:5.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap555:5.2": "…pe…",
  "tha-ap555:5.3": "viharāmi anāsavo.",
  "tha-ap555:6.1": "Svāgataṃ vata me āsi,",
  "tha-ap555:6.2": "…pe…",
  "tha-ap555:6.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap555:7.1": "Paṭisambhidā catasso,",
  "tha-ap555:7.2": "…pe…",
  "tha-ap555:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap555:8.1": "Itthaṃ sudaṃ āyasmā gayākassapo thero imā gāthāyo abhāsitthāti.",
  "tha-ap555:9.1": "Gayākassapattherassāpadānaṃ tatiyaṃ."
}