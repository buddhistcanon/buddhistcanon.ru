{
  "tha-ap156:0.1": "Therāpadāna",
  "tha-ap156:0.2": "Bandhujīvakavagga",
  "tha-ap156:0.3": "4 Kakkārupupphapūjakattheraapadāna",
  "tha-ap156:1.1": "“Devaputto ahaṃ santo,",
  "tha-ap156:1.2": "pūjayiṃ sikhināyakaṃ;",
  "tha-ap156:1.3": "Kakkārupupphaṃ paggayha,",
  "tha-ap156:1.4": "buddhassa abhiropayiṃ.",
  "tha-ap156:2.1": "Ekattiṃse ito kappe,",
  "tha-ap156:2.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap156:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap156:2.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap156:3.1": "Ito ca navame kappe,",
  "tha-ap156:3.2": "rājā sattuttamo ahuṃ;",
  "tha-ap156:3.3": "Sattaratanasampanno,",
  "tha-ap156:3.4": "cakkavattī mahabbalo.",
  "tha-ap156:4.1": "Paṭisambhidā catasso,",
  "tha-ap156:4.2": "…pe…",
  "tha-ap156:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap156:5.1": "Itthaṃ sudaṃ āyasmā kakkārupupphapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap156:6.1": "Kakkārupupphapūjakattherassāpadānaṃ catutthaṃ."
}