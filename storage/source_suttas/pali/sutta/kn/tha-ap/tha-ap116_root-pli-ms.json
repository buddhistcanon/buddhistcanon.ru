{
  "tha-ap116:0.1": "Therāpadāna",
  "tha-ap116:0.2": "Mahāparivāravagga",
  "tha-ap116:0.3": "4. Ekāsaniyattheraapadāna",
  "tha-ap116:1.1": "“Varuṇo nāma nāmena,",
  "tha-ap116:1.2": "devarājā ahaṃ tadā;",
  "tha-ap116:1.3": "Upaṭṭhahesiṃ sambuddhaṃ,",
  "tha-ap116:1.4": "sayoggabalavāhano.",
  "tha-ap116:2.1": "Nibbute lokanāthamhi,",
  "tha-ap116:2.2": "atthadassīnaruttame;",
  "tha-ap116:2.3": "Tūriyaṃ sabbamādāya,",
  "tha-ap116:2.4": "agamaṃ bodhimuttamaṃ.",
  "tha-ap116:3.1": "Vāditena ca naccena,",
  "tha-ap116:3.2": "sammatāḷasamāhito;",
  "tha-ap116:3.3": "Sammukhā viya sambuddhaṃ,",
  "tha-ap116:3.4": "upaṭṭhiṃ bodhimuttamaṃ.",
  "tha-ap116:4.1": "Upaṭṭhahitvā taṃ bodhiṃ,",
  "tha-ap116:4.2": "dharaṇīruhapādapaṃ;",
  "tha-ap116:4.3": "Pallaṅkaṃ ābhujitvāna,",
  "tha-ap116:4.4": "tattha kālaṅkato ahaṃ.",
  "tha-ap116:5.1": "Sakakammābhiraddhohaṃ,",
  "tha-ap116:5.2": "pasanno bodhimuttame;",
  "tha-ap116:5.3": "Tena cittappasādena,",
  "tha-ap116:5.4": "nimmānaṃ upapajjahaṃ.",
  "tha-ap116:6.1": "Saṭṭhitūriyasahassāni,",
  "tha-ap116:6.2": "parivārenti maṃ sadā;",
  "tha-ap116:6.3": "Manussesu ca devesu,",
  "tha-ap116:6.4": "vattamānaṃ bhavābhave.",
  "tha-ap116:7.1": "Tividhaggī nibbutā mayhaṃ,",
  "tha-ap116:7.2": "bhavā sabbe samūhatā;",
  "tha-ap116:7.3": "Dhāremi antimaṃ dehaṃ,",
  "tha-ap116:7.4": "sammāsambuddhasāsane.",
  "tha-ap116:8.1": "Subāhū nāma nāmena,",
  "tha-ap116:8.2": "catuttiṃsāsu khattiyā;",
  "tha-ap116:8.3": "Sattaratanasampannā,",
  "tha-ap116:8.4": "pañcakappasate ito.",
  "tha-ap116:9.1": "Paṭisambhidā catasso,",
  "tha-ap116:9.2": "…pe…",
  "tha-ap116:9.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap116:10.1": "Itthaṃ sudaṃ āyasmā ekāsaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap116:11.1": "Ekāsaniyattherassāpadānaṃ catutthaṃ."
}