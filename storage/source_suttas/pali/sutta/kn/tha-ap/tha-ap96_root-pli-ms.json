{
  "tha-ap96:0.1": "Therāpadāna",
  "tha-ap96:0.2": "Sudhāvagga",
  "tha-ap96:0.3": "4. Sūcidāyakattheraapadāna",
  "tha-ap96:1.1": "“Kammārohaṃ pure āsiṃ,",
  "tha-ap96:1.2": "bandhumāyaṃ puruttame;",
  "tha-ap96:1.3": "Sūcidānaṃ mayā dinnaṃ,",
  "tha-ap96:1.4": "vipassissa mahesino.",
  "tha-ap96:2.1": "Vajiraggasamaṃ ñāṇaṃ,",
  "tha-ap96:2.2": "hoti kammena tādisaṃ;",
  "tha-ap96:2.3": "Virāgomhi vimuttomhi,",
  "tha-ap96:2.4": "pattomhi āsavakkhayaṃ.",
  "tha-ap96:3.1": "Atīte ca bhave sabbe,",
  "tha-ap96:3.2": "vattamāne canāgate;",
  "tha-ap96:3.3": "Ñāṇena viciniṃ sabbaṃ,",
  "tha-ap96:3.4": "sūcidānassidaṃ phalaṃ.",
  "tha-ap96:4.1": "Ekanavutito kappe,",
  "tha-ap96:4.2": "sattāsuṃ vajiravhayā;",
  "tha-ap96:4.3": "Sattaratanasampannā,",
  "tha-ap96:4.4": "cakkavattī mahabbalā.",
  "tha-ap96:5.1": "Paṭisambhidā catasso,",
  "tha-ap96:5.2": "…pe…",
  "tha-ap96:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap96:6.1": "Itthaṃ sudaṃ āyasmā sūcidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap96:7.1": "Sūcidāyakattherassāpadānaṃ catutthaṃ."
}