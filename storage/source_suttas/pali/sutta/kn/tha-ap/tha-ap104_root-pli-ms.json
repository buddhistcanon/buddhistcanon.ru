{
  "tha-ap104:0.1": "Therāpadāna",
  "tha-ap104:0.2": "Bhikkhadāyivagga",
  "tha-ap104:0.3": "2 Ñāṇasaññikattheraapadāna",
  "tha-ap104:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap104:1.2": "Nisabhājāniyaṃ yathā;",
  "tha-ap104:1.3": "Tidhāpabhinnaṃ mātaṅgaṃ,",
  "tha-ap104:1.4": "Kuñjaraṃva mahesinaṃ.",
  "tha-ap104:2.1": "Obhāsentaṃ disā sabbā,",
  "tha-ap104:2.2": "uḷurājaṃva pūritaṃ;",
  "tha-ap104:2.3": "Rathiyaṃ paṭipajjantaṃ,",
  "tha-ap104:2.4": "lokajeṭṭhaṃ apassahaṃ.",
  "tha-ap104:3.1": "Ñāṇe cittaṃ pasādetvā,",
  "tha-ap104:3.2": "paggahetvāna añjaliṃ;",
  "tha-ap104:3.3": "Pasannacitto sumano,",
  "tha-ap104:3.4": "siddhatthamabhivādayiṃ.",
  "tha-ap104:4.1": "Catunnavutito kappe,",
  "tha-ap104:4.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap104:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap104:4.4": "ñāṇasaññāyidaṃ phalaṃ.",
  "tha-ap104:5.1": "Tesattatimhito kappe,",
  "tha-ap104:5.2": "soḷasāsuṃ naruttamā;",
  "tha-ap104:5.3": "Sattaratanasampannā,",
  "tha-ap104:5.4": "cakkavattī mahabbalā.",
  "tha-ap104:6.1": "Paṭisambhidā catasso,",
  "tha-ap104:6.2": "…pe…",
  "tha-ap104:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap104:7.1": "Itthaṃ sudaṃ āyasmā ñāṇasaññiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap104:8.1": "Ñāṇasaññikattherassāpadānaṃ dutiyaṃ."
}