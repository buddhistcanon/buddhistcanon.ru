{
  "tha-ap387:0.1": "Therāpadāna",
  "tha-ap387:0.2": "Avaṭaphalavagga",
  "tha-ap387:0.3": "5. Phārusaphaladāyakattheraapadāna",
  "tha-ap387:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap387:1.2": "Āhutīnaṃ paṭiggahaṃ;",
  "tha-ap387:1.3": "Rathiyaṃ paṭipajjantaṃ,",
  "tha-ap387:1.4": "Phārusaphalamadāsahaṃ.",
  "tha-ap387:2.1": "Ekanavutito kappe,",
  "tha-ap387:2.2": "yaṃ phalamadadiṃ ahaṃ;",
  "tha-ap387:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap387:2.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap387:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap387:3.2": "…pe…",
  "tha-ap387:3.3": "viharāmi anāsavo.",
  "tha-ap387:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap387:4.2": "…pe…",
  "tha-ap387:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap387:5.1": "Paṭisambhidā catasso,",
  "tha-ap387:5.2": "…pe…",
  "tha-ap387:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap387:6.1": "Itthaṃ sudaṃ āyasmā phārusaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap387:7.1": "Phārusaphaladāyakattherassāpadānaṃ pañcamaṃ."
}