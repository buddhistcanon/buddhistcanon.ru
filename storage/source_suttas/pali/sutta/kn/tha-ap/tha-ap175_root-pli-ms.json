{
  "tha-ap175:0.1": "Therāpadāna",
  "tha-ap175:0.2": "Kumudavagga",
  "tha-ap175:0.3": "3 Rattipupphiyattheraapadāna",
  "tha-ap175:1.1": "“Migaluddo pure āsiṃ,",
  "tha-ap175:1.2": "araññe kānane ahaṃ;",
  "tha-ap175:1.3": "Vipassiṃ addasaṃ buddhaṃ,",
  "tha-ap175:1.4": "devadevaṃ narāsabhaṃ.",
  "tha-ap175:2.1": "Rattikaṃ pupphitaṃ disvā,",
  "tha-ap175:2.2": "kuṭajaṃ dharaṇīruhaṃ;",
  "tha-ap175:2.3": "Samūlaṃ paggahetvāna,",
  "tha-ap175:2.4": "upanesiṃ mahesino.",
  "tha-ap175:3.1": "Ekanavutito kappe,",
  "tha-ap175:3.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap175:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap175:3.4": "pupphadānassidaṃ phalaṃ.",
  "tha-ap175:4.1": "Ito ca aṭṭhame kappe,",
  "tha-ap175:4.2": "suppasannasanāmako;",
  "tha-ap175:4.3": "Sattaratanasampanno,",
  "tha-ap175:4.4": "rājāhosiṃ mahabbalo.",
  "tha-ap175:5.1": "Paṭisambhidā catasso,",
  "tha-ap175:5.2": "…pe…",
  "tha-ap175:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap175:6.1": "Itthaṃ sudaṃ āyasmā rattipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap175:7.1": "Rattipupphiyattherassāpadānaṃ tatiyaṃ."
}