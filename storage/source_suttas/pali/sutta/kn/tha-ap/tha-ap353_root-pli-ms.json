{
  "tha-ap353:0.1": "Therāpadāna",
  "tha-ap353:0.2": "Saddasaññakavagga",
  "tha-ap353:0.3": "1. Saddasaññakattheraapadāna",
  "tha-ap353:1.1": "“Migaluddo pure āsiṃ,",
  "tha-ap353:1.2": "araññe kānane ahaṃ;",
  "tha-ap353:1.3": "Tatthaddasāsiṃ sambuddhaṃ,",
  "tha-ap353:1.4": "devasaṅghapurakkhataṃ.",
  "tha-ap353:2.1": "Catusaccaṃ pakāsentaṃ,",
  "tha-ap353:2.2": "uddharantaṃ mahājanaṃ;",
  "tha-ap353:2.3": "Assosiṃ madhuraṃ vācaṃ,",
  "tha-ap353:2.4": "karavīkarudopamaṃ.",
  "tha-ap353:3.1": "Brahmasarassa munino,",
  "tha-ap353:3.2": "sikhino lokabandhuno;",
  "tha-ap353:3.3": "Ghose cittaṃ pasādetvā,",
  "tha-ap353:3.4": "pattomhi āsavakkhayaṃ.",
  "tha-ap353:4.1": "Ekattiṃse ito kappe,",
  "tha-ap353:4.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap353:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap353:4.4": "pasādassa idaṃ phalaṃ.",
  "tha-ap353:5.1": "Paṭisambhidā catasso,",
  "tha-ap353:5.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap353:5.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap353:5.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap353:6.1": "Itthaṃ sudaṃ āyasmā saddasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap353:7.1": "Saddasaññakattherassāpadānaṃ paṭhamaṃ."
}