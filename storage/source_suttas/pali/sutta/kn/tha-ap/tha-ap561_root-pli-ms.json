{
  "tha-ap561:0.1": "Therāpadāna",
  "tha-ap561:0.2": "Yasavagga",
  "tha-ap561:0.3": "9. Sivakattheraapadāna",
  "tha-ap561:1.1": "“Esanāya carantassa,",
  "tha-ap561:1.2": "vipassissa mahesino;",
  "tha-ap561:1.3": "Rittakaṃ pattaṃ disvāna,",
  "tha-ap561:1.4": "kummāsaṃ pūrayiṃ ahaṃ.",
  "tha-ap561:2.1": "Ekanavutito kappe,",
  "tha-ap561:2.2": "yaṃ bhikkhamadadiṃ tadā;",
  "tha-ap561:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap561:2.4": "kummāsassa idaṃ phalaṃ.",
  "tha-ap561:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap561:3.2": "…pe…",
  "tha-ap561:3.3": "viharāmi anāsavo.",
  "tha-ap561:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap561:4.2": "…pe…",
  "tha-ap561:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap561:5.1": "Paṭisambhidā catasso,",
  "tha-ap561:5.2": "…pe…",
  "tha-ap561:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap561:6.1": "Itthaṃ sudaṃ āyasmā sivakatthero imā gāthāyo abhāsitthāti.",
  "tha-ap561:7.1": "Sivakattherassāpadānaṃ navamaṃ."
}