{
  "tha-ap105:0.1": "Therāpadāna",
  "tha-ap105:0.2": "Bhikkhadāyivagga",
  "tha-ap105:0.3": "3. Uppalahatthiyattheraapadāna",
  "tha-ap105:1.1": "“Tivarāyaṃ nivāsīhaṃ,",
  "tha-ap105:1.2": "ahosiṃ māliko tadā;",
  "tha-ap105:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap105:1.4": "siddhatthaṃ lokapūjitaṃ.",
  "tha-ap105:2.1": "Pasannacitto sumano,",
  "tha-ap105:2.2": "pupphahatthamadāsahaṃ;",
  "tha-ap105:2.3": "Yattha yatthupapajjāmi,",
  "tha-ap105:2.4": "tassa kammassa vāhasā.",
  "tha-ap105:3.1": "Anubhomi phalaṃ iṭṭhaṃ,",
  "tha-ap105:3.2": "pubbe sukatamattano;",
  "tha-ap105:3.3": "Parikkhitto sumallehi,",
  "tha-ap105:3.4": "pupphadānassidaṃ phalaṃ.",
  "tha-ap105:4.1": "Catunnavutito kappe,",
  "tha-ap105:4.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap105:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap105:4.4": "pupphapūjāyidaṃ phalaṃ.",
  "tha-ap105:5.1": "Catunnavutupādāya,",
  "tha-ap105:5.2": "ṭhapetvā vattamānakaṃ;",
  "tha-ap105:5.3": "Pañcarājasatā tattha,",
  "tha-ap105:5.4": "najjasamasanāmakā.",
  "tha-ap105:6.1": "Paṭisambhidā catasso,",
  "tha-ap105:6.2": "…pe…",
  "tha-ap105:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap105:7.1": "Itthaṃ sudaṃ āyasmā uppalahatthiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap105:8.1": "Uppalahatthiyattherassāpadānaṃ tatiyaṃ."
}