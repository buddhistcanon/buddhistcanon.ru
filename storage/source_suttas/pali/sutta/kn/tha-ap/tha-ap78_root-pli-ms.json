{
  "tha-ap78:0.1": "Therāpadāna",
  "tha-ap78:0.2": "Nāgasamālavagga",
  "tha-ap78:0.3": "6 Tiṇasantharadāyakattheraapadāna",
  "tha-ap78:1.1": "“Himavantassāvidūre,",
  "tha-ap78:1.2": "mahājātassaro ahu;",
  "tha-ap78:1.3": "Satapattehi sañchanno,",
  "tha-ap78:1.4": "nānāsakuṇamālayo.",
  "tha-ap78:2.1": "Tamhi nhatvā ca pitvā ca,",
  "tha-ap78:2.2": "avidūre vasāmahaṃ;",
  "tha-ap78:2.3": "Addasaṃ samaṇānaggaṃ,",
  "tha-ap78:2.4": "gacchantaṃ anilañjase.",
  "tha-ap78:3.1": "Mama saṅkappamaññāya,",
  "tha-ap78:3.2": "satthā loke anuttaro;",
  "tha-ap78:3.3": "Abbhato oruhitvāna,",
  "tha-ap78:3.4": "bhūmiyaṃṭhāsi tāvade.",
  "tha-ap78:4.1": "Visāṇena tiṇaṃ gayha,",
  "tha-ap78:4.2": "nisīdanamadāsahaṃ;",
  "tha-ap78:4.3": "Nisīdi bhagavā tattha,",
  "tha-ap78:4.4": "tisso lokagganāyako.",
  "tha-ap78:5.1": "Sakaṃ cittaṃ pasādetvā,",
  "tha-ap78:5.2": "avandi lokanāyakaṃ;",
  "tha-ap78:5.3": "Paṭikuṭiko apasakkiṃ,",
  "tha-ap78:5.4": "nijjhāyanto mahāmuniṃ.",
  "tha-ap78:6.1": "Tena cittappasādena,",
  "tha-ap78:6.2": "nimmānaṃ upapajjahaṃ;",
  "tha-ap78:6.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap78:6.4": "santharassa idaṃ phalaṃ.",
  "tha-ap78:7.1": "Ito dutiyake kappe,",
  "tha-ap78:7.2": "miga sammatakhattiyo;",
  "tha-ap78:7.3": "Sattaratanasampanno,",
  "tha-ap78:7.4": "cakkavattī mahabbalo.",
  "tha-ap78:8.1": "Paṭisambhidā catasso,",
  "tha-ap78:8.2": "…pe…",
  "tha-ap78:8.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap78:9.1": "Itthaṃ sudaṃ āyasmā tiṇasantharadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap78:10.1": "Tiṇasantharadāyakattherassāpadānaṃ chaṭṭhaṃ."
}