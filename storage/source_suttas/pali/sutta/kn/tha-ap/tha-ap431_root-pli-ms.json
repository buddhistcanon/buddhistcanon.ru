{
  "tha-ap431:0.1": "Therāpadāna",
  "tha-ap431:0.2": "Sakiṃsammajjakavagga",
  "tha-ap431:0.3": "9 Sovaṇṇakiṅkaṇiyattheraapadāna",
  "tha-ap431:1.1": "“Saddhāya abhinikkhamma,",
  "tha-ap431:1.2": "pabbajiṃ anagāriyaṃ;",
  "tha-ap431:1.3": "Vākacīradharo āsiṃ,",
  "tha-ap431:1.4": "tapokammamapassito.",
  "tha-ap431:2.1": "Atthadassī tu bhagavā,",
  "tha-ap431:2.2": "lokajeṭṭho narāsabho;",
  "tha-ap431:2.3": "Uppajji tamhi samaye,",
  "tha-ap431:2.4": "tārayanto mahājanaṃ.",
  "tha-ap431:3.1": "Balañca vata me khīṇaṃ,",
  "tha-ap431:3.2": "byādhinā paramena taṃ;",
  "tha-ap431:3.3": "Buddhaseṭṭhaṃ saritvāna,",
  "tha-ap431:3.4": "puline thūpamuttamaṃ.",
  "tha-ap431:4.1": "Karitvā haṭṭhacittohaṃ,",
  "tha-ap431:4.2": "sahatthena samokiriṃ;",
  "tha-ap431:4.3": "Soṇṇakiṅkaṇipupphāni,",
  "tha-ap431:4.4": "udaggamanaso ahaṃ.",
  "tha-ap431:5.1": "Sammukhā viya sambuddhaṃ,",
  "tha-ap431:5.2": "thūpaṃ paricariṃ ahaṃ;",
  "tha-ap431:5.3": "Tena cetopasādena,",
  "tha-ap431:5.4": "atthadassissa tādino.",
  "tha-ap431:6.1": "Devalokaṃ gato santo,",
  "tha-ap431:6.2": "labhāmi vipulaṃ sukhaṃ;",
  "tha-ap431:6.3": "Suvaṇṇavaṇṇo tatthāsiṃ,",
  "tha-ap431:6.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap431:7.1": "Asītikoṭiyo mayhaṃ,",
  "tha-ap431:7.2": "nāriyo samalaṅkatā;",
  "tha-ap431:7.3": "Sadā mayhaṃ upaṭṭhanti,",
  "tha-ap431:7.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap431:8.1": "Saṭṭhituriyasahassāni,",
  "tha-ap431:8.2": "bheriyo paṇavāni ca;",
  "tha-ap431:8.3": "Saṅkhā ca ḍiṇḍimā tattha,",
  "tha-ap431:8.4": "vaggū vajjanti dundubhī.",
  "tha-ap431:9.1": "Cullāsītisahassāni,",
  "tha-ap431:9.2": "hatthino samalaṅkatā;",
  "tha-ap431:9.3": "Tidhāpabhinnamātaṅgā,",
  "tha-ap431:9.4": "kuñjarā saṭṭhihāyanā.",
  "tha-ap431:10.1": "Hemajālābhisañchannā,",
  "tha-ap431:10.2": "upaṭṭhānaṃ karonti me;",
  "tha-ap431:10.3": "Balakāye gaje ceva,",
  "tha-ap431:10.4": "ūnatā me na vijjati.",
  "tha-ap431:11.1": "Soṇṇakiṅkaṇipupphānaṃ,",
  "tha-ap431:11.2": "vipākaṃ anubhomahaṃ;",
  "tha-ap431:11.3": "Aṭṭhapaññāsakkhattuñca,",
  "tha-ap431:11.4": "devarajjamakārayiṃ.",
  "tha-ap431:12.1": "Ekasattatikkhattuñca,",
  "tha-ap431:12.2": "cakkavattī ahosahaṃ;",
  "tha-ap431:12.3": "Pathabyā rajjaṃ ekasataṃ,",
  "tha-ap431:12.4": "mahiyā kārayiṃ ahaṃ.",
  "tha-ap431:13.1": "So dāni amataṃ patto,",
  "tha-ap431:13.2": "asaṅkhataṃ sududdasaṃ;",
  "tha-ap431:13.3": "Saṃyojanaparikkhīṇo,",
  "tha-ap431:13.4": "natthi dāni punabbhavo.",
  "tha-ap431:14.1": "Aṭṭhārase kappasate,",
  "tha-ap431:14.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap431:14.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap431:14.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap431:15.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap431:15.2": "…pe…",
  "tha-ap431:15.3": "viharāmi anāsavo.",
  "tha-ap431:16.1": "Svāgataṃ vata me āsi,",
  "tha-ap431:16.2": "…pe…",
  "tha-ap431:16.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap431:17.1": "Paṭisambhidā catasso,",
  "tha-ap431:17.2": "…pe…",
  "tha-ap431:17.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap431:18.1": "Itthaṃ sudaṃ āyasmā sovaṇṇakiṅkaṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap431:19.1": "Sovaṇṇakiṅkaṇiyattherassāpadānaṃ navamaṃ."
}