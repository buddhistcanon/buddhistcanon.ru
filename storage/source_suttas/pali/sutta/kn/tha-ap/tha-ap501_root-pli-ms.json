{
  "tha-ap501:0.1": "Therāpadāna",
  "tha-ap501:0.2": "Kiṅkaṇipupphavagga",
  "tha-ap501:0.3": "9 Naḷakuṭidāyakattheraapadāna",
  "tha-ap501:1.1": "“Himavantassāvidūre,",
  "tha-ap501:1.2": "hārito nāma pabbato;",
  "tha-ap501:1.3": "Sayambhū nārado nāma,",
  "tha-ap501:1.4": "rukkhamūle vasī tadā.",
  "tha-ap501:2.1": "Naḷāgāraṃ karitvāna,",
  "tha-ap501:2.2": "tiṇena chādayiṃ ahaṃ;",
  "tha-ap501:2.3": "Caṅkamaṃ sodhayitvāna,",
  "tha-ap501:2.4": "sayambhussa adāsahaṃ.",
  "tha-ap501:3.1": "Tena kammena sukatena,",
  "tha-ap501:3.2": "cetanāpaṇidhīhi ca;",
  "tha-ap501:3.3": "Jahitvā mānusaṃ dehaṃ,",
  "tha-ap501:3.4": "tāvatiṃsamagacchahaṃ.",
  "tha-ap501:4.1": "Tattha me sukataṃ byamhaṃ,",
  "tha-ap501:4.2": "naḷakuṭikanimmitaṃ;",
  "tha-ap501:4.3": "Saṭṭhiyojanamubbedhaṃ,",
  "tha-ap501:4.4": "tiṃsayojanavitthataṃ.",
  "tha-ap501:5.1": "Catuddasesu kappesu,",
  "tha-ap501:5.2": "devaloke ramiṃ ahaṃ;",
  "tha-ap501:5.3": "Ekasattatikkhattuñca,",
  "tha-ap501:5.4": "devarajjamakārayiṃ.",
  "tha-ap501:6.1": "Catutiṃsatikkhattuñca,",
  "tha-ap501:6.2": "cakkavattī ahosahaṃ;",
  "tha-ap501:6.3": "Padesarajjaṃ vipulaṃ,",
  "tha-ap501:6.4": "gaṇanāto asaṅkhiyaṃ.",
  "tha-ap501:7.1": "Dhammapāsādamāruyha,",
  "tha-ap501:7.2": "sabbākāravarūpamaṃ;",
  "tha-ap501:7.3": "Yadicchakāhaṃ vihare,",
  "tha-ap501:7.4": "sakyaputtassa sāsane.",
  "tha-ap501:8.1": "Ekatiṃse ito kappe,",
  "tha-ap501:8.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap501:8.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap501:8.4": "naḷakuṭiyidaṃ phalaṃ.",
  "tha-ap501:9.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap501:9.2": "…pe…",
  "tha-ap501:9.3": "viharāmi anāsavo.",
  "tha-ap501:10.1": "Svāgataṃ vata me āsi,",
  "tha-ap501:10.2": "…pe…",
  "tha-ap501:10.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap501:11.1": "Paṭisambhidā catasso,",
  "tha-ap501:11.2": "…pe…",
  "tha-ap501:11.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap501:12.1": "Itthaṃ sudaṃ āyasmā naḷakuṭidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap501:13.1": "Naḷakuṭidāyakattherassāpadānaṃ navamaṃ."
}