{
  "tha-ap390:0.1": "Therāpadāna",
  "tha-ap390:0.2": "Avaṭaphalavagga",
  "tha-ap390:0.3": "8. Panasaphaladāyakattheraapadāna",
  "tha-ap390:1.1": "“Ajjuno nāma sambuddho,",
  "tha-ap390:1.2": "himavante vasī tadā;",
  "tha-ap390:1.3": "Caraṇena ca sampanno,",
  "tha-ap390:1.4": "samādhikusalo muni.",
  "tha-ap390:2.1": "Kumbhamattaṃ gahetvāna,",
  "tha-ap390:2.2": "panasaṃ jīvajīvakaṃ;",
  "tha-ap390:2.3": "Chattapaṇṇe ṭhapetvāna,",
  "tha-ap390:2.4": "adāsiṃ satthuno ahaṃ.",
  "tha-ap390:3.1": "Ekattiṃse ito kappe,",
  "tha-ap390:3.2": "yaṃ phalamadadiṃ tadā;",
  "tha-ap390:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap390:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap390:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap390:4.2": "…pe…",
  "tha-ap390:4.3": "viharāmi anāsavo.",
  "tha-ap390:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap390:5.2": "…pe…",
  "tha-ap390:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap390:6.1": "Paṭisambhidā catasso,",
  "tha-ap390:6.2": "…pe…",
  "tha-ap390:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap390:7.1": "Itthaṃ sudaṃ āyasmā panasaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap390:8.1": "Panasaphaladāyakattherassāpadānaṃ aṭṭhamaṃ."
}