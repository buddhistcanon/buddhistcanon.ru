{
  "tha-ap509:0.1": "Therāpadāna",
  "tha-ap509:0.2": "Kaṇikāravagga",
  "tha-ap509:0.3": "7 Ajeliphaladāyakattheraapadāna",
  "tha-ap509:1.1": "“Ajjuno nāma sambuddho,",
  "tha-ap509:1.2": "himavante vasī tadā;",
  "tha-ap509:1.3": "Caraṇena ca sampanno,",
  "tha-ap509:1.4": "samādhikusalo muni.",
  "tha-ap509:2.1": "Kumbhamattaṃ gahetvāna,",
  "tha-ap509:2.2": "ajeliṃ jīvajīvakaṃ;",
  "tha-ap509:2.3": "Chattapaṇṇaṃ gahetvāna,",
  "tha-ap509:2.4": "adāsiṃ satthuno ahaṃ.",
  "tha-ap509:3.1": "Catunnavutito kappe,",
  "tha-ap509:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap509:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap509:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap509:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap509:4.2": "…pe…",
  "tha-ap509:4.3": "viharāmi anāsavo.",
  "tha-ap509:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap509:5.2": "…pe…",
  "tha-ap509:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap509:6.1": "Paṭisambhidā catasso,",
  "tha-ap509:6.2": "…pe…",
  "tha-ap509:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap509:7.1": "Itthaṃ sudaṃ āyasmā ajeliphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap509:8.1": "Ajeliphaladāyakattherassāpadānaṃ sattamaṃ."
}