{
  "tha-ap30:0.1": "Therāpadāna",
  "tha-ap30:0.2": "Subhūtivagga",
  "tha-ap30:0.3": "8 Uttiyattheraapadāna",
  "tha-ap30:1.1": "“Candabhāgānadītīre,",
  "tha-ap30:1.2": "susumāro ahaṃ tadā;",
  "tha-ap30:1.3": "Sagocarappasutohaṃ,",
  "tha-ap30:1.4": "nadītitthaṃ agacchahaṃ.",
  "tha-ap30:2.1": "Siddhattho tamhi samaye,",
  "tha-ap30:2.2": "sayambhū aggapuggalo;",
  "tha-ap30:2.3": "Nadiṃ taritukāmo so,",
  "tha-ap30:2.4": "nadītitthaṃ upāgami.",
  "tha-ap30:3.1": "Upāgate ca sambuddhe,",
  "tha-ap30:3.2": "ahampi tatthupāgamiṃ;",
  "tha-ap30:3.3": "Upagantvāna sambuddhaṃ,",
  "tha-ap30:3.4": "imaṃ vācaṃ udīrayiṃ.",
  "tha-ap30:4.1": "‘Abhirūha mahāvīra,",
  "tha-ap30:4.2": "tāressāmi ahaṃ tuvaṃ;",
  "tha-ap30:4.3": "Pettikaṃ visayaṃ mayhaṃ,",
  "tha-ap30:4.4": "anukampa mahāmuni’.",
  "tha-ap30:5.1": "Mama uggajjanaṃ sutvā,",
  "tha-ap30:5.2": "abhirūhi mahāmuni;",
  "tha-ap30:5.3": "Haṭṭho haṭṭhena cittena,",
  "tha-ap30:5.4": "tāresiṃ lokanāyakaṃ.",
  "tha-ap30:6.1": "Nadiyā pārime tīre,",
  "tha-ap30:6.2": "siddhattho lokanāyako;",
  "tha-ap30:6.3": "Assāsesi mamaṃ tattha,",
  "tha-ap30:6.4": "amataṃ pāpuṇissasi.",
  "tha-ap30:7.1": "Tamhā kāyā cavitvāna,",
  "tha-ap30:7.2": "devalokaṃ āgacchahaṃ;",
  "tha-ap30:7.3": "Dibbasukhaṃ anubhaviṃ,",
  "tha-ap30:7.4": "accharāhi purakkhato.",
  "tha-ap30:8.1": "Sattakkhattuñca devindo,",
  "tha-ap30:8.2": "devarajjamakāsahaṃ;",
  "tha-ap30:8.3": "Tīṇikkhattuṃ cakkavattī,",
  "tha-ap30:8.4": "mahiyā issaro ahuṃ.",
  "tha-ap30:9.1": "Vivekamanuyuttohaṃ,",
  "tha-ap30:9.2": "nipako ca susaṃvuto;",
  "tha-ap30:9.3": "Dhāremi antimaṃ dehaṃ,",
  "tha-ap30:9.4": "sammāsambuddhasāsane.",
  "tha-ap30:10.1": "Catunnavutito kappe,",
  "tha-ap30:10.2": "tāresiṃ yaṃ narāsabhaṃ;",
  "tha-ap30:10.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap30:10.4": "taraṇāya idaṃ phalaṃ.",
  "tha-ap30:11.1": "Paṭisambhidā catasso,",
  "tha-ap30:11.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap30:11.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap30:11.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap30:12.1": "Itthaṃ sudaṃ āyasmā uttiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap30:13.1": "Uttiyattherassāpadānaṃ aṭṭhamaṃ."
}