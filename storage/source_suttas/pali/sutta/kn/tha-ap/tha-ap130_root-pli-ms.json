{
  "tha-ap130:0.1": "Therāpadāna",
  "tha-ap130:0.2": "Sereyyavagga",
  "tha-ap130:0.3": "8 Ñāṇasaññikattheraapadāna",
  "tha-ap130:1.1": "“Pabbate himavantamhi,",
  "tha-ap130:1.2": "vasāmi pabbatantare;",
  "tha-ap130:1.3": "Pulinaṃ sobhanaṃ disvā,",
  "tha-ap130:1.4": "buddhaseṭṭhaṃ anussariṃ.",
  "tha-ap130:2.1": "Ñāṇe upanidhā natthi,",
  "tha-ap130:2.2": "saṅkhāraṃ natthi satthuno;",
  "tha-ap130:2.3": "Sabbadhammaṃ abhiññāya,",
  "tha-ap130:2.4": "ñāṇena adhimuccati.",
  "tha-ap130:3.1": "Namo te purisājañña,",
  "tha-ap130:3.2": "namo te purisuttama;",
  "tha-ap130:3.3": "Ñāṇena te samo natthi,",
  "tha-ap130:3.4": "yāvatā ñāṇamuttamaṃ.",
  "tha-ap130:4.1": "Ñāṇe cittaṃ pasādetvā,",
  "tha-ap130:4.2": "kappaṃ saggamhi modahaṃ;",
  "tha-ap130:4.3": "Avasesesu kappesu,",
  "tha-ap130:4.4": "kusalaṃ caritaṃ mayā.",
  "tha-ap130:5.1": "Ekanavutito kappe,",
  "tha-ap130:5.2": "yaṃ saññamalabhiṃ tadā;",
  "tha-ap130:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap130:5.4": "ñāṇasaññāyidaṃ phalaṃ.",
  "tha-ap130:6.1": "Ito sattatikappamhi,",
  "tha-ap130:6.2": "eko pulinapupphiyo;",
  "tha-ap130:6.3": "Sattaratanasampanno,",
  "tha-ap130:6.4": "cakkavattī mahabbalo.",
  "tha-ap130:7.1": "Paṭisambhidā catasso,",
  "tha-ap130:7.2": "…pe…",
  "tha-ap130:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap130:8.1": "Itthaṃ sudaṃ āyasmā ñāṇasaññiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap130:9.1": "Ñāṇasaññikattherassāpadānaṃ aṭṭhamaṃ."
}