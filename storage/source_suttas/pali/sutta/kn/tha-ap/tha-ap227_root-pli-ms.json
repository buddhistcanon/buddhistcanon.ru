{
  "tha-ap227:0.1": "Therāpadāna",
  "tha-ap227:0.2": "Ālambaṇadāyakavagga",
  "tha-ap227:0.3": "5. Abyādhikattheraapadāna",
  "tha-ap227:1.1": "“Vipassissa bhagavato,",
  "tha-ap227:1.2": "aggisālaṃ adāsahaṃ;",
  "tha-ap227:1.3": "Byādhikānañca āvāsaṃ,",
  "tha-ap227:1.4": "uṇhodakapaṭiggahaṃ.",
  "tha-ap227:2.1": "Tena kammenayaṃ mayhaṃ,",
  "tha-ap227:2.2": "attabhāvo sunimmito;",
  "tha-ap227:2.3": "Byādhāhaṃ nābhijānāmi,",
  "tha-ap227:2.4": "puññakammassidaṃ phalaṃ.",
  "tha-ap227:3.1": "Ekanavutito kappe,",
  "tha-ap227:3.2": "yaṃ sālamadadiṃ tadā;",
  "tha-ap227:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap227:3.4": "aggisālāyidaṃ phalaṃ.",
  "tha-ap227:4.1": "Ito ca sattame kappe,",
  "tha-ap227:4.2": "ekosiṃ aparājito;",
  "tha-ap227:4.3": "Sattaratanasampanno,",
  "tha-ap227:4.4": "cakkavattī mahabbalo.",
  "tha-ap227:5.1": "Paṭisambhidā catasso,",
  "tha-ap227:5.2": "…pe…",
  "tha-ap227:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap227:6.1": "Itthaṃ sudaṃ āyasmā abyādhiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap227:7.1": "Abyādhikattherassāpadānaṃ pañcamaṃ."
}