{
  "tha-ap364:0.1": "Therāpadāna",
  "tha-ap364:0.2": "Mandāravapupphiyavagga",
  "tha-ap364:0.3": "2. Kakkārupupphiyattheraapadāna",
  "tha-ap364:1.1": "“Yāmā devā idhāgantvā,",
  "tha-ap364:1.2": "gotamaṃ sirivacchasaṃ;",
  "tha-ap364:1.3": "Kakkārumālaṃ paggayha,",
  "tha-ap364:1.4": "buddhassa abhiropayiṃ.",
  "tha-ap364:2.1": "Dvenavute ito kappe,",
  "tha-ap364:2.2": "yaṃ buddhamabhipūjayiṃ;",
  "tha-ap364:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap364:2.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap364:3.1": "Paṭisambhidā catasso,",
  "tha-ap364:3.2": "…pe…",
  "tha-ap364:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap364:4.1": "Itthaṃ sudaṃ āyasmā kakkārupupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap364:5.1": "Kakkārupupphiyattherassāpadānaṃ dutiyaṃ."
}