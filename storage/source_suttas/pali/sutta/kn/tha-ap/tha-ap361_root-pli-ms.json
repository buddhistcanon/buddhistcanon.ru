{
  "tha-ap361:0.1": "Therāpadāna",
  "tha-ap361:0.2": "Saddasaññakavagga",
  "tha-ap361:0.3": "9 Saraṇagamaniyattheraapadāna",
  "tha-ap361:1.1": "“Āruhimha tadā nāvaṃ,",
  "tha-ap361:1.2": "bhikkhu cājīvako cahaṃ;",
  "tha-ap361:1.3": "Nāvāya bhijjamānāya,",
  "tha-ap361:1.4": "bhikkhu me saraṇaṃ adā.",
  "tha-ap361:2.1": "Ekattiṃse ito kappe,",
  "tha-ap361:2.2": "yañca me saraṇaṃ adā;",
  "tha-ap361:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap361:2.4": "saraṇāgamane phalaṃ.",
  "tha-ap361:3.1": "Paṭisambhidā catasso,",
  "tha-ap361:3.2": "…pe…",
  "tha-ap361:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap361:4.1": "Itthaṃ sudaṃ āyasmā saraṇagamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap361:5.1": "Saraṇagamaniyattherassāpadānaṃ navamaṃ."
}