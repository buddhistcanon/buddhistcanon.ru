{
  "tha-ap275:0.1": "Therāpadāna",
  "tha-ap275:0.2": "Suvaṇṇabibbohanavagga",
  "tha-ap275:0.3": "3 Caṅkoṭakiyattheraapadāna",
  "tha-ap275:1.1": "“Mahāsamuddaṃ nissāya,",
  "tha-ap275:1.2": "vasatī pabbatantare;",
  "tha-ap275:1.3": "Paccuggantvāna katvāna,",
  "tha-ap275:1.4": "caṅkoṭaka madāsahaṃ.",
  "tha-ap275:2.1": "Siddhatthassa mahesino,",
  "tha-ap275:2.2": "sabbasattānukampino;",
  "tha-ap275:2.3": "Pupphacaṅkoṭakaṃ datvā,",
  "tha-ap275:2.4": "kappaṃ saggamhi modahaṃ.",
  "tha-ap275:3.1": "Catunnavutito kappe,",
  "tha-ap275:3.2": "caṅkoṭakamadaṃ tadā;",
  "tha-ap275:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap275:3.4": "caṅkoṭakassidaṃ phalaṃ.",
  "tha-ap275:4.1": "Paṭisambhidā catasso,",
  "tha-ap275:4.2": "…pe…",
  "tha-ap275:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap275:5.1": "Itthaṃ sudaṃ āyasmā caṅkoṭakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap275:6.1": "Caṅkoṭakiyattherassāpadānaṃ tatiyaṃ."
}