{
  "tha-ap250:0.1": "Therāpadāna",
  "tha-ap250:0.2": "Tuvaradāyakavagga",
  "tha-ap250:0.3": "8 Sattalipupphapūjakattheraapadāna",
  "tha-ap250:1.1": "“Satta sattalipupphāni,",
  "tha-ap250:1.2": "sīse katvānahaṃ tadā;",
  "tha-ap250:1.3": "Buddhassa abhiropesiṃ,",
  "tha-ap250:1.4": "vessabhumhi naruttame.",
  "tha-ap250:2.1": "Ekattiṃse ito kappe,",
  "tha-ap250:2.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap250:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap250:2.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap250:3.1": "Paṭisambhidā catasso,",
  "tha-ap250:3.2": "…pe…",
  "tha-ap250:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap250:4.1": "Itthaṃ sudaṃ āyasmā sattalipupphapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap250:5.1": "Sattalipupphapūjakattherassāpadānaṃ aṭṭhamaṃ."
}