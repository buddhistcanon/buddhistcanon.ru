{
  "tha-ap67:0.1": "Therāpadāna",
  "tha-ap67:0.2": "Sakacintaniyavagga",
  "tha-ap67:0.3": "5. Bhisadāyakattheraapadāna",
  "tha-ap67:1.1": "“Vessabhū nāma nāmena,",
  "tha-ap67:1.2": "isīnaṃ tatiyo ahu;",
  "tha-ap67:1.3": "Kānanaṃ vanamogayha,",
  "tha-ap67:1.4": "vihāsi purisuttamo.",
  "tha-ap67:2.1": "Bhisamuḷālaṃ gaṇhitvā,",
  "tha-ap67:2.2": "agamaṃ buddhasantikaṃ;",
  "tha-ap67:2.3": "Tañca buddhassa pādāsiṃ,",
  "tha-ap67:2.4": "pasanno sehi pāṇibhi.",
  "tha-ap67:3.1": "Karena ca parāmaṭṭho,",
  "tha-ap67:3.2": "vessabhūvarabuddhinā;",
  "tha-ap67:3.3": "Sukhāhaṃ nābhijānāmi,",
  "tha-ap67:3.4": "samaṃ tena kutottariṃ.",
  "tha-ap67:4.1": "Carimo vattate mayhaṃ,",
  "tha-ap67:4.2": "bhavā sabbe samūhatā;",
  "tha-ap67:4.3": "Hatthināgena santena,",
  "tha-ap67:4.4": "kusalaṃ ropitaṃ mayā.",
  "tha-ap67:5.1": "Ekattiṃse ito kappe,",
  "tha-ap67:5.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap67:5.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap67:5.4": "bhisadānassidaṃ phalaṃ.",
  "tha-ap67:6.1": "Samodhānā ca rājāno,",
  "tha-ap67:6.2": "soḷasa manujādhipā;",
  "tha-ap67:6.3": "Kappamhi cuddase āsuṃ,",
  "tha-ap67:6.4": "cakkavattī mahabbalā.",
  "tha-ap67:7.1": "Paṭisambhidā catasso,",
  "tha-ap67:7.2": "…pe…",
  "tha-ap67:7.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap67:8.1": "Itthaṃ sudaṃ āyasmā bhisadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap67:9.1": "Bhisadāyakattherassāpadānaṃ pañcamaṃ."
}