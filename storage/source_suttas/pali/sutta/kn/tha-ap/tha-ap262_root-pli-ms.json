{
  "tha-ap262:0.1": "Therāpadāna",
  "tha-ap262:0.2": "Thomakavagga",
  "tha-ap262:0.3": "10. Aggapupphiyattheraapadāna",
  "tha-ap262:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap262:1.2": "Nisinnaṃ pabbatantare;",
  "tha-ap262:1.3": "Obhāsayantaṃ raṃsena,",
  "tha-ap262:1.4": "Sikhinaṃ sikhinaṃ yathā.",
  "tha-ap262:2.1": "Aggajaṃ pupphamādāya,",
  "tha-ap262:2.2": "upāgacchiṃ naruttamaṃ;",
  "tha-ap262:2.3": "Pasannacitto sumano,",
  "tha-ap262:2.4": "buddhassa abhiropayiṃ.",
  "tha-ap262:3.1": "Ekattiṃse ito kappe,",
  "tha-ap262:3.2": "yaṃ pupphamabhipūjayiṃ;",
  "tha-ap262:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap262:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap262:4.1": "Pañcavīsatikappamhi,",
  "tha-ap262:4.2": "ahosi amitogato;",
  "tha-ap262:4.3": "Sattaratanasampanno,",
  "tha-ap262:4.4": "cakkavattī mahabbalo.",
  "tha-ap262:5.1": "Paṭisambhidā catasso,",
  "tha-ap262:5.2": "…pe…",
  "tha-ap262:5.3": "kataṃ buddhassa sāsanaṃ”. (2535)",
  "tha-ap262:6.1": "Itthaṃ sudaṃ āyasmā aggapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap262:7.1": "Aggapupphiyattherassāpadānaṃ dasamaṃ.",
  "tha-ap262:8.1": "Thomakavaggo chabbīsatimo.",
  "tha-ap262:9.1": "Tassuddānaṃ",
  "tha-ap262:10.1": "Thomakekāsanacitakaṃ,",
  "tha-ap262:10.2": "campako sattapāṭali;",
  "tha-ap262:10.3": "Pānadhi mañjarī paṇṇaṃ,",
  "tha-ap262:10.4": "kuṭido aggapupphiyo;",
  "tha-ap262:10.5": "Gāthāyo gaṇitā cettha,",
  "tha-ap262:10.6": "ekatālīsameva cāti."
}