{
  "tha-ap456:0.1": "Therāpadāna",
  "tha-ap456:0.2": "Jagatidāyakavagga",
  "tha-ap456:0.3": "4 Tiṇukkadhāriyattheraapadāna",
  "tha-ap456:1.1": "“Padumuttarabuddhassa,",
  "tha-ap456:1.2": "bodhiyā pādaputtame;",
  "tha-ap456:1.3": "Pasannacitto sumano,",
  "tha-ap456:1.4": "tayo ukke adhārayiṃ.",
  "tha-ap456:2.1": "Satasahassito kappe,",
  "tha-ap456:2.2": "sohaṃ ukkamadhārayiṃ;",
  "tha-ap456:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap456:2.4": "ukkadānassidaṃ phalaṃ.",
  "tha-ap456:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap456:3.2": "…pe…",
  "tha-ap456:3.3": "viharāmi anāsavo.",
  "tha-ap456:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap456:4.2": "…pe…",
  "tha-ap456:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap456:5.1": "Paṭisambhidā catasso,",
  "tha-ap456:5.2": "…pe…",
  "tha-ap456:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap456:6.1": "Itthaṃ sudaṃ āyasmā tiṇukkadhāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap456:7.1": "Tiṇukkadhāriyattherassāpadānaṃ catutthaṃ."
}