{
  "tha-ap91:0.1": "Therāpadāna",
  "tha-ap91:0.2": "Timiravagga",
  "tha-ap91:0.3": "9. Bodhisiñcakattheraapadāna",
  "tha-ap91:1.1": "“Vipassissa bhagavato,",
  "tha-ap91:1.2": "mahābodhimaho ahu;",
  "tha-ap91:1.3": "Pabbajjupagato santo,",
  "tha-ap91:1.4": "upagacchiṃ ahaṃ tadā.",
  "tha-ap91:2.1": "Kusumodakamādāya,",
  "tha-ap91:2.2": "bodhiyā okiriṃ ahaṃ;",
  "tha-ap91:2.3": "Mocayissati no mutto,",
  "tha-ap91:2.4": "nibbāpessati nibbuto.",
  "tha-ap91:3.1": "Ekanavutito kappe,",
  "tha-ap91:3.2": "yaṃ bodhimabhisiñcayiṃ;",
  "tha-ap91:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap91:3.4": "bodhisiñcāyidaṃ phalaṃ.",
  "tha-ap91:4.1": "Tettiṃse vattamānamhi,",
  "tha-ap91:4.2": "kappe āsuṃ janādhipā;",
  "tha-ap91:4.3": "Udakasecanā nāma,",
  "tha-ap91:4.4": "aṭṭhete cakkavattino.",
  "tha-ap91:5.1": "Paṭisambhidā catasso,",
  "tha-ap91:5.2": "…pe…",
  "tha-ap91:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap91:6.1": "Itthaṃ sudaṃ āyasmā bodhisiñcako thero imā gāthāyo abhāsitthāti.",
  "tha-ap91:7.1": "Bodhisiñcakattherassāpadānaṃ navamaṃ."
}