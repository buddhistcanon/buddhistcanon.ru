{
  "tha-ap176:0.1": "Therāpadāna",
  "tha-ap176:0.2": "Kumudavagga",
  "tha-ap176:0.3": "4. Udapānadāyakattheraapadāna",
  "tha-ap176:1.1": "“Vipassino bhagavato,",
  "tha-ap176:1.2": "udapāno kato mayā;",
  "tha-ap176:1.3": "Piṇḍapātañca datvāna,",
  "tha-ap176:1.4": "niyyādesimahaṃ tadā.",
  "tha-ap176:2.1": "Ekanavutito kappe,",
  "tha-ap176:2.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap176:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap176:2.4": "udapānassidaṃ phalaṃ.",
  "tha-ap176:3.1": "Paṭisambhidā catasso,",
  "tha-ap176:3.2": "…pe…",
  "tha-ap176:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap176:4.1": "Itthaṃ sudaṃ āyasmā udapānadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap176:5.1": "Udapānadāyakattherassāpadānaṃ catutthaṃ."
}