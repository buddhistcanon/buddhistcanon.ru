{
  "tha-ap71:0.1": "Therāpadāna",
  "tha-ap71:0.2": "Sakacintaniyavagga",
  "tha-ap71:0.3": "9. Sumanattheraapadāna",
  "tha-ap71:1.1": "“Sumano nāma nāmena,",
  "tha-ap71:1.2": "mālākāro ahaṃ tadā;",
  "tha-ap71:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap71:1.4": "lokāhutipaṭiggahaṃ.",
  "tha-ap71:2.1": "Ubho hatthehi paggayha,",
  "tha-ap71:2.2": "sumanaṃ pupphamuttamaṃ;",
  "tha-ap71:2.3": "Buddhassa abhiropesiṃ,",
  "tha-ap71:2.4": "sikhino lokabandhuno.",
  "tha-ap71:3.1": "Imāya pupphapūjāya,",
  "tha-ap71:3.2": "cetanāpaṇidhīhi ca;",
  "tha-ap71:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap71:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap71:4.1": "Ekattiṃse ito kappe,",
  "tha-ap71:4.2": "yaṃ pupphamabhiropayiṃ;",
  "tha-ap71:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap71:4.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap71:5.1": "Chabbīsatimhi kappamhi,",
  "tha-ap71:5.2": "cattārosuṃ mahāyasā;",
  "tha-ap71:5.3": "Sattaratanasampannā,",
  "tha-ap71:5.4": "rājāno cakkavattino.",
  "tha-ap71:6.1": "Paṭisambhidā catasso,",
  "tha-ap71:6.2": "…pe…",
  "tha-ap71:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap71:7.1": "Itthaṃ sudaṃ āyasmā sumano thero imā gāthāyo abhāsitthāti.",
  "tha-ap71:8.1": "Sumanattherassāpadānaṃ navamaṃ."
}