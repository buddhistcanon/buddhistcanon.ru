{
  "tha-ap121:0.1": "Therāpadāna",
  "tha-ap121:0.2": "Mahāparivāravagga",
  "tha-ap121:0.3": "9. Paccupaṭṭhānasaññakattheraapadāna",
  "tha-ap121:1.1": "“Atthadassimhi sugate,",
  "tha-ap121:1.2": "nibbute samanantarā;",
  "tha-ap121:1.3": "Yakkhayoniṃ upapajjiṃ,",
  "tha-ap121:1.4": "yasaṃ patto cahaṃ tadā.",
  "tha-ap121:2.1": "Dulladdhaṃ vata me āsi,",
  "tha-ap121:2.2": "duppabhātaṃ duruṭṭhitaṃ;",
  "tha-ap121:2.3": "Yaṃ me bhoge vijjamāne,",
  "tha-ap121:2.4": "parinibbāyi cakkhumā.",
  "tha-ap121:3.1": "Mama saṅkappamaññāya,",
  "tha-ap121:3.2": "sāgaro nāma sāvako;",
  "tha-ap121:3.3": "Mamuddharitukāmo so,",
  "tha-ap121:3.4": "āgacchi mama santikaṃ.",
  "tha-ap121:4.1": "Kiṃ nu socasi mā bhāyi,",
  "tha-ap121:4.2": "cara dhammaṃ sumedhasa;",
  "tha-ap121:4.3": "Anuppadinnā buddhena,",
  "tha-ap121:4.4": "sabbesaṃ bījasampadā.",
  "tha-ap121:5.1": "Yo ce pūjeyya sambuddhaṃ,",
  "tha-ap121:5.2": "tiṭṭhantaṃ lokanāyakaṃ;",
  "tha-ap121:5.3": "Dhātuṃ sāsapamattampi,",
  "tha-ap121:5.4": "nibbutassāpi pūjaye.",
  "tha-ap121:6.1": "Same cittappasādamhi,",
  "tha-ap121:6.2": "samaṃ puññaṃ mahaggataṃ;",
  "tha-ap121:6.3": "Tasmā thūpaṃ karitvāna,",
  "tha-ap121:6.4": "pūjehi jinadhātuyo.",
  "tha-ap121:7.1": "Sāgarassa vaco sutvā,",
  "tha-ap121:7.2": "buddhathūpaṃ akāsahaṃ;",
  "tha-ap121:7.3": "Pañcavasse paricariṃ,",
  "tha-ap121:7.4": "munino thūpamuttamaṃ.",
  "tha-ap121:8.1": "Tena kammena dvipadinda,",
  "tha-ap121:8.2": "lokajeṭṭha narāsabha;",
  "tha-ap121:8.3": "Sampattiṃ anubhotvāna,",
  "tha-ap121:8.4": "arahattamapāpuṇiṃ.",
  "tha-ap121:9.1": "Bhūripaññā ca cattāro,",
  "tha-ap121:9.2": "sattakappasate ito;",
  "tha-ap121:9.3": "Sattaratanasampannā,",
  "tha-ap121:9.4": "cakkavattī mahabbalā.",
  "tha-ap121:10.1": "Paṭisambhidā catasso,",
  "tha-ap121:10.2": "…pe…",
  "tha-ap121:10.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap121:11.1": "Itthaṃ sudaṃ āyasmā paccupaṭṭhānasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap121:12.1": "Paccupaṭṭhānasaññakattherassāpadānaṃ navamaṃ."
}