{
  "tha-ap432:0.1": "Therāpadāna",
  "tha-ap432:0.2": "Sakiṃsammajjakavagga",
  "tha-ap432:0.3": "10 Soṇṇakontarikattheraapadāna",
  "tha-ap432:1.1": "“Manobhāvaniyaṃ buddhaṃ,",
  "tha-ap432:1.2": "attadantaṃ samāhitaṃ;",
  "tha-ap432:1.3": "Iriyamānaṃ brahmapathe,",
  "tha-ap432:1.4": "cittavūpasame rataṃ.",
  "tha-ap432:2.1": "Nittiṇṇaoghaṃ sambuddhaṃ,",
  "tha-ap432:2.2": "jhāyiṃ jhānarataṃ muniṃ;",
  "tha-ap432:2.3": "Upatitthaṃ samāpannaṃ,",
  "tha-ap432:2.4": "indivaradalappabhaṃ.",
  "tha-ap432:3.1": "Alābunodakaṃ gayha,",
  "tha-ap432:3.2": "buddhaseṭṭhaṃ upāgamiṃ;",
  "tha-ap432:3.3": "Buddhassa pāde dhovitvā,",
  "tha-ap432:3.4": "alābukamadāsahaṃ.",
  "tha-ap432:4.1": "Āṇāpesi ca sambuddho,",
  "tha-ap432:4.2": "padumuttaranāmako;",
  "tha-ap432:4.3": "‘Iminā dakamāhatvā,",
  "tha-ap432:4.4": "pādamūle ṭhapehi me’.",
  "tha-ap432:5.1": "Sādhūtihaṃ paṭissutvā,",
  "tha-ap432:5.2": "satthugāravatāya ca;",
  "tha-ap432:5.3": "Dakaṃ alābunāhatvā,",
  "tha-ap432:5.4": "buddhaseṭṭhaṃ upāgamiṃ.",
  "tha-ap432:6.1": "Anumodi mahāvīro,",
  "tha-ap432:6.2": "cittaṃ nibbāpayaṃ mama;",
  "tha-ap432:6.3": "‘Iminālābudānena,",
  "tha-ap432:6.4": "saṅkappo te samijjhatu’.",
  "tha-ap432:7.1": "Pannarasesu kappesu,",
  "tha-ap432:7.2": "devaloke ramiṃ ahaṃ;",
  "tha-ap432:7.3": "Tiṃsatikkhattuṃ rājā ca,",
  "tha-ap432:7.4": "cakkavattī ahosahaṃ.",
  "tha-ap432:8.1": "Divā vā yadi vā rattiṃ,",
  "tha-ap432:8.2": "caṅkamantassa tiṭṭhato;",
  "tha-ap432:8.3": "Sovaṇṇaṃ kontaraṃ gayha,",
  "tha-ap432:8.4": "tiṭṭhate purato mama.",
  "tha-ap432:9.1": "Buddhassa datvānalābuṃ,",
  "tha-ap432:9.2": "labhāmi soṇṇakontaraṃ;",
  "tha-ap432:9.3": "Appakampi kataṃ kāraṃ,",
  "tha-ap432:9.4": "vipulaṃ hoti tādisu.",
  "tha-ap432:10.1": "Satasahassito kappe,",
  "tha-ap432:10.2": "yaṃlābumadadiṃ tadā;",
  "tha-ap432:10.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap432:10.4": "alābussa idaṃ phalaṃ.",
  "tha-ap432:11.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap432:11.2": "…pe…",
  "tha-ap432:11.3": "viharāmi anāsavo.",
  "tha-ap432:12.1": "Svāgataṃ vata me āsi,",
  "tha-ap432:12.2": "…pe…",
  "tha-ap432:12.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap432:13.1": "Paṭisambhidā catasso,",
  "tha-ap432:13.2": "…pe…",
  "tha-ap432:13.3": "kataṃ buddhassa sāsanaṃ”. (4537)",
  "tha-ap432:14.1": "Itthaṃ sudaṃ āyasmā soṇṇakontariko thero imā gāthāyo abhāsitthāti.",
  "tha-ap432:15.1": "Soṇṇakontarikattherassāpadānaṃ dasamaṃ.",
  "tha-ap432:16.1": "Sakiṃsammajjakavaggo tecattālīsamo.",
  "tha-ap432:17.1": "Tassuddānaṃ",
  "tha-ap432:18.1": "Sakiṃsammajjako thero,",
  "tha-ap432:18.2": "ekadussī ekāsanī;",
  "tha-ap432:18.3": "Kadambakoraṇḍakado,",
  "tha-ap432:18.4": "ghatassavanikopi ca.",
  "tha-ap432:19.1": "Sucintiko kiṅkaṇiko,",
  "tha-ap432:19.2": "soṇṇakontarikopi ca;",
  "tha-ap432:19.3": "Ekagāthāsatañcettha,",
  "tha-ap432:19.4": "ekasattatimeva ca."
}