{
  "tha-ap380:0.1": "Therāpadāna",
  "tha-ap380:0.2": "Bodhivandanavagga",
  "tha-ap380:0.3": "8. Sumanatālavaṇṭiyattheraapadāna",
  "tha-ap380:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap380:1.2": "tālavaṇṭamadāsahaṃ;",
  "tha-ap380:1.3": "Sumanehi paṭicchannaṃ,",
  "tha-ap380:1.4": "dhārayāmi mahārahaṃ.",
  "tha-ap380:2.1": "Catunnavutito kappe,",
  "tha-ap380:2.2": "tālavaṇṭamadāsahaṃ;",
  "tha-ap380:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap380:2.4": "tālavaṇṭassidaṃ phalaṃ.",
  "tha-ap380:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap380:3.2": "…pe…",
  "tha-ap380:3.3": "viharāmi anāsavo.",
  "tha-ap380:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap380:4.2": "…pe…",
  "tha-ap380:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap380:5.1": "Paṭisambhidā catasso,",
  "tha-ap380:5.2": "…pe…",
  "tha-ap380:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap380:6.1": "Itthaṃ sudaṃ āyasmā sumanatālavaṇṭiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap380:7.1": "Sumanatālavaṇṭiyattherassāpadānaṃ aṭṭhamaṃ."
}