{
  "tha-ap315:0.1": "Therāpadāna",
  "tha-ap315:0.2": "Ārakkhadāyakavagga",
  "tha-ap315:0.3": "3. Gatasaññakattheraapadāna",
  "tha-ap315:1.1": "“Ākāseva padaṃ natthi,",
  "tha-ap315:1.2": "ambare anilañjase;",
  "tha-ap315:1.3": "Siddhatthaṃ jinamaddakkhiṃ,",
  "tha-ap315:1.4": "gacchantaṃ tidivaṅgaṇe.",
  "tha-ap315:2.1": "Anileneritaṃ disvā,",
  "tha-ap315:2.2": "sammāsambuddhacīvaraṃ;",
  "tha-ap315:2.3": "Vitti mamāhu tāvade,",
  "tha-ap315:2.4": "disvāna gamanaṃ muniṃ.",
  "tha-ap315:3.1": "Catunnavutito kappe,",
  "tha-ap315:3.2": "yaṃ saññamalabhiṃ tadā;",
  "tha-ap315:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap315:3.4": "buddhasaññāyidaṃ phalaṃ.",
  "tha-ap315:4.1": "Paṭisambhidā catasso,",
  "tha-ap315:4.2": "…pe…",
  "tha-ap315:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap315:5.1": "Itthaṃ sudaṃ āyasmā gatasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap315:6.1": "Gatasaññakattherassāpadānaṃ tatiyaṃ."
}