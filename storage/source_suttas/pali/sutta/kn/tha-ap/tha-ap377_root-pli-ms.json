{
  "tha-ap377:0.1": "Therāpadāna",
  "tha-ap377:0.2": "Bodhivandanavagga",
  "tha-ap377:0.3": "5. Sattapaṇṇiyattheraapadāna",
  "tha-ap377:1.1": "“Sumano nāma sambuddho,",
  "tha-ap377:1.2": "uppajji lokanāyako;",
  "tha-ap377:1.3": "Pasannacitto sumano,",
  "tha-ap377:1.4": "sattapaṇṇimapūjayiṃ.",
  "tha-ap377:2.1": "Satasahassito kappe,",
  "tha-ap377:2.2": "Sattapaṇṇimapūjayiṃ;",
  "tha-ap377:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap377:2.4": "Sattapaṇṇipūjāyidaṃ phalaṃ.",
  "tha-ap377:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap377:3.2": "…pe…",
  "tha-ap377:3.3": "viharāmi anāsavo.",
  "tha-ap377:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap377:4.2": "…pe…",
  "tha-ap377:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap377:5.1": "Paṭisambhidā catasso,",
  "tha-ap377:5.2": "…pe…",
  "tha-ap377:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap377:6.1": "Itthaṃ sudaṃ āyasmā sattapaṇṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap377:7.1": "Sattapaṇṇiyattherassāpadānaṃ pañcamaṃ."
}