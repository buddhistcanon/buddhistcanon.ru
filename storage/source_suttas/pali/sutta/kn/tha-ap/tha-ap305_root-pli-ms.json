{
  "tha-ap305:0.1": "Therāpadāna",
  "tha-ap305:0.2": "Padumakesaravagga",
  "tha-ap305:0.3": "3. Paramannadāyakattheraapadāna",
  "tha-ap305:1.1": "“Kaṇikāraṃva jotantaṃ,",
  "tha-ap305:1.2": "udayantaṃva bhāṇumaṃ;",
  "tha-ap305:1.3": "Vipassiṃ addasaṃ buddhaṃ,",
  "tha-ap305:1.4": "lokajeṭṭhaṃ narāsabhaṃ.",
  "tha-ap305:2.1": "Añjaliṃ paggahetvāna,",
  "tha-ap305:2.2": "abhinesiṃ sakaṃ gharaṃ;",
  "tha-ap305:2.3": "Abhinetvāna sambuddhaṃ,",
  "tha-ap305:2.4": "paramannamadāsahaṃ.",
  "tha-ap305:3.1": "Ekanavutito kappe,",
  "tha-ap305:3.2": "paramannamadiṃ tadā;",
  "tha-ap305:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap305:3.4": "paramannassidaṃ phalaṃ.",
  "tha-ap305:4.1": "Paṭisambhidā catasso,",
  "tha-ap305:4.2": "…pe…",
  "tha-ap305:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap305:5.1": "Itthaṃ sudaṃ āyasmā paramannadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap305:6.1": "Paramannadāyakattherassāpadānaṃ tatiyaṃ."
}