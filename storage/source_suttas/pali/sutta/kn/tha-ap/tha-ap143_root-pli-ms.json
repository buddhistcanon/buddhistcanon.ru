{
  "tha-ap143:0.1": "Therāpadāna",
  "tha-ap143:0.2": "Chattavagga",
  "tha-ap143:0.3": "1 Atichattiyattheraapadāna",
  "tha-ap143:1.1": "“Parinibbute bhagavati,",
  "tha-ap143:1.2": "atthadassīnaruttame;",
  "tha-ap143:1.3": "Chattātichattaṃ kāretvā,",
  "tha-ap143:1.4": "thūpamhi abhiropayiṃ.",
  "tha-ap143:2.1": "Kālena kālamāgantvā,",
  "tha-ap143:2.2": "namassiṃ lokanāyakaṃ;",
  "tha-ap143:2.3": "Pupphacchadanaṃ katvāna,",
  "tha-ap143:2.4": "chattamhi abhiropayiṃ.",
  "tha-ap143:3.1": "Sattarase kappasate,",
  "tha-ap143:3.2": "devarajjamakārayiṃ;",
  "tha-ap143:3.3": "Manussattaṃ na gacchāmi,",
  "tha-ap143:3.4": "thūpapūjāyidaṃ phalaṃ.",
  "tha-ap143:4.1": "Paṭisambhidā catasso,",
  "tha-ap143:4.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap143:4.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap143:4.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap143:5.1": "Itthaṃ sudaṃ āyasmā atichattiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap143:6.1": "Atichattiyattherassāpadānaṃ paṭhamaṃ."
}