{
  "tha-ap188:0.1": "Therāpadāna",
  "tha-ap188:0.2": "Kuṭajapupphiyavagga",
  "tha-ap188:0.3": "6 Bodhiupaṭṭhākattheraapadāna",
  "tha-ap188:1.1": "“Nagare rammavatiyā,",
  "tha-ap188:1.2": "āsiṃ murajavādako;",
  "tha-ap188:1.3": "Niccupaṭṭhānayuttomhi,",
  "tha-ap188:1.4": "gatohaṃ bodhimuttamaṃ.",
  "tha-ap188:2.1": "Sāyaṃ pātaṃ upaṭṭhitvā,",
  "tha-ap188:2.2": "sukkamūlena codito;",
  "tha-ap188:2.3": "Aṭṭhārasakappasate,",
  "tha-ap188:2.4": "duggatiṃ nupapajjahaṃ.",
  "tha-ap188:3.1": "Pannarase kappasate,",
  "tha-ap188:3.2": "ito āsiṃ janādhipo;",
  "tha-ap188:3.3": "Murajo nāma nāmena,",
  "tha-ap188:3.4": "cakkavattī mahabbalo.",
  "tha-ap188:4.1": "Paṭisambhidā catasso,",
  "tha-ap188:4.2": "…pe…",
  "tha-ap188:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap188:5.1": "Itthaṃ sudaṃ āyasmā bodhiupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
  "tha-ap188:6.1": "Bodhiupaṭṭhākattherassāpadānaṃ chaṭṭhaṃ."
}