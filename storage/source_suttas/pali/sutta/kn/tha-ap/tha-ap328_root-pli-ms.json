{
  "tha-ap328:0.1": "Therāpadāna",
  "tha-ap328:0.2": "Umāpupphiyavagga",
  "tha-ap328:0.3": "6. Annasaṃsāvakattheraapadāna",
  "tha-ap328:1.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap328:1.2": "Gacchantaṃ antarāpaṇe;",
  "tha-ap328:1.3": "Kañcanagghiyasaṅkāsaṃ,",
  "tha-ap328:1.4": "Bāttiṃsavaralakkhaṇaṃ.",
  "tha-ap328:2.1": "Siddhatthaṃ sabbasiddhatthaṃ,",
  "tha-ap328:2.2": "anejaṃ aparājitaṃ;",
  "tha-ap328:2.3": "Sambuddhaṃ atināmetvā,",
  "tha-ap328:2.4": "bhojayiṃ taṃ mahāmuniṃ.",
  "tha-ap328:3.1": "Muni kāruṇiko loke,",
  "tha-ap328:3.2": "obhāsayi mamaṃ tadā;",
  "tha-ap328:3.3": "Buddhe cittaṃ pasādetvā,",
  "tha-ap328:3.4": "kappaṃ saggamhi modahaṃ.",
  "tha-ap328:4.1": "Catunnavutito kappe,",
  "tha-ap328:4.2": "yaṃ dānamadadiṃ tadā;",
  "tha-ap328:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap328:4.4": "bhikkhādānassidaṃ phalaṃ.",
  "tha-ap328:5.1": "Paṭisambhidā catasso,",
  "tha-ap328:5.2": "…pe…",
  "tha-ap328:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap328:6.1": "Itthaṃ sudaṃ āyasmā annasaṃsāvako thero imā gāthāyo abhāsitthāti.",
  "tha-ap328:7.1": "Annasaṃsāvakattherassāpadānaṃ chaṭṭhaṃ."
}