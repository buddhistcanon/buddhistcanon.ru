{
  "tha-ap266:0.1": "Therāpadāna",
  "tha-ap266:0.2": "Padumukkhipavagga",
  "tha-ap266:0.3": "4 Padīpadāyakattheraapadāna",
  "tha-ap266:1.1": "“Devabhūto ahaṃ santo,",
  "tha-ap266:1.2": "oruyha pathaviṃ tadā;",
  "tha-ap266:1.3": "Padīpe pañca pādāsiṃ,",
  "tha-ap266:1.4": "pasanno sehi pāṇibhi.",
  "tha-ap266:2.1": "Catunnavutito kappe,",
  "tha-ap266:2.2": "yaṃ padīpamadaṃ tadā;",
  "tha-ap266:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap266:2.4": "dīpadānassidaṃ phalaṃ.",
  "tha-ap266:3.1": "Pañcapaññāsake kappe,",
  "tha-ap266:3.2": "eko āsiṃ mahīpati;",
  "tha-ap266:3.3": "Samantacakkhunāmena,",
  "tha-ap266:3.4": "cakkavattī mahabbalo.",
  "tha-ap266:4.1": "Paṭisambhidā catasso,",
  "tha-ap266:4.2": "…pe…",
  "tha-ap266:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap266:5.1": "Itthaṃ sudaṃ āyasmā padīpadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap266:6.1": "Padīpadāyakattherassāpadānaṃ catutthaṃ."
}