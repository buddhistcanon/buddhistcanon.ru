{
  "tha-ap271:0.1": "Therāpadāna",
  "tha-ap271:0.2": "Padumukkhipavagga",
  "tha-ap271:0.3": "9 Upāgatāsayattheraapadāna",
  "tha-ap271:1.1": "“Himavantassa vemajjhe,",
  "tha-ap271:1.2": "saro āsi sunimmito;",
  "tha-ap271:1.3": "Tatthāhaṃ rakkhaso āsiṃ,",
  "tha-ap271:1.4": "heṭhasīlo bhayānako.",
  "tha-ap271:2.1": "Anukampako kāruṇiko,",
  "tha-ap271:2.2": "vipassī lokanāyako;",
  "tha-ap271:2.3": "Mamuddharitukāmo so,",
  "tha-ap271:2.4": "āgacchi mama santikaṃ.",
  "tha-ap271:3.1": "Upāgataṃ mahāvīraṃ,",
  "tha-ap271:3.2": "devadevaṃ narāsabhaṃ;",
  "tha-ap271:3.3": "Āsayā abhinikkhamma,",
  "tha-ap271:3.4": "avandiṃ satthuno ahaṃ.",
  "tha-ap271:4.1": "Ekanavutito kappe,",
  "tha-ap271:4.2": "yaṃ vandiṃ purisuttamaṃ;",
  "tha-ap271:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap271:4.4": "vandanāya idaṃ phalaṃ.",
  "tha-ap271:5.1": "Paṭisambhidā catasso,",
  "tha-ap271:5.2": "…pe…",
  "tha-ap271:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap271:6.1": "Itthaṃ sudaṃ āyasmā upāgatāsayo thero imā gāthāyo abhāsitthāti.",
  "tha-ap271:7.1": "Upāgatāsayattherassāpadānaṃ navamaṃ."
}