{
  "tha-ap272:0.1": "Therāpadāna",
  "tha-ap272:0.2": "Padumukkhipavagga",
  "tha-ap272:0.3": "10. Taraṇiyattheraapadāna",
  "tha-ap272:1.1": "“Suvaṇṇavaṇṇo sambuddho,",
  "tha-ap272:1.2": "vipassī dakkhiṇāraho;",
  "tha-ap272:1.3": "Nadītīre ṭhito satthā,",
  "tha-ap272:1.4": "bhikkhusaṃghapurakkhato.",
  "tha-ap272:2.1": "Nāvā na vijjate tattha,",
  "tha-ap272:2.2": "santāraṇī mahaṇṇave;",
  "tha-ap272:2.3": "Nadiyā abhinikkhamma,",
  "tha-ap272:2.4": "tāresiṃ lokanāyakaṃ.",
  "tha-ap272:3.1": "Ekanavutito kappe,",
  "tha-ap272:3.2": "yaṃ tāresiṃ naruttamaṃ;",
  "tha-ap272:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap272:3.4": "taraṇāya idaṃ phalaṃ.",
  "tha-ap272:4.1": "Paṭisambhidā catasso,",
  "tha-ap272:4.2": "…pe…",
  "tha-ap272:4.3": "kataṃ buddhassa sāsanaṃ”. (2577)",
  "tha-ap272:5.1": "Itthaṃ sudaṃ āyasmā taraṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap272:6.1": "Taraṇiyattherassāpadānaṃ dasamaṃ.",
  "tha-ap272:7.1": "Padumukkhipavaggo sattavīsatimo.",
  "tha-ap272:8.1": "Tassuddānaṃ",
  "tha-ap272:9.1": "Ukkhipī telacandī ca,",
  "tha-ap272:9.2": "dīpado ca biḷālido;",
  "tha-ap272:9.3": "Maccho javo saḷalado,",
  "tha-ap272:9.4": "rakkhaso taraṇo dasa;",
  "tha-ap272:9.5": "Gāthāyo cettha saṅkhātā,",
  "tha-ap272:9.6": "tālīsaṃ cekameva cāti."
}