{
  "tha-ap508:0.1": "Therāpadāna",
  "tha-ap508:0.2": "Kaṇikāravagga",
  "tha-ap508:0.3": "6. Mātuluṅgaphaladāyakattheraapadāna",
  "tha-ap508:1.1": "“Kaṇikāraṃva jalitaṃ,",
  "tha-ap508:1.2": "puṇṇamāyeva candimaṃ;",
  "tha-ap508:1.3": "Jalantaṃ dīparukkhaṃva,",
  "tha-ap508:1.4": "addasaṃ lokanāyakaṃ.",
  "tha-ap508:2.1": "Mātuluṅgaphalaṃ gayha,",
  "tha-ap508:2.2": "adāsiṃ satthuno ahaṃ;",
  "tha-ap508:2.3": "Dakkhiṇeyyassa vīrassa,",
  "tha-ap508:2.4": "pasanno sehi pāṇibhi.",
  "tha-ap508:3.1": "Ekatiṃse ito kappe,",
  "tha-ap508:3.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap508:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap508:3.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap508:4.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap508:4.2": "…pe…",
  "tha-ap508:4.3": "viharāmi anāsavo.",
  "tha-ap508:5.1": "Svāgataṃ vata me āsi,",
  "tha-ap508:5.2": "…pe…",
  "tha-ap508:5.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap508:6.1": "Paṭisambhidā catasso,",
  "tha-ap508:6.2": "…pe…",
  "tha-ap508:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap508:7.1": "Itthaṃ sudaṃ āyasmā mātuluṅgaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap508:8.1": "Mātuluṅgaphaladāyakattherassāpadānaṃ chaṭṭhaṃ."
}