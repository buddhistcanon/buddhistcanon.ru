{
  "tha-ap247:0.1": "Therāpadāna",
  "tha-ap247:0.2": "Tuvaradāyakavagga",
  "tha-ap247:0.3": "5. Kuṭidhūpakattheraapadāna",
  "tha-ap247:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap247:1.2": "ahosiṃ kuṭigopako;",
  "tha-ap247:1.3": "Kālena kālaṃ dhūpesiṃ,",
  "tha-ap247:1.4": "pasanno sehi pāṇibhi.",
  "tha-ap247:2.1": "Catunnavutito kappe,",
  "tha-ap247:2.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap247:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap247:2.4": "dhūpadānassidaṃ phalaṃ.",
  "tha-ap247:3.1": "Paṭisambhidā catasso,",
  "tha-ap247:3.2": "…pe…",
  "tha-ap247:3.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap247:4.1": "Itthaṃ sudaṃ āyasmā kuṭidhūpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap247:5.1": "Kuṭidhūpakattherassāpadānaṃ pañcamaṃ."
}