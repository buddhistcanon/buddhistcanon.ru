{
  "tha-ap194:0.1": "Therāpadāna",
  "tha-ap194:0.2": "Tamālapupphiyavagga",
  "tha-ap194:0.3": "2 Tiṇasanthārakattheraapadāna",
  "tha-ap194:1.1": "“Yaṃ dāyavāsiko isi,",
  "tha-ap194:1.2": "tiṇaṃ lāyati satthuno;",
  "tha-ap194:1.3": "Sabbe padakkhiṇāvaṭṭā,",
  "tha-ap194:1.4": "pathabyā nipatiṃsu te.",
  "tha-ap194:2.1": "Tamahaṃ tiṇamādāya,",
  "tha-ap194:2.2": "santhariṃ dharaṇuttame;",
  "tha-ap194:2.3": "Tīṇeva tālapattāni,",
  "tha-ap194:2.4": "āharitvānahaṃ tadā.",
  "tha-ap194:3.1": "Tiṇena chadanaṃ katvā,",
  "tha-ap194:3.2": "siddhatthassa adāsahaṃ;",
  "tha-ap194:3.3": "Sattāhaṃ dhārayuṃ tassa,",
  "tha-ap194:3.4": "devamānusasatthuno.",
  "tha-ap194:4.1": "Catunnavutito kappe,",
  "tha-ap194:4.2": "yaṃ tiṇaṃ adadiṃ tadā;",
  "tha-ap194:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap194:4.4": "tiṇadānassidaṃ phalaṃ.",
  "tha-ap194:5.1": "Pañcasaṭṭhimhito kappe,",
  "tha-ap194:5.2": "cattārosuṃ mahaddhanā;",
  "tha-ap194:5.3": "Sattaratanasampannā,",
  "tha-ap194:5.4": "cakkavattī mahabbalā.",
  "tha-ap194:6.1": "Paṭisambhidā catasso,",
  "tha-ap194:6.2": "…pe…",
  "tha-ap194:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap194:7.1": "Itthaṃ sudaṃ āyasmā tiṇasanthārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap194:8.1": "Tiṇasanthārakattherassāpadānaṃ dutiyaṃ."
}