{
  "tha-ap457:0.1": "Therāpadāna",
  "tha-ap457:0.2": "Jagatidāyakavagga",
  "tha-ap457:0.3": "5. Akkamanadāyakattheraapadāna",
  "tha-ap457:1.1": "“Kakusandhassa munino,",
  "tha-ap457:1.2": "brāhmaṇassa vusīmato;",
  "tha-ap457:1.3": "Divāvihāraṃ vajato,",
  "tha-ap457:1.4": "akkamanamadāsahaṃ.",
  "tha-ap457:2.1": "Imasmiṃyeva kappamhi,",
  "tha-ap457:2.2": "yaṃ dānamadadiṃ tadā;",
  "tha-ap457:2.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap457:2.4": "akkamanassidaṃ phalaṃ.",
  "tha-ap457:3.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap457:3.2": "…pe…",
  "tha-ap457:3.3": "viharāmi anāsavo.",
  "tha-ap457:4.1": "Svāgataṃ vata me āsi,",
  "tha-ap457:4.2": "…pe…",
  "tha-ap457:4.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap457:5.1": "Paṭisambhidā catasso,",
  "tha-ap457:5.2": "…pe…",
  "tha-ap457:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap457:6.1": "Itthaṃ sudaṃ āyasmā akkamanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap457:7.1": "Akkamanadāyakattherassāpadānaṃ pañcamaṃ."
}