{
  "iti72:0.1": "Itivuttaka 72",
  "iti72:0.2": "Tikanipāta",
  "iti72:0.3": "Tatiyavagga",
  "iti72:0.4": "Nissaraṇiyasutta",
  "iti72:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti72:2.1": "“Tisso imā, bhikkhave, nissaraṇiyā dhātuyo.",
  "iti72:2.2": "Katamā tisso?",
  "iti72:2.3": "Kāmānametaṃ nissaraṇaṃ yadidaṃ nekkhammaṃ, rūpānametaṃ nissaraṇaṃ yadidaṃ āruppaṃ, yaṃ kho pana kiñci bhūtaṃ saṅkhataṃ paṭiccasamuppannaṃ nirodho tassa nissaraṇaṃ—",
  "iti72:2.4": "imā kho, bhikkhave, tisso nissaraṇiyā dhātuyo”ti.",
  "iti72:2.5": "Etamatthaṃ bhagavā avoca.",
  "iti72:2.6": "Tatthetaṃ iti vuccati:",
  "iti72:3.1": "“Kāmanissaraṇaṃ ñatvā,",
  "iti72:3.2": "rūpānañca atikkamaṃ;",
  "iti72:3.3": "Sabbasaṅkhārasamathaṃ,",
  "iti72:3.4": "phusaṃ ātāpi sabbadā.",
  "iti72:4.1": "Sa ve sammaddaso bhikkhu,",
  "iti72:4.2": "yato tattha vimuccati;",
  "iti72:4.3": "Abhiññāvosito santo,",
  "iti72:4.4": "sa ve yogātigo munī”ti.",
  "iti72:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti72:5.2": "Tatiyaṃ."
}