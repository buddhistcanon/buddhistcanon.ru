{
  "iti50:0.1": "Itivuttaka 50",
  "iti50:0.2": "Tikanipāta",
  "iti50:0.3": "Paṭhamavagga",
  "iti50:0.4": "Mūlasutta",
  "iti50:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti50:2.1": "“Tīṇimāni, bhikkhave, akusalamūlāni.",
  "iti50:2.2": "Katamāni tīṇi?",
  "iti50:2.3": "Lobho akusalamūlaṃ, doso akusalamūlaṃ, moho akusalamūlaṃ—",
  "iti50:2.4": "imāni kho, bhikkhave, tīṇi akusalamūlānī”ti.",
  "iti50:2.5": "Etamatthaṃ bhagavā avoca.",
  "iti50:2.6": "Tatthetaṃ iti vuccati:",
  "iti50:3.1": "“Lobho doso ca moho ca,",
  "iti50:3.2": "purisaṃ pāpacetasaṃ;",
  "iti50:3.3": "Hiṃsanti attasambhūtā,",
  "iti50:3.4": "tacasāraṃva samphalan”ti.",
  "iti50:4.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti50:4.2": "Paṭhamaṃ."
}