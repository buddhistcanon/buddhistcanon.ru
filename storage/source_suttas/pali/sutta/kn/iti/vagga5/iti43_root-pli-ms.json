{
  "iti43:0.1": "Itivuttaka 43",
  "iti43:0.2": "Dukanipāta",
  "iti43:0.3": "Dutiyavagga",
  "iti43:0.4": "Ajātasutta",
  "iti43:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti43:2.1": "“Atthi, bhikkhave, ajātaṃ abhūtaṃ akataṃ asaṅkhataṃ.",
  "iti43:2.2": "No cetaṃ, bhikkhave, abhavissa ajātaṃ abhūtaṃ akataṃ asaṅkhataṃ, nayidha jātassa bhūtassa katassa saṅkhatassa nissaraṇaṃ paññāyetha.",
  "iti43:2.3": "Yasmā ca kho, bhikkhave, atthi ajātaṃ abhūtaṃ akataṃ asaṅkhataṃ, tasmā jātassa bhūtassa katassa saṅkhatassa nissaraṇaṃ paññāyatī”ti.",
  "iti43:2.4": "Etamatthaṃ bhagavā avoca.",
  "iti43:2.5": "Tatthetaṃ iti vuccati:",
  "iti43:3.1": "“Jātaṃ bhūtaṃ samuppannaṃ,",
  "iti43:3.2": "kataṃ saṅkhatamaddhuvaṃ;",
  "iti43:3.3": "Jarāmaraṇasaṅghāṭaṃ,",
  "iti43:3.4": "roganīḷaṃ pabhaṅguraṃ.",
  "iti43:4.1": "Āhāranettippabhavaṃ,",
  "iti43:4.2": "nālaṃ tadabhinandituṃ;",
  "iti43:4.3": "Tassa nissaraṇaṃ santaṃ,",
  "iti43:4.4": "atakkāvacaraṃ dhuvaṃ.",
  "iti43:5.1": "Ajātaṃ asamuppannaṃ,",
  "iti43:5.2": "asokaṃ virajaṃ padaṃ;",
  "iti43:5.3": "Nirodho dukkhadhammānaṃ,",
  "iti43:5.4": "saṅkhārūpasamo sukho”ti.",
  "iti43:6.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti43:6.2": "Chaṭṭhaṃ."
}