{
  "iti46:0.1": "Itivuttaka 46",
  "iti46:0.2": "Dukanipāta",
  "iti46:0.3": "Dutiyavagga",
  "iti46:0.4": "Sikkhānisaṃsasutta",
  "iti46:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti46:2.1": "“Sikkhānisaṃsā, bhikkhave, viharatha paññuttarā vimuttisārā satādhipateyyā.",
  "iti46:2.2": "Sikkhānisaṃsānaṃ, bhikkhave, viharataṃ paññuttarānaṃ vimuttisārānaṃ satādhipateyyānaṃ dvinnaṃ phalānaṃ aññataraṃ phalaṃ pāṭikaṅkhaṃ—",
  "iti46:2.3": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitā”ti.",
  "iti46:2.4": "Etamatthaṃ bhagavā avoca.",
  "iti46:2.5": "Tatthetaṃ iti vuccati:",
  "iti46:3.1": "“Paripuṇṇasikkhaṃ apahānadhammaṃ,",
  "iti46:3.2": "Paññuttaraṃ jātikhayantadassiṃ;",
  "iti46:3.3": "Taṃ ve muniṃ antimadehadhāriṃ,",
  "iti46:3.4": "Mārañjahaṃ brūmi jarāya pāraguṃ.",
  "iti46:4.1": "Tasmā sadā jhānaratā samāhitā,",
  "iti46:4.2": "Ātāpino jātikhayantadassino;",
  "iti46:4.3": "Māraṃ sasenaṃ abhibhuyya bhikkhavo,",
  "iti46:4.4": "Bhavatha jātimaraṇassa pāragā”ti.",
  "iti46:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti46:5.2": "Navamaṃ."
}