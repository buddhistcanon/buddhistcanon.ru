{
  "iti96:0.1": "Itivuttaka 96",
  "iti96:0.2": "Tikanipāta",
  "iti96:0.3": "Pañcamavagga",
  "iti96:0.4": "Kāmayogasutta",
  "iti96:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti96:2.1": "“Kāmayogayutto, bhikkhave, bhavayogayutto āgāmī hoti āgantā itthattaṃ.",
  "iti96:2.2": "Kāmayogavisaṃyutto, bhikkhave, bhavayogayutto anāgāmī hoti anāgantā itthattaṃ.",
  "iti96:2.3": "Kāmayogavisaṃyutto, bhikkhave, bhavayogavisaṃyutto arahā hoti, khīṇāsavo”ti.",
  "iti96:2.4": "Etamatthaṃ bhagavā avoca.",
  "iti96:2.5": "Tatthetaṃ iti vuccati:",
  "iti96:3.1": "“Kāmayogena saṃyuttā,",
  "iti96:3.2": "bhavayogena cūbhayaṃ;",
  "iti96:3.3": "Sattā gacchanti saṃsāraṃ,",
  "iti96:3.4": "jātimaraṇagāmino.",
  "iti96:4.1": "Ye ca kāme pahantvāna,",
  "iti96:4.2": "appattā āsavakkhayaṃ;",
  "iti96:4.3": "Bhavayogena saṃyuttā,",
  "iti96:4.4": "anāgāmīti vuccare.",
  "iti96:5.1": "Ye ca kho chinnasaṃsayā,",
  "iti96:5.2": "khīṇamānapunabbhavā;",
  "iti96:5.3": "Te ve pāraṅgatā loke,",
  "iti96:5.4": "ye pattā āsavakkhayan”ti.",
  "iti96:6.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti96:6.2": "Sattamaṃ.",
  "iti96:6.3": "Tatiyabhāṇavāraṃ."
}