{
  "iti98:0.1": "Itivuttaka 98",
  "iti98:0.2": "Tikanipāta",
  "iti98:0.3": "Pañcamavagga",
  "iti98:0.4": "Dānasutta",
  "iti98:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti98:2.1": "“Dvemāni, bhikkhave, dānāni—",
  "iti98:2.2": "āmisadānañca dhammadānañca.",
  "iti98:2.3": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ dānānaṃ yadidaṃ—",
  "iti98:2.4": "dhammadānaṃ.",
  "iti98:3.1": "Dveme, bhikkhave, saṃvibhāgā—",
  "iti98:3.2": "āmisasaṃvibhāgo ca dhammasaṃvibhāgo ca.",
  "iti98:3.3": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ saṃvibhāgānaṃ yadidaṃ—",
  "iti98:3.4": "dhammasaṃvibhāgo.",
  "iti98:4.1": "Dveme, bhikkhave, anuggahā—",
  "iti98:4.2": "āmisānuggaho ca dhammānuggaho ca.",
  "iti98:4.3": "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ anuggahānaṃ yadidaṃ—",
  "iti98:4.4": "dhammānuggaho”ti.",
  "iti98:4.5": "Etamatthaṃ bhagavā avoca.",
  "iti98:4.6": "Tatthetaṃ iti vuccati:",
  "iti98:5.1": "“Yamāhu dānaṃ paramaṃ anuttaraṃ,",
  "iti98:5.2": "Yaṃ saṃvibhāgaṃ bhagavā avaṇṇayi;",
  "iti98:5.3": "Aggamhi khettamhi pasannacitto,",
  "iti98:5.4": "Viññū pajānaṃ ko na yajetha kāle.",
  "iti98:6.1": "Ye ceva bhāsanti suṇanti cūbhayaṃ,",
  "iti98:6.2": "Pasannacittā sugatassa sāsane;",
  "iti98:6.3": "Tesaṃ so attho paramo visujjhati,",
  "iti98:6.4": "Ye appamattā sugatassa sāsane”ti.",
  "iti98:7.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti98:7.2": "Navamaṃ."
}