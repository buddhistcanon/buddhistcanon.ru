{
  "iti36:0.1": "Itivuttaka 36",
  "iti36:0.2": "Dukanipāta",
  "iti36:0.3": "Paṭhamavagga",
  "iti36:0.4": "Dutiyajananakuhanasutta",
  "iti36:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti36:2.1": "“Nayidaṃ, bhikkhave, brahmacariyaṃ vussati janakuhanatthaṃ, na janalapanatthaṃ, na lābhasakkārasilokānisaṃsatthaṃ, na ‘iti maṃ jano jānātū’ti.",
  "iti36:2.2": "Atha kho idaṃ, bhikkhave, brahmacariyaṃ vussati abhiññatthañceva pariññatthañcā”ti.",
  "iti36:2.3": "Etamatthaṃ bhagavā avoca.",
  "iti36:2.4": "Tatthetaṃ iti vuccati:",
  "iti36:3.1": "“Abhiññatthaṃ pariññatthaṃ,",
  "iti36:3.2": "brahmacariyaṃ anītihaṃ;",
  "iti36:3.3": "Adesayi so bhagavā,",
  "iti36:3.4": "nibbānogadhagāminaṃ.",
  "iti36:4.1": "Esa maggo mahattehi,",
  "iti36:4.2": "anuyāto mahesibhi;",
  "iti36:4.3": "Ye ye taṃ paṭipajjanti,",
  "iti36:4.4": "yathā buddhena desitaṃ;",
  "iti36:4.5": "Dukkhassantaṃ karissanti,",
  "iti36:4.6": "satthusāsanakārino”ti.",
  "iti36:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti36:5.2": "Navamaṃ."
}