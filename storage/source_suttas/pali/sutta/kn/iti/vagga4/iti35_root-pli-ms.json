{
  "iti35:0.1": "Itivuttaka 35",
  "iti35:0.2": "Dukanipāta",
  "iti35:0.3": "Paṭhamavagga",
  "iti35:0.4": "Paṭhamajananakuhanasutta",
  "iti35:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti35:2.1": "“Nayidaṃ, bhikkhave, brahmacariyaṃ vussati janakuhanatthaṃ, na janalapanatthaṃ, na lābhasakkārasilokānisaṃsatthaṃ, na ‘iti maṃ jano jānātū’ti.",
  "iti35:2.2": "Atha kho idaṃ, bhikkhave, brahmacariyaṃ vussati saṃvaratthañceva pahānatthañcā”ti.",
  "iti35:2.3": "Etamatthaṃ bhagavā avoca.",
  "iti35:2.4": "Tatthetaṃ iti vuccati:",
  "iti35:3.1": "“Saṃvaratthaṃ pahānatthaṃ,",
  "iti35:3.2": "brahmacariyaṃ anītihaṃ;",
  "iti35:3.3": "Adesayi so bhagavā,",
  "iti35:3.4": "nibbānogadhagāminaṃ.",
  "iti35:4.1": "Esa maggo mahattehi,",
  "iti35:4.2": "anuyāto mahesibhi;",
  "iti35:4.3": "Ye ye taṃ paṭipajjanti,",
  "iti35:4.4": "yathā buddhena desitaṃ;",
  "iti35:4.5": "Dukkhassantaṃ karissanti,",
  "iti35:4.6": "satthusāsanakārino”ti.",
  "iti35:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti35:5.2": "Aṭṭhamaṃ."
}