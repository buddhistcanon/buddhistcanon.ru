{
  "iti80:0.1": "Itivuttaka 80",
  "iti80:0.2": "Tikanipāta",
  "iti80:0.3": "Catutthavagga",
  "iti80:0.4": "Vitakkasutta",
  "iti80:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti80:2.1": "“Tayome, bhikkhave, akusalavitakkā.",
  "iti80:2.2": "Katame tayo?",
  "iti80:2.3": "Anavaññattipaṭisaṃyutto vitakko, lābhasakkārasilokapaṭisaṃyutto vitakko, parānuddayatāpaṭisaṃyutto vitakko.",
  "iti80:2.4": "Ime kho, bhikkhave, tayo akusalavitakkā”ti.",
  "iti80:2.5": "Etamatthaṃ bhagavā avoca.",
  "iti80:2.6": "Tatthetaṃ iti vuccati:",
  "iti80:3.1": "“Anavaññattisaṃyutto,",
  "iti80:3.2": "lābhasakkāragāravo;",
  "iti80:3.3": "Sahanandī amaccehi,",
  "iti80:3.4": "ārā saṃyojanakkhayā.",
  "iti80:4.1": "Yo ca puttapasuṃ hitvā,",
  "iti80:4.2": "Vivāhe saṃharāni ca;",
  "iti80:4.3": "Bhabbo so tādiso bhikkhu,",
  "iti80:4.4": "Phuṭṭhuṃ sambodhimuttaman”ti.",
  "iti80:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti80:5.2": "Paṭhamaṃ."
}