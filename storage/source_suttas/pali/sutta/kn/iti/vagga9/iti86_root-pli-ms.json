{
  "iti86:0.1": "Itivuttaka 86",
  "iti86:0.2": "Tikanipāta",
  "iti86:0.3": "Catutthavagga",
  "iti86:0.4": "Dhammānudhammapaṭipannasutta",
  "iti86:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti86:2.1": "“Dhammānudhammapaṭipannassa bhikkhuno ayamanudhammo hoti veyyākaraṇāya—",
  "iti86:2.2": "dhammānudhammapaṭipannoyanti bhāsamāno dhammaññeva bhāsati no adhammaṃ, vitakkayamāno vā dhammavitakkaññeva vitakketi no adhammavitakkaṃ, tadubhayaṃ vā pana abhinivejjetvā upekkhako viharati sato sampajāno”ti.",
  "iti86:2.3": "Etamatthaṃ bhagavā avoca.",
  "iti86:2.4": "Tatthetaṃ iti vuccati:",
  "iti86:3.1": "“Dhammārāmo dhammarato,",
  "iti86:3.2": "dhammaṃ anuvicintayaṃ;",
  "iti86:3.3": "Dhammaṃ anussaraṃ bhikkhu,",
  "iti86:3.4": "saddhammā na parihāyati.",
  "iti86:4.1": "Caraṃ vā yadi vā tiṭṭhaṃ,",
  "iti86:4.2": "nisinno uda vā sayaṃ;",
  "iti86:4.3": "Ajjhattaṃ samayaṃ cittaṃ,",
  "iti86:4.4": "santimevādhigacchatī”ti.",
  "iti86:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti86:5.2": "Sattamaṃ."
}