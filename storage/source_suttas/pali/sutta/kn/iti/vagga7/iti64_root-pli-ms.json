{
  "iti64:0.1": "Itivuttaka 64",
  "iti64:0.2": "Tikanipāta",
  "iti64:0.3": "Dutiyavagga",
  "iti64:0.4": "Duccaritasutta",
  "iti64:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti64:2.1": "“Tīṇimāni, bhikkhave, duccaritāni.",
  "iti64:2.2": "Katamāni tīṇi?",
  "iti64:2.3": "Kāyaduccaritaṃ, vacīduccaritaṃ, manoduccaritaṃ—",
  "iti64:2.4": "imāni kho, bhikkhave, tīṇi duccaritānī”ti.",
  "iti64:2.5": "Etamatthaṃ bhagavā avoca.",
  "iti64:2.6": "Tatthetaṃ iti vuccati:",
  "iti64:3.1": "“Kāyaduccaritaṃ katvā,",
  "iti64:3.2": "vacīduccaritāni ca;",
  "iti64:3.3": "Manoduccaritaṃ katvā,",
  "iti64:3.4": "yañcaññaṃ dosasaṃhitaṃ.",
  "iti64:4.1": "Akatvā kusalaṃ kammaṃ,",
  "iti64:4.2": "katvānākusalaṃ bahuṃ;",
  "iti64:4.3": "Kāyassa bhedā duppañño,",
  "iti64:4.4": "nirayaṃ sopapajjatī”ti.",
  "iti64:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti64:5.2": "Pañcamaṃ."
}