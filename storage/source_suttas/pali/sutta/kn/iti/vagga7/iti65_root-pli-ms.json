{
  "iti65:0.1": "Itivuttaka 65",
  "iti65:0.2": "Tikanipāta",
  "iti65:0.3": "Dutiyavagga",
  "iti65:0.4": "Sucaritasutta",
  "iti65:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti65:2.1": "“Tīṇimāni, bhikkhave, sucaritāni.",
  "iti65:2.2": "Katamāni tīṇi?",
  "iti65:2.3": "Kāyasucaritaṃ, vacīsucaritaṃ, manosucaritaṃ—",
  "iti65:2.4": "imāni kho, bhikkhave, tīṇi sucaritānī”ti.",
  "iti65:2.5": "Etamatthaṃ bhagavā avoca.",
  "iti65:2.6": "Tatthetaṃ iti vuccati:",
  "iti65:3.1": "“Kāyaduccaritaṃ hitvā,",
  "iti65:3.2": "vacīduccaritāni ca;",
  "iti65:3.3": "Manoduccaritaṃ hitvā,",
  "iti65:3.4": "yañcaññaṃ dosasaṃhitaṃ.",
  "iti65:4.1": "Akatvākusalaṃ kammaṃ,",
  "iti65:4.2": "katvāna kusalaṃ bahuṃ;",
  "iti65:4.3": "Kāyassa bhedā sappañño,",
  "iti65:4.4": "saggaṃ so upapajjatī”ti.",
  "iti65:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti65:5.2": "Chaṭṭhaṃ."
}