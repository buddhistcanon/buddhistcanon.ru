{
  "iti2:0.1": "Itivuttaka 2",
  "iti2:0.2": "Ekakanipāta",
  "iti2:0.3": "Paṭhamavagga",
  "iti2:0.4": "Dosasutta",
  "iti2:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti2:2.1": "“Ekadhammaṃ, bhikkhave, pajahatha;",
  "iti2:2.2": "ahaṃ vo pāṭibhogo anāgāmitāya.",
  "iti2:2.3": "Katamaṃ ekadhammaṃ?",
  "iti2:2.4": "Dosaṃ, bhikkhave, ekadhammaṃ pajahatha;",
  "iti2:2.5": "ahaṃ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti2:2.6": "Etamatthaṃ bhagavā avoca.",
  "iti2:2.7": "Tatthetaṃ iti vuccati:",
  "iti2:3.1": "“Yena dosena duṭṭhāse,",
  "iti2:3.2": "sattā gacchanti duggatiṃ;",
  "iti2:3.3": "Taṃ dosaṃ sammadaññāya,",
  "iti2:3.4": "pajahanti vipassino;",
  "iti2:3.5": "Pahāya na punāyanti,",
  "iti2:3.6": "imaṃ lokaṃ kudācanan”ti.",
  "iti2:4.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti2:4.2": "Dutiyaṃ."
}