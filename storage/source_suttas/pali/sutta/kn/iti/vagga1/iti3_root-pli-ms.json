{
  "iti3:0.1": "Itivuttaka 3",
  "iti3:0.2": "Ekakanipāta",
  "iti3:0.3": "Paṭhamavagga",
  "iti3:0.4": "Mohasutta",
  "iti3:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti3:2.1": "“Ekadhammaṃ, bhikkhave, pajahatha;",
  "iti3:2.2": "ahaṃ vo pāṭibhogo anāgāmitāya.",
  "iti3:2.3": "Katamaṃ ekadhammaṃ?",
  "iti3:2.4": "Mohaṃ, bhikkhave, ekadhammaṃ pajahatha;",
  "iti3:2.5": "ahaṃ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti3:2.6": "Etamatthaṃ bhagavā avoca.",
  "iti3:2.7": "Tatthetaṃ iti vuccati:",
  "iti3:3.1": "“Yena mohena mūḷhāse,",
  "iti3:3.2": "sattā gacchanti duggatiṃ;",
  "iti3:3.3": "Taṃ mohaṃ sammadaññāya,",
  "iti3:3.4": "pajahanti vipassino;",
  "iti3:3.5": "Pahāya na punāyanti,",
  "iti3:3.6": "imaṃ lokaṃ kudācanan”ti.",
  "iti3:4.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti3:4.2": "Tatiyaṃ."
}