{
  "iti5:0.1": "Itivuttaka 5",
  "iti5:0.2": "Ekakanipāta",
  "iti5:0.3": "Paṭhamavagga",
  "iti5:0.4": "Makkhasutta",
  "iti5:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti5:2.1": "“Ekadhammaṃ, bhikkhave, pajahatha;",
  "iti5:2.2": "ahaṃ vo pāṭibhogo anāgāmitāya.",
  "iti5:2.3": "Katamaṃ ekadhammaṃ?",
  "iti5:2.4": "Makkhaṃ, bhikkhave, ekadhammaṃ pajahatha;",
  "iti5:2.5": "ahaṃ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti5:2.6": "Etamatthaṃ bhagavā avoca.",
  "iti5:2.7": "Tatthetaṃ iti vuccati:",
  "iti5:3.1": "“Yena makkhena makkhāse,",
  "iti5:3.2": "sattā gacchanti duggatiṃ;",
  "iti5:3.3": "Taṃ makkhaṃ sammadaññāya,",
  "iti5:3.4": "pajahanti vipassino;",
  "iti5:3.5": "Pahāya na punāyanti,",
  "iti5:3.6": "imaṃ lokaṃ kudācanan”ti.",
  "iti5:4.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti5:4.2": "Pañcamaṃ."
}