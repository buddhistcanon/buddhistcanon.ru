{
  "iti10:0.1": "Itivuttaka 10",
  "iti10:0.2": "Ekakanipāta",
  "iti10:0.3": "Paṭhamavagga",
  "iti10:0.4": "Dosapariññāsutta",
  "iti10:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti10:2.1": "“Dosaṃ, bhikkhave, anabhijānaṃ aparijānaṃ tattha cittaṃ avirājayaṃ appajahaṃ abhabbo dukkhakkhayāya.",
  "iti10:2.2": "Dosañca kho, bhikkhave, abhijānaṃ parijānaṃ tattha cittaṃ virājayaṃ pajahaṃ bhabbo dukkhakkhayāyā”ti.",
  "iti10:2.3": "Etamatthaṃ bhagavā avoca.",
  "iti10:2.4": "Tatthetaṃ iti vuccati:",
  "iti10:3.1": "“Yena dosena duṭṭhāse,",
  "iti10:3.2": "sattā gacchanti duggatiṃ;",
  "iti10:3.3": "Taṃ dosaṃ sammadaññāya,",
  "iti10:3.4": "pajahanti vipassino;",
  "iti10:3.5": "Pahāya na punāyanti,",
  "iti10:3.6": "imaṃ lokaṃ kudācanan”ti.",
  "iti10:4.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti10:4.2": "Dasamaṃ.",
  "iti10:4.3": "Pāṭibhogavaggo paṭhamo.",
  "iti10:5.1": "Rāgadosā atha moho,",
  "iti10:5.2": "Kodhamakkhā mānaṃ sabbaṃ;",
  "iti10:5.3": "Mānato rāgadosā puna dve,",
  "iti10:5.4": "Pakāsitā vaggamāhu paṭhamanti."
}