{
  "iti6:0.1": "Itivuttaka 6",
  "iti6:0.2": "Ekakanipāta",
  "iti6:0.3": "Paṭhamavagga",
  "iti6:0.4": "Mānasutta",
  "iti6:1.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
  "iti6:2.1": "“Ekadhammaṃ, bhikkhave, pajahatha;",
  "iti6:2.2": "ahaṃ vo pāṭibhogo anāgāmitāya.",
  "iti6:2.3": "Katamaṃ ekadhammaṃ?",
  "iti6:2.4": "Mānaṃ, bhikkhave, ekadhammaṃ pajahatha;",
  "iti6:2.5": "ahaṃ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti6:2.6": "Etamatthaṃ bhagavā avoca.",
  "iti6:2.7": "Tatthetaṃ iti vuccati:",
  "iti6:3.1": "“Yena mānena mattāse,",
  "iti6:3.2": "sattā gacchanti duggatiṃ;",
  "iti6:3.3": "Taṃ mānaṃ sammadaññāya,",
  "iti6:3.4": "pajahanti vipassino;",
  "iti6:3.5": "Pahāya na punāyanti,",
  "iti6:3.6": "imaṃ lokaṃ kudācanan”ti.",
  "iti6:4.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti6:4.2": "Chaṭṭhaṃ."
}