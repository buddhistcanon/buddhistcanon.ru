{
  "vv62:0.1": "Vimānavatthu",
  "vv62:0.2": "Purisavimāna",
  "vv62:0.3": "Mahārathavagga",
  "vv62:0.4": "12. Tatiyanāgavimānavatthu",
  "vv62:1.1": "“Ko nu dibbena yānena,",
  "vv62:1.2": "sabbasetena hatthinā;",
  "vv62:1.3": "Tūriyatāḷitanigghoso,",
  "vv62:1.4": "antalikkhe mahīyati.",
  "vv62:2.1": "Devatā nusi gandhabbo,",
  "vv62:2.2": "adu sakko purindado;",
  "vv62:2.3": "Ajānantā taṃ pucchāma,",
  "vv62:2.4": "kathaṃ jānemu taṃ mayan”ti.",
  "vv62:3.1": "“Namhi devo na gandhabbo,",
  "vv62:3.2": "nāpi sakko purindado;",
  "vv62:3.3": "Sudhammā nāma ye devā,",
  "vv62:3.4": "tesaṃ aññataro ahan”ti.",
  "vv62:4.1": "“Pucchāma devaṃ sudhammaṃ,",
  "vv62:4.2": "Puthuṃ katvāna añjaliṃ;",
  "vv62:4.3": "Kiṃ katvā mānuse kammaṃ,",
  "vv62:4.4": "Sudhammaṃ upapajjatī”ti.",
  "vv62:5.1": "“Ucchāgāraṃ tiṇāgāraṃ,",
  "vv62:5.2": "vatthāgārañca yo dade;",
  "vv62:5.3": "Tiṇṇaṃ aññataraṃ datvā,",
  "vv62:5.4": "sudhammaṃ upapajjatī”ti.",
  "vv62:6.1": "Tatiyanāgavimānaṃ dvādasamaṃ."
}