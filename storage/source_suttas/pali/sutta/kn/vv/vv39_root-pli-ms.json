{
  "vv39:0.1": "Vimānavatthu",
  "vv39:0.2": "Itthivimāna",
  "vv39:0.3": "Mañjiṭṭhakavagga",
  "vv39:0.4": "1. Mañjiṭṭhakavimānavatthu",
  "vv39:1.1": "“Mañjiṭṭhake vimānasmiṃ,",
  "vv39:1.2": "soṇṇavālukasanthate;",
  "vv39:1.3": "Pañcaṅgikena tūriyena,",
  "vv39:1.4": "ramasi suppavādite.",
  "vv39:2.1": "Tamhā vimānā oruyha,",
  "vv39:2.2": "nimmitā ratanāmayā;",
  "vv39:2.3": "Ogāhasi sālavanaṃ,",
  "vv39:2.4": "pupphitaṃ sabbakālikaṃ.",
  "vv39:3.1": "Yassa yasseva sālassa,",
  "vv39:3.2": "mūle tiṭṭhasi devate;",
  "vv39:3.3": "So so muñcati pupphāni,",
  "vv39:3.4": "onamitvā dumuttamo.",
  "vv39:4.1": "Vāteritaṃ sālavanaṃ,",
  "vv39:4.2": "ādhutaṃ dijasevitaṃ;",
  "vv39:4.3": "Vāti gandho disā sabbā,",
  "vv39:4.4": "rukkho mañjūsako yathā.",
  "vv39:5.1": "Ghāyase taṃ sucigandhaṃ,",
  "vv39:5.2": "rūpaṃ passasi amānusaṃ;",
  "vv39:5.3": "Devate pucchitācikkha,",
  "vv39:5.4": "kissa kammassidaṃ phalan”ti.",
  "vv39:6.1": "“Ahaṃ manussesu manussabhūtā,",
  "vv39:6.2": "Dāsī ayirakule ahuṃ;",
  "vv39:6.3": "Buddhaṃ nisinnaṃ disvāna,",
  "vv39:6.4": "Sālapupphehi okiriṃ.",
  "vv39:7.1": "Vaṭaṃsakañca sukataṃ,",
  "vv39:7.2": "sālapupphamayaṃ ahaṃ;",
  "vv39:7.3": "Buddhassa upanāmesiṃ,",
  "vv39:7.4": "pasannā sehi pāṇibhi.",
  "vv39:8.1": "Tāhaṃ kammaṃ karitvāna,",
  "vv39:8.2": "kusalaṃ buddhavaṇṇitaṃ;",
  "vv39:8.3": "Apetasokā sukhitā,",
  "vv39:8.4": "sampamodāmanāmayā”ti.",
  "vv39:9.1": "Mañjiṭṭhakavimānaṃ paṭhamaṃ."
}