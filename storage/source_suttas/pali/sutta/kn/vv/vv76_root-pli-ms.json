{
  "vv76:0.1": "Vimānavatthu",
  "vv76:0.2": "Purisavimāna",
  "vv76:0.3": "Sunikkhittavagga",
  "vv76:0.4": "2. Nandanavimānavatthu",
  "vv76:1.1": "“Yathā vanaṃ nandanaṃ pabhāsati,",
  "vv76:1.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
  "vv76:1.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
  "vv76:1.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
  "vv76:2.1": "Deviddhipattosi mahānubhāvo,",
  "vv76:2.2": "Manussabhūto kimakāsi puññaṃ;",
  "vv76:2.3": "Kenāsi evaṃ jalitānubhāvo,",
  "vv76:2.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv76:3.1": "So devaputto attamano,",
  "vv76:3.2": "…pe…",
  "vv76:3.3": "yassa kammassidaṃ phalaṃ.",
  "vv76:4.1": "“Ahaṃ manussesu manussabhūto,",
  "vv76:4.2": "Daliddo atāṇo kapaṇo kammakaro ahosiṃ;",
  "vv76:4.3": "Jiṇṇe ca mātāpitaro abhāriṃ,",
  "vv76:4.4": "Piyā ca me sīlavanto ahesuṃ;",
  "vv76:4.5": "Annañca pānañca pasannacitto,",
  "vv76:4.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
  "vv76:5.1": "Tena metādiso vaṇṇo,",
  "vv76:5.2": "…pe…",
  "vv76:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv76:6.1": "Nandanavimānaṃ dutiyaṃ."
}