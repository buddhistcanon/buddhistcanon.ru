{
  "vv49:0.1": "Vimānavatthu",
  "vv49:0.2": "Itthivimāna",
  "vv49:0.3": "Mañjiṭṭhakavagga",
  "vv49:0.4": "11. Vandanavimānavatthu",
  "vv49:1.1": "“Abhikkantena vaṇṇena,",
  "vv49:1.2": "yā tvaṃ tiṭṭhasi devate;",
  "vv49:1.3": "Obhāsentī disā sabbā,",
  "vv49:1.4": "osadhī viya tārakā.",
  "vv49:2.1": "Kena tetādiso vaṇṇo,",
  "vv49:2.2": "…pe…",
  "vv49:2.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv49:3.1": "Sā devatā attamanā,",
  "vv49:3.2": "…pe…",
  "vv49:3.3": "yassa kammassidaṃ phalaṃ.",
  "vv49:4.1": "“Ahaṃ manussesu manussabhūtā,",
  "vv49:4.2": "Disvāna samaṇe sīlavante;",
  "vv49:4.3": "Pādāni vanditvā manaṃ pasādayiṃ,",
  "vv49:4.4": "Vittā cahaṃ añjalikaṃ akāsiṃ.",
  "vv49:5.1": "Tena metādiso vaṇṇo,",
  "vv49:5.2": "…pe…",
  "vv49:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv49:6.1": "Vandanavimānaṃ ekādasamaṃ."
}