{
  "vv29:0.1": "Vimānavatthu",
  "vv29:0.2": "Itthivimāna",
  "vv29:0.3": "Pāricchattakavagga",
  "vv29:0.4": "1. Uḷāravimānavatthu",
  "vv29:1.1": "“Uḷāro te yaso vaṇṇo,",
  "vv29:1.2": "sabbā obhāsate disā;",
  "vv29:1.3": "Nāriyo naccanti gāyanti,",
  "vv29:1.4": "devaputtā alaṅkatā.",
  "vv29:2.1": "Modenti parivārenti,",
  "vv29:2.2": "tava pūjāya devate;",
  "vv29:2.3": "Sovaṇṇāni vimānāni,",
  "vv29:2.4": "tavimāni sudassane.",
  "vv29:3.1": "Tuvaṃsi issarā tesaṃ,",
  "vv29:3.2": "sabbakāmasamiddhinī;",
  "vv29:3.3": "Abhijātā mahantāsi,",
  "vv29:3.4": "devakāye pamodasi;",
  "vv29:3.5": "Devate pucchitācikkha,",
  "vv29:3.6": "kissa kammassidaṃ phalan”ti.",
  "vv29:4.1": "“Ahaṃ manussesu manussabhūtā,",
  "vv29:4.2": "Purimāya jātiyā manussaloke;",
  "vv29:4.3": "Dussīlakule suṇisā ahosiṃ,",
  "vv29:4.4": "Assaddhesu kadariyesu ahaṃ.",
  "vv29:5.1": "Saddhā sīlena sampannā,",
  "vv29:5.2": "saṃvibhāgaratā sadā;",
  "vv29:5.3": "Piṇḍāya caramānassa,",
  "vv29:5.4": "apūvaṃ te adāsahaṃ.",
  "vv29:6.1": "Tadāhaṃ sassuyācikkhiṃ,",
  "vv29:6.2": "samaṇo āgato idha;",
  "vv29:6.3": "Tassa adāsahaṃ pūvaṃ,",
  "vv29:6.4": "pasannā sehi pāṇibhi.",
  "vv29:7.1": "Itissā sassu paribhāsi,",
  "vv29:7.2": "avinītāsi tvaṃ vadhu;",
  "vv29:7.3": "Na maṃ sampucchituṃ icchi,",
  "vv29:7.4": "samaṇassa dadāmahaṃ.",
  "vv29:8.1": "Tato me sassu kupitā,",
  "vv29:8.2": "pahāsi musalena maṃ;",
  "vv29:8.3": "Kūṭaṅgacchi avadhi maṃ,",
  "vv29:8.4": "nāsakkhiṃ jīvituṃ ciraṃ.",
  "vv29:9.1": "Sā ahaṃ kāyassa bhedā,",
  "vv29:9.2": "vippamuttā tato cutā;",
  "vv29:9.3": "Devānaṃ tāvatiṃsānaṃ,",
  "vv29:9.4": "upapannā sahabyataṃ.",
  "vv29:10.1": "Tena metādiso vaṇṇo,",
  "vv29:10.2": "…pe…",
  "vv29:10.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv29:11.1": "Uḷāravimānaṃ paṭhamaṃ."
}