{
  "vv80:0.1": "Vimānavatthu",
  "vv80:0.2": "Purisavimāna",
  "vv80:0.3": "Sunikkhittavagga",
  "vv80:0.4": "6. Gopālavimānavatthu",
  "vv80:1.1": "“Disvāna devaṃ paṭipucchi bhikkhu,",
  "vv80:1.2": "Ucce vimānamhi ciraṭṭhitike;",
  "vv80:1.3": "Āmuttahatthābharaṇaṃ yasassiṃ,",
  "vv80:1.4": "Dibbe vimānamhi yathāpi candimā.",
  "vv80:2.1": "Alaṅkato malyadharo suvattho,",
  "vv80:2.2": "Sukuṇḍalī kappitakesamassu;",
  "vv80:2.3": "Āmuttahatthābharaṇo yasassī,",
  "vv80:2.4": "Dibbe vimānamhi yathāpi candimā.",
  "vv80:3.1": "Dibbā ca vīṇā pavadanti vagguṃ,",
  "vv80:3.2": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
  "vv80:3.3": "Dibbā ca kaññā tidasacarā uḷārā,",
  "vv80:3.4": "Naccanti gāyanti pamodayanti.",
  "vv80:4.1": "Deviddhipattosi mahānubhāvo,",
  "vv80:4.2": "Manussabhūto kimakāsi puññaṃ;",
  "vv80:4.3": "Kenāsi evaṃ jalitānubhāvo,",
  "vv80:4.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv80:5.1": "So devaputto attamano,",
  "vv80:5.2": "…pe…",
  "vv80:5.3": "yassa kammassidaṃ phalaṃ.",
  "vv80:6.1": "“Ahaṃ manussesu manussabhūto,",
  "vv80:6.2": "Saṅgamma rakkhissaṃ paresaṃ dhenuyo;",
  "vv80:6.3": "Tato ca āgā samaṇo mamantike,",
  "vv80:6.4": "Gāvo ca māse agamaṃsu khādituṃ.",
  "vv80:7.1": "Dvayajja kiccaṃ ubhayañca kāriyaṃ,",
  "vv80:7.2": "Iccevahaṃ bhante tadā vicintayiṃ;",
  "vv80:7.3": "Tato ca saññaṃ paṭiladdha yoniso,",
  "vv80:7.4": "Dadāmi bhanteti khipiṃ anantakaṃ.",
  "vv80:8.1": "So māsakhettaṃ turito avāsariṃ,",
  "vv80:8.2": "Purā ayaṃ bhañjati yassidaṃ dhanaṃ;",
  "vv80:8.3": "Tato ca kaṇho urago mahāviso,",
  "vv80:8.4": "Aḍaṃsi pāde turitassa me sato.",
  "vv80:9.1": "Svāhaṃ aṭṭomhi dukkhena pīḷito,",
  "vv80:9.2": "Bhikkhu ca taṃ sāmaṃ muñcitvānantakaṃ;",
  "vv80:9.3": "Ahāsi kummāsaṃ mamānukampayā,",
  "vv80:9.4": "Tato cuto kālakatomhi devatā.",
  "vv80:10.1": "Tadeva kammaṃ kusalaṃ kataṃ mayā,",
  "vv80:10.2": "Sukhañca kammaṃ anubhomi attanā;",
  "vv80:10.3": "Tayā hi bhante anukampito bhusaṃ,",
  "vv80:10.4": "Kataññutāya abhivādayāmi taṃ.",
  "vv80:11.1": "Sadevake loke samārake ca,",
  "vv80:11.2": "Añño muni natthi tayānukampako;",
  "vv80:11.3": "Tayā hi bhante anukampito bhusaṃ,",
  "vv80:11.4": "Kataññutāya abhivādayāmi taṃ.",
  "vv80:12.1": "Imasmiṃ loke parasmiṃ vā pana,",
  "vv80:12.2": "Añño munī natthi tayānukampako;",
  "vv80:12.3": "Tayā hi bhante anukampito bhusaṃ,",
  "vv80:12.4": "Kataññutāya abhivādayāmi tan”ti.",
  "vv80:13.1": "Gopālavimānaṃ chaṭṭhaṃ."
}