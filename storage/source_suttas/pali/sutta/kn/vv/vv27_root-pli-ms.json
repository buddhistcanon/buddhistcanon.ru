{
  "vv27:0.1": "Vimānavatthu",
  "vv27:0.2": "Itthivimāna",
  "vv27:0.3": "Cittalatāvagga",
  "vv27:0.4": "10. Paṭhamabhikkhādāyikāvimānavatthu",
  "vv27:1.1": "“Abhikkantena vaṇṇena,",
  "vv27:1.2": "yā tvaṃ tiṭṭhasi devate;",
  "vv27:1.3": "Obhāsentī disā sabbā,",
  "vv27:1.4": "osadhī viya tārakā.",
  "vv27:2.1": "Kena tetādiso vaṇṇo,",
  "vv27:2.2": "…pe…",
  "vv27:2.3": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv27:3.1": "Sā devatā attamanā,",
  "vv27:3.2": "…pe…",
  "vv27:3.3": "yassa kammassidaṃ phalaṃ.",
  "vv27:4.1": "“Ahaṃ manussesu manussabhūtā,",
  "vv27:4.2": "Purimāya jātiyā manussaloke.",
  "vv27:5.1": "Addasaṃ virajaṃ buddhaṃ,",
  "vv27:5.2": "vippasannamanāvilaṃ;",
  "vv27:5.3": "Tassa adāsahaṃ bhikkhaṃ,",
  "vv27:5.4": "pasannā sehi pāṇibhi.",
  "vv27:6.1": "Tena metādiso vaṇṇo,",
  "vv27:6.2": "…pe…",
  "vv27:6.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv27:7.1": "Paṭhamabhikkhādāyikāvimānaṃ dasamaṃ."
}