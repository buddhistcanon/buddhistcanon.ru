{
  "vv78:0.1": "Vimānavatthu",
  "vv78:0.2": "Purisavimāna",
  "vv78:0.3": "Sunikkhittavagga",
  "vv78:0.4": "4. Suvaṇṇavimānavatthu",
  "vv78:1.1": "“Sovaṇṇamaye pabbatasmiṃ,",
  "vv78:1.2": "vimānaṃ sabbatopabhaṃ;",
  "vv78:1.3": "Hemajālapaṭicchannaṃ,",
  "vv78:1.4": "kiṅkiṇijālakappitaṃ.",
  "vv78:2.1": "Aṭṭhaṃsā sukatā thambhā,",
  "vv78:2.2": "sabbe veḷuriyāmayā;",
  "vv78:2.3": "Ekamekāya aṃsiyā,",
  "vv78:2.4": "ratanā satta nimmitā.",
  "vv78:3.1": "Veḷuriyasuvaṇṇassa,",
  "vv78:3.2": "phalikā rūpiyassa ca;",
  "vv78:3.3": "Masāragallamuttāhi,",
  "vv78:3.4": "lohitaṅgamaṇīhi ca.",
  "vv78:4.1": "Citrā manoramā bhūmi,",
  "vv78:4.2": "na tatthuddhaṃsatī rajo;",
  "vv78:4.3": "Gopānasīgaṇā pītā,",
  "vv78:4.4": "kūṭaṃ dhārenti nimmitā.",
  "vv78:5.1": "Sopānāni ca cattāri,",
  "vv78:5.2": "nimmitā caturo disā;",
  "vv78:5.3": "Nānāratanagabbhehi,",
  "vv78:5.4": "ādiccova virocati.",
  "vv78:6.1": "Vediyā catasso tattha,",
  "vv78:6.2": "vibhattā bhāgaso mitā;",
  "vv78:6.3": "Daddallamānā ābhanti,",
  "vv78:6.4": "samantā caturo disā.",
  "vv78:7.1": "Tasmiṃ vimāne pavare,",
  "vv78:7.2": "devaputto mahappabho;",
  "vv78:7.3": "Atirocasi vaṇṇena,",
  "vv78:7.4": "udayantova bhāṇumā.",
  "vv78:8.1": "Dānassa te idaṃ phalaṃ,",
  "vv78:8.2": "atho sīlassa vā pana;",
  "vv78:8.3": "Atho añjalikammassa,",
  "vv78:8.4": "taṃ me akkhāhi pucchito”.",
  "vv78:9.1": "So devaputto attamano,",
  "vv78:9.2": "…pe…",
  "vv78:9.3": "yassa kammassidaṃ phalaṃ.",
  "vv78:10.1": "“Ahaṃ andhakavindasmiṃ,",
  "vv78:10.2": "buddhassādiccabandhuno;",
  "vv78:10.3": "Vihāraṃ satthu kāresiṃ,",
  "vv78:10.4": "pasanno sehi pāṇibhi.",
  "vv78:11.1": "Tattha gandhañca mālañca,",
  "vv78:11.2": "paccayañca vilepanaṃ;",
  "vv78:11.3": "Vihāraṃ satthu adāsiṃ,",
  "vv78:11.4": "vippasannena cetasā;",
  "vv78:11.5": "Tena mayhaṃ idaṃ laddhaṃ,",
  "vv78:11.6": "vasaṃ vattemi nandane.",
  "vv78:12.1": "Nandane ca vane ramme,",
  "vv78:12.2": "nānādijagaṇāyute;",
  "vv78:12.3": "Ramāmi naccagītehi,",
  "vv78:12.4": "accharāhi purakkhato”ti.",
  "vv78:13.1": "Suvaṇṇavimānaṃ catutthaṃ."
}