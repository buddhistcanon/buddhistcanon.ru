{
  "vv65:0.1": "Vimānavatthu",
  "vv65:0.2": "Purisavimāna",
  "vv65:0.3": "Pāyāsivagga",
  "vv65:0.4": "1. Paṭhamaagāriyavimānavatthu",
  "vv65:1.1": "“Yathā vanaṃ cittalataṃ pabhāsati,",
  "vv65:1.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
  "vv65:1.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
  "vv65:1.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
  "vv65:2.1": "Deviddhipattosi mahānubhāvo,",
  "vv65:2.2": "Manussabhūto kimakāsi puññaṃ;",
  "vv65:2.3": "Kenāsi evaṃ jalitānubhāvo,",
  "vv65:2.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv65:3.1": "So devaputto attamano,",
  "vv65:3.2": "moggallānena pucchito;",
  "vv65:3.3": "Pañhaṃ puṭṭho viyākāsi,",
  "vv65:3.4": "yassa kammassidaṃ phalaṃ.",
  "vv65:4.1": "“Ahañca bhariyā ca manussaloke,",
  "vv65:4.2": "Opānabhūtā gharamāvasimha;",
  "vv65:4.3": "Annañca pānañca pasannacittā,",
  "vv65:4.4": "Sakkacca dānaṃ vipulaṃ adamha.",
  "vv65:5.1": "Tena metādiso vaṇṇo,",
  "vv65:5.2": "…pe…",
  "vv65:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv65:6.1": "Paṭhamaagāriyavimānaṃ paṭhamaṃ."
}