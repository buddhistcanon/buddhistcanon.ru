{
  "vv75:0.1": "Vimānavatthu",
  "vv75:0.2": "Purisavimāna",
  "vv75:0.3": "Sunikkhittavagga",
  "vv75:0.4": "1. Cittalatāvimānavatthu",
  "vv75:1.1": "“Yathā vanaṃ cittalataṃ pabhāsati,",
  "vv75:1.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
  "vv75:1.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
  "vv75:1.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
  "vv75:2.1": "Deviddhipattosi mahānubhāvo,",
  "vv75:2.2": "Manussabhūto kimakāsi puññaṃ;",
  "vv75:2.3": "Kenāsi evaṃ jalitānubhāvo,",
  "vv75:2.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv75:3.1": "So devaputto attamano,",
  "vv75:3.2": "…pe…",
  "vv75:3.3": "yassa kammassidaṃ phalaṃ.",
  "vv75:4.1": "“Ahaṃ manussesu manussabhūto,",
  "vv75:4.2": "Daliddo atāṇo kapaṇo kammakaro ahosiṃ;",
  "vv75:4.3": "Jiṇṇe ca mātāpitaro abhāriṃ,",
  "vv75:4.4": "Piyā ca me sīlavanto ahesuṃ;",
  "vv75:4.5": "Annañca pānañca pasannacitto,",
  "vv75:4.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
  "vv75:5.1": "Tena metādiso vaṇṇo,",
  "vv75:5.2": "…pe…",
  "vv75:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv75:6.1": "Cittalatāvimānaṃ paṭhamaṃ."
}