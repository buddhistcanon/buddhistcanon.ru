{
  "vv66:0.1": "Vimānavatthu",
  "vv66:0.2": "Purisavimāna",
  "vv66:0.3": "Pāyāsivagga",
  "vv66:0.4": "2. Dutiyaagāriyavimānavatthu",
  "vv66:1.1": "“Yathā vanaṃ cittalataṃ pabhāsati,",
  "vv66:1.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
  "vv66:1.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
  "vv66:1.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
  "vv66:2.1": "Deviddhipattosi mahānubhāvo,",
  "vv66:2.2": "Manussabhūto kimakāsi puññaṃ;",
  "vv66:2.3": "Kenāsi evaṃ jalitānubhāvo,",
  "vv66:2.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv66:3.1": "So devaputto attamano,",
  "vv66:3.2": "…pe…",
  "vv66:3.3": "yassa kammassidaṃ phalaṃ.",
  "vv66:4.1": "“Ahañca bhariyā ca manussaloke,",
  "vv66:4.2": "Opānabhūtā gharamāvasimha;",
  "vv66:4.3": "Annañca pānañca pasannacittā,",
  "vv66:4.4": "Sakkacca dānaṃ vipulaṃ adamha.",
  "vv66:5.1": "Tena metādiso vaṇṇo,",
  "vv66:5.2": "…pe…",
  "vv66:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv66:6.1": "Dutiyaagāriyavimānaṃ dutiyaṃ."
}