{
  "vv12:0.1": "Vimānavatthu",
  "vv12:0.2": "Itthivimāna",
  "vv12:0.3": "Pīṭhavagga",
  "vv12:0.4": "12. Dutiyapatibbatāvimānavatthu",
  "vv12:1.1": "“Veḷuriyathambhaṃ ruciraṃ pabhassaraṃ,",
  "vv12:1.2": "Vimānamāruyha anekacittaṃ;",
  "vv12:1.3": "Tatthacchasi devi mahānubhāve,",
  "vv12:1.4": "Uccāvacā iddhi vikubbamānā;",
  "vv12:1.5": "Imā ca te accharāyo samantato,",
  "vv12:1.6": "Naccanti gāyanti pamodayanti ca.",
  "vv12:2.1": "Deviddhipattāsi mahānubhāve,",
  "vv12:2.2": "Manussabhūtā kimakāsi puññaṃ;",
  "vv12:2.3": "Kenāsi evaṃ jalitānubhāvā,",
  "vv12:2.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv12:3.1": "Sā devatā attamanā,",
  "vv12:3.2": "moggallānena pucchitā;",
  "vv12:3.3": "Pañhaṃ puṭṭhā viyākāsi,",
  "vv12:3.4": "yassa kammassidaṃ phalaṃ.",
  "vv12:4.1": "“Ahaṃ manussesu manussabhūtā,",
  "vv12:4.2": "Upāsikā cakkhumato ahosiṃ;",
  "vv12:4.3": "Pāṇātipātā viratā ahosiṃ,",
  "vv12:4.4": "Loke adinnaṃ parivajjayissaṃ.",
  "vv12:5.1": "Amajjapā no ca musā abhāṇiṃ,",
  "vv12:5.2": "Sakena sāminā ahosiṃ tuṭṭhā;",
  "vv12:5.3": "Annañca pānañca pasannacittā,",
  "vv12:5.4": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
  "vv12:6.1": "Tena metādiso vaṇṇo,",
  "vv12:6.2": "tena me idha mijjhati;",
  "vv12:6.3": "Uppajjanti ca me bhogā,",
  "vv12:6.4": "ye keci manaso piyā.",
  "vv12:7.1": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv12:7.2": "Manussabhūtā yamakāsi puññaṃ;",
  "vv12:7.3": "Tenamhi evaṃ jalitānubhāvā,",
  "vv12:7.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv12:8.1": "Dutiyapatibbatāvimānaṃ dvādasamaṃ."
}