{
  "vv34:0.1": "Vimānavatthu",
  "vv34:0.2": "Itthivimāna",
  "vv34:0.3": "Pāricchattakavagga",
  "vv34:0.4": "6. Daddallavimānavatthu",
  "vv34:1.1": "“Daddallamānā vaṇṇena,",
  "vv34:1.2": "yasasā ca yasassinī;",
  "vv34:1.3": "Sabbe deve tāvatiṃse,",
  "vv34:1.4": "vaṇṇena atirocasi.",
  "vv34:2.1": "Dassanaṃ nābhijānāmi,",
  "vv34:2.2": "idaṃ paṭhamadassanaṃ;",
  "vv34:2.3": "Kasmā kāyā nu āgamma,",
  "vv34:2.4": "nāmena bhāsase maman”ti.",
  "vv34:3.1": "“Ahaṃ bhadde subhaddāsiṃ,",
  "vv34:3.2": "pubbe mānusake bhave;",
  "vv34:3.3": "Sahabhariyā ca te āsiṃ,",
  "vv34:3.4": "bhaginī ca kaniṭṭhikā.",
  "vv34:4.1": "Sā ahaṃ kāyassa bhedā,",
  "vv34:4.2": "vippamuttā tato cutā;",
  "vv34:4.3": "Nimmānaratīnaṃ devānaṃ,",
  "vv34:4.4": "upapannā sahabyatan”ti.",
  "vv34:5.1": "“Pahūtakatakalyāṇā,",
  "vv34:5.2": "te deve yanti pāṇino;",
  "vv34:5.3": "Yesaṃ tvaṃ kittayissasi,",
  "vv34:5.4": "subhadde jātimattano.",
  "vv34:6.1": "Atha tvaṃ kena vaṇṇena,",
  "vv34:6.2": "kena vā anusāsitā;",
  "vv34:6.3": "Kīdiseneva dānena,",
  "vv34:6.4": "subbatena yasassinī.",
  "vv34:7.1": "Yasaṃ etādisaṃ pattā,",
  "vv34:7.2": "visesaṃ vipulamajjhagā;",
  "vv34:7.3": "Devate pucchitācikkha,",
  "vv34:7.4": "kissa kammassidaṃ phalan”ti.",
  "vv34:8.1": "“Aṭṭheva piṇḍapātāni,",
  "vv34:8.2": "yaṃ dānaṃ adadaṃ pure;",
  "vv34:8.3": "Dakkhiṇeyyassa saṃghassa,",
  "vv34:8.4": "pasannā sehi pāṇibhi.",
  "vv34:9.1": "Tena metādiso vaṇṇo,",
  "vv34:9.2": "…pe…",
  "vv34:9.3": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv34:10.1": "“Ahaṃ tayā bahutare bhikkhū,",
  "vv34:10.2": "Saññate brahmacārayo;",
  "vv34:10.3": "Tappesiṃ annapānena,",
  "vv34:10.4": "Pasannā sehi pāṇibhi.",
  "vv34:11.1": "Tayā bahutaraṃ datvā,",
  "vv34:11.2": "hīnakāyūpagā ahaṃ;",
  "vv34:11.3": "Kathaṃ tvaṃ appataraṃ datvā,",
  "vv34:11.4": "visesaṃ vipulamajjhagā;",
  "vv34:11.5": "Devate pucchitācikkha,",
  "vv34:11.6": "kissa kammassidaṃ phalan”ti.",
  "vv34:12.1": "“Manobhāvanīyo bhikkhu,",
  "vv34:12.2": "sandiṭṭho me pure ahu;",
  "vv34:12.3": "Tāhaṃ bhattena nimantesiṃ,",
  "vv34:12.4": "revataṃ attanaṭṭhamaṃ.",
  "vv34:13.1": "So me atthapurekkhāro,",
  "vv34:13.2": "anukampāya revato;",
  "vv34:13.3": "Saṃghe dehīti maṃvoca,",
  "vv34:13.4": "tassāhaṃ vacanaṃ kariṃ.",
  "vv34:14.1": "Sā dakkhiṇā saṃghagatā,",
  "vv34:14.2": "appameyye patiṭṭhitā;",
  "vv34:14.3": "Puggalesu tayā dinnaṃ,",
  "vv34:14.4": "na taṃ tava mahapphalan”ti.",
  "vv34:15.1": "“Idānevāhaṃ jānāmi,",
  "vv34:15.2": "saṃghe dinnaṃ mahapphalaṃ;",
  "vv34:15.3": "Sāhaṃ gantvā manussattaṃ,",
  "vv34:15.4": "vadaññū vītamaccharā;",
  "vv34:15.5": "Saṃghe dānāni dassāmi,",
  "vv34:15.6": "appamattā punappunan”ti.",
  "vv34:16.1": "“Kā esā devatā bhadde,",
  "vv34:16.2": "tayā mantayate saha;",
  "vv34:16.3": "Sabbe deve tāvatiṃse,",
  "vv34:16.4": "vaṇṇena atirocatī”ti.",
  "vv34:17.1": "“Manussabhūtā devinda,",
  "vv34:17.2": "pubbe mānusake bhave;",
  "vv34:17.3": "Sahabhariyā ca me āsi,",
  "vv34:17.4": "bhaginī ca kaniṭṭhikā;",
  "vv34:17.5": "Saṃghe dānāni datvāna,",
  "vv34:17.6": "katapuññā virocatī”ti.",
  "vv34:18.1": "“Dhammena pubbe bhaginī,",
  "vv34:18.2": "tayā bhadde virocati;",
  "vv34:18.3": "Yaṃ saṃghamhi appameyye,",
  "vv34:18.4": "patiṭṭhāpesi dakkhiṇaṃ”.",
  "vv34:19.1": "“Pucchito hi mayā buddho,",
  "vv34:19.2": "gijjhakūṭamhi pabbate;",
  "vv34:19.3": "Vipākaṃ saṃvibhāgassa,",
  "vv34:19.4": "yattha dinnaṃ mahapphalaṃ.",
  "vv34:20.1": "Yajamānānaṃ manussānaṃ,",
  "vv34:20.2": "puññapekkhāna pāṇinaṃ;",
  "vv34:20.3": "Karotaṃ opadhikaṃ puññaṃ,",
  "vv34:20.4": "yattha dinnaṃ mahapphalaṃ.",
  "vv34:21.1": "Taṃ me buddho viyākāsi,",
  "vv34:21.2": "jānaṃ kammaphalaṃ sakaṃ;",
  "vv34:21.3": "Vipākaṃ saṃvibhāgassa,",
  "vv34:21.4": "yattha dinnaṃ mahapphalaṃ.",
  "vv34:22.1": "Cattāro ca paṭipannā,",
  "vv34:22.2": "cattāro ca phale ṭhitā;",
  "vv34:22.3": "Esa saṃgho ujubhūto,",
  "vv34:22.4": "paññāsīlasamāhito.",
  "vv34:23.1": "Yajamānānaṃ manussānaṃ,",
  "vv34:23.2": "puññapekkhāna pāṇinaṃ;",
  "vv34:23.3": "Karotaṃ opadhikaṃ puññaṃ,",
  "vv34:23.4": "saṃghe dinnaṃ mahapphalaṃ.",
  "vv34:24.1": "Eso hi saṃgho vipulo mahaggato,",
  "vv34:24.2": "Esappameyyo udadhīva sāgaro;",
  "vv34:24.3": "Ete hi seṭṭhā naravīrasāvakā,",
  "vv34:24.4": "Pabhaṅkarā dhammamudīrayanti.",
  "vv34:25.1": "Tesaṃ sudinnaṃ suhutaṃ suyiṭṭhaṃ,",
  "vv34:25.2": "Ye saṃghamuddissa dadanti dānaṃ;",
  "vv34:25.3": "Sā dakkhiṇā saṃghagatā patiṭṭhitā,",
  "vv34:25.4": "Mahapphalā lokavidūna vaṇṇitā.",
  "vv34:26.1": "Etādisaṃ yaññamanussarantā,",
  "vv34:26.2": "Ye vedajātā vicaranti loke;",
  "vv34:26.3": "Vineyya maccheramalaṃ samūlaṃ,",
  "vv34:26.4": "Aninditā saggamupenti ṭhānan”ti.",
  "vv34:27.1": "Daddallavimānaṃ chaṭṭhaṃ."
}