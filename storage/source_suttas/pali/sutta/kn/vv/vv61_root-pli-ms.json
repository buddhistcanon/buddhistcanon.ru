{
  "vv61:0.1": "Vimānavatthu",
  "vv61:0.2": "Purisavimāna",
  "vv61:0.3": "Mahārathavagga",
  "vv61:0.4": "11. Dutiyanāgavimānavatthu",
  "vv61:1.1": "“Mahantaṃ nāgaṃ abhiruyha,",
  "vv61:1.2": "Sabbasetaṃ gajuttamaṃ;",
  "vv61:1.3": "Vanā vanaṃ anupariyāsi,",
  "vv61:1.4": "Nārīgaṇapurakkhato;",
  "vv61:1.5": "Obhāsento disā sabbā,",
  "vv61:1.6": "Osadhī viya tārakā.",
  "vv61:2.1": "Kena tetādiso vaṇṇo,",
  "vv61:2.2": "…pe…",
  "vv61:2.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv61:3.1": "So devaputto attamano,",
  "vv61:3.2": "vaṅgīseneva pucchito;",
  "vv61:3.3": "Pañhaṃ puṭṭho viyākāsi,",
  "vv61:3.4": "yassa kammassidaṃ phalaṃ.",
  "vv61:4.1": "“Ahaṃ manussesu manussabhūto,",
  "vv61:4.2": "Upāsako cakkhumato ahosiṃ;",
  "vv61:4.3": "Pāṇātipātā virato ahosiṃ,",
  "vv61:4.4": "Loke adinnaṃ parivajjayissaṃ.",
  "vv61:5.1": "Amajjapo no ca musā abhāṇiṃ,",
  "vv61:5.2": "Sakena dārena ca tuṭṭho ahosiṃ;",
  "vv61:5.3": "Annañca pānañca pasannacitto,",
  "vv61:5.4": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
  "vv61:6.1": "Tena metādiso vaṇṇo,",
  "vv61:6.2": "…pe…",
  "vv61:6.3": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv61:7.1": "Dutiyanāgavimānaṃ ekādasamaṃ."
}