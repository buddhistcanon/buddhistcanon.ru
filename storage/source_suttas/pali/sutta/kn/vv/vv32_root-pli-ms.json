{
  "vv32:0.1": "Vimānavatthu",
  "vv32:0.2": "Itthivimāna",
  "vv32:0.3": "Pāricchattakavagga",
  "vv32:0.4": "4. Latāvimānavatthu",
  "vv32:1.1": "Latā ca sajjā pavarā ca devatā,",
  "vv32:1.2": "Accimatī rājavarassa sirīmato;",
  "vv32:1.3": "Sutā ca rañño vessavaṇassa dhītā,",
  "vv32:1.4": "Rājīmatī dhammaguṇehi sobhatha.",
  "vv32:2.1": "Pañcettha nāriyo āgamaṃsu nhāyituṃ,",
  "vv32:2.2": "Sītodakaṃ uppaliniṃ sivaṃ nadiṃ;",
  "vv32:2.3": "Tā tattha nhāyitvā rametvā devatā,",
  "vv32:2.4": "Naccitvā gāyitvā sutā lataṃ bravi.",
  "vv32:3.1": "“Pucchāmi taṃ uppalamāladhārini,",
  "vv32:3.2": "Āveḷini kañcanasannibhattace;",
  "vv32:3.3": "Timiratambakkhi nabheva sobhane,",
  "vv32:3.4": "Dīghāyukī kena kato yaso tava.",
  "vv32:4.1": "Kenāsi bhadde patino piyatarā,",
  "vv32:4.2": "Visiṭṭhakalyāṇitarassu rūpato;",
  "vv32:4.3": "Padakkhiṇā naccanagītavādite,",
  "vv32:4.4": "Ācikkha no tvaṃ naranāripucchitā”ti.",
  "vv32:5.1": "“Ahaṃ manussesu manussabhūtā,",
  "vv32:5.2": "Uḷārabhoge kule suṇisā ahosiṃ;",
  "vv32:5.3": "Akkodhanā bhattuvasānuvattinī,",
  "vv32:5.4": "Uposathe appamattā ahosiṃ.",
  "vv32:6.1": "Manussabhūtā daharā apāpikā,",
  "vv32:6.2": "Pasannacittā patimābhirādhayiṃ;",
  "vv32:6.3": "Sadevaraṃ sassasuraṃ sadāsakaṃ,",
  "vv32:6.4": "Abhirādhayiṃ tamhi kato yaso mama.",
  "vv32:7.1": "Sāhaṃ tena kusalena kammunā,",
  "vv32:7.2": "Catubbhi ṭhānehi visesamajjhagā;",
  "vv32:7.3": "Āyuñca vaṇṇañca sukhaṃ balañca,",
  "vv32:7.4": "Khiḍḍāratiṃ paccanubhomanappakaṃ”.",
  "vv32:8.1": "“Sutaṃ nu taṃ bhāsati yaṃ ayaṃ latā,",
  "vv32:8.2": "Yaṃ no apucchimha akittayī no;",
  "vv32:8.3": "Patino kiramhākaṃ visiṭṭha nārīnaṃ,",
  "vv32:8.4": "Gatī ca tāsaṃ pavarā ca devatā.",
  "vv32:9.1": "Patīsu dhammaṃ pacarāma sabbā,",
  "vv32:9.2": "Patibbatā yattha bhavanti itthiyo;",
  "vv32:9.3": "Patīsu dhammaṃ pacaritva sabbā,",
  "vv32:9.4": "Lacchāmase bhāsati yaṃ ayaṃ latā.",
  "vv32:10.1": "Sīho yathā pabbatasānugocaro,",
  "vv32:10.2": "Mahindharaṃ pabbatamāvasitvā;",
  "vv32:10.3": "Pasayha hantvā itare catuppade,",
  "vv32:10.4": "Khudde mige khādati maṃsabhojano.",
  "vv32:11.1": "Tatheva saddhā idha ariyasāvikā,",
  "vv32:11.2": "Bhattāraṃ nissāya patiṃ anubbatā;",
  "vv32:11.3": "Kodhaṃ vadhitvā abhibhuyya maccharaṃ,",
  "vv32:11.4": "Saggamhi sā modati dhammacārinī”ti.",
  "vv32:12.1": "Latāvimānaṃ catutthaṃ."
}