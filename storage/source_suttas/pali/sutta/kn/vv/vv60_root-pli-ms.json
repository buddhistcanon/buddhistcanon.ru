{
  "vv60:0.1": "Vimānavatthu",
  "vv60:0.2": "Purisavimāna",
  "vv60:0.3": "Mahārathavagga",
  "vv60:0.4": "10. Paṭhamanāgavimānavatthu",
  "vv60:1.1": "“Susukkakhandhaṃ abhiruyha nāgaṃ,",
  "vv60:1.2": "Akācinaṃ dantiṃ baliṃ mahājavaṃ;",
  "vv60:1.3": "Abhiruyha gajavaraṃ sukappitaṃ,",
  "vv60:1.4": "Idhāgamā vehāyasaṃ antalikkhe.",
  "vv60:2.1": "Nāgassa dantesu duvesu nimmitā,",
  "vv60:2.2": "Acchodakā paduminiyo suphullā;",
  "vv60:2.3": "Padumesu ca tūriyagaṇā pavajjare,",
  "vv60:2.4": "Imā ca naccanti manoharāyo.",
  "vv60:3.1": "Deviddhipattosi mahānubhāvo,",
  "vv60:3.2": "Manussabhūto kimakāsi puññaṃ;",
  "vv60:3.3": "Kenāsi evaṃ jalitānubhāvo,",
  "vv60:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv60:4.1": "So devaputto attamano,",
  "vv60:4.2": "moggallānena pucchito;",
  "vv60:4.3": "Pañhaṃ puṭṭho viyākāsi,",
  "vv60:4.4": "yassa kammassidaṃ phalaṃ.",
  "vv60:5.1": "“Aṭṭheva muttapupphāni,",
  "vv60:5.2": "kassapassa mahesino;",
  "vv60:5.3": "Thūpasmiṃ abhiropesiṃ,",
  "vv60:5.4": "pasanno sehi pāṇibhi.",
  "vv60:6.1": "Tena metādiso vaṇṇo,",
  "vv60:6.2": "…pe…",
  "vv60:6.3": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv60:7.1": "Paṭhamanāgavimānaṃ dasamaṃ."
}