{
  "vv50:0.1": "Vimānavatthu",
  "vv50:0.2": "Itthivimāna",
  "vv50:0.3": "Mañjiṭṭhakavagga",
  "vv50:0.4": "12. Rajjumālāvimānavatthu",
  "vv50:1.1": "“Abhikkantena vaṇṇena,",
  "vv50:1.2": "yā tvaṃ tiṭṭhasi devate;",
  "vv50:1.3": "Hatthapāde ca viggayha,",
  "vv50:1.4": "naccasi suppavādite.",
  "vv50:2.1": "Tassā te naccamānāya,",
  "vv50:2.2": "aṅgamaṅgehi sabbaso;",
  "vv50:2.3": "Dibbā saddā niccharanti,",
  "vv50:2.4": "savanīyā manoramā.",
  "vv50:3.1": "Tassā te naccamānāya,",
  "vv50:3.2": "aṅgamaṅgehi sabbaso;",
  "vv50:3.3": "Dibbā gandhā pavāyanti,",
  "vv50:3.4": "sucigandhā manoramā.",
  "vv50:4.1": "Vivattamānā kāyena,",
  "vv50:4.2": "yā veṇīsu piḷandhanā;",
  "vv50:4.3": "Tesaṃ suyyati nigghoso,",
  "vv50:4.4": "tūriye pañcaṅgike yathā.",
  "vv50:5.1": "Vaṭaṃsakā vātadhutā,",
  "vv50:5.2": "vātena sampakampitā;",
  "vv50:5.3": "Tesaṃ suyyati nigghoso,",
  "vv50:5.4": "tūriye pañcaṅgike yathā.",
  "vv50:6.1": "Yāpi te sirasmiṃ mālā,",
  "vv50:6.2": "sucigandhā manoramā;",
  "vv50:6.3": "Vāti gandho disā sabbā,",
  "vv50:6.4": "rukkho mañjūsako yathā.",
  "vv50:7.1": "Ghāyase taṃ sucigandhaṃ,",
  "vv50:7.2": "rūpaṃ passasi amānusaṃ;",
  "vv50:7.3": "Devate pucchitācikkha,",
  "vv50:7.4": "kissa kammassidaṃ phalan”ti.",
  "vv50:8.1": "“Dāsī ahaṃ pure āsiṃ,",
  "vv50:8.2": "gayāyaṃ brāhmaṇassahaṃ;",
  "vv50:8.3": "Appapuññā alakkhikā,",
  "vv50:8.4": "rajjumālāti maṃ viduṃ.",
  "vv50:9.1": "Akkosānaṃ vadhānañca,",
  "vv50:9.2": "tajjanāya ca uggatā;",
  "vv50:9.3": "Kuṭaṃ gahetvā nikkhamma,",
  "vv50:9.4": "agañchiṃ udahāriyā.",
  "vv50:10.1": "Vipathe kuṭaṃ nikkhipitvā,",
  "vv50:10.2": "vanasaṇḍaṃ upāgamiṃ;",
  "vv50:10.3": "Idhevāhaṃ marissāmi,",
  "vv50:10.4": "ko attho jīvitena me.",
  "vv50:11.1": "Daḷhaṃ pāsaṃ karitvāna,",
  "vv50:11.2": "āsumbhitvāna pādape;",
  "vv50:11.3": "Tato disā vilokesiṃ,",
  "vv50:11.4": "‘ko nu kho vanamassito’.",
  "vv50:12.1": "Tatthaddasāsiṃ sambuddhaṃ,",
  "vv50:12.2": "sabbalokahitaṃ muniṃ;",
  "vv50:12.3": "Nisinnaṃ rukkhamūlasmiṃ,",
  "vv50:12.4": "jhāyantaṃ akutobhayaṃ.",
  "vv50:13.1": "Tassā me ahu saṃvego,",
  "vv50:13.2": "abbhuto lomahaṃsano;",
  "vv50:13.3": "Ko nu kho vanamassito,",
  "vv50:13.4": "manusso udāhu devatā.",
  "vv50:14.1": "Pāsādikaṃ pasādanīyaṃ,",
  "vv50:14.2": "vanā nibbanamāgataṃ;",
  "vv50:14.3": "Disvā mano me pasīdi,",
  "vv50:14.4": "nāyaṃ yādisakīdiso.",
  "vv50:15.1": "Guttindriyo jhānarato,",
  "vv50:15.2": "abahiggatamānaso;",
  "vv50:15.3": "Hito sabbassa lokassa,",
  "vv50:15.4": "buddho ayaṃ bhavissati.",
  "vv50:16.1": "Bhayabheravo durāsado,",
  "vv50:16.2": "sīhova guhamassito;",
  "vv50:16.3": "Dullabhāyaṃ dassanāya,",
  "vv50:16.4": "pupphaṃ odumbaraṃ yathā.",
  "vv50:17.1": "So maṃ mudūhi vācāhi,",
  "vv50:17.2": "ālapitvā tathāgato;",
  "vv50:17.3": "Rajjumāleti maṃvoca,",
  "vv50:17.4": "saraṇaṃ gaccha tathāgataṃ.",
  "vv50:18.1": "Tāhaṃ giraṃ suṇitvāna,",
  "vv50:18.2": "nelaṃ atthavatiṃ suciṃ;",
  "vv50:18.3": "Saṇhaṃ muduñca vagguñca,",
  "vv50:18.4": "sabbasokāpanūdanaṃ.",
  "vv50:19.1": "Kallacittañca maṃ ñatvā,",
  "vv50:19.2": "pasannaṃ suddhamānasaṃ;",
  "vv50:19.3": "Hito sabbassa lokassa,",
  "vv50:19.4": "anusāsi tathāgato.",
  "vv50:20.1": "Idaṃ dukkhanti maṃvoca,",
  "vv50:20.2": "Ayaṃ dukkhassa sambhavo;",
  "vv50:20.3": "Dukkha nirodho maggo ca,",
  "vv50:20.4": "Añjaso amatogadho.",
  "vv50:21.1": "Anukampakassa kusalassa,",
  "vv50:21.2": "Ovādamhi ahaṃ ṭhitā;",
  "vv50:21.3": "Ajjhagā amataṃ santiṃ,",
  "vv50:21.4": "Nibbānaṃ padamaccutaṃ.",
  "vv50:22.1": "Sāhaṃ avaṭṭhitāpemā,",
  "vv50:22.2": "dassane avikampinī;",
  "vv50:22.3": "Mūlajātāya saddhāya,",
  "vv50:22.4": "dhītā buddhassa orasā.",
  "vv50:23.1": "Sāhaṃ ramāmi kīḷāmi,",
  "vv50:23.2": "modāmi akutobhayā;",
  "vv50:23.3": "Dibbamālaṃ dhārayāmi,",
  "vv50:23.4": "pivāmi madhumaddavaṃ.",
  "vv50:24.1": "Saṭṭhitūriyasahassāni,",
  "vv50:24.2": "paṭibodhaṃ karonti me;",
  "vv50:24.3": "Āḷambo gaggaro bhīmo,",
  "vv50:24.4": "sādhuvādī ca saṃsayo.",
  "vv50:25.1": "Pokkharo ca suphasso ca,",
  "vv50:25.2": "vīṇāmokkhā ca nāriyo;",
  "vv50:25.3": "Nandā ceva sunandā ca,",
  "vv50:25.4": "soṇadinnā sucimhitā.",
  "vv50:26.1": "Alambusā missakesī ca,",
  "vv50:26.2": "puṇḍarīkātidāruṇī;",
  "vv50:26.3": "Eṇīphassā suphassā ca,",
  "vv50:26.4": "subhaddā muduvādinī.",
  "vv50:27.1": "Etā caññā ca seyyāse,",
  "vv50:27.2": "accharānaṃ pabodhikā;",
  "vv50:27.3": "Tā maṃ kālenupāgantvā,",
  "vv50:27.4": "abhibhāsanti devatā.",
  "vv50:28.1": "Handa naccāma gāyāma,",
  "vv50:28.2": "handa taṃ ramayāmase;",
  "vv50:28.3": "Nayidaṃ akatapuññānaṃ,",
  "vv50:28.4": "katapuññānamevidaṃ.",
  "vv50:29.1": "Asokaṃ nandanaṃ rammaṃ,",
  "vv50:29.2": "tidasānaṃ mahāvanaṃ;",
  "vv50:29.3": "Sukhaṃ akatapuññānaṃ,",
  "vv50:29.4": "idha natthi parattha ca.",
  "vv50:30.1": "Sukhañca katapuññānaṃ,",
  "vv50:30.2": "idha ceva parattha ca;",
  "vv50:30.3": "Tesaṃ sahabyakāmānaṃ,",
  "vv50:30.4": "kattabbaṃ kusalaṃ bahuṃ;",
  "vv50:30.5": "Katapuññā hi modanti,",
  "vv50:30.6": "sagge bhogasamaṅgino.",
  "vv50:31.1": "Bahūnaṃ vata atthāya,",
  "vv50:31.2": "uppajjanti tathāgatā;",
  "vv50:31.3": "Dakkhiṇeyyā manussānaṃ,",
  "vv50:31.4": "puññakhettānamākarā;",
  "vv50:31.5": "Yattha kāraṃ karitvāna,",
  "vv50:31.6": "sagge modanti dāyakā”ti.",
  "vv50:32.1": "Rajjumālāvimānaṃ dvādasamaṃ.",
  "vv50:33.1": "Tassuddānaṃ",
  "vv50:34.1": "Mañjiṭṭhā pabhassarā nāgā,",
  "vv50:34.2": "Alomākañjikadāyikā;",
  "vv50:34.3": "Vihāracaturitthambā,",
  "vv50:34.4": "Pītā ucchuvandanarajjumālā ca;",
  "vv50:34.5": "Vaggo tena pavuccatīti.",
  "vv50:35.1": "Mañjiṭṭhakavaggo catuttho.",
  "vv50:36.1": "Itthivimānaṃ samattaṃ."
}