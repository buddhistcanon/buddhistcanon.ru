{
  "vv3:0.1": "Vimānavatthu",
  "vv3:0.2": "Itthivimāna",
  "vv3:0.3": "Pīṭhavagga",
  "vv3:0.4": "3. Tatiyapīṭhavimānavatthu",
  "vv3:1.1": "“Pīṭhaṃ te sovaṇṇamayaṃ uḷāraṃ,",
  "vv3:1.2": "Manojavaṃ gacchati yenakāmaṃ;",
  "vv3:1.3": "Alaṅkate malyadhare suvatthe,",
  "vv3:1.4": "Obhāsasi vijjurivabbhakūṭaṃ.",
  "vv3:2.1": "Kena tetādiso vaṇṇo,",
  "vv3:2.2": "kena te idha mijjhati;",
  "vv3:2.3": "Uppajjanti ca te bhogā,",
  "vv3:2.4": "ye keci manaso piyā.",
  "vv3:3.1": "Pucchāmi taṃ devi mahānubhāve,",
  "vv3:3.2": "Manussabhūtā kimakāsi puññaṃ;",
  "vv3:3.3": "Kenāsi evaṃ jalitānubhāvā,",
  "vv3:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv3:4.1": "Sā devatā attamanā,",
  "vv3:4.2": "moggallānena pucchitā;",
  "vv3:4.3": "Pañhaṃ puṭṭhā viyākāsi,",
  "vv3:4.4": "yassa kammassidaṃ phalaṃ.",
  "vv3:5.1": "“Appassa kammassa phalaṃ mamedaṃ,",
  "vv3:5.2": "Yenamhi evaṃ jalitānubhāvā;",
  "vv3:5.3": "Ahaṃ manussesu manussabhūtā,",
  "vv3:5.4": "Purimāya jātiyā manussaloke.",
  "vv3:6.1": "Addasaṃ virajaṃ bhikkhuṃ,",
  "vv3:6.2": "vippasannamanāvilaṃ;",
  "vv3:6.3": "Tassa adāsahaṃ pīṭhaṃ,",
  "vv3:6.4": "pasannā sehi pāṇibhi.",
  "vv3:7.1": "Tena metādiso vaṇṇo,",
  "vv3:7.2": "tena me idha mijjhati;",
  "vv3:7.3": "Uppajjanti ca me bhogā,",
  "vv3:7.4": "ye keci manaso piyā.",
  "vv3:8.1": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv3:8.2": "Manussabhūtā yamakāsi puññaṃ;",
  "vv3:8.3": "Tenamhi evaṃ jalitānubhāvā,",
  "vv3:8.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv3:9.1": "Tatiyapīṭhavimānaṃ tatiyaṃ."
}