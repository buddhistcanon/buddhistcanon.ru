{
  "vv33:0.1": "Vimānavatthu",
  "vv33:0.2": "Itthivimāna",
  "vv33:0.3": "Pāricchattakavagga",
  "vv33:0.4": "Guttilavimānavatthu",
  "vv33:1.0.1": "1. Vatthuttamadāyikāvimānavatthu",
  "vv33:1.1": "“Sattatantiṃ sumadhuraṃ,",
  "vv33:1.2": "Rāmaṇeyyaṃ avācayiṃ;",
  "vv33:1.3": "So maṃ raṅgamhi avheti,",
  "vv33:1.4": "‘Saraṇaṃ me hohi kosiyā’”ti.",
  "vv33:2.1": "“Ahaṃ te saraṇaṃ homi,",
  "vv33:2.2": "Ahamācariyapūjako;",
  "vv33:2.3": "Na taṃ jayissati sisso,",
  "vv33:2.4": "Sissamācariya jessasī”ti.",
  "vv33:3.1": "“Abhikkantena vaṇṇena,",
  "vv33:3.2": "yā tvaṃ tiṭṭhasi devate;",
  "vv33:3.3": "Obhāsentī disā sabbā,",
  "vv33:3.4": "osadhī viya tārakā.",
  "vv33:4.1": "Kena tetādiso vaṇṇo,",
  "vv33:4.2": "kena te idha mijjhati;",
  "vv33:4.3": "Uppajjanti ca te bhogā,",
  "vv33:4.4": "ye keci manaso piyā.",
  "vv33:5.1": "Pucchāmi taṃ devi mahānubhāve,",
  "vv33:5.2": "Manussabhūtā kimakāsi puññaṃ;",
  "vv33:5.3": "Kenāsi evaṃ jalitānubhāvā,",
  "vv33:5.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv33:6.1": "Sā devatā attamanā,",
  "vv33:6.2": "moggallānena pucchitā;",
  "vv33:6.3": "Pañhaṃ puṭṭhā viyākāsi,",
  "vv33:6.4": "yassa kammassidaṃ phalaṃ.",
  "vv33:7.1": "“Vatthuttamadāyikā nārī,",
  "vv33:7.2": "Pavarā hoti naresu nārīsu;",
  "vv33:7.3": "Evaṃ piyarūpadāyikā manāpaṃ,",
  "vv33:7.4": "Dibbaṃ sā labhate upecca ṭhānaṃ.",
  "vv33:8.1": "Tassā me passa vimānaṃ,",
  "vv33:8.2": "Accharā kāmavaṇṇinīhamasmi;",
  "vv33:8.3": "Accharāsahassassāhaṃ,",
  "vv33:8.4": "Pavarā passa puññānaṃ vipākaṃ.",
  "vv33:9.1": "Tena metādiso vaṇṇo,",
  "vv33:9.2": "tena me idha mijjhati;",
  "vv33:9.3": "Uppajjanti ca me bhogā,",
  "vv33:9.4": "ye keci manaso piyā.",
  "vv33:10.1": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv33:10.2": "Manussabhūtā yamakāsi puññaṃ;",
  "vv33:10.3": "Tenamhi evaṃ jalitānubhāvā,",
  "vv33:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv33:11.1": "(Anantaraṃ caturavimānaṃ yathā vatthadāyikāvimānaṃ tathā vitthāretabbaṃ.)",
