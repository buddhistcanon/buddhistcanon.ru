{
  "vv35:0.1": "Vimānavatthu",
  "vv35:0.2": "Itthivimāna",
  "vv35:0.3": "Pāricchattakavagga",
  "vv35:0.4": "7. Pesavatīvimānavatthu",
  "vv35:1.1": "“Phalikarajatahemajālachannaṃ,",
  "vv35:1.2": "Vividhacitratalamaddasaṃ surammaṃ;",
  "vv35:1.3": "Byamhaṃ sunimmitaṃ toraṇūpapannaṃ,",
  "vv35:1.4": "Rucakupakiṇṇamidaṃ subhaṃ vimānaṃ.",
  "vv35:2.1": "Bhāti ca dasa disā nabheva suriyo,",
  "vv35:2.2": "Sarade tamonudo sahassaraṃsī;",
  "vv35:2.3": "Tathā tapati midaṃ tava vimānaṃ,",
  "vv35:2.4": "Jalamiva dhūmasikho nise nabhagge.",
  "vv35:3.1": "Musatīva nayanaṃ sateratāva,",
  "vv35:3.2": "Ākāse ṭhapitamidaṃ manuññaṃ;",
  "vv35:3.3": "Vīṇāmurajasammatāḷaghuṭṭhaṃ,",
  "vv35:3.4": "Iddhaṃ indapuraṃ yathā tavedaṃ.",
  "vv35:4.1": "Padumakumuduppalakuvalayaṃ,",
  "vv35:4.2": "Yodhika bandhukanojakā ca santi;",
  "vv35:4.3": "Sālakusumitapupphitā asokā,",
  "vv35:4.4": "Vividhadumaggasugandhasevitamidaṃ.",
  "vv35:5.1": "Saḷalalabujabhujaka saṃyuttā,",
  "vv35:5.2": "Kusakasuphullitalatāvalambinīhi;",
  "vv35:5.3": "Maṇijālasadisā yasassinī,",
  "vv35:5.4": "Rammā pokkharaṇī upaṭṭhitā te.",
  "vv35:6.1": "Udakaruhā ca yetthi pupphajātā,",
  "vv35:6.2": "Thalajā ye ca santi rukkhajātā;",
  "vv35:6.3": "Mānusakāmānussakā ca dibbā,",
  "vv35:6.4": "Sabbe tuyhaṃ nivesanamhi jātā.",
  "vv35:7.1": "Kissa saṃyamadamassayaṃ vipāko,",
  "vv35:7.2": "Kenāsi kammaphalenidhūpapannā;",
  "vv35:7.3": "Yathā ca te adhigatamidaṃ vimānaṃ,",
  "vv35:7.4": "Tadanupadaṃ avacāsiḷārapamhe”ti.",
  "vv35:8.1": "“Yathā ca me adhigatamidaṃ vimānaṃ,",
  "vv35:8.2": "Koñcamayūracakorasaṅghacaritaṃ;",
  "vv35:8.3": "Dibyapilavahaṃsarājaciṇṇaṃ,",
  "vv35:8.4": "Dijakāraṇḍavakokilābhinaditaṃ.",
  "vv35:9.1": "Nānāsantānakapuppharukkhavividhā,",
  "vv35:9.2": "Pāṭalijambuasokarukkhavantaṃ;",
  "vv35:9.3": "Yathā ca me adhigatamidaṃ vimānaṃ,",
  "vv35:9.4": "Taṃ te pavedayāmi suṇohi bhante.",
  "vv35:10.1": "Magadhavarapuratthimena,",
  "vv35:10.2": "Nāḷakagāmo nāma atthi bhante;",
  "vv35:10.3": "Tattha ahosiṃ pure suṇisā,",
  "vv35:10.4": "Pesavatīti tattha jāniṃsu mamaṃ.",
  "vv35:11.1": "Sāhamapacitatthadhammakusalaṃ,",
  "vv35:11.2": "Devamanussapūjitaṃ mahantaṃ;",
  "vv35:11.3": "Upatissaṃ nibbutamappameyyaṃ,",
  "vv35:11.4": "Muditamanā kusumehi abbhukiriṃ.",
  "vv35:12.1": "Paramagatigatañca pūjayitvā,",
  "vv35:12.2": "Antimadehadharaṃ isiṃ uḷāraṃ;",
  "vv35:12.3": "Pahāya mānusakaṃ samussayaṃ,",
  "vv35:12.4": "Tidasagatā idha māvasāmi ṭhānan”ti.",
  "vv35:13.1": "Pesavatīvimānaṃ sattamaṃ."
}