{
  "vv71:0.1": "Vimānavatthu",
  "vv71:0.2": "Purisavimāna",
  "vv71:0.3": "Pāyāsivagga",
  "vv71:0.4": "7. Yavapālakavimānavatthu",
  "vv71:1.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
  "vv71:1.2": "…pe…",
  "vv71:1.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv71:2.1": "So devaputto attamano,",
  "vv71:2.2": "…pe…",
  "vv71:2.3": "yassa kammassidaṃ phalaṃ.",
  "vv71:3.1": "“Ahaṃ manussesu manussabhūto,",
  "vv71:3.2": "Ahosiṃ yavapālako;",
  "vv71:3.3": "Addasaṃ virajaṃ bhikkhuṃ,",
  "vv71:3.4": "Vippasannamanāvilaṃ.",
  "vv71:4.1": "Tassa adāsahaṃ bhāgaṃ,",
  "vv71:4.2": "pasanno sehi pāṇibhi;",
  "vv71:4.3": "Kummāsapiṇḍaṃ datvāna,",
  "vv71:4.4": "modāmi nandane vane.",
  "vv71:5.1": "Tena metādiso vaṇṇo,",
  "vv71:5.2": "…pe…",
  "vv71:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv71:6.1": "Yavapālakavimānaṃ sattamaṃ."
}