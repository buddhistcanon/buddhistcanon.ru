{
  "thig5.9:0.1": "Therīgāthā",
  "thig5.9:0.2": "Pañcakanipāta",
  "thig5.9:0.3": "9. Bhaddākuṇḍalakesātherīgāthā",
  "thig5.9:1.1": "“Lūnakesī paṅkadharī,",
  "thig5.9:1.2": "ekasāṭī pure cariṃ;",
  "thig5.9:1.3": "Avajje vajjamatinī,",
  "thig5.9:1.4": "vajje cāvajjadassinī.",
  "thig5.9:2.1": "Divāvihārā nikkhamma,",
  "thig5.9:2.2": "gijjhakūṭamhi pabbate;",
  "thig5.9:2.3": "Addasaṃ virajaṃ buddhaṃ,",
  "thig5.9:2.4": "bhikkhusaṃghapurakkhataṃ.",
  "thig5.9:3.1": "Nihacca jāṇuṃ vanditvā,",
  "thig5.9:3.2": "sammukhā añjaliṃ akaṃ;",
  "thig5.9:3.3": "‘Ehi bhadde’ti maṃ avaca,",
  "thig5.9:3.4": "sā me āsūpasampadā.",
  "thig5.9:4.1": "Ciṇṇā aṅgā ca magadhā,",
  "thig5.9:4.2": "vajjī kāsī ca kosalā;",
  "thig5.9:4.3": "Anaṇā paṇṇāsa vassāni,",
  "thig5.9:4.4": "raṭṭhapiṇḍaṃ abhuñjahaṃ.",
  "thig5.9:5.1": "Puññaṃ vata pasavi bahuṃ,",
  "thig5.9:5.2": "Sappañño vatāyaṃ upāsako;",
  "thig5.9:5.3": "Yo bhaddāya cīvaraṃ adāsi,",
  "thig5.9:5.4": "Vippamuttāya sabbaganthehī”ti.",
  "thig5.9:6.1": "…",
  "thig5.9:6.2": "Bhaddā kuṇḍalakesā therī …."
}