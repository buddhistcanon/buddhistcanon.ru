{
  "thig1.13:0.1": "Therīgāthā",
  "thig1.13:0.2": "Ekakanipāta",
  "thig1.13:0.3": "13. Visākhātherīgāthā",
  "thig1.13:1.1": "“Karotha buddhasāsanaṃ,",
  "thig1.13:1.2": "yaṃ katvā nānutappati;",
  "thig1.13:1.3": "Khippaṃ pādāni dhovitvā,",
  "thig1.13:1.4": "ekamante nisīdathā”ti.",
  "thig1.13:2.1": "…",
  "thig1.13:2.2": "Visākhā therī …."
}