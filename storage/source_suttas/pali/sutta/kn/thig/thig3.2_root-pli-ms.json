{
  "thig3.2:0.1": "Therīgāthā",
  "thig3.2:0.2": "Tikanipāta",
  "thig3.2:0.3": "2. Uttamātherīgāthā",
  "thig3.2:1.1": "“Catukkhattuṃ pañcakkhattuṃ,",
  "thig3.2:1.2": "vihārā upanikkhamiṃ;",
  "thig3.2:1.3": "Aladdhā cetaso santiṃ,",
  "thig3.2:1.4": "citte avasavattinī.",
  "thig3.2:2.1": "Sā bhikkhuniṃ upagacchiṃ,",
  "thig3.2:2.2": "yā me saddhāyikā ahu;",
  "thig3.2:2.3": "Sā me dhammamadesesi,",
  "thig3.2:2.4": "khandhāyatanadhātuyo.",
  "thig3.2:3.1": "Tassā dhammaṃ suṇitvāna,",
  "thig3.2:3.2": "Yathā maṃ anusāsi sā;",
  "thig3.2:3.3": "Sattāhaṃ ekapallaṅkena,",
  "thig3.2:3.4": "Nisīdiṃ pītisukhasamappitā;",
  "thig3.2:3.5": "Aṭṭhamiyā pāde pasāresiṃ,",
  "thig3.2:3.6": "Tamokhandhaṃ padāliyā”ti.",
  "thig3.2:4.1": "…",
  "thig3.2:4.2": "Uttamā therī …."
}