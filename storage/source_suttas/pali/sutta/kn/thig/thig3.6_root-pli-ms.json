{
  "thig3.6:0.1": "Therīgāthā",
  "thig3.6:0.2": "Tikanipāta",
  "thig3.6:0.3": "6. Sukkātherīgāthā",
  "thig3.6:1.1": "“Kiṃme katā rājagahe manussā,",
  "thig3.6:1.2": "Madhuṃ pītāva acchare;",
  "thig3.6:1.3": "Ye sukkaṃ na upāsanti,",
  "thig3.6:1.4": "Desentiṃ buddhasāsanaṃ.",
  "thig3.6:2.1": "Tañca appaṭivānīyaṃ,",
  "thig3.6:2.2": "asecanakamojavaṃ;",
  "thig3.6:2.3": "Pivanti maññe sappaññā,",
  "thig3.6:2.4": "valāhakamivaddhagū.",
  "thig3.6:3.1": "Sukkā sukkehi dhammehi,",
  "thig3.6:3.2": "vītarāgā samāhitā;",
  "thig3.6:3.3": "Dhāreti antimaṃ dehaṃ,",
  "thig3.6:3.4": "jetvā māraṃ savāhinin”ti.",
  "thig3.6:4.1": "…",
  "thig3.6:4.2": "Sukkā therī …."
}