{
  "thig7.2:0.1": "Therīgāthā",
  "thig7.2:0.2": "Sattakanipāta",
  "thig7.2:0.3": "2. Cālātherīgāthā",
  "thig7.2:1.1": "“Satiṃ upaṭṭhapetvāna,",
  "thig7.2:1.2": "bhikkhunī bhāvitindriyā;",
  "thig7.2:1.3": "Paṭivijjhi padaṃ santaṃ,",
  "thig7.2:1.4": "saṅkhārūpasamaṃ sukhaṃ”.",
  "thig7.2:2.1": "“Kaṃ nu uddissa muṇḍāsi,",
  "thig7.2:2.2": "samaṇī viya dissasi;",
  "thig7.2:2.3": "Na ca rocesi pāsaṇḍe,",
  "thig7.2:2.4": "kimidaṃ carasi momuhā”.",
  "thig7.2:3.1": "“Ito bahiddhā pāsaṇḍā,",
  "thig7.2:3.2": "diṭṭhiyo upanissitā;",
  "thig7.2:3.3": "Na te dhammaṃ vijānanti,",
  "thig7.2:3.4": "na te dhammassa kovidā.",
  "thig7.2:4.1": "Atthi sakyakule jāto,",
  "thig7.2:4.2": "buddho appaṭipuggalo;",
  "thig7.2:4.3": "So me dhammamadesesi,",
  "thig7.2:4.4": "diṭṭhīnaṃ samatikkamaṃ.",
  "thig7.2:5.1": "Dukkhaṃ dukkhasamuppādaṃ,",
  "thig7.2:5.2": "Dukkhassa ca atikkamaṃ;",
  "thig7.2:5.3": "Ariyaṃ caṭṭhaṅgikaṃ maggaṃ,",
  "thig7.2:5.4": "Dukkhūpasamagāminaṃ.",
  "thig7.2:6.1": "Tassāhaṃ vacanaṃ sutvā,",
  "thig7.2:6.2": "vihariṃ sāsane ratā;",
  "thig7.2:6.3": "Tisso vijjā anuppattā,",
  "thig7.2:6.4": "kataṃ buddhassa sāsanaṃ.",
  "thig7.2:7.1": "Sabbattha vihatā nandī,",
  "thig7.2:7.2": "tamokhandho padālito;",
  "thig7.2:7.3": "Evaṃ jānāhi pāpima,",
  "thig7.2:7.4": "nihato tvamasi antaka”.",
  "thig7.2:8.1": "…",
  "thig7.2:8.2": "Cālā therī …."
}