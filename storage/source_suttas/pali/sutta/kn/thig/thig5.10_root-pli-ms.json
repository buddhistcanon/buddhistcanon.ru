{
  "thig5.10:0.1": "Therīgāthā",
  "thig5.10:0.2": "Pañcakanipāta",
  "thig5.10:0.3": "10. Paṭācārātherīgāthā",
  "thig5.10:1.1": "“Naṅgalehi kasaṃ khettaṃ,",
  "thig5.10:1.2": "bījāni pavapaṃ chamā;",
  "thig5.10:1.3": "Puttadārāni posentā,",
  "thig5.10:1.4": "dhanaṃ vindanti māṇavā.",
  "thig5.10:2.1": "Kimahaṃ sīlasampannā,",
  "thig5.10:2.2": "satthusāsanakārikā;",
  "thig5.10:2.3": "Nibbānaṃ nādhigacchāmi,",
  "thig5.10:2.4": "akusītā anuddhatā.",
  "thig5.10:3.1": "Pāde pakkhālayitvāna,",
  "thig5.10:3.2": "udakesu karomahaṃ;",
  "thig5.10:3.3": "Pādodakañca disvāna,",
  "thig5.10:3.4": "thalato ninnamāgataṃ.",
  "thig5.10:4.1": "Tato cittaṃ samādhesiṃ,",
  "thig5.10:4.2": "assaṃ bhadraṃvajāniyaṃ;",
  "thig5.10:4.3": "Tato dīpaṃ gahetvāna,",
  "thig5.10:4.4": "vihāraṃ pāvisiṃ ahaṃ;",
  "thig5.10:4.5": "Seyyaṃ olokayitvāna,",
  "thig5.10:4.6": "mañcakamhi upāvisiṃ.",
  "thig5.10:5.1": "Tato sūciṃ gahetvāna,",
  "thig5.10:5.2": "vaṭṭiṃ okassayāmahaṃ;",
  "thig5.10:5.3": "Padīpasseva nibbānaṃ,",
  "thig5.10:5.4": "vimokkho ahu cetaso”ti.",
  "thig5.10:6.1": "…",
  "thig5.10:6.2": "Paṭācārā therī …."
}