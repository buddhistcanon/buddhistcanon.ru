{
  "thig6.8:0.1": "Therīgāthā",
  "thig6.8:0.2": "Chakkanipāta",
  "thig6.8:0.3": "8. Vijayātherīgāthā",
  "thig6.8:1.1": "“Catukkhattuṃ pañcakkhattuṃ,",
  "thig6.8:1.2": "vihārā upanikkhamiṃ;",
  "thig6.8:1.3": "Aladdhā cetaso santiṃ,",
  "thig6.8:1.4": "citte avasavattinī.",
  "thig6.8:2.1": "Bhikkhuniṃ upasaṅkamma,",
  "thig6.8:2.2": "sakkaccaṃ paripucchahaṃ;",
  "thig6.8:2.3": "Sā me dhammamadesesi,",
  "thig6.8:2.4": "dhātuāyatanāni ca.",
  "thig6.8:3.1": "Cattāri ariyasaccāni,",
  "thig6.8:3.2": "indriyāni balāni ca;",
  "thig6.8:3.3": "Bojjhaṅgaṭṭhaṅgikaṃ maggaṃ,",
  "thig6.8:3.4": "uttamatthassa pattiyā.",
  "thig6.8:4.1": "Tassāhaṃ vacanaṃ sutvā,",
  "thig6.8:4.2": "karontī anusāsaniṃ;",
  "thig6.8:4.3": "Rattiyā purime yāme,",
  "thig6.8:4.4": "pubbajātimanussariṃ.",
  "thig6.8:5.1": "Rattiyā majjhime yāme,",
  "thig6.8:5.2": "dibbacakkhuṃ visodhayiṃ;",
  "thig6.8:5.3": "Rattiyā pacchime yāme,",
  "thig6.8:5.4": "tamokhandhaṃ padālayiṃ.",
  "thig6.8:6.1": "Pītisukhena ca kāyaṃ,",
  "thig6.8:6.2": "Pharitvā vihariṃ tadā;",
  "thig6.8:6.3": "Sattamiyā pāde pasāresiṃ,",
  "thig6.8:6.4": "Tamokhandhaṃ padāliyā”ti.",
  "thig6.8:7.1": "…",
  "thig6.8:7.2": "Vijayā therī ….",
  "thig6.8:8.1": "Chakkanipāto niṭṭhito."
}