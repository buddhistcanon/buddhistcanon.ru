{
  "thig5.4:0.1": "Therīgāthā",
  "thig5.4:0.2": "Pañcakanipāta",
  "thig5.4:0.3": "4. Sundarīnandātherīgāthā",
  "thig5.4:1.1": "“Āturaṃ asuciṃ pūtiṃ,",
  "thig5.4:1.2": "passa nande samussayaṃ;",
  "thig5.4:1.3": "Asubhāya cittaṃ bhāvehi,",
  "thig5.4:1.4": "ekaggaṃ susamāhitaṃ.",
  "thig5.4:2.1": "Yathā idaṃ tathā etaṃ,",
  "thig5.4:2.2": "yathā etaṃ tathā idaṃ;",
  "thig5.4:2.3": "Duggandhaṃ pūtikaṃ vāti,",
  "thig5.4:2.4": "bālānaṃ abhinanditaṃ.",
  "thig5.4:3.1": "Evametaṃ avekkhantī,",
  "thig5.4:3.2": "rattindivamatanditā;",
  "thig5.4:3.3": "Tato sakāya paññāya,",
  "thig5.4:3.4": "abhinibbijjha dakkhisaṃ”.",
  "thig5.4:4.1": "“Tassā me appamattāya,",
  "thig5.4:4.2": "vicinantiyā yoniso;",
  "thig5.4:4.3": "Yathābhūtaṃ ayaṃ kāyo,",
  "thig5.4:4.4": "diṭṭho santarabāhiro.",
  "thig5.4:5.1": "Atha nibbindahaṃ kāye,",
  "thig5.4:5.2": "ajjhattañca virajjahaṃ;",
  "thig5.4:5.3": "Appamattā visaṃyuttā,",
  "thig5.4:5.4": "upasantāmhi nibbutā”ti.",
  "thig5.4:6.1": "…",
  "thig5.4:6.2": "Sundarīnandā therī …."
}