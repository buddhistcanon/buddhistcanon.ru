{
  "thig3.7:0.1": "Therīgāthā",
  "thig3.7:0.2": "Tikanipāta",
  "thig3.7:0.3": "7. Selātherīgāthā",
  "thig3.7:1.1": "“Natthi nissaraṇaṃ loke,",
  "thig3.7:1.2": "kiṃ vivekena kāhasi;",
  "thig3.7:1.3": "Bhuñjāhi kāmaratiyo,",
  "thig3.7:1.4": "māhu pacchānutāpinī”.",
  "thig3.7:2.1": "“Sattisūlūpamā kāmā,",
  "thig3.7:2.2": "khandhāsaṃ adhikuṭṭanā;",
  "thig3.7:2.3": "Yaṃ tvaṃ ‘kāmaratiṃ’ brūsi,",
  "thig3.7:2.4": "‘aratī’ dāni sā mama.",
  "thig3.7:3.1": "Sabbattha vihatā nandī,",
  "thig3.7:3.2": "Tamokhandho padālito;",
  "thig3.7:3.3": "Evaṃ jānāhi pāpima,",
  "thig3.7:3.4": "Nihato tvamasi antakā”ti.",
  "thig3.7:4.1": "…",
  "thig3.7:4.2": "Selā therī …."
}