{
  "thig1.7:0.1": "Therīgāthā",
  "thig1.7:0.2": "Ekakanipāta",
  "thig1.7:0.3": "7. Vīrātherīgāthā",
  "thig1.7:1.1": "“Vīrā vīrehi dhammehi,",
  "thig1.7:1.2": "bhikkhunī bhāvitindriyā;",
  "thig1.7:1.3": "Dhāreti antimaṃ dehaṃ,",
  "thig1.7:1.4": "jetvā māraṃ savāhinin”ti.",
  "thig1.7:2.1": "…",
  "thig1.7:2.2": "Vīrā therī …."
}