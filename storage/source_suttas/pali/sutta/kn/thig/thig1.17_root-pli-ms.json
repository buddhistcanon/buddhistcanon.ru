{
  "thig1.17:0.1": "Therīgāthā",
  "thig1.17:0.2": "Ekakanipāta",
  "thig1.17:0.3": "17. Dhammātherīgāthā",
  "thig1.17:1.1": "“Piṇḍapātaṃ caritvāna,",
  "thig1.17:1.2": "daṇḍamolubbha dubbalā;",
  "thig1.17:1.3": "Vedhamānehi gattehi,",
  "thig1.17:1.4": "tattheva nipatiṃ chamā;",
  "thig1.17:1.5": "Disvā ādīnavaṃ kāye,",
  "thig1.17:1.6": "atha cittaṃ vimucci me”ti.",
  "thig1.17:2.1": "…",
  "thig1.17:2.2": "Dhammā therī …."
}