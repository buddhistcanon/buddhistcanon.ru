{
  "thig1.10:0.1": "Therīgāthā",
  "thig1.10:0.2": "Ekakanipāta",
  "thig1.10:0.3": "10. Upasamātherīgāthā",
  "thig1.10:1.1": "“Upasame tare oghaṃ,",
  "thig1.10:1.2": "maccudheyyaṃ suduttaraṃ;",
  "thig1.10:1.3": "Dhārehi antimaṃ dehaṃ,",
  "thig1.10:1.4": "jetvā māraṃ savāhinin”ti.",
  "thig1.10:2.1": "…",
  "thig1.10:2.2": "Upasamā therī …."
}