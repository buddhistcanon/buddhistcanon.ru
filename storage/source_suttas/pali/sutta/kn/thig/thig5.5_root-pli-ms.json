{
  "thig5.5:0.1": "Therīgāthā",
  "thig5.5:0.2": "Pañcakanipāta",
  "thig5.5:0.3": "5. Nanduttarātherīgāthā",
  "thig5.5:1.1": "“Aggiṃ candañca sūriyañca,",
  "thig5.5:1.2": "devatā ca namassihaṃ;",
  "thig5.5:1.3": "Nadītitthāni gantvāna,",
  "thig5.5:1.4": "udakaṃ oruhāmihaṃ.",
  "thig5.5:2.1": "Bahūvatasamādānā,",
  "thig5.5:2.2": "aḍḍhaṃ sīsassa olikhiṃ;",
  "thig5.5:2.3": "Chamāya seyyaṃ kappemi,",
  "thig5.5:2.4": "rattiṃ bhattaṃ na bhuñjahaṃ.",
  "thig5.5:3.1": "Vibhūsāmaṇḍanaratā,",
  "thig5.5:3.2": "nhāpanucchādanehi ca;",
  "thig5.5:3.3": "Upakāsiṃ imaṃ kāyaṃ,",
  "thig5.5:3.4": "kāmarāgena aṭṭitā.",
  "thig5.5:4.1": "Tato saddhaṃ labhitvāna,",
  "thig5.5:4.2": "pabbajiṃ anagāriyaṃ;",
  "thig5.5:4.3": "Disvā kāyaṃ yathābhūtaṃ,",
  "thig5.5:4.4": "kāmarāgo samūhato.",
  "thig5.5:5.1": "Sabbe bhavā samucchinnā,",
  "thig5.5:5.2": "icchā ca patthanāpi ca;",
  "thig5.5:5.3": "Sabbayogavisaṃyuttā,",
  "thig5.5:5.4": "santiṃ pāpuṇi cetaso”ti.",
  "thig5.5:6.1": "…",
  "thig5.5:6.2": "Nanduttarā therī …."
}