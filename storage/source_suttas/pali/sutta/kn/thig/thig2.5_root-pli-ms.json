{
  "thig2.5:0.1": "Therīgāthā",
  "thig2.5:0.2": "Dukanipāta",
  "thig2.5:0.3": "5. Cittātherīgāthā",
  "thig2.5:1.1": "“Kiñcāpi khomhi kisikā,",
  "thig2.5:1.2": "gilānā bāḷhadubbalā;",
  "thig2.5:1.3": "Daṇḍamolubbha gacchāmi,",
  "thig2.5:1.4": "pabbataṃ abhirūhiya.",
  "thig2.5:2.1": "Saṅghāṭiṃ nikkhipitvāna,",
  "thig2.5:2.2": "pattakañca nikujjiya;",
  "thig2.5:2.3": "Sele khambhesimattānaṃ,",
  "thig2.5:2.4": "tamokhandhaṃ padāliyā”ti.",
  "thig2.5:3.1": "…",
  "thig2.5:3.2": "Cittā therī …."
}