{
  "thig7.3:0.1": "Therīgāthā",
  "thig7.3:0.2": "Sattakanipāta",
  "thig7.3:0.3": "3. Upacālātherīgāthā",
  "thig7.3:1.1": "“Satimatī cakkhumatī,",
  "thig7.3:1.2": "bhikkhunī bhāvitindriyā;",
  "thig7.3:1.3": "Paṭivijjhi padaṃ santaṃ,",
  "thig7.3:1.4": "akāpurisasevitaṃ”.",
  "thig7.3:2.1": "“Kiṃ nu jātiṃ na rocesi,",
  "thig7.3:2.2": "jāto kāmāni bhuñjati;",
  "thig7.3:2.3": "Bhuñjāhi kāmaratiyo,",
  "thig7.3:2.4": "māhu pacchānutāpinī”.",
  "thig7.3:3.1": "“Jātassa maraṇaṃ hoti,",
  "thig7.3:3.2": "hatthapādāna chedanaṃ;",
  "thig7.3:3.3": "Vadhabandhapariklesaṃ,",
  "thig7.3:3.4": "jāto dukkhaṃ nigacchati.",
  "thig7.3:4.1": "Atthi sakyakule jāto,",
  "thig7.3:4.2": "sambuddho aparājito;",
  "thig7.3:4.3": "So me dhammamadesesi,",
  "thig7.3:4.4": "jātiyā samatikkamaṃ.",
  "thig7.3:5.1": "Dukkhaṃ dukkhasamuppādaṃ,",
  "thig7.3:5.2": "dukkhassa ca atikkamaṃ;",
  "thig7.3:5.3": "Ariyaṃ caṭṭhaṅgikaṃ maggaṃ,",
  "thig7.3:5.4": "dukkhūpasamagāminaṃ.",
  "thig7.3:6.1": "Tassāhaṃ vacanaṃ sutvā,",
  "thig7.3:6.2": "vihariṃ sāsane ratā;",
  "thig7.3:6.3": "Tisso vijjā anuppattā,",
  "thig7.3:6.4": "kataṃ buddhassa sāsanaṃ.",
  "thig7.3:7.1": "Sabbattha vihatā nandī,",
  "thig7.3:7.2": "tamokhandho padālito;",
  "thig7.3:7.3": "Evaṃ jānāhi pāpima,",
  "thig7.3:7.4": "nihato tvamasi antaka”.",
  "thig7.3:8.1": "…",
  "thig7.3:8.2": "Upacālā therī ….",
  "thig7.3:9.1": "Sattakanipāto niṭṭhito."
}