{
  "thig2.3:0.1": "Therīgāthā",
  "thig2.3:0.2": "Dukanipāta",
  "thig2.3:0.3": "3. Sumaṅgalamātātherīgāthā",
  "thig2.3:1.1": "“Sumuttikā sumuttikā,",
  "thig2.3:1.2": "Sādhumuttikāmhi musalassa;",
  "thig2.3:1.3": "Ahiriko me chattakaṃ vāpi,",
  "thig2.3:1.4": "Ukkhalikā me deḍḍubhaṃ vāti.",
  "thig2.3:2.1": "Rāgañca ahaṃ dosañca,",
  "thig2.3:2.2": "Cicciṭi cicciṭīti vihanāmi;",
  "thig2.3:2.3": "Sā rukkhamūlamupagamma,",
  "thig2.3:2.4": "Aho sukhanti sukhato jhāyāmī”ti.",
  "thig2.3:3.1": "…",
  "thig2.3:3.2": "Sumaṅgalamātā therī."
}