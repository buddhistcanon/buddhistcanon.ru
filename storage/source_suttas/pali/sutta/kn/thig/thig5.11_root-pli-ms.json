{
  "thig5.11:0.1": "Therīgāthā",
  "thig5.11:0.2": "Pañcakanipāta",
  "thig5.11:0.3": "11. Tiṃsamattātherīgāthā",
  "thig5.11:1.1": "“Musalāni gahetvāna,",
  "thig5.11:1.2": "dhaññaṃ koṭṭenti māṇavā;",
  "thig5.11:1.3": "Puttadārāni posentā,",
  "thig5.11:1.4": "dhanaṃ vindanti māṇavā.",
  "thig5.11:2.1": "Karotha buddhasāsanaṃ,",
  "thig5.11:2.2": "yaṃ katvā nānutappati;",
  "thig5.11:2.3": "Khippaṃ pādāni dhovitvā,",
  "thig5.11:2.4": "ekamante nisīdatha;",
  "thig5.11:2.5": "Cetosamathamanuyuttā,",
  "thig5.11:2.6": "karotha buddhasāsanaṃ”.",
  "thig5.11:3.1": "Tassā tā vacanaṃ sutvā,",
  "thig5.11:3.2": "paṭācārāya sāsanaṃ;",
  "thig5.11:3.3": "Pāde pakkhālayitvāna,",
  "thig5.11:3.4": "ekamantaṃ upāvisuṃ;",
  "thig5.11:3.5": "Cetosamathamanuyuttā,",
  "thig5.11:3.6": "akaṃsu buddhasāsanaṃ.",
  "thig5.11:4.1": "Rattiyā purime yāme,",
  "thig5.11:4.2": "pubbajātimanussaruṃ;",
  "thig5.11:4.3": "Rattiyā majjhime yāme,",
  "thig5.11:4.4": "dibbacakkhuṃ visodhayuṃ;",
  "thig5.11:4.5": "Rattiyā pacchime yāme,",
  "thig5.11:4.6": "tamokhandhaṃ padālayuṃ.",
  "thig5.11:5.1": "Uṭṭhāya pāde vandiṃsu,",
  "thig5.11:5.2": "“katā te anusāsanī;",
  "thig5.11:5.3": "Indaṃva devā tidasā,",
  "thig5.11:5.4": "saṅgāme aparājitaṃ;",
  "thig5.11:5.5": "Purakkhatvā vihassāma,",
  "thig5.11:5.6": "tevijjāmha anāsavā”ti.",
  "thig5.11:6.1": "Itthaṃ sudaṃ tiṃsamattā therī bhikkhuniyo paṭācārāya santike aññaṃ byākariṃsūti."
}