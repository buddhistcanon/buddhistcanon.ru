{
  "thig1.18:0.1": "Therīgāthā",
  "thig1.18:0.2": "Ekakanipāta",
  "thig1.18:0.3": "18. Saṃghātherīgāthā",
  "thig1.18:1.1": "“Hitvā ghare pabbajitvā,",
  "thig1.18:1.2": "hitvā puttaṃ pasuṃ piyaṃ;",
  "thig1.18:1.3": "Hitvā rāgañca dosañca,",
  "thig1.18:1.4": "avijjañca virājiya;",
  "thig1.18:1.5": "Samūlaṃ taṇhamabbuyha,",
  "thig1.18:1.6": "upasantāmhi nibbutā”ti.",
  "thig1.18:2.1": "…",
  "thig1.18:2.2": "Saṃghā therī ….",
  "thig1.18:3.1": "Ekakanipāto niṭṭhito."
}