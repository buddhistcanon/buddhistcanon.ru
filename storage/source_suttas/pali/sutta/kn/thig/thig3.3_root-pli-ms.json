{
  "thig3.3:0.1": "Therīgāthā",
  "thig3.3:0.2": "Tikanipāta",
  "thig3.3:0.3": "3. Aparāuttamātherīgāthā",
  "thig3.3:1.1": "“Ye ime satta bojjhaṅgā,",
  "thig3.3:1.2": "maggā nibbānapattiyā;",
  "thig3.3:1.3": "Bhāvitā te mayā sabbe,",
  "thig3.3:1.4": "yathā buddhena desitā.",
  "thig3.3:2.1": "Suññatassānimittassa,",
  "thig3.3:2.2": "lābhinīhaṃ yadicchakaṃ;",
  "thig3.3:2.3": "Orasā dhītā buddhassa,",
  "thig3.3:2.4": "nibbānābhiratā sadā.",
  "thig3.3:3.1": "Sabbe kāmā samucchinnā,",
  "thig3.3:3.2": "ye dibbā ye ca mānusā;",
  "thig3.3:3.3": "Vikkhīṇo jātisaṃsāro,",
  "thig3.3:3.4": "natthi dāni punabbhavo”ti.",
  "thig3.3:4.1": "…",
  "thig3.3:4.2": "Aparā uttamā therī …."
}