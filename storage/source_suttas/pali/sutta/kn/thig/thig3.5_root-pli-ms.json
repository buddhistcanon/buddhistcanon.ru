{
  "thig3.5:0.1": "Therīgāthā",
  "thig3.5:0.2": "Tikanipāta",
  "thig3.5:0.3": "5. Ubbiritherīgāthā",
  "thig3.5:1.1": "“Amma jīvāti vanamhi kandasi,",
  "thig3.5:1.2": "Attānaṃ adhigaccha ubbiri;",
  "thig3.5:1.3": "Cullāsītisahassāni,",
  "thig3.5:1.4": "Sabbā jīvasanāmikā;",
  "thig3.5:1.5": "Etamhāḷāhane daḍḍhā,",
  "thig3.5:1.6": "Tāsaṃ kamanusocasi”.",
  "thig3.5:2.1": "“Abbahī vata me sallaṃ,",
  "thig3.5:2.2": "duddasaṃ hadayassitaṃ;",
  "thig3.5:2.3": "Yaṃ me sokaparetāya,",
  "thig3.5:2.4": "dhītusokaṃ byapānudi.",
  "thig3.5:3.1": "Sājja abbūḷhasallāhaṃ,",
  "thig3.5:3.2": "Nicchātā parinibbutā;",
  "thig3.5:3.3": "Buddhaṃ dhammañca saṃghañca,",
  "thig3.5:3.4": "Upemi saraṇaṃ munin”ti.",
  "thig3.5:4.1": "…",
  "thig3.5:4.2": "Ubbirī therī …."
}