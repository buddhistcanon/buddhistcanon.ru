{
  "thig5.6:0.1": "Therīgāthā",
  "thig5.6:0.2": "Pañcakanipāta",
  "thig5.6:0.3": "6 Mittākāḷītherīgāthā",
  "thig5.6:1.1": "“Saddhāya pabbajitvāna,",
  "thig5.6:1.2": "agārasmānagāriyaṃ;",
  "thig5.6:1.3": "Vicariṃhaṃ tena tena,",
  "thig5.6:1.4": "lābhasakkāraussukā.",
  "thig5.6:2.1": "Riñcitvā paramaṃ atthaṃ,",
  "thig5.6:2.2": "hīnamatthaṃ asevihaṃ;",
  "thig5.6:2.3": "Kilesānaṃ vasaṃ gantvā,",
  "thig5.6:2.4": "sāmaññatthaṃ na bujjhihaṃ.",
  "thig5.6:3.1": "Tassā me ahu saṃvego,",
  "thig5.6:3.2": "nisinnāya vihārake;",
  "thig5.6:3.3": "Ummaggapaṭipannāmhi,",
  "thig5.6:3.4": "taṇhāya vasamāgatā.",
  "thig5.6:4.1": "Appakaṃ jīvitaṃ mayhaṃ,",
  "thig5.6:4.2": "jarā byādhi ca maddati;",
  "thig5.6:4.3": "Purāyaṃ bhijjati kāyo,",
  "thig5.6:4.4": "na me kālo pamajjituṃ.",
  "thig5.6:5.1": "Yathābhūtamavekkhantī,",
  "thig5.6:5.2": "khandhānaṃ udayabbayaṃ;",
  "thig5.6:5.3": "Vimuttacittā uṭṭhāsiṃ,",
  "thig5.6:5.4": "kataṃ buddhassa sāsanan”ti.",
  "thig5.6:6.1": "…",
  "thig5.6:6.2": "Mittā kāḷī therī …."
}