{
  "thig1.15:0.1": "Therīgāthā",
  "thig1.15:0.2": "Ekakanipāta",
  "thig1.15:0.3": "15. Uttarātherīgāthā",
  "thig1.15:1.1": "“Kāyena saṃvutā āsiṃ,",
  "thig1.15:1.2": "vācāya uda cetasā;",
  "thig1.15:1.3": "Samūlaṃ taṇhamabbuyha,",
  "thig1.15:1.4": "sītibhūtāmhi nibbutā”ti.",
  "thig1.15:2.1": "…",
  "thig1.15:2.2": "Uttarā therī …."
}