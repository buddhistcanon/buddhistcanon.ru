{
  "snp2.8:0.1": "Sutta Nipāta 2.8",
  "snp2.8:0.2": "Dhamma (nāvā) sutta",
  "snp2.8:1.1": "Yasmā hi dhammaṃ puriso vijaññā,",
  "snp2.8:1.2": "Indaṃva naṃ devatā pūjayeyya;",
  "snp2.8:1.3": "So pūjito tasmi pasannacitto,",
  "snp2.8:1.4": "Bahussuto pātukaroti dhammaṃ.",
  "snp2.8:2.1": "Tadaṭṭhikatvāna nisamma dhīro,",
  "snp2.8:2.2": "Dhammānudhammaṃ paṭipajjamāno;",
  "snp2.8:2.3": "Viññū vibhāvī nipuṇo ca hoti,",
  "snp2.8:2.4": "Yo tādisaṃ bhajati appamatto.",
  "snp2.8:3.1": "Khuddañca bālaṃ upasevamāno,",
  "snp2.8:3.2": "Anāgatatthañca usūyakañca;",
  "snp2.8:3.3": "Idheva dhammaṃ avibhāvayitvā,",
  "snp2.8:3.4": "Avitiṇṇakaṅkho maraṇaṃ upeti.",
  "snp2.8:4.1": "Yathā naro āpagamotaritvā,",
  "snp2.8:4.2": "Mahodakaṃ salilaṃ sīghasotaṃ;",
  "snp2.8:4.3": "So vuyhamāno anusotagāmī,",
  "snp2.8:4.4": "Kiṃ so pare sakkhati tārayetuṃ.",
  "snp2.8:5.1": "Tatheva dhammaṃ avibhāvayitvā,",
  "snp2.8:5.2": "Bahussutānaṃ anisāmayatthaṃ;",
  "snp2.8:5.3": "Sayaṃ ajānaṃ avitiṇṇakaṅkho,",
  "snp2.8:5.4": "Kiṃ so pare sakkhati nijjhapetuṃ.",
  "snp2.8:6.1": "Yathāpi nāvaṃ daḷhamāruhitvā,",
  "snp2.8:6.2": "Phiyena rittena samaṅgibhūto;",
  "snp2.8:6.3": "So tāraye tattha bahūpi aññe,",
  "snp2.8:6.4": "Tatrūpayaññū kusalo mutīmā.",
  "snp2.8:7.1": "Evampi yo vedagu bhāvitatto,",
  "snp2.8:7.2": "Bahussuto hoti avedhadhammo;",
  "snp2.8:7.3": "So kho pare nijjhapaye pajānaṃ,",
  "snp2.8:7.4": "Sotāvadhānūpanisūpapanne.",
  "snp2.8:8.1": "Tasmā have sappurisaṃ bhajetha,",
  "snp2.8:8.2": "Medhāvinañceva bahussutañca;",
  "snp2.8:8.3": "Aññāya atthaṃ paṭipajjamāno,",
  "snp2.8:8.4": "Viññātadhammo sa sukhaṃ labhethāti.",
  "snp2.8:8.5": "Nāvāsuttaṃ aṭṭhamaṃ."
}