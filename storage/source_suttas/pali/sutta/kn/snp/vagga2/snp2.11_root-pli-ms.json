{
  "snp2.11:0.1": "Sutta Nipāta 2.11",
  "snp2.11:0.2": "Rāhulasutta",
  "snp2.11:1.1": "“Kacci abhiṇhasaṃvāsā,",
  "snp2.11:1.2": "nāvajānāsi paṇḍitaṃ;",
  "snp2.11:1.3": "Ukkādhāro manussānaṃ,",
  "snp2.11:1.4": "kacci apacito tayā”.",
  "snp2.11:2.1": "“Nāhaṃ abhiṇhasaṃvāsā,",
  "snp2.11:2.2": "avajānāmi paṇḍitaṃ;",
  "snp2.11:2.3": "Ukkādhāro manussānaṃ,",
  "snp2.11:2.4": "niccaṃ apacito mayā”.",
  "snp2.11:3.1": "“Pañca kāmaguṇe hitvā,",
  "snp2.11:3.2": "piyarūpe manorame;",
  "snp2.11:3.3": "Saddhāya gharā nikkhamma,",
  "snp2.11:3.4": "dukkhassantakaro bhava.",
  "snp2.11:4.1": "Mitte bhajassu kalyāṇe,",
  "snp2.11:4.2": "pantañca sayanāsanaṃ;",
  "snp2.11:4.3": "Vivittaṃ appanigghosaṃ,",
  "snp2.11:4.4": "mattaññū hohi bhojane.",
  "snp2.11:5.1": "Cīvare piṇḍapāte ca,",
  "snp2.11:5.2": "paccaye sayanāsane;",
  "snp2.11:5.3": "Etesu taṇhaṃ mākāsi,",
  "snp2.11:5.4": "mā lokaṃ punarāgami.",
  "snp2.11:6.1": "Saṃvuto pātimokkhasmiṃ,",
  "snp2.11:6.2": "indriyesu ca pañcasu;",
  "snp2.11:6.3": "Sati kāyagatā tyatthu,",
  "snp2.11:6.4": "nibbidābahulo bhava.",
  "snp2.11:7.1": "Nimittaṃ parivajjehi,",
  "snp2.11:7.2": "subhaṃ rāgūpasañhitaṃ;",
  "snp2.11:7.3": "Asubhāya cittaṃ bhāvehi,",
  "snp2.11:7.4": "ekaggaṃ susamāhitaṃ.",
  "snp2.11:8.1": "Animittañca bhāvehi,",
  "snp2.11:8.2": "Mānānusayamujjaha;",
  "snp2.11:8.3": "Tato mānābhisamayā,",
  "snp2.11:8.4": "Upasanto carissatī”ti.",
  "snp2.11:9.1": "Itthaṃ sudaṃ bhagavā āyasmantaṃ rāhulaṃ imāhi gāthāhi abhiṇhaṃ ovadatīti.",
  "snp2.11:9.2": "Rāhulasuttaṃ ekādasamaṃ."
}