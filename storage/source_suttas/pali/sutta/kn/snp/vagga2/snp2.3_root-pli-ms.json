{
  "snp2.3:0.1": "Sutta Nipāta 2.3",
  "snp2.3:0.2": "Hirisutta",
  "snp2.3:1.1": "Hiriṃ tarantaṃ vijigucchamānaṃ,",
  "snp2.3:1.2": "Tavāhamasmi iti bhāsamānaṃ;",
  "snp2.3:1.3": "Sayhāni kammāni anādiyantaṃ,",
  "snp2.3:1.4": "Neso mamanti iti naṃ vijaññā.",
  "snp2.3:2.1": "Ananvayaṃ piyaṃ vācaṃ,",
  "snp2.3:2.2": "yo mittesu pakubbati;",
  "snp2.3:2.3": "Akarontaṃ bhāsamānaṃ,",
  "snp2.3:2.4": "parijānanti paṇḍitā.",
  "snp2.3:3.1": "Na so mitto yo sadā appamatto,",
  "snp2.3:3.2": "Bhedāsaṅkī randhamevānupassī;",
  "snp2.3:3.3": "Yasmiñca seti urasīva putto,",
  "snp2.3:3.4": "Sa ve mitto yo parehi abhejjo.",
  "snp2.3:4.1": "Pāmujjakaraṇaṃ ṭhānaṃ,",
  "snp2.3:4.2": "pasaṃsāvahanaṃ sukhaṃ;",
  "snp2.3:4.3": "Phalānisaṃso bhāveti,",
  "snp2.3:4.4": "vahanto porisaṃ dhuraṃ.",
  "snp2.3:5.1": "Pavivekarasaṃ pitvā,",
  "snp2.3:5.2": "Rasaṃ upasamassa ca;",
  "snp2.3:5.3": "Niddaro hoti nippāpo,",
  "snp2.3:5.4": "Dhammapītirasaṃ pivanti.",
  "snp2.3:5.5": "Hirisuttaṃ tatiyaṃ."
}