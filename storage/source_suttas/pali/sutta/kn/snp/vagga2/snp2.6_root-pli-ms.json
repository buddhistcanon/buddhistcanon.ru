{
  "snp2.6:0.1": "Sutta Nipāta 2.6",
  "snp2.6:0.2": "Kapilasutta (dhammacariyasutta)",
  "snp2.6:1.1": "Dhammacariyaṃ brahmacariyaṃ,",
  "snp2.6:1.2": "Etadāhu vasuttamaṃ;",
  "snp2.6:1.3": "Pabbajitopi ce hoti,",
  "snp2.6:1.4": "Agārā anagāriyaṃ.",
  "snp2.6:2.1": "So ce mukharajātiko,",
  "snp2.6:2.2": "vihesābhirato mago;",
  "snp2.6:2.3": "Jīvitaṃ tassa pāpiyo,",
  "snp2.6:2.4": "rajaṃ vaḍḍheti attano.",
  "snp2.6:3.1": "Kalahābhirato bhikkhu,",
  "snp2.6:3.2": "Mohadhammena āvuto;",
  "snp2.6:3.3": "Akkhātampi na jānāti,",
  "snp2.6:3.4": "Dhammaṃ buddhena desitaṃ.",
  "snp2.6:4.1": "Vihesaṃ bhāvitattānaṃ,",
  "snp2.6:4.2": "avijjāya purakkhato;",
  "snp2.6:4.3": "Saṅkilesaṃ na jānāti,",
  "snp2.6:4.4": "maggaṃ nirayagāminaṃ.",
  "snp2.6:5.1": "Vinipātaṃ samāpanno,",
  "snp2.6:5.2": "gabbhā gabbhaṃ tamā tamaṃ;",
  "snp2.6:5.3": "Sa ve tādisako bhikkhu,",
  "snp2.6:5.4": "pecca dukkhaṃ nigacchati.",
  "snp2.6:6.1": "Gūthakūpo yathā assa,",
  "snp2.6:6.2": "sampuṇṇo gaṇavassiko;",
  "snp2.6:6.3": "Yo ca evarūpo assa,",
  "snp2.6:6.4": "dubbisodho hi sāṅgaṇo.",
  "snp2.6:7.1": "Yaṃ evarūpaṃ jānātha,",
  "snp2.6:7.2": "bhikkhavo gehanissitaṃ;",
  "snp2.6:7.3": "Pāpicchaṃ pāpasaṅkappaṃ,",
  "snp2.6:7.4": "pāpaācāragocaraṃ.",
  "snp2.6:8.1": "Sabbe samaggā hutvāna,",
  "snp2.6:8.2": "abhinibbajjiyātha naṃ;",
  "snp2.6:8.3": "Kāraṇḍavaṃ niddhamatha,",
  "snp2.6:8.4": "kasambuṃ apakassatha.",
  "snp2.6:9.1": "Tato palāpe vāhetha,",
  "snp2.6:9.2": "Assamaṇe samaṇamānine;",
  "snp2.6:9.3": "Niddhamitvāna pāpicche,",
  "snp2.6:9.4": "Pāpaācāragocare.",
  "snp2.6:10.1": "Suddhā suddhehi saṃvāsaṃ,",
  "snp2.6:10.2": "Kappayavho patissatā;",
  "snp2.6:10.3": "Tato samaggā nipakā,",
  "snp2.6:10.4": "Dukkhassantaṃ karissathāti.",
  "snp2.6:10.5": "Dhammacariyasuttaṃ chaṭṭhaṃ."
}