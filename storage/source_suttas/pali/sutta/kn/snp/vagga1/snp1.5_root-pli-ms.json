{
  "snp1.5:0.1": "Sutta Nipāta 1.5",
  "snp1.5:0.2": "Cundasutta",
  "snp1.5:1.1": "“Pucchāmi muniṃ pahūtapaññaṃ,",
  "snp1.5:1.2": "Buddhaṃ dhammassāmiṃ vītataṇhaṃ;",
  "snp1.5:1.3": "Dvipaduttamaṃ sārathīnaṃ pavaraṃ,",
  "snp1.5:1.4": "Kati loke samaṇā tadiṅgha brūhi”.",
  "snp1.5:2.1": "“Caturo samaṇā na pañcamatthi,",
  "snp1.5:2.2": "Te te āvikaromi sakkhipuṭṭho;",
  "snp1.5:2.3": "Maggajino maggadesako ca,",
  "snp1.5:2.4": "Magge jīvati yo ca maggadūsī”.",
  "snp1.5:3.1": "“Kaṃ maggajinaṃ vadanti buddhā,",
  "snp1.5:3.2": "Maggakkhāyī kathaṃ atulyo hoti;",
  "snp1.5:3.3": "Magge jīvati me brūhi puṭṭho,",
  "snp1.5:3.4": "Atha me āvikarohi maggadūsiṃ”.",
  "snp1.5:4.1": "“Yo tiṇṇakathaṅkatho visallo,",
  "snp1.5:4.2": "Nibbānābhirato anānugiddho;",
  "snp1.5:4.3": "Lokassa sadevakassa netā,",
  "snp1.5:4.4": "Tādiṃ maggajinaṃ vadanti buddhā.",
  "snp1.5:5.1": "Paramaṃ paramanti yodha ñatvā,",
  "snp1.5:5.2": "Akkhāti vibhajate idheva dhammaṃ;",
  "snp1.5:5.3": "Taṃ kaṅkhachidaṃ muniṃ anejaṃ,",
  "snp1.5:5.4": "Dutiyaṃ bhikkhunamāhu maggadesiṃ.",
  "snp1.5:6.1": "Yo dhammapade sudesite,",
  "snp1.5:6.2": "Magge jīvati saññato satīmā;",
  "snp1.5:6.3": "Anavajjapadāni sevamāno,",
  "snp1.5:6.4": "Tatiyaṃ bhikkhunamāhu maggajīviṃ.",
  "snp1.5:7.1": "Chadanaṃ katvāna subbatānaṃ,",
  "snp1.5:7.2": "Pakkhandī kuladūsako pagabbho;",
  "snp1.5:7.3": "Māyāvī asaññato palāpo,",
  "snp1.5:7.4": "Patirūpena caraṃ sa maggadūsī.",
  "snp1.5:8.1": "Ete ca paṭivijjhi yo gahaṭṭho,",
  "snp1.5:8.2": "Sutavā ariyasāvako sapañño;",
  "snp1.5:8.3": "Sabbe netādisāti ñatvā,",
  "snp1.5:8.4": "Iti disvā na hāpeti tassa saddhā;",
  "snp1.5:8.5": "Kathaṃ hi duṭṭhena asampaduṭṭhaṃ,",
  "snp1.5:8.6": "Suddhaṃ asuddhena samaṃ kareyyā”ti.",
  "snp1.5:8.7": "Cundasuttaṃ pañcamaṃ."
}