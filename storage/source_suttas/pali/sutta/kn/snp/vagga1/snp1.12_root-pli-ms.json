{
  "snp1.12:0.1": "Sutta Nipāta 1.12",
  "snp1.12:0.2": "Munisutta",
  "snp1.12:1.1": "Santhavāto bhayaṃ jātaṃ,",
  "snp1.12:1.2": "niketā jāyate rajo;",
  "snp1.12:1.3": "Aniketamasanthavaṃ,",
  "snp1.12:1.4": "etaṃ ve munidassanaṃ.",
  "snp1.12:2.1": "Yo jātamucchijja na ropayeyya,",
  "snp1.12:2.2": "Jāyantamassa nānuppavecche;",
  "snp1.12:2.3": "Tamāhu ekaṃ muninaṃ carantaṃ,",
  "snp1.12:2.4": "Addakkhi so santipadaṃ mahesi.",
  "snp1.12:3.1": "Saṅkhāya vatthūni pamāya bījaṃ,",
  "snp1.12:3.2": "Sinehamassa nānuppavecche;",
  "snp1.12:3.3": "Sa ve munī jātikhayantadassī,",
  "snp1.12:3.4": "Takkaṃ pahāya na upeti saṅkhaṃ.",
  "snp1.12:4.1": "Aññāya sabbāni nivesanāni,",
  "snp1.12:4.2": "Anikāmayaṃ aññatarampi tesaṃ;",
  "snp1.12:4.3": "Sa ve munī vītagedho agiddho,",
  "snp1.12:4.4": "Nāyūhatī pāragato hi hoti.",
  "snp1.12:5.1": "Sabbābhibhuṃ sabbaviduṃ sumedhaṃ,",
  "snp1.12:5.2": "Sabbesu dhammesu anūpalittaṃ;",
  "snp1.12:5.3": "Sabbañjahaṃ taṇhakkhaye vimuttaṃ,",
  "snp1.12:5.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:6.1": "Paññābalaṃ sīlavatūpapannaṃ,",
  "snp1.12:6.2": "Samāhitaṃ jhānarataṃ satīmaṃ;",
  "snp1.12:6.3": "Saṅgā pamuttaṃ akhilaṃ anāsavaṃ,",
  "snp1.12:6.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:7.1": "Ekaṃ carantaṃ munimappamattaṃ,",
  "snp1.12:7.2": "Nindāpasaṃsāsu avedhamānaṃ;",
  "snp1.12:7.3": "Sīhaṃva saddesu asantasantaṃ,",
  "snp1.12:7.4": "Vātaṃva jālamhi asajjamānaṃ;",
  "snp1.12:7.5": "Padmaṃva toyena alippamānaṃ,",
  "snp1.12:7.6": "Netāramaññesamanaññaneyyaṃ;",
  "snp1.12:7.7": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:8.1": "Yo ogahaṇe thambhorivābhijāyati,",
  "snp1.12:8.2": "Yasmiṃ pare vācāpariyantaṃ vadanti;",
  "snp1.12:8.3": "Taṃ vītarāgaṃ susamāhitindriyaṃ,",
  "snp1.12:8.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:9.1": "Yo ve ṭhitatto tasaraṃva ujju,",
  "snp1.12:9.2": "Jigucchati kammehi pāpakehi;",
  "snp1.12:9.3": "Vīmaṃsamāno visamaṃ samañca,",
  "snp1.12:9.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:10.1": "Yo saññatatto na karoti pāpaṃ,",
  "snp1.12:10.2": "Daharo majjhimo ca muni yatatto;",
  "snp1.12:10.3": "Arosaneyyo na so roseti kañci,",
  "snp1.12:10.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:11.1": "Yadaggato majjhato sesato vā,",
  "snp1.12:11.2": "Piṇḍaṃ labhetha paradattūpajīvī;",
  "snp1.12:11.3": "Nālaṃ thutuṃ nopi nipaccavādī,",
  "snp1.12:11.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:12.1": "Muniṃ carantaṃ virataṃ methunasmā,",
  "snp1.12:12.2": "Yo yobbane nopanibajjhate kvaci;",
  "snp1.12:12.3": "Madappamādā virataṃ vippamuttaṃ,",
  "snp1.12:12.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:13.1": "Aññāya lokaṃ paramatthadassiṃ,",
  "snp1.12:13.2": "Oghaṃ samuddaṃ atitariya tādiṃ;",
  "snp1.12:13.3": "Taṃ chinnaganthaṃ asitaṃ anāsavaṃ,",
  "snp1.12:13.4": "Taṃ vāpi dhīrā muni vedayanti.",
  "snp1.12:14.1": "Asamā ubho dūravihāravuttino,",
  "snp1.12:14.2": "Gihī dāraposī amamo ca subbato;",
  "snp1.12:14.3": "Parapāṇarodhāya gihī asaññato,",
  "snp1.12:14.4": "Niccaṃ munī rakkhati pāṇine yato.",
  "snp1.12:15.1": "Sikhī yathā nīlagīvo vihaṅgamo,",
  "snp1.12:15.2": "Haṃsassa nopeti javaṃ kudācanaṃ;",
  "snp1.12:15.3": "Evaṃ gihī nānukaroti bhikkhuno,",
  "snp1.12:15.4": "Munino vivittassa vanamhi jhāyatoti.",
  "snp1.12:15.5": "Munisuttaṃ dvādasamaṃ.",
  "snp1.12:15.6": "Uragavaggo paṭhamo.",
  "snp1.12:16.1": "Urago dhaniyo ceva,",
  "snp1.12:16.2": "visāṇañca tathā kasi;",
  "snp1.12:16.3": "Cundo parābhavo ceva,",
  "snp1.12:16.4": "vasalo mettabhāvanā.",
  "snp1.12:17.1": "Sātāgiro āḷavako,",
  "snp1.12:17.2": "Vijayo ca tathā muni;",
  "snp1.12:17.3": "Dvādasetāni suttāni,",
  "snp1.12:17.4": "Uragavaggoti vuccatīti."
}