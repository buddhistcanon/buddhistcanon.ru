{
  "snp5.16:0.1": "Sutta Nipāta 5.16",
  "snp5.16:0.2": "Mogharājamāṇavapucchā",
  "snp5.16:1.1": "“Dvāhaṃ sakkaṃ apucchissaṃ,",
  "snp5.16:1.2": "Na me byākāsi cakkhumā;",
  "snp5.16:1.3": "Yāvatatiyañca devīsi,",
  "snp5.16:1.4": "Byākarotīti me sutaṃ.",
  "snp5.16:2.1": "Ayaṃ loko paro loko,",
  "snp5.16:2.2": "brahmaloko sadevako;",
  "snp5.16:2.3": "Diṭṭhiṃ te nābhijānāti,",
  "snp5.16:2.4": "gotamassa yasassino.",
  "snp5.16:3.1": "Evaṃ abhikkantadassāviṃ,",
  "snp5.16:3.2": "atthi pañhena āgamaṃ;",
  "snp5.16:3.3": "Kathaṃ lokaṃ avekkhantaṃ,",
  "snp5.16:3.4": "maccurājā na passati”.",
  "snp5.16:4.1": "“Suññato lokaṃ avekkhassu,",
  "snp5.16:4.2": "Mogharāja sadā sato;",
  "snp5.16:4.3": "Attānudiṭṭhiṃ ūhacca,",
  "snp5.16:4.4": "Evaṃ maccutaro siyā;",
  "snp5.16:4.5": "Evaṃ lokaṃ avekkhantaṃ,",
  "snp5.16:4.6": "Maccurājā na passatī”ti.",
  "snp5.16:4.7": "Mogharājamāṇavapucchā pannarasamā."
}