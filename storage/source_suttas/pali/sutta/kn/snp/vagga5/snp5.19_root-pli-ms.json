{
  "snp5.19:0.1": "Sutta Nipāta 5.19",
  "snp5.19:0.2": "Pārāyanānugītigāthā",
  "snp5.19:1.1": "“Pārāyanamanugāyissaṃ,",
  "snp5.19:1.2": "Yathāddakkhi tathākkhāsi;",
  "snp5.19:1.3": "Vimalo bhūrimedhaso,",
  "snp5.19:1.4": "Nikkāmo nibbano nāgo;",
  "snp5.19:1.5": "Kissa hetu musā bhaṇe.",
  "snp5.19:2.1": "Pahīnamalamohassa,",
  "snp5.19:2.2": "mānamakkhappahāyino;",
  "snp5.19:2.3": "Handāhaṃ kittayissāmi,",
  "snp5.19:2.4": "giraṃ vaṇṇūpasañhitaṃ.",
  "snp5.19:3.1": "Tamonudo buddho samantacakkhu,",
  "snp5.19:3.2": "Lokantagū sabbabhavātivatto;",
  "snp5.19:3.3": "Anāsavo sabbadukkhappahīno,",
  "snp5.19:3.4": "Saccavhayo brahme upāsito me.",
  "snp5.19:4.1": "Dijo yathā kubbanakaṃ pahāya,",
  "snp5.19:4.2": "Bahupphalaṃ kānanamāvaseyya;",
  "snp5.19:4.3": "Evampahaṃ appadasse pahāya,",
  "snp5.19:4.4": "Mahodadhiṃ haṃsoriva ajjhapatto.",
  "snp5.19:5.1": "Ye me pubbe viyākaṃsu,",
  "snp5.19:5.2": "Huraṃ gotamasāsanā;",
  "snp5.19:5.3": "Iccāsi iti bhavissati,",
  "snp5.19:5.4": "Sabbaṃ taṃ itihītihaṃ;",
  "snp5.19:5.5": "Sabbaṃ taṃ takkavaḍḍhanaṃ.",
  "snp5.19:6.1": "Eko tamanudāsino,",
  "snp5.19:6.2": "jutimā so pabhaṅkaro;",
  "snp5.19:6.3": "Gotamo bhūripaññāṇo,",
  "snp5.19:6.4": "gotamo bhūrimedhaso.",
  "snp5.19:7.1": "Yo me dhammamadesesi,",
  "snp5.19:7.2": "Sandiṭṭhikamakālikaṃ;",
  "snp5.19:7.3": "Taṇhakkhayamanītikaṃ,",
  "snp5.19:7.4": "Yassa natthi upamā kvaci”.",
  "snp5.19:8.1": "“Kiṃ nu tamhā vippavasasi,",
  "snp5.19:8.2": "muhuttamapi piṅgiya;",
  "snp5.19:8.3": "Gotamā bhūripaññāṇā,",
  "snp5.19:8.4": "gotamā bhūrimedhasā.",
  "snp5.19:9.1": "Yo te dhammamadesesi,",
  "snp5.19:9.2": "Sandiṭṭhikamakālikaṃ;",
  "snp5.19:9.3": "Taṇhakkhayamanītikaṃ,",
  "snp5.19:9.4": "Yassa natthi upamā kvaci”.",
  "snp5.19:10.1": "“Nāhaṃ tamhā vippavasāmi,",
  "snp5.19:10.2": "Muhuttamapi brāhmaṇa;",
  "snp5.19:10.3": "Gotamā bhūripaññāṇā,",
  "snp5.19:10.4": "Gotamā bhūrimedhasā.",
  "snp5.19:11.1": "Yo me dhammamadesesi,",
  "snp5.19:11.2": "Sandiṭṭhikamakālikaṃ;",
  "snp5.19:11.3": "Taṇhakkhayamanītikaṃ,",
  "snp5.19:11.4": "Yassa natthi upamā kvaci.",
  "snp5.19:12.1": "Passāmi naṃ manasā cakkhunāva,",
  "snp5.19:12.2": "Rattindivaṃ brāhmaṇa appamatto;",
  "snp5.19:12.3": "Namassamāno vivasemi rattiṃ,",
  "snp5.19:12.4": "Teneva maññāmi avippavāsaṃ.",
  "snp5.19:13.1": "Saddhā ca pīti ca mano sati ca,",
  "snp5.19:13.2": "Nāpentime gotamasāsanamhā;",
  "snp5.19:13.3": "Yaṃ yaṃ disaṃ vajati bhūripañño,",
  "snp5.19:13.4": "Sa tena teneva natohamasmi.",
  "snp5.19:14.1": "Jiṇṇassa me dubbalathāmakassa,",
  "snp5.19:14.2": "Teneva kāyo na paleti tattha;",
  "snp5.19:14.3": "Saṃkappayantāya vajāmi niccaṃ,",
  "snp5.19:14.4": "Mano hi me brāhmaṇa tena yutto.",
  "snp5.19:15.1": "Paṅke sayāno pariphandamāno,",
  "snp5.19:15.2": "Dīpā dīpaṃ upaplaviṃ;",
  "snp5.19:15.3": "Athaddasāsiṃ sambuddhaṃ,",
  "snp5.19:15.4": "Oghatiṇṇamanāsavaṃ”.",
  "snp5.19:16.1": "“Yathā ahū vakkali muttasaddho,",
  "snp5.19:16.2": "Bhadrāvudho āḷavigotamo ca;",
  "snp5.19:16.3": "Evamevaṃ tvampi pamuñcassu saddhaṃ,",
  "snp5.19:16.4": "Gamissasi tvaṃ piṅgiya maccudheyyassa pāraṃ”.",
  "snp5.19:17.1": "“Esa bhiyyo pasīdāmi,",
  "snp5.19:17.2": "Sutvāna munino vaco;",
  "snp5.19:17.3": "Vivaṭṭacchado sambuddho,",
  "snp5.19:17.4": "Akhilo paṭibhānavā.",
  "snp5.19:18.1": "Adhideve abhiññāya,",
  "snp5.19:18.2": "Sabbaṃ vedi varovaraṃ;",
  "snp5.19:18.3": "Pañhānantakaro satthā,",
  "snp5.19:18.4": "Kaṅkhīnaṃ paṭijānataṃ.",
  "snp5.19:19.1": "Asaṃhīraṃ asaṅkuppaṃ,",
  "snp5.19:19.2": "Yassa natthi upamā kvaci;",
  "snp5.19:19.3": "Addhā gamissāmi na mettha kaṅkhā,",
  "snp5.19:19.4": "Evaṃ maṃ dhārehi adhimuttacittan”ti.",
  "snp5.19:19.5": "Pārāyanavaggo pañcamo.",
  "snp5.19:19.6": "Suttuddānaṃ",
  "snp5.19:20.1": "Urago dhaniyopi ca,",
  "snp5.19:20.2": "Khaggavisāṇo kasi ca;",
  "snp5.19:20.3": "Cundo bhavo punadeva,",
  "snp5.19:20.4": "Vasalo ca karaṇīyañca;",
  "snp5.19:20.5": "Hemavato atha yakkho,",
  "snp5.19:20.6": "Vijayasuttaṃ munisuttavaranti.",
  "snp5.19:21.1": "Paṭhamakaṭṭhavaro varavaggo,",
  "snp5.19:21.2": "Dvādasasuttadharo suvibhatto;",
  "snp5.19:21.3": "Desito cakkhumatā vimalena,",
  "snp5.19:21.4": "Suyyati vaggavaro uragoti.",
  "snp5.19:22.1": "Ratanāmagandho hirimaṅgalanāmo,",
  "snp5.19:22.2": "Sucilomakapilo ca brāhmaṇadhammo;",
  "snp5.19:22.3": "Nāvā kiṃsīlauṭṭhahano ca,",
  "snp5.19:22.4": "Rāhulo ca punapi vaṅgīso.",
  "snp5.19:23.1": "Sammāparibbājanīyopi cettha,",
  "snp5.19:23.2": "Dhammikasuttavaro suvibhatto;",
  "snp5.19:23.3": "Cuddasasuttadharo dutiyamhi,",
  "snp5.19:23.4": "Cūḷakavaggavaroti tamāhu.",
  "snp5.19:24.1": "Pabbajjapadhānasubhāsitanāmo,",
  "snp5.19:24.2": "Pūraḷāso punadeva māgho ca;",
  "snp5.19:24.3": "Sabhiyaṃ keṇiyameva sallanāmo,",
  "snp5.19:24.4": "Vāseṭṭhavaro kālikopi ca.",
  "snp5.19:25.1": "Nālakasuttavaro suvibhatto,",
  "snp5.19:25.2": "Taṃ anupassī tathā punadeva;",
  "snp5.19:25.3": "Dvādasasuttadharo tatiyamhi,",
  "snp5.19:25.4": "Suyyati vaggavaro mahānāmo.",
  "snp5.19:26.1": "Kāmaguhaṭṭhakaduṭṭhakanāmā,",
  "snp5.19:26.2": "Suddhavaro paramaṭṭhakanāmo;",
  "snp5.19:26.3": "Jarā mettiyavaro suvibhatto,",
  "snp5.19:26.4": "Pasūramāgaṇḍiyā purābhedo.",
  "snp5.19:27.1": "Kalahavivādo ubho viyuhā ca,",
  "snp5.19:27.2": "Tuvaṭakaattadaṇḍasāriputtā;",
  "snp5.19:27.3": "Soḷasasuttadharo catutthamhi,",
  "snp5.19:27.4": "Aṭṭhakavaggavaroti tamāhu.",
  "snp5.19:28.1": "Magadhe janapade ramaṇīye,",
  "snp5.19:28.2": "Desavare katapuññanivese;",
  "snp5.19:28.3": "Pāsāṇakacetiyavare suvibhatte,",
  "snp5.19:28.4": "Vasi bhagavā gaṇaseṭṭho.",
  "snp5.19:29.1": "Ubhayavāsamāgatiyamhi,",
  "snp5.19:29.2": "Dvādasayojaniyā parisāya;",
  "snp5.19:29.3": "Soḷasabrāhmaṇānaṃ kira puṭṭho,",
  "snp5.19:29.4": "Pucchāya soḷasapañhakammiyā;",
  "snp5.19:29.5": "Nippakāsayi dhammamadāsi.",
  "snp5.19:30.1": "Atthapakāsakabyañjanapuṇṇaṃ,",
  "snp5.19:30.2": "Dhammamadesesi parakhemajaniyaṃ;",
  "snp5.19:30.3": "Lokahitāya jino dvipadaggo,",
  "snp5.19:30.4": "Suttavaraṃ bahudhammavicitraṃ;",
  "snp5.19:30.5": "Sabbakilesapamocanahetuṃ,",
  "snp5.19:30.6": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:31.1": "Byañjanamatthapadaṃ samayuttaṃ,",
  "snp5.19:31.2": "Akkharasaññitaopamagāḷhaṃ;",
  "snp5.19:31.3": "Lokavicāraṇañāṇapabhaggaṃ,",
  "snp5.19:31.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:32.1": "Rāgamale amalaṃ vimalaggaṃ,",
  "snp5.19:32.2": "Dosamale amalaṃ vimalaggaṃ;",
  "snp5.19:32.3": "Mohamale amalaṃ vimalaggaṃ,",
  "snp5.19:32.4": "Lokavicāraṇañāṇapabhaggaṃ;",
  "snp5.19:32.5": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:33.1": "Klesamale amalaṃ vimalaggaṃ,",
  "snp5.19:33.2": "Duccaritamale amalaṃ vimalaggaṃ;",
  "snp5.19:33.3": "Lokavicāraṇañāṇapabhaggaṃ,",
  "snp5.19:33.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:34.1": "Āsavabandhanayogākilesaṃ,",
  "snp5.19:34.2": "Nīvaraṇāni ca tīṇi malāni;",
  "snp5.19:34.3": "Tassa kilesapamocanahetuṃ,",
  "snp5.19:34.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:35.1": "Nimmalasabbakilesapanūdaṃ,",
  "snp5.19:35.2": "Rāgavirāgamanejamasokaṃ;",
  "snp5.19:35.3": "Santapaṇītasududdasadhammaṃ,",
  "snp5.19:35.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:36.1": "Rāgañca dosakamabhañjitasantaṃ,",
  "snp5.19:36.2": "Yonicatuggatipañcaviññāṇaṃ;",
  "snp5.19:36.3": "Taṇhāratacchadanatāṇalatāpamokkhaṃ,",
  "snp5.19:36.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:37.1": "Gambhīraduddasasaṇhanipuṇaṃ,",
  "snp5.19:37.2": "Paṇḍitavedaniyaṃ nipuṇatthaṃ;",
  "snp5.19:37.3": "Lokavicāraṇañāṇapabhaggaṃ,",
  "snp5.19:37.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:38.1": "Navaṅgakusumamālagīveyyaṃ,",
  "snp5.19:38.2": "Indriyajhānavimokkhavibhattaṃ;",
  "snp5.19:38.3": "Aṭṭhaṅgamaggadharaṃ varayānaṃ,",
  "snp5.19:38.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:39.1": "Somupamaṃ vimalaṃ parisuddhaṃ,",
  "snp5.19:39.2": "Aṇṇavamūpamaratanasucittaṃ;",
  "snp5.19:39.3": "Pupphasamaṃ ravimūpamatejaṃ,",
  "snp5.19:39.4": "Desayi suttavaraṃ dvipadaggo.",
  "snp5.19:40.1": "Khemasivaṃ sukhasītalasantaṃ,",
  "snp5.19:40.2": "Maccutatāṇaparaṃ paramatthaṃ;",
  "snp5.19:40.3": "Tassa sunibbutadassanahetuṃ,",
  "snp5.19:40.4": "Desayi suttavaraṃ dvipadaggoti.",
  "snp5.19:40.5": "Suttanipātapāḷi samattā."
}