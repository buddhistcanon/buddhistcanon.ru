{
  "snp5.15:0.1": "Sutta Nipāta 5.15",
  "snp5.15:0.2": "Posālamāṇavapucchā",
  "snp5.15:1.1": "“Yo atītaṃ ādisati,",
  "snp5.15:1.2": "Anejo chinnasaṃsayo;",
  "snp5.15:1.3": "Pāraguṃ sabbadhammānaṃ,",
  "snp5.15:1.4": "Atthi pañhena āgamaṃ.",
  "snp5.15:2.1": "Vibhūtarūpasaññissa,",
  "snp5.15:2.2": "sabbakāyappahāyino;",
  "snp5.15:2.3": "Ajjhattañca bahiddhā ca,",
  "snp5.15:2.4": "natthi kiñcīti passato;",
  "snp5.15:2.5": "Ñāṇaṃ sakkānupucchāmi,",
  "snp5.15:2.6": "kathaṃ neyyo tathāvidho”.",
  "snp5.15:3.1": "“Viññāṇaṭṭhitiyo sabbā,",
  "snp5.15:3.2": "Abhijānaṃ tathāgato;",
  "snp5.15:3.3": "Tiṭṭhantamenaṃ jānāti,",
  "snp5.15:3.4": "Vimuttaṃ tapparāyaṇaṃ.",
  "snp5.15:4.1": "Ākiñcaññasambhavaṃ ñatvā,",
  "snp5.15:4.2": "Nandī saṃyojanaṃ iti;",
  "snp5.15:4.3": "Evametaṃ abhiññāya,",
  "snp5.15:4.4": "Tato tattha vipassati;",
  "snp5.15:4.5": "Etaṃ ñāṇaṃ tathaṃ tassa,",
  "snp5.15:4.6": "Brāhmaṇassa vusīmato”ti.",
  "snp5.15:4.7": "Posālamāṇavapucchā cuddasamā."
}