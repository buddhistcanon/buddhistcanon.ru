{
  "snp5.5:0.1": "Sutta Nipāta 5.5",
  "snp5.5:0.2": "Mettagūmāṇavapucchā",
  "snp5.5:1.1": "“Pucchāmi taṃ bhagavā brūhi me taṃ,",
  "snp5.5:1.2": "Maññāmi taṃ vedaguṃ bhāvitattaṃ;",
  "snp5.5:1.3": "Kuto nu dukkhā samudāgatā ime,",
  "snp5.5:1.4": "Ye keci lokasmimanekarūpā”.",
  "snp5.5:2.1": "“Dukkhassa ve maṃ pabhavaṃ apucchasi,",
  "snp5.5:2.2": "Taṃ te pavakkhāmi yathā pajānaṃ;",
  "snp5.5:2.3": "Upadhinidānā pabhavanti dukkhā,",
  "snp5.5:2.4": "Ye keci lokasmimanekarūpā.",
  "snp5.5:3.1": "Yo ve avidvā upadhiṃ karoti,",
  "snp5.5:3.2": "Punappunaṃ dukkhamupeti mando;",
  "snp5.5:3.3": "Tasmā pajānaṃ upadhiṃ na kayirā,",
  "snp5.5:3.4": "Dukkhassa jātippabhavānupassī”.",
  "snp5.5:4.1": "“Yaṃ taṃ apucchimha akittayī no,",
  "snp5.5:4.2": "Aññaṃ taṃ pucchāma tadiṅgha brūhi;",
  "snp5.5:4.3": "Kathaṃ nu dhīrā vitaranti oghaṃ,",
  "snp5.5:4.4": "Jātiṃ jaraṃ sokapariddavañca;",
  "snp5.5:4.5": "Taṃ me muni sādhu viyākarohi,",
  "snp5.5:4.6": "Tathā hi te vidito esa dhammo”.",
  "snp5.5:5.1": "“Kittayissāmi te dhammaṃ,",
  "snp5.5:5.2": "Diṭṭhe dhamme anītihaṃ;",
  "snp5.5:5.3": "Yaṃ viditvā sato caraṃ,",
  "snp5.5:5.4": "Tare loke visattikaṃ”.",
  "snp5.5:6.1": "“Tañcāhaṃ abhinandāmi,",
  "snp5.5:6.2": "mahesi dhammamuttamaṃ;",
  "snp5.5:6.3": "Yaṃ viditvā sato caraṃ,",
  "snp5.5:6.4": "tare loke visattikaṃ”.",
  "snp5.5:7.1": "“Yaṃ kiñci sampajānāsi,",
  "snp5.5:7.2": "Uddhaṃ adho tiriyañcāpi majjhe;",
  "snp5.5:7.3": "Etesu nandiñca nivesanañca,",
  "snp5.5:7.4": "Panujja viññāṇaṃ bhave na tiṭṭhe.",
  "snp5.5:8.1": "Evaṃvihārī sato appamatto,",
  "snp5.5:8.2": "Bhikkhu caraṃ hitvā mamāyitāni;",
  "snp5.5:8.3": "Jātiṃ jaraṃ sokapariddavañca,",
  "snp5.5:8.4": "Idheva vidvā pajaheyya dukkhaṃ”.",
  "snp5.5:9.1": "“Etābhinandāmi vaco mahesino,",
  "snp5.5:9.2": "Sukittitaṃ gotamanūpadhīkaṃ;",
  "snp5.5:9.3": "Addhā hi bhagavā pahāsi dukkhaṃ,",
  "snp5.5:9.4": "Tathā hi te vidito esa dhammo.",
  "snp5.5:10.1": "Te cāpi nūnappajaheyyu dukkhaṃ,",
  "snp5.5:10.2": "Ye tvaṃ muni aṭṭhitaṃ ovadeyya;",
  "snp5.5:10.3": "Taṃ taṃ namassāmi samecca nāga,",
  "snp5.5:10.4": "Appeva maṃ bhagavā aṭṭhitaṃ ovadeyya”.",
  "snp5.5:11.1": "“Yaṃ brāhmaṇaṃ vedagumābhijaññā,",
  "snp5.5:11.2": "Akiñcanaṃ kāmabhave asattaṃ;",
  "snp5.5:11.3": "Addhā hi so oghamimaṃ atāri,",
  "snp5.5:11.4": "Tiṇṇo ca pāraṃ akhilo akaṅkho.",
  "snp5.5:12.1": "Vidvā ca yo vedagū naro idha,",
  "snp5.5:12.2": "Bhavābhave saṅgamimaṃ visajja;",
  "snp5.5:12.3": "So vītataṇho anīgho nirāso,",
  "snp5.5:12.4": "Atāri so jātijaranti brūmī”ti.",
  "snp5.5:12.5": "Mettagūmāṇavapucchā catutthī."
}