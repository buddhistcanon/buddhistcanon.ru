{
  "snp5.10:0.1": "Sutta Nipāta 5.10",
  "snp5.10:0.2": "Todeyyamāṇavapucchā",
  "snp5.10:1.1": "“Yasmiṃ kāmā na vasanti,",
  "snp5.10:1.2": "Taṇhā yassa na vijjati;",
  "snp5.10:1.3": "Kathaṃkathā ca yo tiṇṇo,",
  "snp5.10:1.4": "Vimokkho tassa kīdiso”.",
  "snp5.10:2.1": "“Yasmiṃ kāmā na vasanti,",
  "snp5.10:2.2": "Taṇhā yassa na vijjati;",
  "snp5.10:2.3": "Kathaṅkathā ca yo tiṇṇo,",
  "snp5.10:2.4": "Vimokkho tassa nāparo”.",
  "snp5.10:3.1": "“Nirāsaso so uda āsasāno,",
  "snp5.10:3.2": "Paññāṇavā so uda paññakappī;",
  "snp5.10:3.3": "Muniṃ ahaṃ sakka yathā vijaññaṃ,",
  "snp5.10:3.4": "Taṃ me viyācikkha samantacakkhu”.",
  "snp5.10:4.1": "“Nirāsaso so na ca āsasāno,",
  "snp5.10:4.2": "Paññāṇavā so na ca paññakappī;",
  "snp5.10:4.3": "Evampi todeyya muniṃ vijāna,",
  "snp5.10:4.4": "Akiñcanaṃ kāmabhave asattan”ti.",
  "snp5.10:4.5": "Todeyyamāṇavapucchā navamā."
}