{
  "snp5.14:0.1": "Sutta Nipāta 5.14",
  "snp5.14:0.2": "Udayamāṇavapucchā",
  "snp5.14:1.1": "“Jhāyiṃ virajamāsīnaṃ,",
  "snp5.14:1.2": "Katakiccaṃ anāsavaṃ;",
  "snp5.14:1.3": "Pāraguṃ sabbadhammānaṃ,",
  "snp5.14:1.4": "Atthi pañhena āgamaṃ;",
  "snp5.14:1.5": "Aññāvimokkhaṃ pabrūhi,",
  "snp5.14:1.6": "Avijjāya pabhedanaṃ”.",
  "snp5.14:2.1": "“Pahānaṃ kāmacchandānaṃ,",
  "snp5.14:2.2": "Domanassāna cūbhayaṃ;",
  "snp5.14:2.3": "Thinassa ca panūdanaṃ,",
  "snp5.14:2.4": "Kukkuccānaṃ nivāraṇaṃ.",
  "snp5.14:3.1": "Upekkhāsatisaṃsuddhaṃ,",
  "snp5.14:3.2": "dhammatakkapurejavaṃ;",
  "snp5.14:3.3": "Aññāvimokkhaṃ pabrūmi,",
  "snp5.14:3.4": "avijjāya pabhedanaṃ”.",
  "snp5.14:4.1": "“Kiṃsu saṃyojano loko,",
  "snp5.14:4.2": "kiṃsu tassa vicāraṇaṃ;",
  "snp5.14:4.3": "Kissassa vippahānena,",
  "snp5.14:4.4": "nibbānaṃ iti vuccati”.",
  "snp5.14:5.1": "“Nandisaṃyojano loko,",
  "snp5.14:5.2": "vitakkassa vicāraṇaṃ;",
  "snp5.14:5.3": "Taṇhāya vippahānena,",
  "snp5.14:5.4": "nibbānaṃ iti vuccati”.",
  "snp5.14:6.1": "“Kathaṃ satassa carato,",
  "snp5.14:6.2": "viññāṇaṃ uparujjhati;",
  "snp5.14:6.3": "Bhagavantaṃ puṭṭhumāgamma,",
  "snp5.14:6.4": "taṃ suṇoma vaco tava”.",
  "snp5.14:7.1": "“Ajjhattañca bahiddhā ca,",
  "snp5.14:7.2": "Vedanaṃ nābhinandato;",
  "snp5.14:7.3": "Evaṃ satassa carato,",
  "snp5.14:7.4": "Viññāṇaṃ uparujjhatī”ti.",
  "snp5.14:7.5": "Udayamāṇavapucchā terasamā."
}