{
  "snp5.17:0.1": "Sutta Nipāta 5.17",
  "snp5.17:0.2": "Piṅgiyamāṇavapucchā",
  "snp5.17:1.1": "“Jiṇṇohamasmi abalo vītavaṇṇo,",
  "snp5.17:1.2": "Nettā na suddhā savanaṃ na phāsu;",
  "snp5.17:1.3": "Māhaṃ nassaṃ momuho antarāva,",
  "snp5.17:1.4": "Ācikkha dhammaṃ yamahaṃ vijaññaṃ;",
  "snp5.17:1.5": "Jātijarāya idha vippahānaṃ”.",
  "snp5.17:2.1": "“Disvāna rūpesu vihaññamāne,",
  "snp5.17:2.2": "Ruppanti rūpesu janā pamattā;",
  "snp5.17:2.3": "Tasmā tuvaṃ piṅgiya appamatto,",
  "snp5.17:2.4": "Jahassu rūpaṃ apunabbhavāya”.",
  "snp5.17:3.1": "“Disā catasso vidisā catasso,",
  "snp5.17:3.2": "Uddhaṃ adho dasa disā imāyo;",
  "snp5.17:3.3": "Na tuyhaṃ adiṭṭhaṃ asutaṃ amutaṃ,",
  "snp5.17:3.4": "Atho aviññātaṃ kiñcanamatthi loke;",
  "snp5.17:3.5": "Ācikkha dhammaṃ yamahaṃ vijaññaṃ,",
  "snp5.17:3.6": "Jātijarāya idha vippahānaṃ”.",
  "snp5.17:4.1": "“Taṇhādhipanne manuje pekkhamāno,",
  "snp5.17:4.2": "Santāpajāte jarasā parete;",
  "snp5.17:4.3": "Tasmā tuvaṃ piṅgiya appamatto,",
  "snp5.17:4.4": "Jahassu taṇhaṃ apunabbhavāyā”ti.",
  "snp5.17:4.5": "Piṅgiyamāṇavapucchā soḷasamā."
}