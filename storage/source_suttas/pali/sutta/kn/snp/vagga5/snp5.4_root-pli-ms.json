{
  "snp5.4:0.1": "Sutta Nipāta 5.4",
  "snp5.4:0.2": "Puṇṇakamāṇavapucchā",
  "snp5.4:1.1": "“Anejaṃ mūladassāviṃ,",
  "snp5.4:1.2": "Atthi pañhena āgamaṃ;",
  "snp5.4:1.3": "Kiṃ nissitā isayo manujā,",
  "snp5.4:1.4": "Khattiyā brāhmaṇā devatānaṃ;",
  "snp5.4:1.5": "Yaññamakappayiṃsu puthūdha loke,",
  "snp5.4:1.6": "Pucchāmi taṃ bhagavā brūhi me taṃ”.",
  "snp5.4:2.1": "“Ye kecime isayo manujā,",
  "snp5.4:2.2": "Khattiyā brāhmaṇā devatānaṃ;",
  "snp5.4:2.3": "Yaññamakappayiṃsu puthūdha loke,",
  "snp5.4:2.4": "Āsīsamānā puṇṇaka itthattaṃ;",
  "snp5.4:2.5": "Jaraṃ sitā yaññamakappayiṃsu”.",
  "snp5.4:3.1": "“Ye kecime isayo manujā,",
  "snp5.4:3.2": "Khattiyā brāhmaṇā devatānaṃ;",
  "snp5.4:3.3": "Yaññamakappayiṃsu puthūdha loke,",
  "snp5.4:3.4": "Kaccissu te bhagavā yaññapathe appamattā;",
  "snp5.4:3.5": "Atāruṃ jātiñca jarañca mārisa,",
  "snp5.4:3.6": "Pucchāmi taṃ bhagavā brūhi me taṃ”.",
  "snp5.4:4.1": "“Āsīsanti thomayanti abhijappanti juhanti,",
  "snp5.4:4.2": "Kāmābhijappanti paṭicca lābhaṃ;",
  "snp5.4:4.3": "Te yājayogā bhavarāgarattā,",
  "snp5.4:4.4": "Nātariṃsu jātijaranti brūmi”.",
  "snp5.4:5.1": "“Te ce nātariṃsu yājayogā,",
  "snp5.4:5.2": "Yaññehi jātiñca jarañca mārisa;",
  "snp5.4:5.3": "Atha ko carahi devamanussaloke,",
  "snp5.4:5.4": "Atāri jātiñca jarañca mārisa;",
  "snp5.4:5.5": "Pucchāmi taṃ bhagavā brūhi me taṃ”.",
  "snp5.4:6.1": "“Saṅkhāya lokasmi paroparāni,",
  "snp5.4:6.2": "Yassiñjitaṃ natthi kuhiñci loke;",
  "snp5.4:6.3": "Santo vidhūmo anīgho nirāso,",
  "snp5.4:6.4": "Atāri so jātijaranti brūmī”ti.",
  "snp5.4:6.5": "Puṇṇakamāṇavapucchā tatiyā."
}