{
  "snp5.8:0.1": "Sutta Nipāta 5.8",
  "snp5.8:0.2": "Nandamāṇavapucchā",
  "snp5.8:1.1": "“Santi loke munayo,",
  "snp5.8:1.2": "Janā vadanti tayidaṃ kathaṃsu;",
  "snp5.8:1.3": "Ñāṇūpapannaṃ no muniṃ vadanti,",
  "snp5.8:1.4": "Udāhu ve jīvitenūpapannaṃ”.",
  "snp5.8:2.1": "“Na diṭṭhiyā na sutiyā na ñāṇena, ()",
  "snp5.8:2.2": "Munīdha nanda kusalā vadanti;",
  "snp5.8:2.3": "Visenikatvā anīghā nirāsā,",
  "snp5.8:2.4": "Caranti ye te munayoti brūmi”.",
  "snp5.8:3.1": "“Ye kecime samaṇabrāhmaṇāse,",
  "snp5.8:3.2": "Diṭṭhassutenāpi vadanti suddhiṃ;",
  "snp5.8:3.3": "Sīlabbatenāpi vadanti suddhiṃ,",
  "snp5.8:3.4": "Anekarūpena vadanti suddhiṃ.",
  "snp5.8:3.5": "Kaccissu te bhagavā tattha yatā carantā,",
  "snp5.8:3.6": "Atāru jātiñca jarañca mārisa;",
  "snp5.8:3.7": "Pucchāmi taṃ bhagavā brūhi metaṃ”.",
  "snp5.8:4.1": "“Ye kecime samaṇabrāhmaṇāse,",
  "snp5.8:4.2": "Diṭṭhassutenāpi vadanti suddhiṃ;",
  "snp5.8:4.3": "Sīlabbatenāpi vadanti suddhiṃ,",
  "snp5.8:4.4": "Anekarūpena vadanti suddhiṃ;",
  "snp5.8:4.5": "Kiñcāpi te tattha yatā caranti,",
  "snp5.8:4.6": "Nātariṃsu jātijaranti brūmi”.",
  "snp5.8:5.1": "“Ye kecime samaṇabrāhmaṇāse,",
  "snp5.8:5.2": "Diṭṭhassutenāpi vadanti suddhiṃ;",
  "snp5.8:5.3": "Sīlabbatenāpi vadanti suddhiṃ,",
  "snp5.8:5.4": "Anekarūpena vadanti suddhiṃ.",
  "snp5.8:5.5": "Te ce muni brūsi anoghatiṇṇe,",
  "snp5.8:5.6": "Atha ko carahi devamanussaloke;",
  "snp5.8:5.7": "Atāri jātiñca jarañca mārisa,",
  "snp5.8:5.8": "Pucchāmi taṃ bhagavā brūhi metaṃ”.",
  "snp5.8:6.1": "“Nāhaṃ sabbe samaṇabrāhmaṇāse,",
  "snp5.8:6.2": "Jātijarāya nivutāti brūmi;",
  "snp5.8:6.3": "Ye sīdha diṭṭhaṃ va sutaṃ mutaṃ vā,",
  "snp5.8:6.4": "Sīlabbataṃ vāpi pahāya sabbaṃ.",
  "snp5.8:6.5": "Anekarūpampi pahāya sabbaṃ,",
  "snp5.8:6.6": "Taṇhaṃ pariññāya anāsavāse;",
  "snp5.8:6.7": "Te ve narā oghatiṇṇāti brūmi”.",
  "snp5.8:7.1": "“Etābhinandāmi vaco mahesino,",
  "snp5.8:7.2": "Sukittitaṃ gotamanūpadhīkaṃ;",
  "snp5.8:7.3": "Ye sīdha diṭṭhaṃ va sutaṃ mutaṃ vā,",
  "snp5.8:7.4": "Sīlabbataṃ vāpi pahāya sabbaṃ.",
  "snp5.8:7.5": "Anekarūpampi pahāya sabbaṃ,",
  "snp5.8:7.6": "Taṇhaṃ pariññāya anāsavāse;",
  "snp5.8:7.7": "Ahampi te oghatiṇṇāti brūmī”ti.",
  "snp5.8:7.8": "Nandamāṇavapucchā sattamā."
}