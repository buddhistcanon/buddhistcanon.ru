{
  "snp5.6:0.1": "Sutta Nipāta 5.6",
  "snp5.6:0.2": "Dhotakamāṇavapucchā",
  "snp5.6:1.1": "“Pucchāmi taṃ bhagavā brūhi metaṃ,",
  "snp5.6:1.2": "Vācābhikaṅkhāmi mahesi tuyhaṃ;",
  "snp5.6:1.3": "Tava sutvāna nigghosaṃ,",
  "snp5.6:1.4": "Sikkhe nibbānamattano”.",
  "snp5.6:2.1": "“Tenahātappaṃ karohi,",
  "snp5.6:2.2": "Idheva nipako sato;",
  "snp5.6:2.3": "Ito sutvāna nigghosaṃ,",
  "snp5.6:2.4": "Sikkhe nibbānamattano”.",
  "snp5.6:3.1": "“Passāmahaṃ devamanussaloke,",
  "snp5.6:3.2": "Akiñcanaṃ brāhmaṇamiriyamānaṃ;",
  "snp5.6:3.3": "Taṃ taṃ namassāmi samantacakkhu,",
  "snp5.6:3.4": "Pamuñca maṃ sakka kathaṅkathāhi”.",
  "snp5.6:4.1": "“Nāhaṃ sahissāmi pamocanāya,",
  "snp5.6:4.2": "Kathaṃkathiṃ dhotaka kañci loke;",
  "snp5.6:4.3": "Dhammañca seṭṭhaṃ abhijānamāno,",
  "snp5.6:4.4": "Evaṃ tuvaṃ oghamimaṃ taresi”.",
  "snp5.6:5.1": "“Anusāsa brahme karuṇāyamāno,",
  "snp5.6:5.2": "Vivekadhammaṃ yamahaṃ vijaññaṃ;",
  "snp5.6:5.3": "Yathāhaṃ ākāsova abyāpajjamāno,",
  "snp5.6:5.4": "Idheva santo asito careyyaṃ”.",
  "snp5.6:6.1": "“Kittayissāmi te santiṃ,",
  "snp5.6:6.2": "Diṭṭhe dhamme anītihaṃ;",
  "snp5.6:6.3": "Yaṃ viditvā sato caraṃ,",
  "snp5.6:6.4": "Tare loke visattikaṃ”.",
  "snp5.6:7.1": "“Tañcāhaṃ abhinandāmi,",
  "snp5.6:7.2": "mahesi santimuttamaṃ;",
  "snp5.6:7.3": "Yaṃ viditvā sato caraṃ,",
  "snp5.6:7.4": "tare loke visattikaṃ”.",
  "snp5.6:8.1": "“Yaṃ kiñci sampajānāsi,",
  "snp5.6:8.2": "Uddhaṃ adho tiriyañcāpi majjhe;",
  "snp5.6:8.3": "Etaṃ viditvā saṅgoti loke,",
  "snp5.6:8.4": "Bhavābhavāya mākāsi taṇhan”ti.",
  "snp5.6:8.5": "Dhotakamāṇavapucchā pañcamī."
}