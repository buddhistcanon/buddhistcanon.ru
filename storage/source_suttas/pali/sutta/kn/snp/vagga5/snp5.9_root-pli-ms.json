{
  "snp5.9:0.1": "Sutta Nipāta 5.9",
  "snp5.9:0.2": "Hemakamāṇavapucchā",
  "snp5.9:1.1": "“Ye me pubbe viyākaṃsu,",
  "snp5.9:1.2": "Huraṃ gotamasāsanā;",
  "snp5.9:1.3": "Iccāsi iti bhavissati,",
  "snp5.9:1.4": "Sabbaṃ taṃ itihītihaṃ;",
  "snp5.9:1.5": "Sabbaṃ taṃ takkavaḍḍhanaṃ,",
  "snp5.9:1.6": "Nāhaṃ tattha abhiramiṃ.",
  "snp5.9:2.1": "Tvañca me dhammamakkhāhi,",
  "snp5.9:2.2": "taṇhānigghātanaṃ muni;",
  "snp5.9:2.3": "Yaṃ viditvā sato caraṃ,",
  "snp5.9:2.4": "tare loke visattikaṃ”.",
  "snp5.9:3.1": "“Idha diṭṭhasutamutaviññātesu,",
  "snp5.9:3.2": "Piyarūpesu hemaka;",
  "snp5.9:3.3": "Chandarāgavinodanaṃ,",
  "snp5.9:3.4": "Nibbānapadamaccutaṃ.",
  "snp5.9:4.1": "Etadaññāya ye satā,",
  "snp5.9:4.2": "Diṭṭhadhammābhinibbutā;",
  "snp5.9:4.3": "Upasantā ca te sadā,",
  "snp5.9:4.4": "Tiṇṇā loke visattikan”ti.",
  "snp5.9:4.5": "Hemakamāṇavapucchā aṭṭhamā."
}