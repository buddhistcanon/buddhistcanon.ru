{
  "snp5.11:0.1": "Sutta Nipāta 5.11",
  "snp5.11:0.2": "Kappamāṇavapucchā",
  "snp5.11:1.1": "“Majjhe sarasmiṃ tiṭṭhataṃ,",
  "snp5.11:1.2": "Oghe jāte mahabbhaye;",
  "snp5.11:1.3": "Jarāmaccuparetānaṃ,",
  "snp5.11:1.4": "Dīpaṃ pabrūhi mārisa;",
  "snp5.11:1.5": "Tvañca me dīpamakkhāhi,",
  "snp5.11:1.6": "Yathāyidaṃ nāparaṃ siyā”.",
  "snp5.11:2.1": "“Majjhe sarasmiṃ tiṭṭhataṃ,",
  "snp5.11:2.2": "Oghe jāte mahabbhaye;",
  "snp5.11:2.3": "Jarāmaccuparetānaṃ,",
  "snp5.11:2.4": "Dīpaṃ pabrūmi kappa te.",
  "snp5.11:3.1": "Akiñcanaṃ anādānaṃ,",
  "snp5.11:3.2": "etaṃ dīpaṃ anāparaṃ;",
  "snp5.11:3.3": "Nibbānaṃ iti naṃ brūmi,",
  "snp5.11:3.4": "jarāmaccuparikkhayaṃ.",
  "snp5.11:4.1": "Etadaññāya ye satā,",
  "snp5.11:4.2": "Diṭṭhadhammābhinibbutā;",
  "snp5.11:4.3": "Na te māravasānugā,",
  "snp5.11:4.4": "Na te mārassa paddhagū”ti.",
  "snp5.11:4.5": "Kappamāṇavapucchā dasamā."
}