{
  "snp5.2:0.1": "Sutta Nipāta 5.2",
  "snp5.2:0.2": "Ajitamāṇavapucchā",
  "snp5.2:1.1": "“Kenassu nivuto loko,",
  "snp5.2:1.2": "Kenassu nappakāsati;",
  "snp5.2:1.3": "Kissābhilepanaṃ brūsi,",
  "snp5.2:1.4": "Kiṃsu tassa mahabbhayaṃ”.",
  "snp5.2:2.1": "“Avijjāya nivuto loko,",
  "snp5.2:2.2": "Vevicchā pamādā nappakāsati;",
  "snp5.2:2.3": "Jappābhilepanaṃ brūmi,",
  "snp5.2:2.4": "Dukkhamassa mahabbhayaṃ”.",
  "snp5.2:3.1": "“Savanti sabbadhi sotā,",
  "snp5.2:3.2": "Sotānaṃ kiṃ nivāraṇaṃ;",
  "snp5.2:3.3": "Sotānaṃ saṃvaraṃ brūhi,",
  "snp5.2:3.4": "Kena sotā pidhiyyare”.",
  "snp5.2:4.1": "“Yāni sotāni lokasmiṃ,",
  "snp5.2:4.2": "Sati tesaṃ nivāraṇaṃ;",
  "snp5.2:4.3": "Sotānaṃ saṃvaraṃ brūmi,",
  "snp5.2:4.4": "Paññāyete pidhiyyare”.",
  "snp5.2:5.1": "“Paññā ceva sati yañca,",
  "snp5.2:5.2": "Nāmarūpañca mārisa;",
  "snp5.2:5.3": "Etaṃ me puṭṭho pabrūhi,",
  "snp5.2:5.4": "Katthetaṃ uparujjhati”.",
  "snp5.2:6.1": "“Yametaṃ pañhaṃ apucchi,",
  "snp5.2:6.2": "ajita taṃ vadāmi te;",
  "snp5.2:6.3": "Yattha nāmañca rūpañca,",
  "snp5.2:6.4": "asesaṃ uparujjhati;",
  "snp5.2:6.5": "Viññāṇassa nirodhena,",
  "snp5.2:6.6": "etthetaṃ uparujjhati”.",
  "snp5.2:7.1": "“Ye ca saṅkhātadhammāse,",
  "snp5.2:7.2": "ye ca sekhā puthū idha;",
  "snp5.2:7.3": "Tesaṃ me nipako iriyaṃ,",
  "snp5.2:7.4": "puṭṭho pabrūhi mārisa”.",
  "snp5.2:8.1": "“Kāmesu nābhigijjheyya,",
  "snp5.2:8.2": "Manasānāvilo siyā;",
  "snp5.2:8.3": "Kusalo sabbadhammānaṃ,",
  "snp5.2:8.4": "Sato bhikkhu paribbaje”ti.",
  "snp5.2:8.5": "Ajitamāṇavapucchā paṭhamā."
}