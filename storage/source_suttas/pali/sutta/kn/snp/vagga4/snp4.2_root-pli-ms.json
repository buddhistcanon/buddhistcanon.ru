{
  "snp4.2:0.1": "Sutta Nipāta 4.2",
  "snp4.2:0.2": "Guhaṭṭhakasutta",
  "snp4.2:1.1": "Satto guhāyaṃ bahunābhichanno,",
  "snp4.2:1.2": "Tiṭṭhaṃ naro mohanasmiṃ pagāḷho;",
  "snp4.2:1.3": "Dūre vivekā hi tathāvidho so,",
  "snp4.2:1.4": "Kāmā hi loke na hi suppahāyā.",
  "snp4.2:2.1": "Icchānidānā bhavasātabaddhā,",
  "snp4.2:2.2": "Te duppamuñcā na hi aññamokkhā;",
  "snp4.2:2.3": "Pacchā pure vāpi apekkhamānā,",
  "snp4.2:2.4": "Ime va kāme purime va jappaṃ.",
  "snp4.2:3.1": "Kāmesu giddhā pasutā pamūḷhā,",
  "snp4.2:3.2": "Avadāniyā te visame niviṭṭhā;",
  "snp4.2:3.3": "Dukkhūpanītā paridevayanti,",
  "snp4.2:3.4": "Kiṃsū bhavissāma ito cutāse.",
  "snp4.2:4.1": "Tasmā hi sikkhetha idheva jantu,",
  "snp4.2:4.2": "Yaṃ kiñci jaññā visamanti loke;",
  "snp4.2:4.3": "Na tassa hetū visamaṃ careyya,",
  "snp4.2:4.4": "Appañhidaṃ jīvitamāhu dhīrā.",
  "snp4.2:5.1": "Passāmi loke pariphandamānaṃ,",
  "snp4.2:5.2": "Pajaṃ imaṃ taṇhagataṃ bhavesu;",
  "snp4.2:5.3": "Hīnā narā maccumukhe lapanti,",
  "snp4.2:5.4": "Avītataṇhāse bhavābhavesu.",
  "snp4.2:6.1": "Mamāyite passatha phandamāne,",
  "snp4.2:6.2": "Maccheva appodake khīṇasote;",
  "snp4.2:6.3": "Etampi disvā amamo careyya,",
  "snp4.2:6.4": "Bhavesu āsattimakubbamāno.",
  "snp4.2:7.1": "Ubhosu antesu vineyya chandaṃ,",
  "snp4.2:7.2": "Phassaṃ pariññāya anānugiddho;",
  "snp4.2:7.3": "Yadattagarahī tadakubbamāno,",
  "snp4.2:7.4": "Na lippatī diṭṭhasutesu dhīro.",
  "snp4.2:8.1": "Saññaṃ pariññā vitareyya oghaṃ,",
  "snp4.2:8.2": "Pariggahesu muni nopalitto;",
  "snp4.2:8.3": "Abbūḷhasallo caramappamatto,",
  "snp4.2:8.4": "Nāsīsatī lokamimaṃ parañcāti.",
  "snp4.2:8.5": "Guhaṭṭhakasuttaṃ dutiyaṃ."
}