{
  "snp4.6:0.1": "Sutta Nipāta 4.6",
  "snp4.6:0.2": "Jarāsutta",
  "snp4.6:1.1": "Appaṃ vata jīvitaṃ idaṃ,",
  "snp4.6:1.2": "Oraṃ vassasatāpi miyyati;",
  "snp4.6:1.3": "Yo cepi aticca jīvati,",
  "snp4.6:1.4": "Atha kho so jarasāpi miyyati.",
  "snp4.6:2.1": "Socanti janā mamāyite,",
  "snp4.6:2.2": "Na hi santi niccā pariggahā;",
  "snp4.6:2.3": "Vinābhāvasantamevidaṃ,",
  "snp4.6:2.4": "Iti disvā nāgāramāvase.",
  "snp4.6:3.1": "Maraṇenapi taṃ pahīyati,",
  "snp4.6:3.2": "Yaṃ puriso mamidanti maññati;",
  "snp4.6:3.3": "Etampi viditvā paṇḍito,",
  "snp4.6:3.4": "Na mamattāya nametha māmako.",
  "snp4.6:4.1": "Supinena yathāpi saṅgataṃ,",
  "snp4.6:4.2": "Paṭibuddho puriso na passati;",
  "snp4.6:4.3": "Evampi piyāyitaṃ janaṃ,",
  "snp4.6:4.4": "Petaṃ kālaṅkataṃ na passati.",
  "snp4.6:5.1": "Diṭṭhāpi sutāpi te janā,",
  "snp4.6:5.2": "Yesaṃ nāmamidaṃ pavuccati;",
  "snp4.6:5.3": "Nāmaṃyevāvasissati,",
  "snp4.6:5.4": "Akkheyyaṃ petassa jantuno.",
  "snp4.6:6.1": "Sokapparidevamaccharaṃ,",
  "snp4.6:6.2": "Na jahanti giddhā mamāyite;",
  "snp4.6:6.3": "Tasmā munayo pariggahaṃ,",
  "snp4.6:6.4": "Hitvā acariṃsu khemadassino.",
  "snp4.6:7.1": "Patilīnacarassa bhikkhuno,",
  "snp4.6:7.2": "Bhajamānassa vivittamāsanaṃ;",
  "snp4.6:7.3": "Sāmaggiyamāhu tassa taṃ,",
  "snp4.6:7.4": "Yo attānaṃ bhavane na dassaye.",
  "snp4.6:8.1": "Sabbattha munī anissito,",
  "snp4.6:8.2": "Na piyaṃ kubbati nopi appiyaṃ;",
  "snp4.6:8.3": "Tasmiṃ paridevamaccharaṃ,",
  "snp4.6:8.4": "Paṇṇe vāri yathā na limpati.",
  "snp4.6:9.1": "Udabindu yathāpi pokkhare,",
  "snp4.6:9.2": "Padume vāri yathā na limpati;",
  "snp4.6:9.3": "Evaṃ muni nopalimpati,",
  "snp4.6:9.4": "Yadidaṃ diṭṭhasutaṃ mutesu vā.",
  "snp4.6:10.1": "Dhono na hi tena maññati,",
  "snp4.6:10.2": "Yadidaṃ diṭṭhasutaṃ mutesu vā;",
  "snp4.6:10.3": "Nāññena visuddhimicchati,",
  "snp4.6:10.4": "Na hi so rajjati no virajjatīti.",
  "snp4.6:10.5": "Jarāsuttaṃ chaṭṭhaṃ."
}