{
  "kp1:0.1": "Khuddakapāṭha",
  "kp1:0.2": "1. Saraṇattaya",
  "kp1:1.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "kp1:2.1": "Buddhaṃ saraṇaṃ gacchāmi,",
  "kp1:2.2": "Dhammaṃ saraṇaṃ gacchāmi,",
  "kp1:2.3": "Saṃghaṃ saraṇaṃ gacchāmi.",
  "kp1:3.1": "Dutiyampi buddhaṃ saraṇaṃ gacchāmi,",
  "kp1:3.2": "Dutiyampi dhammaṃ saraṇaṃ gacchāmi,",
  "kp1:3.3": "Dutiyampi saṃghaṃ saraṇaṃ gacchāmi.",
  "kp1:4.1": "Tatiyampi buddhaṃ saraṇaṃ gacchāmi,",
  "kp1:4.2": "Tatiyampi dhammaṃ saraṇaṃ gacchāmi,",
  "kp1:4.3": "Tatiyampi saṃghaṃ saraṇaṃ gacchāmi.",
  "kp1:5.1": "Saraṇattayaṃ."
}