{
  "kv14.5:0.1": "Kathāvatthu",
  "kv14.5:0.2": "Mahāpaṇṇāsaka",
  "kv14.5:0.3": "Cuddasamavagga",
  "kv14.5:0.4": "Aññoanusayotikathā",
  "kv14.5:1.1": "☑ Añño kāmarāgānusayo aññaṃ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.2": "Āmantā.",
  "kv14.5:1.3": "Añño kāmarāgo aññaṃ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:1.5": "sveva kāmarāgo taṃ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.6": "Āmantā.",
  "kv14.5:1.7": "Sveva kāmarāgānusayo taṃ kāmarāgapariyuṭṭhānanti?",
  "kv14.5:1.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:2.1": "☑ Añño paṭighānusayo aññaṃ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.2": "Āmantā.",
  "kv14.5:2.3": "Aññaṃ paṭighaṃ aññaṃ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:2.5": "taññeva paṭighaṃ taṃ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.6": "Āmantā.",
  "kv14.5:2.7": "Sveva paṭighānusayo taṃ paṭighapariyuṭṭhānanti?",
  "kv14.5:2.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:3.1": "☑ Añño mānānusayo aññaṃ mānapariyuṭṭhānanti?",
  "kv14.5:3.2": "Āmantā.",
  "kv14.5:3.3": "Añño māno aññaṃ mānapariyuṭṭhānanti?",
  "kv14.5:3.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:3.5": "sveva māno taṃ mānapariyuṭṭhānanti?",
  "kv14.5:3.6": "Āmantā.",
  "kv14.5:3.7": "Sveva mānānusayo taṃ mānapariyuṭṭhānanti?",
  "kv14.5:3.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:4.1": "☑ Añño diṭṭhānusayo aññaṃ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.2": "Āmantā.",
  "kv14.5:4.3": "Aññā diṭṭhi aññaṃ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:4.5": "sāva diṭṭhi taṃ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.6": "Āmantā.",
  "kv14.5:4.7": "Sveva diṭṭhānusayo taṃ diṭṭhipariyuṭṭhānanti?",
  "kv14.5:4.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:5.1": "☑ Añño vicikicchānusayo aññaṃ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.2": "Āmantā.",
  "kv14.5:5.3": "Aññā vicikicchā aññaṃ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:5.5": "sāva vicikicchā taṃ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.6": "Āmantā.",
  "kv14.5:5.7": "Sveva vicikicchānusayo taṃ vicikicchāpariyuṭṭhānanti?",
  "kv14.5:5.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:6.1": "☑ Añño bhavarāgānusayo aññaṃ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.2": "Āmantā.",
  "kv14.5:6.3": "Añño bhavarāgo aññaṃ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:6.5": "sveva bhavarāgo taṃ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.6": "Āmantā.",
  "kv14.5:6.7": "Sveva bhavarāgānusayo taṃ bhavarāgapariyuṭṭhānanti?",
  "kv14.5:6.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:7.1": "☑ Añño avijjānusayo aññaṃ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.2": "Āmantā.",
  "kv14.5:7.3": "Aññā avijjā aññaṃ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.4": "Na hevaṃ vattabbe …pe…",
  "kv14.5:7.5": "sāva avijjā taṃ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.6": "Āmantā.",
  "kv14.5:7.7": "Sveva avijjānusayo taṃ avijjāpariyuṭṭhānanti?",
  "kv14.5:7.8": "Na hevaṃ vattabbe …pe….",
  "kv14.5:8.1": "☒ Na vattabbaṃ—",
  "kv14.5:8.2": "“añño anusayo aññaṃ pariyuṭṭhānan”ti?",
  "kv14.5:8.3": "Āmantā.",
  "kv14.5:8.4": "Puthujjano kusalābyākate citte vattamāne “sānusayo”ti vattabboti?",
  "kv14.5:8.5": "Āmantā.",
  "kv14.5:8.6": "“Pariyuṭṭhito”ti vattabboti?",
  "kv14.5:8.7": "Na hevaṃ vattabbe.",
  "kv14.5:8.8": "Tena hi añño anusayo aññaṃ pariyuṭṭhānanti.",
  "kv14.5:8.9": "Puthujjano kusalābyākate citte vattamāne “sarāgo”ti vattabboti?",
  "kv14.5:8.10": "Āmantā.",
  "kv14.5:8.11": "“Pariyuṭṭhito”ti vattabboti?",
  "kv14.5:8.12": "Na hevaṃ vattabbe.",
  "kv14.5:8.13": "Tena hi añño rāgo aññaṃ pariyuṭṭhānanti.",
  "kv14.5:8.14": "Añño anusayotikathā niṭṭhitā."
}