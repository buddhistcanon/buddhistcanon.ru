{
  "kv14.2:0.1": "Kathāvatthu",
  "kv14.2:0.2": "Mahāpaṇṇāsaka",
  "kv14.2:0.3": "Cuddasamavagga",
  "kv14.2:0.4": "Saḷāyatanuppattikathā",
  "kv14.2:1.1": "☑ Saḷāyatanaṃ apubbaṃ acarimaṃ mātukucchismiṃ saṇṭhātīti?",
  "kv14.2:1.2": "Āmantā.",
  "kv14.2:1.3": "Sabbaṅgapaccaṅgī ahīnindriyo mātukucchismiṃ okkamatīti?",
  "kv14.2:1.4": "Na hevaṃ vattabbe …pe….",
  "kv14.2:2.1": "☑ Upapattesiyena cittena cakkhāyatanaṃ saṇṭhātīti?",
  "kv14.2:2.2": "Āmantā.",
  "kv14.2:2.3": "Upapattesiyena cittena hatthā saṇṭhanti, pādā saṇṭhanti, sīsaṃ saṇṭhāti, kaṇṇo saṇṭhāti, nāsikā saṇṭhāti, mukhaṃ saṇṭhāti, dantā saṇṭhantīti?",
  "kv14.2:2.4": "Na hevaṃ vattabbe …pe….",
  "kv14.2:3.1": "☑ Upapattesiyena cittena sotāyatanaṃ …pe…",
  "kv14.2:3.2": "ghānāyatanaṃ …pe…",
  "kv14.2:3.3": "jivhāyatanaṃ saṇṭhātīti?",
  "kv14.2:3.4": "Āmantā.",
  "kv14.2:3.5": "Upapattesiyena cittena hatthā saṇṭhanti, pādā saṇṭhanti, sīsaṃ saṇṭhāti, kaṇṇo saṇṭhāti, nāsikā saṇṭhāti, mukhaṃ saṇṭhāti, dantā saṇṭhantīti?",
  "kv14.2:3.6": "Na hevaṃ vattabbe …pe….",
  "kv14.2:4.1": "☒ Mātukucchigatassa pacchā cakkhāyatanaṃ uppajjatīti?",
  "kv14.2:4.2": "Āmantā.",
  "kv14.2:4.3": "Mātukucchismiṃ cakkhupaṭilābhāya kammaṃ karotīti?",
  "kv14.2:4.4": "Na hevaṃ vattabbe …pe…",
  "kv14.2:4.5": "mātukucchigatassa pacchā sotāyatanaṃ …pe…",
  "kv14.2:4.6": "ghānāyatanaṃ …pe…",
  "kv14.2:4.7": "jivhāyatanaṃ uppajjatīti?",
  "kv14.2:4.8": "Āmantā.",
  "kv14.2:4.9": "Mātukucchismiṃ jivhāpaṭilābhāya kammaṃ karotīti?",
  "kv14.2:4.10": "Na hevaṃ vattabbe …pe….",
  "kv14.2:5.1": "☒ Mātukucchigatassa pacchā kesā lomā nakhā dantā aṭṭhī uppajjantīti?",
  "kv14.2:5.2": "Āmantā.",
  "kv14.2:5.3": "Mātukucchismiṃ aṭṭhipaṭilābhāya kammaṃ karotīti?",
  "kv14.2:5.4": "Na hevaṃ vattabbe …pe….",
  "kv14.2:6.1": "☑ Na vattabbaṃ—",
  "kv14.2:6.2": "“mātukucchigatassa pacchā kesā lomā nakhā dantā aṭṭhī uppajjantī”ti?",
  "kv14.2:6.3": "Āmantā.",
  "kv14.2:6.4": "Nanu vuttaṃ bhagavatā—",
  "kv14.2:7.1": "“Paṭhamaṃ kalalaṃ hoti,",
  "kv14.2:7.2": "kalalā hoti abbudaṃ;",
  "kv14.2:7.3": "Abbudā jāyate pesi,",
  "kv14.2:7.4": "pesi nibbattate ghano;",
  "kv14.2:7.5": "Ghanā pasākhā jāyanti,",
  "kv14.2:7.6": "kesā lomā nakhāpi ca.",
  "kv14.2:8.1": "Yañcassa bhuñjati mātā,",
  "kv14.2:8.2": "annaṃ pānañca bhojanaṃ;",
  "kv14.2:8.3": "Tena so tattha yāpeti,",
  "kv14.2:8.4": "mātukucchigato naro”ti.",
  "kv14.2:9.1": "Attheva suttantoti?",
  "kv14.2:9.2": "Āmantā.",
  "kv14.2:9.3": "Tena hi mātukucchigatassa pacchā kesā lomā nakhā dantā aṭṭhī uppajjantīti.",
  "kv14.2:9.4": "Saḷāyatanuppattikathā niṭṭhitā."
}