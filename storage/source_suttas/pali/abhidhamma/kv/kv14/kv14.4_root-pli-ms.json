{
  "kv14.4:0.1": "Kathāvatthu",
  "kv14.4:0.2": "Mahāpaṇṇāsaka",
  "kv14.4:0.3": "Cuddasamavagga",
  "kv14.4:0.4": "Ariyarūpakathā",
  "kv14.4:1.1": "☑ Ariyarūpaṃ mahābhūtānaṃ upādāyāti?",
  "kv14.4:1.2": "Āmantā.",
  "kv14.4:1.3": "Ariyarūpaṃ kusalanti?",
  "kv14.4:1.4": "Āmantā.",
  "kv14.4:1.5": "Mahābhūtā kusalāti?",
  "kv14.4:1.6": "Na hevaṃ vattabbe …pe…",
  "kv14.4:1.7": "mahābhūtā abyākatāti?",
  "kv14.4:1.8": "Āmantā.",
  "kv14.4:1.9": "Ariyarūpaṃ abyākatanti?",
  "kv14.4:1.10": "Na hevaṃ vattabbe …pe…",
  "kv14.4:1.11": "ariyarūpaṃ mahābhūtānaṃ upādāyāti?",
  "kv14.4:1.12": "Āmantā.",
  "kv14.4:1.13": "Ariyarūpaṃ anāsavaṃ asaṃyojaniyaṃ aganthaniyaṃ anoghaniyaṃ ayoganiyaṃ anīvaraṇiyaṃ aparāmaṭṭhaṃ anupādāniyaṃ asaṃkilesiyanti?",
  "kv14.4:1.14": "Āmantā.",
  "kv14.4:1.15": "Mahābhūtā anāsavā …pe… asaṃkilesiyāti?",
  "kv14.4:1.16": "Na hevaṃ vattabbe …pe…",
  "kv14.4:1.17": "mahābhūtā sāsavā saṃyojaniyā …pe… saṃkilesiyāti?",
  "kv14.4:1.18": "Āmantā.",
  "kv14.4:1.19": "Ariyarūpaṃ sāsavaṃ saṃyojaniyaṃ …pe… saṃkilesiyanti?",
  "kv14.4:1.20": "Na hevaṃ vattabbe …pe….",
  "kv14.4:2.1": "☒ Na vattabbaṃ—",
  "kv14.4:2.2": "“ariyarūpaṃ mahābhūtānaṃ upādāyā”ti?",
  "kv14.4:2.3": "Āmantā.",
  "kv14.4:2.4": "Nanu vuttaṃ bhagavatā—",
  "kv14.4:2.5": "“yaṃ kiñci, bhikkhave, rūpaṃ cattāri mahābhūtāni catunnañca mahābhūtānaṃ upādāyarūpan”ti.",
  "kv14.4:2.6": "Attheva suttantoti?",
  "kv14.4:2.7": "Āmantā.",
  "kv14.4:2.8": "Tena hi ariyarūpaṃ mahābhūtānaṃ upādāyāti.",
  "kv14.4:2.9": "Ariyarūpakathā niṭṭhitā."
}