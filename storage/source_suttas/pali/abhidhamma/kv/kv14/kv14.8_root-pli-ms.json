{
  "kv14.8:0.1": "Kathāvatthu",
  "kv14.8:0.2": "Mahāpaṇṇāsaka",
  "kv14.8:0.3": "Cuddasamavagga",
  "kv14.8:0.4": "Abyākatakathā",
  "kv14.8:1.1": "☑ Diṭṭhigataṃ abyākatanti?",
  "kv14.8:1.2": "Āmantā.",
  "kv14.8:1.3": "Vipākābyākataṃ kiriyābyākataṃ rūpaṃ nibbānaṃ cakkhāyatanaṃ …pe…",
  "kv14.8:1.4": "phoṭṭhabbāyatananti?",
  "kv14.8:1.5": "Na hevaṃ vattabbe …pe…",
  "kv14.8:1.6": "diṭṭhigataṃ abyākatanti?",
  "kv14.8:1.7": "Āmantā.",
  "kv14.8:1.8": "Diṭṭhigatasampayutto phasso abyākatoti?",
  "kv14.8:1.9": "Na hevaṃ vattabbe …pe…",
  "kv14.8:1.10": "diṭṭhigataṃ abyākatanti?",
  "kv14.8:1.11": "Āmantā.",
  "kv14.8:1.12": "Diṭṭhigatasampayuttā vedanā …pe…",
  "kv14.8:1.13": "saññā …pe…",
  "kv14.8:1.14": "cetanā …pe…",
  "kv14.8:1.15": "cittaṃ abyākatanti?",
  "kv14.8:1.16": "Na hevaṃ vattabbe …pe….",
  "kv14.8:2.1": "☑ Diṭṭhigatasampayutto phasso akusaloti?",
  "kv14.8:2.2": "Āmantā.",
  "kv14.8:2.3": "Diṭṭhigataṃ akusalanti?",
  "kv14.8:2.4": "Na hevaṃ vattabbe …pe…",
  "kv14.8:2.5": "diṭṭhigatasampayuttā vedanā saññā cetanā cittaṃ akusalanti?",
  "kv14.8:2.6": "Āmantā.",
  "kv14.8:2.7": "Diṭṭhigataṃ akusalanti?",
  "kv14.8:2.8": "Na hevaṃ vattabbe …pe….",
  "kv14.8:3.1": "☑ Diṭṭhigataṃ abyākatanti?",
  "kv14.8:3.2": "Āmantā.",
  "kv14.8:3.3": "Aphalaṃ avipākanti?",
  "kv14.8:3.4": "Na hevaṃ vattabbe …pe…",
  "kv14.8:3.5": "nanu saphalaṃ savipākanti?",
  "kv14.8:3.6": "Āmantā.",
  "kv14.8:3.7": "Hañci saphalaṃ savipākaṃ, no ca vata re vattabbe—",
  "kv14.8:3.8": "“diṭṭhigataṃ abyākatan”ti.",
  "kv14.8:4.1": "☑ Diṭṭhigataṃ abyākatanti?",
  "kv14.8:4.2": "Āmantā.",
  "kv14.8:4.3": "Nanu micchādiṭṭhiparamāni vajjāni vuttāni bhagavatāti?",
  "kv14.8:4.4": "Āmantā.",
  "kv14.8:4.5": "Hañci micchādiṭṭhiparamāni vajjāni vuttāni bhagavatā, no ca vata re vattabbe—",
  "kv14.8:4.6": "“diṭṭhigataṃ abyākatan”ti.",
  "kv14.8:5.1": "☑ Diṭṭhigataṃ abyākatanti?",
  "kv14.8:5.2": "Āmantā.",
  "kv14.8:5.3": "Nanu vuttaṃ bhagavatā—",
  "kv14.8:5.4": "“micchādiṭṭhi kho, vaccha, akusalā, sammādiṭṭhi kusalā”ti.",
  "kv14.8:5.5": "Attheva suttantoti?",
  "kv14.8:5.6": "Āmantā.",
  "kv14.8:5.7": "Tena hi na vattabbaṃ—",
  "kv14.8:5.8": "“diṭṭhigataṃ abyākatan”ti.",
  "kv14.8:6.1": "☑ Diṭṭhigataṃ abyākatanti?",
  "kv14.8:6.2": "Āmantā.",
  "kv14.8:6.3": "Nanu vuttaṃ bhagavatā—",
  "kv14.8:6.4": "“micchādiṭṭhissa kho ahaṃ, puṇṇa, dvinnaṃ gatīnaṃ aññataraṃ gatiṃ vadāmi—",
  "kv14.8:6.5": "nirayaṃ vā tiracchānayoniṃ vā”ti.",
  "kv14.8:6.6": "Attheva suttantoti?",
  "kv14.8:6.7": "Āmantā.",
  "kv14.8:6.8": "Tena hi na vattabbaṃ—",
  "kv14.8:6.9": "“diṭṭhigataṃ abyākatan”ti.",
  "kv14.8:7.1": "☒ Na vattabbaṃ—",
  "kv14.8:7.2": "“diṭṭhigataṃ abyākatan”ti?",
  "kv14.8:7.3": "Āmantā.",
  "kv14.8:7.4": "Nanu vuttaṃ bhagavatā—",
  "kv14.8:7.5": "“‘sassato loko’ti kho, vaccha, abyākatametaṃ,",
  "kv14.8:7.6": "‘asassato loko’ti kho, vaccha, abyākatametaṃ,",
  "kv14.8:7.7": "‘antavā loko’ti kho, vaccha, abyākatametaṃ,",
  "kv14.8:7.8": "‘anantavā loko’ti kho, vaccha …pe…",
  "kv14.8:7.9": "‘taṃ jīvaṃ taṃ sarīran’ti kho, vaccha …pe…",
  "kv14.8:7.10": "‘aññaṃ jīvaṃ aññaṃ sarīran’ti kho, vaccha …pe…",
  "kv14.8:7.11": "‘hoti tathāgato paraṃ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.12": "‘na hoti tathāgato paraṃ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.13": "‘hoti ca na ca hoti tathāgato paraṃ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.14": "‘neva hoti na na hoti tathāgato paraṃ maraṇā’ti kho, vaccha, abyākatametan”ti.",
  "kv14.8:7.15": "Attheva suttantoti?",
  "kv14.8:7.16": "Āmantā.",
  "kv14.8:7.17": "Tena hi diṭṭhigataṃ abyākatanti.",
  "kv14.8:8.1": "☑ Diṭṭhigataṃ abyākatanti?",
  "kv14.8:8.2": "Āmantā.",
  "kv14.8:8.3": "Nanu vuttaṃ bhagavatā—",
  "kv14.8:8.4": "“micchādiṭṭhikassa, bhikkhave, purisapuggalassa yañceva kāyakammaṃ yathādiṭṭhisamattaṃ samādinnaṃ, yañca vacīkammaṃ …pe… yañca manokammaṃ, yā ca cetanā, yā ca patthanā, yo ca paṇidhi, ye ca saṅkhārā, sabbe te dhammā aniṭṭhāya akantāya amanāpāya ahitāya dukkhāya saṃvattantī”ti.",
  "kv14.8:8.5": "Attheva suttantoti?",
  "kv14.8:8.6": "Āmantā.",
  "kv14.8:8.7": "Tena hi na vattabbaṃ—",
  "kv14.8:8.8": "“diṭṭhigataṃ abyākatan”ti.",
  "kv14.8:8.9": "Abyākatakathā niṭṭhitā."
}