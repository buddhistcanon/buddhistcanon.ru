{
  "kv14.9:0.1": "Kathāvatthu",
  "kv14.9:0.2": "Mahāpaṇṇāsaka",
  "kv14.9:0.3": "Cuddasamavagga",
  "kv14.9:0.4": "Apariyāpannakathā",
  "kv14.9:1.1": "☑ Diṭṭhigataṃ apariyāpannanti?",
  "kv14.9:1.2": "Āmantā.",
  "kv14.9:1.3": "Maggo phalaṃ nibbānaṃ, sotāpattimaggo sotāpattiphalaṃ, sakadāgāmimaggo sakadāgāmiphalaṃ, anāgāmimaggo anāgāmiphalaṃ, arahattamaggo arahattaphalaṃ, satipaṭṭhānaṃ sammappadhānaṃ iddhipādo indriyaṃ balaṃ bojjhaṅgoti?",
  "kv14.9:1.4": "Na hevaṃ vattabbe …pe….",
  "kv14.9:2.1": "☒ Na vattabbaṃ—",
  "kv14.9:2.2": "“diṭṭhigataṃ apariyāpannan”ti?",
  "kv14.9:2.3": "Āmantā.",
  "kv14.9:2.4": "Puthujjano “kāmesu vītarāgo”ti vattabboti?",
  "kv14.9:2.5": "Āmantā.",
  "kv14.9:2.6": "“Vigatadiṭṭhiyo”ti vattabboti?",
  "kv14.9:2.7": "Na hevaṃ vattabbe.",
  "kv14.9:2.8": "Tena hi diṭṭhigataṃ apariyāpannanti.",
  "kv14.9:2.9": "Apariyāpannakathā niṭṭhitā.",
  "kv14.9:2.10": "Cuddasamo vaggo.",
  "kv14.9:2.11": "Tassuddānaṃ",
  "kv14.9:3.1": "Akusalamūlaṃ paṭisandahati kusalamūlaṃ,",
  "kv14.9:3.2": "kusalamūlaṃ paṭisandahati akusalamūlaṃ,",
  "kv14.9:3.3": "saḷāyatanaṃ chaviññāṇakāyā,",
  "kv14.9:3.4": "ariyarūpaṃ mahābhūtānaṃ upādāya,",
  "kv14.9:3.5": "sveva anusayo taṃ pariyuṭṭhānaṃ,",
  "kv14.9:3.6": "pariyuṭṭhānaṃ cittavippayuttaṃ,",
  "kv14.9:3.7": "yathādhātu taññeva anuseti,",
  "kv14.9:3.8": "diṭṭhigataṃ abyākataṃ,",
  "kv14.9:3.9": "diṭṭhigataṃ apariyāpannanti."
}