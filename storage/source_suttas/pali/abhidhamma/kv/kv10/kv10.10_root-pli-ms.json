{
  "kv10.10:0.1": "Kathāvatthu",
  "kv10.10:0.2": "Mahāpaṇṇāsaka",
  "kv10.10:0.3": "Dasamavagga",
  "kv10.10:0.4": "Viññattisīlantikathā",
  "kv10.10:1.1": "☑ Viññatti sīlanti?",
  "kv10.10:1.2": "Āmantā.",
  "kv10.10:1.3": "Pāṇātipātā veramaṇīti?",
  "kv10.10:1.4": "Na hevaṃ vattabbe …pe…",
  "kv10.10:1.5": "adinnādānā veramaṇīti?",
  "kv10.10:1.6": "Na hevaṃ vattabbe …pe…",
  "kv10.10:1.7": "kāmesumicchācārā veramaṇīti?",
  "kv10.10:1.8": "Na hevaṃ vattabbe …pe…",
  "kv10.10:1.9": "musāvādā veramaṇīti?",
  "kv10.10:1.10": "Na hevaṃ vattabbe …pe…",
  "kv10.10:1.11": "surāmerayamajjapamādaṭṭhānā veramaṇīti?",
  "kv10.10:1.12": "Na hevaṃ vattabbe …pe….",
  "kv10.10:2.1": "☑ Abhivādanaṃ sīlaṃ, paccuṭṭhānaṃ sīlaṃ, añjalikammaṃ sīlaṃ, sāmīcikammaṃ sīlaṃ, āsanābhihāro sīlaṃ, seyyābhihāro sīlaṃ, pādodakābhihāro sīlaṃ, pādakathalikābhihāro sīlaṃ, nhāne piṭṭhiparikammaṃ sīlanti?",
  "kv10.10:2.2": "Āmantā.",
  "kv10.10:2.3": "Pāṇātipātā veramaṇīti?",
  "kv10.10:2.4": "Na hevaṃ vattabbe …pe…",
  "kv10.10:2.5": "surāmerayamajjapamādaṭṭhānā veramaṇīti?",
  "kv10.10:2.6": "Na hevaṃ vattabbe …pe….",
  "kv10.10:3.1": "☒ Na vattabbaṃ—",
  "kv10.10:3.2": "“viññatti sīlan”ti?",
  "kv10.10:3.3": "Āmantā.",
  "kv10.10:3.4": "Dussilyanti?",
  "kv10.10:3.5": "Na hevaṃ vattabbe …pe…",
  "kv10.10:3.6": "tena hi viññatti sīlanti.",
  "kv10.10:3.7": "Viññatti sīlantikathā niṭṭhitā."
}