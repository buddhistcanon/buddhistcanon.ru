{
  "kv16.1:0.1": "Kathāvatthu",
  "kv16.1:0.2": "Niggahapaṇṇāsaka",
  "kv16.1:0.3": "Soḷasamavagga",
  "kv16.1:0.4": "Niggahakathā",
  "kv16.1:1.1": "☑ Paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:1.2": "Āmantā.",
  "kv16.1:1.3": "Paro parassa cittaṃ “mā rajji”, “mā dussi”, “mā muyhi”, “mā kilissī”ti niggaṇhātīti?",
  "kv16.1:1.4": "Na hevaṃ vattabbe …pe…",
  "kv16.1:1.5": "paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:1.6": "Āmantā.",
  "kv16.1:1.7": "Paro parassa uppanno phasso “mā nirujjhī”ti niggaṇhātīti?",
  "kv16.1:1.8": "Na hevaṃ vattabbe …pe…",
  "kv16.1:1.9": "paro parassa uppannā vedanā …pe…",
  "kv16.1:1.10": "uppannā saññā …",
  "kv16.1:1.11": "uppannā cetanā …",
  "kv16.1:1.12": "uppannaṃ cittaṃ …",
  "kv16.1:1.13": "uppannā saddhā …",
  "kv16.1:1.14": "uppannaṃ vīriyaṃ …",
  "kv16.1:1.15": "uppannā sati …",
  "kv16.1:1.16": "uppanno samādhi …pe…",
  "kv16.1:1.17": "uppannā paññā “mā nirujjhī”ti niggaṇhātīti?",
  "kv16.1:1.18": "Na hevaṃ vattabbe …pe….",
  "kv16.1:2.1": "☑ Paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:2.2": "Āmantā.",
  "kv16.1:2.3": "Paro parassa atthāya rāgaṃ pajahati …",
  "kv16.1:2.4": "dosaṃ pajahati …pe…",
  "kv16.1:2.5": "anottappaṃ pajahatīti?",
  "kv16.1:2.6": "Na hevaṃ vattabbe …pe….",
  "kv16.1:3.1": "☑ Paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:3.2": "Āmantā.",
  "kv16.1:3.3": "Paro parassa atthāya maggaṃ bhāveti …",
  "kv16.1:3.4": "satipaṭṭhānaṃ bhāveti …pe…",
  "kv16.1:3.5": "bojjhaṅgaṃ bhāvetīti?",
  "kv16.1:3.6": "Na hevaṃ vattabbe …pe….",
  "kv16.1:4.1": "☑ Paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:4.2": "Āmantā.",
  "kv16.1:4.3": "Paro parassa atthāya dukkhaṃ parijānāti, samudayaṃ pajahati, nirodhaṃ sacchikaroti, maggaṃ bhāvetīti?",
  "kv16.1:4.4": "Na hevaṃ vattabbe …pe….",
  "kv16.1:5.1": "☑ Paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:5.2": "Āmantā.",
  "kv16.1:5.3": "Añño aññassa kārako, paraṃkataṃ sukhaṃ dukkhaṃ añño karoti añño paṭisaṃvedetīti?",
  "kv16.1:5.4": "Na hevaṃ vattabbe …pe….",
  "kv16.1:6.1": "☑ Paro parassa cittaṃ niggaṇhātīti?",
  "kv16.1:6.2": "Āmantā.",
  "kv16.1:6.3": "Nanu vuttaṃ bhagavatā—",
  "kv16.1:7.1": "“Attanāva kataṃ pāpaṃ,",
  "kv16.1:7.2": "attanā saṅkilissati;",
  "kv16.1:7.3": "Attanā akataṃ pāpaṃ,",
  "kv16.1:7.4": "attanāva visujjhati;",
  "kv16.1:7.5": "Suddhi asuddhi paccattaṃ,",
  "kv16.1:7.6": "nāñño aññaṃ visodhaye”ti.",
  "kv16.1:8.1": "Attheva suttantoti?",
  "kv16.1:8.2": "Āmantā.",
  "kv16.1:8.3": "Tena hi na vattabbaṃ—",
  "kv16.1:8.4": "“paro parassa cittaṃ niggaṇhātī”ti.",
  "kv16.1:9.1": "☒ Na vattabbaṃ—",
  "kv16.1:9.2": "“paro parassa cittaṃ niggaṇhātī”ti?",
  "kv16.1:9.3": "Āmantā.",
  "kv16.1:9.4": "Nanu atthi balappattā, atthi vasībhūtāti?",
  "kv16.1:9.5": "Āmantā.",
  "kv16.1:9.6": "Hañci atthi balappattā, atthi vasībhūtā, tena vata re vattabbe—",
  "kv16.1:9.7": "“paro parassa cittaṃ niggaṇhātī”ti.",
  "kv16.1:9.8": "Niggahakathā niṭṭhitā."
}