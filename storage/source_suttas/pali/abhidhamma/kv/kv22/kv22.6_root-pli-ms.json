{
  "kv22.6:0.1": "Kathāvatthu",
  "kv22.6:0.2": "Mahāpaṇṇāsaka",
  "kv22.6:0.3": "Bāvīsatimavagga",
  "kv22.6:0.4": "Abyākatakathā",
  "kv22.6:1.1": "☑ Sabbaṃ supinagatassa cittaṃ abyākatanti?",
  "kv22.6:1.2": "Āmantā.",
  "kv22.6:1.3": "Supinantena pāṇaṃ haneyyāti?",
  "kv22.6:1.4": "Āmantā.",
  "kv22.6:1.5": "Hañci supinantena pāṇaṃ haneyya, no ca vata re vattabbe—",
  "kv22.6:1.6": "“sabbaṃ supinagatassa cittaṃ abyākatan”ti.",
  "kv22.6:2.1": "☑ Supinantena adinnaṃ ādiyeyya …pe…",
  "kv22.6:2.2": "musā bhaṇeyya,",
  "kv22.6:2.3": "pisuṇaṃ bhaṇeyya,",
  "kv22.6:2.4": "pharusaṃ bhaṇeyya,",
  "kv22.6:2.5": "samphaṃ palapeyya,",
  "kv22.6:2.6": "sandhiṃ chindeyya,",
  "kv22.6:2.7": "nillopaṃ hareyya,",
  "kv22.6:2.8": "ekāgārikaṃ kareyya,",
  "kv22.6:2.9": "paripanthe tiṭṭheyya,",
  "kv22.6:2.10": "paradāraṃ gaccheyya,",
  "kv22.6:2.11": "gāmaghātakaṃ kareyya,",
  "kv22.6:2.12": "nigamaghātakaṃ kareyya,",
  "kv22.6:2.13": "supinantena methunaṃ dhammaṃ paṭiseveyya,",
  "kv22.6:2.14": "supinagatassa asuci mucceyya,",
  "kv22.6:2.15": "supinantena dānaṃ dadeyya,",
  "kv22.6:2.16": "cīvaraṃ dadeyya,",
  "kv22.6:2.17": "piṇḍapātaṃ dadeyya,",
  "kv22.6:2.18": "senāsanaṃ dadeyya,",
  "kv22.6:2.19": "gilānapaccayabhesajjaparikkhāraṃ dadeyya,",
  "kv22.6:2.20": "khādanīyaṃ dadeyya,",
  "kv22.6:2.21": "bhojanīyaṃ dadeyya,",
  "kv22.6:2.22": "pānīyaṃ dadeyya,",
  "kv22.6:2.23": "cetiyaṃ vandeyya,",
  "kv22.6:2.24": "cetiye mālaṃ āropeyya,",
  "kv22.6:2.25": "gandhaṃ āropeyya,",
  "kv22.6:2.26": "vilepanaṃ āropeyya …pe…",
  "kv22.6:2.27": "cetiyaṃ abhidakkhiṇaṃ kareyyāti?",
  "kv22.6:2.28": "Āmantā.",
  "kv22.6:2.29": "Hañci supinantena cetiyaṃ abhidakkhiṇaṃ kareyya, no ca vata re vattabbe—",
  "kv22.6:2.30": "“sabbaṃ supinagatassa cittaṃ abyākatan”ti.",
  "kv22.6:3.1": "☒ Na vattabbaṃ—",
  "kv22.6:3.2": "“sabbaṃ supinagatassa cittaṃ abyākatan”ti?",
  "kv22.6:3.3": "Āmantā.",
  "kv22.6:3.4": "Nanu supinagatassa cittaṃ abbohāriyaṃ vuttaṃ bhagavatāti?",
  "kv22.6:3.5": "Āmantā.",
  "kv22.6:3.6": "Hañci supinagatassa cittaṃ abbohāriyaṃ vuttaṃ bhagavatā, tena vata re vattabbe—",
  "kv22.6:3.7": "“sabbaṃ supinagatassa cittaṃ abyākatan”ti.",
  "kv22.6:3.8": "Abyākatakathā niṭṭhitā."
}