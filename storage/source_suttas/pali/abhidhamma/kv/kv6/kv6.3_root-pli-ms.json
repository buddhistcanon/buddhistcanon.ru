{
  "kv6.3:0.1": "Kathāvatthu",
  "kv6.3:0.2": "Mahāpaṇṇāsaka",
  "kv6.3:0.3": "Chaṭṭhavagga",
  "kv6.3:0.4": "Saccakathā",
  "kv6.3:1.1": "☑ Cattāri saccāni asaṅkhatānīti?",
  "kv6.3:1.2": "Āmantā.",
  "kv6.3:1.3": "Cattāri tāṇāni cattāri leṇāni cattāri saraṇāni cattāri parāyanāni cattāri accutāni cattāri amatāni cattāri nibbānānīti?",
  "kv6.3:1.4": "Na hevaṃ vattabbe …pe…",
  "kv6.3:1.5": "cattāri nibbānānīti?",
  "kv6.3:1.6": "Āmantā.",
  "kv6.3:1.7": "Atthi catunnaṃ nibbānānaṃ uccanīcatā hīnapaṇītatā ukkaṃsāvakaṃso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv6.3:1.8": "Na hevaṃ vattabbe …pe….",
  "kv6.3:2.1": "☑ Dukkhasaccaṃ asaṅkhatanti?",
  "kv6.3:2.2": "Āmantā.",
  "kv6.3:2.3": "Dukkhaṃ asaṅkhatanti?",
  "kv6.3:2.4": "Na hevaṃ vattabbe …pe…",
  "kv6.3:2.5": "dukkhasaccaṃ asaṅkhatanti?",
  "kv6.3:2.6": "Āmantā.",
  "kv6.3:2.7": "Kāyikaṃ dukkhaṃ cetasikaṃ dukkhaṃ sokaparidevadukkhadomanassaupāyāsā asaṅkhatāti?",
  "kv6.3:2.8": "Na hevaṃ vattabbe …pe…",
  "kv6.3:2.9": "samudayasaccaṃ asaṅkhatanti?",
  "kv6.3:2.10": "Āmantā.",
  "kv6.3:2.11": "Samudayo asaṅkhatoti?",
  "kv6.3:2.12": "Na hevaṃ vattabbe …pe…",
  "kv6.3:2.13": "samudayasaccaṃ asaṅkhatanti?",
  "kv6.3:2.14": "Āmantā.",
  "kv6.3:2.15": "Kāmataṇhā bhavataṇhā vibhavataṇhā asaṅkhatāti?",
  "kv6.3:2.16": "Na hevaṃ vattabbe …pe…",
  "kv6.3:2.17": "maggasaccaṃ asaṅkhatanti?",
  "kv6.3:2.18": "Āmantā.",
  "kv6.3:2.19": "Maggo asaṅkhatoti?",
  "kv6.3:2.20": "Na hevaṃ vattabbe …pe…",
  "kv6.3:2.21": "maggasaccaṃ asaṅkhatanti?",
  "kv6.3:2.22": "Āmantā.",
  "kv6.3:2.23": "sammādiṭṭhi …pe… sammāsamādhi asaṅkhatoti?",
  "kv6.3:2.24": "Na hevaṃ vattabbe …pe….",
  "kv6.3:3.1": "☑ Dukkhaṃ saṅkhatanti?",
  "kv6.3:3.2": "Āmantā.",
  "kv6.3:3.3": "Dukkhasaccaṃ saṅkhatanti?",
  "kv6.3:3.4": "Na hevaṃ vattabbe …pe…",
  "kv6.3:3.5": "kāyikaṃ dukkhaṃ cetasikaṃ dukkhaṃ sokaparidevadukkhadomanassaupāyāsā saṅkhatāti?",
  "kv6.3:3.6": "Āmantā.",
  "kv6.3:3.7": "Dukkhasaccaṃ saṅkhatanti?",
  "kv6.3:3.8": "Na hevaṃ vattabbe …pe…",
  "kv6.3:3.9": "samudayo saṅkhatoti?",
  "kv6.3:3.10": "Āmantā.",
  "kv6.3:3.11": "Samudayasaccaṃ saṅkhatanti?",
  "kv6.3:3.12": "Na hevaṃ vattabbe …pe…",
  "kv6.3:3.13": "kāmataṇhā bhavataṇhā vibhavataṇhā saṅkhatāti?",
  "kv6.3:3.14": "Āmantā.",
  "kv6.3:3.15": "Samudayasaccaṃ saṅkhatanti?",
  "kv6.3:3.16": "Na hevaṃ vattabbe …pe…",
  "kv6.3:3.17": "maggo saṅkhatoti?",
  "kv6.3:3.18": "Āmantā.",
  "kv6.3:3.19": "Maggasaccaṃ saṅkhatanti?",
  "kv6.3:3.20": "Na hevaṃ vattabbe …pe…",
  "kv6.3:3.21": "sammādiṭṭhi …pe… sammāsamādhi saṅkhatoti?",
  "kv6.3:3.22": "Āmantā.",
  "kv6.3:3.23": "Maggasaccaṃ saṅkhatanti?",
  "kv6.3:3.24": "Na hevaṃ vattabbe …pe….",
  "kv6.3:4.1": "☑ Nirodhasaccaṃ asaṅkhataṃ, nirodho asaṅkhatoti?",
  "kv6.3:4.2": "Āmantā.",
  "kv6.3:4.3": "Dukkhasaccaṃ asaṅkhataṃ, dukkhaṃ asaṅkhatanti?",
  "kv6.3:4.4": "Na hevaṃ vattabbe …pe…",
  "kv6.3:4.5": "nirodhasaccaṃ asaṅkhataṃ, nirodho asaṅkhatoti?",
  "kv6.3:4.6": "Āmantā.",
  "kv6.3:4.7": "Samudayasaccaṃ asaṅkhataṃ, samudayo asaṅkhatoti?",
  "kv6.3:4.8": "Na hevaṃ vattabbe …pe…",
  "kv6.3:4.9": "nirodhasaccaṃ asaṅkhataṃ, nirodho asaṅkhatoti?",
  "kv6.3:4.10": "Āmantā.",
  "kv6.3:4.11": "Maggasaccaṃ asaṅkhataṃ, maggo asaṅkhatoti?",
  "kv6.3:4.12": "Na hevaṃ vattabbe …pe….",
  "kv6.3:5.1": "☑ Dukkhasaccaṃ asaṅkhataṃ, dukkhaṃ saṅkhatanti?",
  "kv6.3:5.2": "Āmantā.",
  "kv6.3:5.3": "Nirodhasaccaṃ asaṅkhataṃ, nirodho saṅkhatoti?",
  "kv6.3:5.4": "Na hevaṃ vattabbe …pe…",
  "kv6.3:5.5": "samudayasaccaṃ asaṅkhataṃ, samudayo saṅkhatoti?",
  "kv6.3:5.6": "Āmantā.",
  "kv6.3:5.7": "Nirodhasaccaṃ asaṅkhataṃ, nirodho saṅkhatoti?",
  "kv6.3:5.8": "Na hevaṃ vattabbe …pe…",
  "kv6.3:5.9": "maggasaccaṃ asaṅkhataṃ, maggo saṅkhatoti?",
  "kv6.3:5.10": "Āmantā.",
  "kv6.3:5.11": "Nirodhasaccaṃ asaṅkhataṃ, nirodho saṅkhatoti?",
  "kv6.3:5.12": "Na hevaṃ vattabbe …pe….",
  "kv6.3:6.1": "☒ Na vattabbaṃ—",
  "kv6.3:6.2": "“cattāri saccāni asaṅkhatānī”ti?",
  "kv6.3:6.3": "Āmantā.",
  "kv6.3:6.4": "Nanu vuttaṃ bhagavatā—",
  "kv6.3:6.5": "“cattārimāni, bhikkhave, tathāni avitathāni anaññathāni.",
  "kv6.3:6.6": "Katamāni cattāri?",
  "kv6.3:6.7": "‘Idaṃ dukkhan’ti, bhikkhave, tathametaṃ avitathametaṃ anaññathametaṃ …pe…",
  "kv6.3:6.8": "‘ayaṃ dukkhasamudayo’ti …pe…",
  "kv6.3:6.9": "‘ayaṃ dukkhanirodho’ti …pe…",
  "kv6.3:6.10": "‘ayaṃ dukkhanirodhagāminī paṭipadā’ti tathametaṃ avitathametaṃ anaññathametaṃ.",
  "kv6.3:6.11": "Imāni kho, bhikkhave, cattāri tathāni avitathāni anaññathānī”ti.",
  "kv6.3:6.12": "Attheva suttantoti?",
  "kv6.3:6.13": "Āmantā.",
  "kv6.3:6.14": "Tena hi cattāri saccāni asaṅkhatānīti.",
  "kv6.3:6.15": "Saccakathā niṭṭhitā."
}