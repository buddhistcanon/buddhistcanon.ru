{
  "kv13.4:0.1": "Kathāvatthu",
  "kv13.4:0.2": "Mahāpaṇṇāsaka",
  "kv13.4:0.3": "Terasamavagga",
  "kv13.4:0.4": "Niyatassaniyāmakathā",
  "kv13.4:1.1": "☑ Niyato niyāmaṃ okkamatīti?",
  "kv13.4:1.2": "Āmantā.",
  "kv13.4:1.3": "Micchattaniyato sammattaniyāmaṃ okkamati, sammattaniyato micchattaniyāmaṃ okkamatīti?",
  "kv13.4:1.4": "Na hevaṃ vattabbe …pe….",
  "kv13.4:2.1": "☑ Niyato niyāmaṃ okkamatīti?",
  "kv13.4:2.2": "Āmantā.",
  "kv13.4:2.3": "Pubbe maggaṃ bhāvetvā pacchā niyāmaṃ okkamatīti?",
  "kv13.4:2.4": "Na hevaṃ vattabbe …pe…",
  "kv13.4:2.5": "pubbe sotāpattimaggaṃ bhāvetvā pacchā sotāpattiniyāmaṃ okkamatīti?",
  "kv13.4:2.6": "Na hevaṃ vattabbe …pe…",
  "kv13.4:2.7": "pubbe sakadāgāmi …pe…",
  "kv13.4:2.8": "anāgāmi …pe…",
  "kv13.4:2.9": "arahattamaggaṃ bhāvetvā pacchā arahattaniyāmaṃ okkamatīti?",
  "kv13.4:2.10": "Na hevaṃ vattabbe …pe….",
  "kv13.4:3.1": "☑ Pubbe satipaṭṭhānaṃ …pe…",
  "kv13.4:3.2": "sammappadhānaṃ … iddhipādaṃ … indriyaṃ … balaṃ … bojjhaṅgaṃ bhāvetvā pacchā niyāmaṃ okkamatīti?",
  "kv13.4:3.3": "Na hevaṃ vattabbe …pe….",
  "kv13.4:4.1": "☒ Na vattabbaṃ—",
  "kv13.4:4.2": "“niyato niyāmaṃ okkamatī”ti?",
  "kv13.4:4.3": "Āmantā.",
  "kv13.4:4.4": "Bhabbo bodhisatto tāya jātiyā dhammaṃ nābhisametunti?",
  "kv13.4:4.5": "Na hevaṃ vattabbe.",
  "kv13.4:4.6": "Tena hi niyato niyāmaṃ okkamatīti.",
  "kv13.4:4.7": "Niyatassa niyāmakathā niṭṭhitā."
}