{
  "kv13.2:0.1": "Kathāvatthu",
  "kv13.2:0.2": "Mahāpaṇṇāsaka",
  "kv13.2:0.3": "Terasamavagga",
  "kv13.2:0.4": "Kusalapaṭilābhakathā",
  "kv13.2:1.1": "☑ Kappaṭṭho kusalaṃ cittaṃ na paṭilabheyyāti?",
  "kv13.2:1.2": "Āmantā.",
  "kv13.2:1.3": "Kappaṭṭho dānaṃ dadeyyāti?",
  "kv13.2:1.4": "Āmantā.",
  "kv13.2:1.5": "Hañci kappaṭṭho dānaṃ dadeyya, no ca vata re vattabbe—",
  "kv13.2:1.6": "“kappaṭṭho kusalaṃ cittaṃ na paṭilabheyyā”ti.",
  "kv13.2:2.1": "☑ Kappaṭṭho kusalaṃ cittaṃ na paṭilabheyyāti?",
  "kv13.2:2.2": "Āmantā.",
  "kv13.2:2.3": "Kappaṭṭho cīvaraṃ dadeyya …pe…",
  "kv13.2:2.4": "piṇḍapātaṃ dadeyya …pe…",
  "kv13.2:2.5": "senāsanaṃ dadeyya …pe…",
  "kv13.2:2.6": "gilānapaccayabhesajjaparikkhāraṃ dadeyya …",
  "kv13.2:2.7": "khādanīyaṃ dadeyya …",
  "kv13.2:2.8": "bhojanīyaṃ dadeyya …",
  "kv13.2:2.9": "pānīyaṃ dadeyya …",
  "kv13.2:2.10": "cetiyaṃ vandeyya …",
  "kv13.2:2.11": "cetiye mālaṃ āropeyya …",
  "kv13.2:2.12": "gandhaṃ āropeyya …",
  "kv13.2:2.13": "vilepanaṃ āropeyya …pe…",
  "kv13.2:2.14": "cetiyaṃ abhidakkhiṇaṃ kareyyāti?",
  "kv13.2:2.15": "Āmantā.",
  "kv13.2:2.16": "Hañci kappaṭṭho cetiyaṃ abhidakkhiṇaṃ kareyya, no ca vata re vattabbe—",
  "kv13.2:2.17": "“kappaṭṭho kusalaṃ cittaṃ na paṭilabheyyā”ti …pe….",
  "kv13.2:3.1": "☒ Kappaṭṭho kusalaṃ cittaṃ paṭilabheyyāti?",
  "kv13.2:3.2": "Āmantā.",
  "kv13.2:3.3": "Tato vuṭṭhānaṃ kusalaṃ cittaṃ paṭilabheyyāti?",
  "kv13.2:3.4": "Āmantā.",
  "kv13.2:3.5": "Rūpāvacaraṃ …pe…",
  "kv13.2:3.6": "arūpāvacaraṃ …pe…",
  "kv13.2:3.7": "lokuttaraṃ kusalaṃ cittaṃ paṭilabheyyāti?",
  "kv13.2:3.8": "Na hevaṃ vattabbe …pe….",
  "kv13.2:3.9": "Kusalapaṭilābhakathā niṭṭhitā."
}