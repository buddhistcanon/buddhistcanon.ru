{
  "kv13.6:0.1": "Kathāvatthu",
  "kv13.6:0.2": "Mahāpaṇṇāsaka",
  "kv13.6:0.3": "Terasamavagga",
  "kv13.6:0.4": "Sammukhībhūtakathā",
  "kv13.6:1.1": "☑ Sammukhībhūto saṃyojanaṃ jahatīti?",
  "kv13.6:1.2": "Āmantā.",
  "kv13.6:1.3": "Ratto rāgaṃ jahati, duṭṭho dosaṃ jahati, mūḷho mohaṃ jahati, kiliṭṭho kilese jahatīti?",
  "kv13.6:1.4": "Na hevaṃ vattabbe …pe…",
  "kv13.6:1.5": "rāgena rāgaṃ jahati, dosena dosaṃ jahati, mohena mohaṃ jahati, kilesehi kilese jahatīti?",
  "kv13.6:1.6": "Na hevaṃ vattabbe …pe…",
  "kv13.6:2.1": "☑ Rāgo cittasampayutto, maggo cittasampayuttoti?",
  "kv13.6:2.2": "Āmantā.",
  "kv13.6:2.3": "Dvinnaṃ phassānaṃ …pe…",
  "kv13.6:2.4": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
  "kv13.6:2.5": "Na hevaṃ vattabbe …pe…",
  "kv13.6:2.6": "rāgo akusalo, maggo kusaloti?",
  "kv13.6:2.7": "Āmantā.",
  "kv13.6:2.8": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṃ āgacchantīti?",
  "kv13.6:2.9": "Na hevaṃ vattabbe …pe….",
  "kv13.6:3.1": "☑ Kusalākusalā …pe…",
  "kv13.6:3.2": "sammukhībhāvaṃ āgacchantīti?",
  "kv13.6:3.3": "Āmantā.",
  "kv13.6:3.4": "Nanu vuttaṃ bhagavatā—",
  "kv13.6:3.5": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv13.6:3.6": "Katamāni cattāri?",
  "kv13.6:3.7": "Nabhañca, bhikkhave, pathavī ca—",
  "kv13.6:3.8": "idaṃ paṭhamaṃ suvidūravidūraṃ …pe… tasmā sataṃ dhammo asabbhi ārakā”ti.",
  "kv13.6:3.9": "Attheva suttantoti?",
  "kv13.6:3.10": "Āmantā.",
  "kv13.6:3.11": "Tena hi na vattabbaṃ—",
  "kv13.6:3.12": "“kusalākusalā …pe…",
  "kv13.6:3.13": "sammukhībhāvaṃ āgacchantī”ti.",
  "kv13.6:4.1": "☑ Sammukhībhūto saṃyojanaṃ jahatīti?",
  "kv13.6:4.2": "Āmantā.",
  "kv13.6:4.3": "Nanu vuttaṃ bhagavatā—",
  "kv13.6:4.4": "“so evaṃ samāhite citte …pe…",
  "kv13.6:4.5": "āsavānaṃ khayañāṇāya cittaṃ abhininnāmetī”ti.",
  "kv13.6:4.6": "Attheva suttantoti?",
  "kv13.6:4.7": "Āmantā.",
  "kv13.6:4.8": "Tena hi na vattabbaṃ—",
  "kv13.6:4.9": "“sammukhībhūto saṃyojanaṃ jahatī”ti.",
  "kv13.6:5.1": "☒ Na vattabbaṃ—",
  "kv13.6:5.2": "“sammukhībhūto saṃyojanaṃ jahatī”ti?",
  "kv13.6:5.3": "Āmantā.",
  "kv13.6:5.4": "Nanu vuttaṃ bhagavatā—",
  "kv13.6:5.5": "“tassa evaṃ jānato evaṃ passato kāmāsavāpi cittaṃ vimuccati …pe…",
  "kv13.6:5.6": "avijjāsavāpi cittaṃ vimuccatī”ti.",
  "kv13.6:5.7": "Attheva suttantoti?",
  "kv13.6:5.8": "Āmantā.",
  "kv13.6:5.9": "Tena hi sammukhībhūto saṃyojanaṃ jahatīti.",
  "kv13.6:5.10": "Sammukhībhūtakathā niṭṭhitā."
}