{
  "kv13.5:0.1": "Kathāvatthu",
  "kv13.5:0.2": "Mahāpaṇṇāsaka",
  "kv13.5:0.3": "Terasamavagga",
  "kv13.5:0.4": "Nivutakathā",
  "kv13.5:1.1": "☑ Nivuto nīvaraṇaṃ jahatīti?",
  "kv13.5:1.2": "Āmantā.",
  "kv13.5:1.3": "Ratto rāgaṃ jahati, duṭṭho dosaṃ jahati, mūḷho mohaṃ jahati, kiliṭṭho kilese jahatīti?",
  "kv13.5:1.4": "Na hevaṃ vattabbe …pe…",
  "kv13.5:1.5": "rāgena rāgaṃ jahati, dosena dosaṃ jahati, mohena mohaṃ jahati, kilesehi kilese jahatīti?",
  "kv13.5:1.6": "Na hevaṃ vattabbe …pe….",
  "kv13.5:2.1": "☑ Rāgo cittasampayutto, maggo cittasampayuttoti?",
  "kv13.5:2.2": "Āmantā.",
  "kv13.5:2.3": "Dvinnaṃ phassānaṃ …pe…",
  "kv13.5:2.4": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
  "kv13.5:2.5": "Na hevaṃ vattabbe …pe…",
  "kv13.5:2.6": "rāgo akusalo, maggo kusaloti?",
  "kv13.5:2.7": "Āmantā.",
  "kv13.5:2.8": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṃ āgacchantīti?",
  "kv13.5:2.9": "Na hevaṃ vattabbe …pe….",
  "kv13.5:3.1": "☑ Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṃ āgacchantīti?",
  "kv13.5:3.2": "Āmantā.",
  "kv13.5:3.3": "Nanu vuttaṃ bhagavatā—",
  "kv13.5:3.4": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv13.5:3.5": "Katamāni cattāri?",
  "kv13.5:3.6": "Nabhañca, bhikkhave, pathavī ca—",
  "kv13.5:3.7": "idaṃ paṭhamaṃ suvidūravidūraṃ …pe… tasmā sataṃ dhammo asabbhi ārakā”ti.",
  "kv13.5:3.8": "Attheva suttantoti?",
  "kv13.5:3.9": "Āmantā.",
  "kv13.5:3.10": "Tena hi na vattabbaṃ—",
  "kv13.5:3.11": "“kusalākusalā …pe…",
  "kv13.5:3.12": "sammukhībhāvaṃ āgacchantī”ti.",
  "kv13.5:4.1": "☑ Nivuto nīvaraṇaṃ jahatīti?",
  "kv13.5:4.2": "Āmantā.",
  "kv13.5:4.3": "Nanu vuttaṃ bhagavatā—",
  "kv13.5:4.4": "“so evaṃ samāhite citte parisuddhe pariyodāte anaṅgaṇe vigatūpakkilese mudubhūte kammaniye ṭhite āneñjappatte āsavānaṃ khayañāṇāya cittaṃ abhininnāmetī”ti.",
  "kv13.5:4.5": "Attheva suttantoti?",
  "kv13.5:4.6": "Āmantā.",
  "kv13.5:4.7": "Tena hi na vattabbaṃ—",
  "kv13.5:4.8": "“nivuto nīvaraṇaṃ jahatī”ti …pe….",
  "kv13.5:5.1": "☑ Na vattabbaṃ—",
  "kv13.5:5.2": "“nivuto nīvaraṇaṃ jahatī”ti?",
  "kv13.5:5.3": "Āmantā.",
  "kv13.5:5.4": "Nanu vuttaṃ bhagavatā—",
  "kv13.5:5.5": "“tassa evaṃ jānato evaṃ passato kāmāsavāpi cittaṃ vimuccati …pe…",
  "kv13.5:5.6": "avijjāsavāpi cittaṃ vimuccatī”ti.",
  "kv13.5:5.7": "Attheva suttantoti?",
  "kv13.5:5.8": "Āmantā.",
  "kv13.5:5.9": "Tena hi nivuto nīvaraṇaṃ jahatīti.",
  "kv13.5:5.10": "Nivutakathā niṭṭhitā."
}