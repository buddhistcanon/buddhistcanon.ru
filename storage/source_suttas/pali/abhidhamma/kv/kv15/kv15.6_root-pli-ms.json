{
  "kv15.6:0.1": "Kathāvatthu",
  "kv15.6:0.2": "Mahāpaṇṇāsaka",
  "kv15.6:0.3": "Pannarasamavagga",
  "kv15.6:0.4": "Jarāmaraṇakathā",
  "kv15.6:1.1": "☑ Lokuttarānaṃ dhammānaṃ jarāmaraṇaṃ lokuttaranti?",
  "kv15.6:1.2": "Āmantā.",
  "kv15.6:1.3": "Maggo phalaṃ nibbānaṃ, sotāpattimaggo sotāpattiphalaṃ …pe… bojjhaṅgoti?",
  "kv15.6:1.4": "Na hevaṃ vattabbe …pe…",
  "kv15.6:1.5": "sotāpattimaggassa jarāmaraṇaṃ sotāpattimaggoti?",
  "kv15.6:1.6": "Na hevaṃ vattabbe …pe…",
  "kv15.6:1.7": "sotāpattimaggassa jarāmaraṇaṃ sotāpattimaggoti?",
  "kv15.6:1.8": "Āmantā.",
  "kv15.6:1.9": "Sotāpattiphalassa jarāmaraṇaṃ sotāpattiphalanti?",
  "kv15.6:1.10": "Na hevaṃ vattabbe …pe…",
  "kv15.6:1.11": "sakadāgāmimaggassa …pe…",
  "kv15.6:1.12": "sakadāgāmiphalassa …pe…",
  "kv15.6:1.13": "anāgāmimaggassa …pe…",
  "kv15.6:1.14": "anāgāmiphalassa …pe…",
  "kv15.6:1.15": "arahattamaggassa jarāmaraṇaṃ arahattamaggoti?",
  "kv15.6:1.16": "Na hevaṃ vattabbe …pe…",
  "kv15.6:1.17": "arahattamaggassa jarāmaraṇaṃ arahattamaggoti?",
  "kv15.6:1.18": "Āmantā.",
  "kv15.6:1.19": "Arahattaphalassa jarāmaraṇaṃ arahattaphalanti?",
  "kv15.6:1.20": "Na hevaṃ vattabbe …pe…",
  "kv15.6:1.21": "satipaṭṭhānānaṃ …",
  "kv15.6:1.22": "sammappadhānānaṃ …",
  "kv15.6:1.23": "iddhipādānaṃ …",
  "kv15.6:1.24": "indriyānaṃ …",
  "kv15.6:1.25": "balānaṃ …",
  "kv15.6:1.26": "bojjhaṅgānaṃ jarāmaraṇaṃ bojjhaṅgoti?",
  "kv15.6:1.27": "Na hevaṃ vattabbe …pe….",
  "kv15.6:2.1": "☒ Na vattabbaṃ—",
  "kv15.6:2.2": "“lokuttarānaṃ dhammānaṃ jarāmaraṇaṃ lokuttaran”ti?",
  "kv15.6:2.3": "Āmantā.",
  "kv15.6:2.4": "Lokiyanti?",
  "kv15.6:2.5": "Na hevaṃ vattabbe.",
  "kv15.6:2.6": "Tena hi lokuttaranti.",
  "kv15.6:2.7": "Jarāmaraṇakathā niṭṭhitā."
}