{
  "kv15.3:0.1": "Kathāvatthu",
  "kv15.3:0.2": "Mahāpaṇṇāsaka",
  "kv15.3:0.3": "Pannarasamavagga",
  "kv15.3:0.4": "Addhākathā",
  "kv15.3:1.1": "☑ Addhā parinipphannoti?",
  "kv15.3:1.2": "Āmantā.",
  "kv15.3:1.3": "Rūpanti?",
  "kv15.3:1.4": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.5": "vedanā …pe…",
  "kv15.3:1.6": "saññā …pe…",
  "kv15.3:1.7": "saṅkhārā …pe…",
  "kv15.3:1.8": "viññāṇanti?",
  "kv15.3:1.9": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.10": "atīto addhā parinipphannoti?",
  "kv15.3:1.11": "Āmantā.",
  "kv15.3:1.12": "Rūpanti?",
  "kv15.3:1.13": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.14": "vedanā …pe…",
  "kv15.3:1.15": "saññā …pe…",
  "kv15.3:1.16": "saṅkhārā …pe…",
  "kv15.3:1.17": "viññāṇanti?",
  "kv15.3:1.18": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.19": "anāgato addhā parinipphannoti?",
  "kv15.3:1.20": "Āmantā.",
  "kv15.3:1.21": "Rūpanti?",
  "kv15.3:1.22": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.23": "vedanā …pe…",
  "kv15.3:1.24": "saññā …pe…",
  "kv15.3:1.25": "saṅkhārā …pe…",
  "kv15.3:1.26": "viññāṇanti?",
  "kv15.3:1.27": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.28": "paccuppanno addhā parinipphannoti?",
  "kv15.3:1.29": "Āmantā.",
  "kv15.3:1.30": "Rūpanti?",
  "kv15.3:1.31": "Na hevaṃ vattabbe …pe…",
  "kv15.3:1.32": "vedanā …pe…",
  "kv15.3:1.33": "saññā …pe…",
  "kv15.3:1.34": "saṅkhārā …pe…",
  "kv15.3:1.35": "viññāṇanti?",
  "kv15.3:1.36": "Na hevaṃ vattabbe …pe….",
  "kv15.3:2.1": "☑ Atītaṃ rūpaṃ vedanā saññā saṅkhārā viññāṇaṃ atīto addhāti?",
  "kv15.3:2.2": "Āmantā.",
  "kv15.3:2.3": "Atītā pañcaddhāti?",
  "kv15.3:2.4": "Na hevaṃ vattabbe …pe…",
  "kv15.3:2.5": "anāgataṃ rūpaṃ vedanā saññā saṅkhārā viññāṇaṃ anāgato addhāti?",
  "kv15.3:2.6": "Āmantā.",
  "kv15.3:2.7": "Anāgatā pañcaddhāti?",
  "kv15.3:2.8": "Na hevaṃ vattabbe …pe…",
  "kv15.3:2.9": "paccuppannaṃ rūpaṃ vedanā saññā saṅkhārā viññāṇaṃ paccuppanno addhāti?",
  "kv15.3:2.10": "Āmantā.",
  "kv15.3:2.11": "Paccuppannā pañcaddhāti?",
  "kv15.3:2.12": "Na hevaṃ vattabbe …pe….",
  "kv15.3:3.1": "☑ Atītā pañcakkhandhā atīto addhā, anāgatā pañcakkhandhā anāgato addhā, paccuppannā pañcakkhandhā paccuppanno addhāti?",
  "kv15.3:3.2": "Āmantā.",
  "kv15.3:3.3": "Pannarasaddhāti?",
  "kv15.3:3.4": "Na hevaṃ vattabbe …pe….",
  "kv15.3:4.1": "☑ Atītāni dvādasāyatanāni atīto addhā, anāgatāni dvādasāyatanāni anāgato addhā, paccuppannāni dvādasāyatanāni paccuppanno addhāti?",
  "kv15.3:4.2": "Āmantā.",
  "kv15.3:4.3": "Chattiṃsa addhāti?",
  "kv15.3:4.4": "Na hevaṃ vattabbe …pe….",
  "kv15.3:5.1": "☑ Atītā aṭṭhārasa dhātuyo atīto addhā, anāgatā aṭṭhārasa dhātuyo anāgato addhā, paccuppannā aṭṭhārasa dhātuyo paccuppanno addhāti?",
  "kv15.3:5.2": "Āmantā.",
  "kv15.3:5.3": "Catupaññāsa addhāti?",
  "kv15.3:5.4": "Na hevaṃ vattabbe …pe….",
  "kv15.3:6.1": "☑ Atītāni bāvīsatindriyāni atīto addhā, anāgatāni bāvīsatindriyāni anāgato addhā, paccuppannāni bāvīsatindriyāni paccuppanno addhāti?",
  "kv15.3:6.2": "Āmantā.",
  "kv15.3:6.3": "Chasaṭṭhi addhāti?",
  "kv15.3:6.4": "Na hevaṃ vattabbe …pe….",
  "kv15.3:7.1": "☒ Na vattabbaṃ—",
  "kv15.3:7.2": "“addhā parinipphanno”ti?",
  "kv15.3:7.3": "Āmantā.",
  "kv15.3:7.4": "Nanu vuttaṃ bhagavatā—",
  "kv15.3:7.5": "“tīṇimāni, bhikkhave, kathāvatthūni.",
  "kv15.3:7.6": "Katamāni tīṇi?",
  "kv15.3:7.7": "Atītaṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
  "kv15.3:7.8": "‘evaṃ ahosi atītamaddhānan’ti;",
  "kv15.3:7.9": "anāgataṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
  "kv15.3:7.10": "‘evaṃ bhavissati anāgatamaddhānan’ti;",
  "kv15.3:7.11": "etarahi vā, bhikkhave, paccuppannaṃ addhānaṃ ārabbha kathaṃ katheyya—",
  "kv15.3:7.12": "‘evaṃ hoti etarahi paccuppannan’ti.",
  "kv15.3:7.13": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
  "kv15.3:7.14": "Attheva suttantoti?",
  "kv15.3:7.15": "Āmantā.",
  "kv15.3:7.16": "Tena hi addhā parinipphannoti.",
  "kv15.3:7.17": "Addhākathā niṭṭhitā."
}