{
  "kv15.4:0.1": "Kathāvatthu",
  "kv15.4:0.2": "Mahāpaṇṇāsaka",
  "kv15.4:0.3": "Pannarasamavagga",
  "kv15.4:0.4": "Khaṇalayamuhuttakathā",
  "kv15.4:1.1": "☑ Khaṇo parinipphanno, layo parinipphanno, muhuttaṃ parinipphannanti?",
  "kv15.4:1.2": "Āmantā.",
  "kv15.4:1.3": "Rūpanti?",
  "kv15.4:1.4": "Na hevaṃ vattabbe …pe…",
  "kv15.4:1.5": "vedanā …",
  "kv15.4:1.6": "saññā …",
  "kv15.4:1.7": "saṅkhārā …",
  "kv15.4:1.8": "viññāṇanti?",
  "kv15.4:1.9": "Na hevaṃ vattabbe …pe….",
  "kv15.4:2.1": "☑ Na vattabbaṃ—",
  "kv15.4:2.2": "“muhuttaṃ parinipphannan”ti?",
  "kv15.4:2.3": "Āmantā.",
  "kv15.4:2.4": "Nanu vuttaṃ bhagavatā—",
  "kv15.4:2.5": "“tīṇimāni, bhikkhave, kathāvatthūni.",
  "kv15.4:2.6": "Katamāni tīṇi?",
  "kv15.4:2.7": "Atītaṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
  "kv15.4:2.8": "‘evaṃ ahosi atītamaddhānan’ti;",
  "kv15.4:2.9": "anāgataṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
  "kv15.4:2.10": "‘evaṃ bhavissati anāgatamaddhānan’ti;",
  "kv15.4:2.11": "etarahi vā, bhikkhave, paccuppannaṃ addhānaṃ ārabbha kathaṃ katheyya—",
  "kv15.4:2.12": "‘evaṃ hoti etarahi paccuppannan’ti.",
  "kv15.4:2.13": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
  "kv15.4:2.14": "Attheva suttantoti?",
  "kv15.4:2.15": "Āmantā.",
  "kv15.4:2.16": "Tena hi muhuttaṃ parinipphannanti.",
  "kv15.4:2.17": "Khaṇalayamuhuttakathā niṭṭhitā."
}