{
  "kv12.6:0.1": "Kathāvatthu",
  "kv12.6:0.2": "Mahāpaṇṇāsaka",
  "kv12.6:0.3": "Dvādasamavagga",
  "kv12.6:0.4": "Kolaṃkolakathā",
  "kv12.6:1.1": "☒ Na vattabbaṃ—",
  "kv12.6:1.2": "“kolaṃkolo puggalo kolaṃkolatāniyato”ti?",
  "kv12.6:1.3": "Āmantā.",
  "kv12.6:1.4": "Nanu so kolaṃkoloti?",
  "kv12.6:1.5": "Āmantā.",
  "kv12.6:1.6": "Hañci so kolaṃkolo, tena vata re vattabbe—",
  "kv12.6:1.7": "“kolaṃkolo puggalo kolaṃkolatāniyato”ti.",
  "kv12.6:1.8": "Kolaṃkolakathā niṭṭhitā."
}