{
  "kv23.1:0.1": "Kathāvatthu",
  "kv23.1:0.2": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv23.1:0.3": "Tevīsatimavagga",
  "kv23.1:0.4": "Ekādhippāyakathā",
  "kv23.1:1.1": "☑ Ekādhippāyena methuno dhammo paṭisevitabboti?",
  "kv23.1:1.2": "Āmantā.",
  "kv23.1:1.3": "Ekādhippāyena assamaṇena hotabbaṃ, abhikkhunā hotabbaṃ, chinnamūlena hotabbaṃ pārājikena hotabbanti?",
  "kv23.1:1.4": "Na hevaṃ vattabbe …pe…",
  "kv23.1:1.5": "ekādhippāyena methuno dhammo paṭisevitabboti?",
  "kv23.1:1.6": "Āmantā.",
  "kv23.1:1.7": "Ekādhippāyena pāṇo hantabbo,",
  "kv23.1:1.8": "adinnaṃ ādiyitabbaṃ,",
  "kv23.1:1.9": "musā bhaṇitabbā,",
  "kv23.1:1.10": "pisuṇaṃ bhaṇitabbaṃ,",
  "kv23.1:1.11": "pharusaṃ bhaṇitabbaṃ,",
  "kv23.1:1.12": "samphaṃ palapitabbaṃ,",
  "kv23.1:1.13": "sandhi cheditabbo,",
  "kv23.1:1.14": "nillopaṃ hātabbaṃ,",
  "kv23.1:1.15": "ekāgārikaṃ kātabbaṃ,",
  "kv23.1:1.16": "paripanthe ṭhātabbaṃ,",
  "kv23.1:1.17": "paradāro gantabbo,",
  "kv23.1:1.18": "gāmaghātako kātabbo,",
  "kv23.1:1.19": "nigamaghātako kātabboti?",
  "kv23.1:1.20": "Na hevaṃ vattabbe …pe….",
  "kv23.1:1.21": "Ekādhippāyakathā niṭṭhitā."
}