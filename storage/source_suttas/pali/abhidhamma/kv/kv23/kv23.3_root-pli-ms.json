{
  "kv23.3:0.1": "Kathāvatthu",
  "kv23.3:0.2": "Mahāpaṇṇāsaka",
  "kv23.3:0.3": "Tevīsatimavagga",
  "kv23.3:0.4": "Issariyakāmakārikādikathā",
  "kv23.3:1.1": "☑ Bodhisatto issariyakāmakārikāhetu vinipātaṃ gacchatīti?",
  "kv23.3:1.2": "Āmantā.",
  "kv23.3:1.3": "Bodhisatto issariyakāmakārikāhetu nirayaṃ gacchati,",
  "kv23.3:1.4": "sañjīvaṃ gacchati,",
  "kv23.3:1.5": "kālasuttaṃ gacchati,",
  "kv23.3:1.6": "tāpanaṃ gacchati,",
  "kv23.3:1.7": "mahātāpanaṃ gacchati,",
  "kv23.3:1.8": "saṅghātakaṃ gacchati,",
  "kv23.3:1.9": "roruvaṃ gacchati …pe…",
  "kv23.3:1.10": "avīciṃ gacchatīti?",
  "kv23.3:1.11": "Na hevaṃ vattabbe …pe….",
  "kv23.3:2.1": "☑ Bodhisatto issariyakāmakārikāhetu vinipātaṃ gacchatīti?",
  "kv23.3:2.2": "Āmantā.",
  "kv23.3:2.3": "“Bodhisatto issariyakāmakārikāhetu vinipātaṃ gacchatī”ti—",
  "kv23.3:2.4": "attheva suttantoti?",
  "kv23.3:2.5": "Natthi.",
  "kv23.3:2.6": "Hañci “bodhisatto issariyakāmakārikāhetu vinipātaṃ gacchatī”ti—",
  "kv23.3:2.7": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:2.8": "“bodhisatto issariyakāmakārikāhetu vinipātaṃ gacchatī”ti.",
  "kv23.3:3.1": "☑ Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṃ okkamatīti?",
  "kv23.3:3.2": "Āmantā.",
  "kv23.3:3.3": "Bodhisatto issariyakāmakārikāhetu nirayaṃ upapajjeyya, tiracchānayoniṃ upapajjeyyāti?",
  "kv23.3:3.4": "Na hevaṃ vattabbe …pe….",
  "kv23.3:4.1": "☑ Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṃ okkamatīti?",
  "kv23.3:4.2": "Āmantā.",
  "kv23.3:4.3": "Bodhisatto iddhimāti?",
  "kv23.3:4.4": "Na hevaṃ vattabbe …pe…",
  "kv23.3:4.5": "bodhisatto iddhimāti?",
  "kv23.3:4.6": "Āmantā.",
  "kv23.3:4.7": "Bodhisattena chandiddhipādo bhāvito …pe…",
  "kv23.3:4.8": "vīriyiddhipādo …pe…",
  "kv23.3:4.9": "cittiddhipādo …pe…",
  "kv23.3:4.10": "vīmaṃsiddhipādo bhāvitoti?",
  "kv23.3:4.11": "Na hevaṃ vattabbe …pe….",
  "kv23.3:5.1": "☑ Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṃ okkamatīti?",
  "kv23.3:5.2": "Āmantā.",
  "kv23.3:5.3": "“Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṃ okkamatī”ti—",
  "kv23.3:5.4": "attheva suttantoti?",
  "kv23.3:5.5": "Natthi.",
  "kv23.3:5.6": "Hañci “bodhisatto issariyakāmakārikāhetu gabbhaseyyaṃ okkamatī”ti—",
  "kv23.3:5.7": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:5.8": "“bodhisatto issariyakāmakārikāhetu gabbhaseyyaṃ okkamatī”ti.",
  "kv23.3:6.1": "☑ Bodhisatto issariyakāmakārikāhetu dukkarakārikaṃ akāsīti?",
  "kv23.3:6.2": "Āmantā.",
  "kv23.3:6.3": "Bodhisatto issariyakāmakārikāhetu “sassato loko”ti paccāgacchi,",
  "kv23.3:6.4": "“asassato loko”ti …pe…",
  "kv23.3:6.5": "“antavā loko”ti …pe…",
  "kv23.3:6.6": "“anantavā loko”ti …",
  "kv23.3:6.7": "“taṃ jīvaṃ taṃ sarīran”ti …",
  "kv23.3:6.8": "“aññaṃ jīvaṃ aññaṃ sarīran”ti …",
  "kv23.3:6.9": "“hoti tathāgato paraṃ maraṇā”ti …",
  "kv23.3:6.10": "“na hoti tathāgato paraṃ maraṇā”ti …",
  "kv23.3:6.11": "“hoti ca na ca hoti tathāgato paraṃ maraṇā”ti …pe…",
  "kv23.3:6.12": "“neva hoti na na hoti tathāgato paraṃ maraṇā”ti paccāgacchīti?",
  "kv23.3:6.13": "Na hevaṃ vattabbe …pe….",
  "kv23.3:7.1": "☑ Bodhisatto issariyakāmakārikāhetu dukkarakārikaṃ akāsīti?",
  "kv23.3:7.2": "Āmantā.",
  "kv23.3:7.3": "“Bodhisatto issariyakāmakārikāhetu dukkarakārikaṃ akāsī”ti—",
  "kv23.3:7.4": "attheva suttantoti?",
  "kv23.3:7.5": "Natthi.",
  "kv23.3:7.6": "Hañci “bodhisatto issariyakāmakārikāhetu dukkarakārikaṃ akāsī”ti—",
  "kv23.3:7.7": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:7.8": "“bodhisatto issariyakāmakārikāhetu dukkarakārikaṃ akāsī”ti.",
  "kv23.3:8.1": "☑ Bodhisatto issariyakāmakārikāhetu aparantapaṃ akāsi, aññaṃ satthāraṃ uddisīti?",
  "kv23.3:8.2": "Āmantā.",
  "kv23.3:8.3": "Bodhisatto issariyakāmakārikāhetu “sassato loko”ti paccāgacchi …pe…",
  "kv23.3:8.4": "“neva hoti na na hoti tathāgato paraṃ maraṇā”ti paccāgacchīti?",
  "kv23.3:8.5": "Na hevaṃ vattabbe …pe….",
  "kv23.3:9.1": "☑ Bodhisatto issariyakāmakārikāhetu aññaṃ satthāraṃ uddisīti?",
  "kv23.3:9.2": "Āmantā.",
  "kv23.3:9.3": "“Bodhisatto issariyakāmakārikāhetu aññaṃ satthāraṃ uddisī”ti—",
  "kv23.3:9.4": "attheva suttantoti?",
  "kv23.3:9.5": "Natthi.",
  "kv23.3:9.6": "Hañci “bodhisatto issariyakāmakārikāhetu aññaṃ satthāraṃ uddisī”ti—",
  "kv23.3:9.7": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:9.8": "“bodhisatto issariyakāmakārikāhetu aññaṃ satthāraṃ uddisī”ti.",
  "kv23.3:9.9": "Issariyakāmakārikākathā niṭṭhitā."
}