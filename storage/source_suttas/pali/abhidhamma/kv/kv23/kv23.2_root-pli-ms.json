{
  "kv23.2:0.1": "Kathāvatthu",
  "kv23.2:0.2": "Mahāpaṇṇāsaka",
  "kv23.2:0.3": "Tevīsatimavagga",
  "kv23.2:0.4": "Arahantavaṇṇakathā",
  "kv23.2:1.1": "☑ Arahantānaṃ vaṇṇena amanussā methunaṃ dhammaṃ paṭisevantīti?",
  "kv23.2:1.2": "Āmantā.",
  "kv23.2:1.3": "Arahantānaṃ vaṇṇena amanussā pāṇaṃ hananti …pe…",
  "kv23.2:1.4": "adinnaṃ ādiyanti,",
  "kv23.2:1.5": "musā bhaṇanti,",
  "kv23.2:1.6": "pisuṇaṃ bhaṇanti,",
  "kv23.2:1.7": "pharusaṃ bhaṇanti,",
  "kv23.2:1.8": "samphaṃ palapanti,",
  "kv23.2:1.9": "sandhiṃ chindanti,",
  "kv23.2:1.10": "nillopaṃ haranti,",
  "kv23.2:1.11": "ekāgārikaṃ karonti,",
  "kv23.2:1.12": "paripanthe tiṭṭhanti,",
  "kv23.2:1.13": "paradāraṃ gacchanti,",
  "kv23.2:1.14": "gāmaghātakaṃ karonti …pe…",
  "kv23.2:1.15": "nigamaghātakaṃ karontīti?",
  "kv23.2:1.16": "Na hevaṃ vattabbe …pe….",
  "kv23.2:1.17": "Arahantavaṇṇakathā niṭṭhitā."
}