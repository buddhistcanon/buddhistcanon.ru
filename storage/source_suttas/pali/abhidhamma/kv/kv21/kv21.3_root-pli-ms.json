{
  "kv21.3:0.1": "Kathāvatthu",
  "kv21.3:0.2": "Mahāpaṇṇāsaka",
  "kv21.3:0.3": "Ekavīsatimavagga",
  "kv21.3:0.4": "Saṃyojanakathā",
  "kv21.3:1.1": "☑ Atthi kiñci saṃyojanaṃ appahāya arahattappattīti?",
  "kv21.3:1.2": "Āmantā.",
  "kv21.3:1.3": "Atthi kiñci sakkāyadiṭṭhiṃ appahāya …pe…",
  "kv21.3:1.4": "vicikicchaṃ appahāya …pe…",
  "kv21.3:1.5": "sīlabbataparāmāsaṃ appahāya …",
  "kv21.3:1.6": "rāgaṃ appahāya …",
  "kv21.3:1.7": "dosaṃ appahāya …",
  "kv21.3:1.8": "mohaṃ appahāya …",
  "kv21.3:1.9": "anottappaṃ appahāya arahattappattīti?",
  "kv21.3:1.10": "Na hevaṃ vattabbe …pe….",
  "kv21.3:2.1": "☑ Atthi kiñci saṃyojanaṃ appahāya arahattappattīti?",
  "kv21.3:2.2": "Āmantā.",
  "kv21.3:2.3": "Arahā sarāgo sadoso samoho samāno samakkho sapaḷāso saupāyāso sakilesoti?",
  "kv21.3:2.4": "Na hevaṃ vattabbe …pe…",
  "kv21.3:2.5": "nanu arahā nirāgo niddoso nimmoho nimmāno nimmakkho nippaḷāso nirupāyāso nikkilesoti?",
  "kv21.3:2.6": "Āmantā.",
  "kv21.3:2.7": "Hañci arahā nirāgo …pe…",
  "kv21.3:2.8": "nikkileso, no ca vata re vattabbe—",
  "kv21.3:2.9": "“atthi kiñci saṃyojanaṃ appahāya arahattappattī”ti.",
  "kv21.3:3.1": "☒ Na vattabbaṃ—",
  "kv21.3:3.2": "“kiñci saṃyojanaṃ appahāya arahattappattī”ti?",
  "kv21.3:3.3": "Āmantā.",
  "kv21.3:3.4": "Arahā sabbaṃ buddhavisayaṃ jānātīti?",
  "kv21.3:3.5": "Na hevaṃ vattabbe.",
  "kv21.3:3.6": "Tena hi atthi kiñci saṃyojanaṃ appahāya arahattappattīti.",
  "kv21.3:3.7": "Saṃyojanakathā niṭṭhitā."
}