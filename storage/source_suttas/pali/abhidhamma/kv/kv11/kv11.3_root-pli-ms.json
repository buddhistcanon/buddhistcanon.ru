{
  "kv11.3:0.1": "Kathāvatthu",
  "kv11.3:0.2": "Mahāpaṇṇāsaka",
  "kv11.3:0.3": "Ekādasamavagga",
  "kv11.3:0.4": "Ñāṇaṃcittavippayuttantikathā",
  "kv11.3:1.1": "☑ Ñāṇaṃ cittavippayuttanti?",
  "kv11.3:1.2": "Āmantā.",
  "kv11.3:1.3": "Rūpaṃ nibbānaṃ cakkhāyatanaṃ …pe… phoṭṭhabbāyatananti?",
  "kv11.3:1.4": "Na hevaṃ vattabbe …pe…",
  "kv11.3:1.5": "ñāṇaṃ cittavippayuttanti?",
  "kv11.3:1.6": "Āmantā.",
  "kv11.3:1.7": "Paññā paññindriyaṃ paññābalaṃ sammādiṭṭhi dhammavicayasambojjhaṅgo cittavippayuttoti?",
  "kv11.3:1.8": "Na hevaṃ vattabbe …pe…",
  "kv11.3:1.9": "paññā paññindriyaṃ paññābalaṃ sammādiṭṭhi dhammavicayasambojjhaṅgo cittasampayuttoti?",
  "kv11.3:1.10": "Āmantā.",
  "kv11.3:1.11": "Ñāṇaṃ cittasampayuttanti?",
  "kv11.3:1.12": "Na hevaṃ vattabbe …pe….",
  "kv11.3:2.1": "☑ Ñāṇaṃ cittavippayuttanti?",
  "kv11.3:2.2": "Āmantā.",
  "kv11.3:2.3": "Katamakkhandhapariyāpannanti?",
  "kv11.3:2.4": "Saṅkhārakkhandhapariyāpannanti.",
  "kv11.3:2.5": "Saṅkhārakkhandho cittavippayuttoti?",
  "kv11.3:2.6": "Na hevaṃ vattabbe …pe…",
  "kv11.3:2.7": "saṅkhārakkhandho cittavippayuttoti?",
  "kv11.3:2.8": "Āmantā.",
  "kv11.3:2.9": "Vedanākkhandho saññākkhandho cittavippayuttoti?",
  "kv11.3:2.10": "Na hevaṃ vattabbe …pe…",
  "kv11.3:2.11": "ñāṇaṃ saṅkhārakkhandhapariyāpannaṃ cittavippayuttanti?",
  "kv11.3:2.12": "Āmantā.",
  "kv11.3:2.13": "Paññā saṅkhārakkhandhapariyāpannā cittavippayuttāti?",
  "kv11.3:2.14": "Na hevaṃ vattabbe …pe…",
  "kv11.3:2.15": "paññā saṅkhārakkhandhapariyāpannā cittasampayuttāti?",
  "kv11.3:2.16": "Āmantā.",
  "kv11.3:2.17": "Ñāṇaṃ saṅkhārakkhandhapariyāpannaṃ cittasampayuttanti?",
  "kv11.3:2.18": "Na hevaṃ vattabbe …pe…",
  "kv11.3:2.19": "ñāṇaṃ saṅkhārakkhandhapariyāpannaṃ cittavippayuttaṃ, paññā saṅkhārakkhandhapariyāpannā cittasampayuttāti?",
  "kv11.3:2.20": "Āmantā.",
  "kv11.3:2.21": "Saṅkhārakkhandho ekadeso cittasampayutto ekadeso cittavippayuttoti?",
  "kv11.3:2.22": "Na hevaṃ vattabbe …pe…",
  "kv11.3:2.23": "saṅkhārakkhandho ekadeso cittasampayutto ekadeso cittavippayuttoti?",
  "kv11.3:2.24": "Āmantā.",
  "kv11.3:2.25": "Vedanākkhandho saññākkhandho ekadeso cittasampayutto ekadeso cittavippayuttoti?",
  "kv11.3:2.26": "Na hevaṃ vattabbe …pe….",
  "kv11.3:3.1": "☒ Na vattabbaṃ—",
  "kv11.3:3.2": "“ñāṇaṃ cittavippayuttan”ti?",
  "kv11.3:3.3": "Āmantā.",
  "kv11.3:3.4": "Arahā cakkhuviññāṇasamaṅgī “ñāṇī”ti vattabboti?",
  "kv11.3:3.5": "Āmantā.",
  "kv11.3:3.6": "Ñāṇaṃ tena cittena sampayuttanti?",
  "kv11.3:3.7": "Na hevaṃ vattabbe.",
  "kv11.3:3.8": "Tena hi ñāṇaṃ cittavippayuttanti.",
  "kv11.3:4.1": "☒ Arahā cakkhuviññāṇasamaṅgī “paññavā”ti vattabboti?",
  "kv11.3:4.2": "Āmantā.",
  "kv11.3:4.3": "Paññā tena cittena sampayuttāti?",
  "kv11.3:4.4": "Na hevaṃ vattabbe.",
  "kv11.3:4.5": "Tena hi paññā cittavippayuttāti.",
  "kv11.3:4.6": "Ñāṇaṃ cittavippayuttantikathā niṭṭhitā."
}