{
  "kv17.6:0.1": "Kathāvatthu",
  "kv17.6:0.2": "Mahāpaṇṇāsaka",
  "kv17.6:0.3": "Sattarasamavagga",
  "kv17.6:0.4": "Navattabbaṃsaṃghodakkhiṇaṃpaṭiggaṇhātikathā",
  "kv17.6:1.1": "☑ Na vattabbaṃ—",
  "kv17.6:1.2": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti?",
  "kv17.6:1.3": "Āmantā.",
  "kv17.6:1.4": "Nanu saṃgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṃ puññakkhettaṃ lokassāti?",
  "kv17.6:1.5": "Āmantā.",
  "kv17.6:1.6": "Hañci saṃgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṃ puññakkhettaṃ lokassa, tena vata re vattabbe—",
  "kv17.6:1.7": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti.",
  "kv17.6:2.1": "☑ Na vattabbaṃ—",
  "kv17.6:2.2": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti?",
  "kv17.6:2.3": "Āmantā.",
  "kv17.6:2.4": "Nanu cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatāti?",
  "kv17.6:2.5": "Āmantā.",
  "kv17.6:2.6": "Hañci cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatā, tena vata re vattabbe—",
  "kv17.6:2.7": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti.",
  "kv17.6:3.1": "☑ Na vattabbaṃ—",
  "kv17.6:3.2": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti?",
  "kv17.6:3.3": "Āmantā.",
  "kv17.6:3.4": "Nanu atthi keci saṃghassa dānaṃ dentīti?",
  "kv17.6:3.5": "Āmantā.",
  "kv17.6:3.6": "Hañci atthi keci saṃghassa dānaṃ denti, tena vata re vattabbe—",
  "kv17.6:3.7": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti.",
  "kv17.6:3.8": "Nanu atthi keci saṃghassa cīvaraṃ denti …pe…",
  "kv17.6:3.9": "piṇḍapātaṃ denti …",
  "kv17.6:3.10": "senāsanaṃ denti …",
  "kv17.6:3.11": "gilānapaccayabhesajjaparikkhāraṃ denti …",
  "kv17.6:3.12": "khādanīyaṃ denti …",
  "kv17.6:3.13": "bhojanīyaṃ denti …pe…",
  "kv17.6:3.14": "pānīyaṃ dentīti?",
  "kv17.6:3.15": "Āmantā.",
  "kv17.6:3.16": "Hañci atthi keci saṃghassa pānīyaṃ denti, tena vata re vattabbe—",
  "kv17.6:3.17": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti.",
  "kv17.6:4.1": "☑ Na vattabbaṃ—",
  "kv17.6:4.2": "“saṃgho dakkhiṇaṃ paṭiggaṇhātī”ti?",
  "kv17.6:4.3": "Āmantā.",
  "kv17.6:4.4": "Nanu vuttaṃ bhagavatā—",
  "kv17.6:5.1": "“Āhutiṃ jātavedova,",
  "kv17.6:5.2": "mahāmeghaṃva medanī;",
  "kv17.6:5.3": "Saṃgho samādhisampanno,",
  "kv17.6:5.4": "paṭiggaṇhāti dakkhiṇan”ti.",
  "kv17.6:6.1": "Attheva suttantoti?",
  "kv17.6:6.2": "Āmantā.",
  "kv17.6:6.3": "Tena hi saṃgho dakkhiṇaṃ paṭiggaṇhātīti.",
  "kv17.6:7.1": "☒ Saṃgho dakkhiṇaṃ paṭiggaṇhātīti?",
  "kv17.6:7.2": "Āmantā.",
  "kv17.6:7.3": "Maggo paṭiggaṇhāti, phalaṃ paṭiggaṇhātīti?",
  "kv17.6:7.4": "Na hevaṃ vattabbe …pe….",
  "kv17.6:7.5": "Na vattabbaṃ saṃgho dakkhiṇaṃ paṭiggaṇhātītikathā niṭṭhitā."
}