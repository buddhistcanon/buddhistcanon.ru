{
  "kv17.3:0.1": "Kathāvatthu",
  "kv17.3:0.2": "Mahāpaṇṇāsaka",
  "kv17.3:0.3": "Sattarasamavagga",
  "kv17.3:0.4": "Sabbamidaṃkammatotikathā",
  "kv17.3:1.1": "☑ Sabbamidaṃ kammatoti?",
  "kv17.3:1.2": "Āmantā.",
  "kv17.3:1.3": "Kammampi kammatoti?",
  "kv17.3:1.4": "Na hevaṃ vattabbe …pe…",
  "kv17.3:1.5": "sabbamidaṃ kammatoti?",
  "kv17.3:1.6": "Āmantā.",
  "kv17.3:1.7": "Sabbamidaṃ pubbekatahetūti?",
  "kv17.3:1.8": "Na hevaṃ vattabbe …pe…",
  "kv17.3:1.9": "sabbamidaṃ kammatoti?",
  "kv17.3:1.10": "Āmantā.",
  "kv17.3:1.11": "Sabbamidaṃ kammavipākatoti?",
  "kv17.3:1.12": "Na hevaṃ vattabbe …pe….",
  "kv17.3:2.1": "☑ Sabbamidaṃ kammavipākatoti?",
  "kv17.3:2.2": "Āmantā.",
  "kv17.3:2.3": "Kammavipākena pāṇaṃ haneyyāti?",
  "kv17.3:2.4": "Āmantā.",
  "kv17.3:2.5": "Pāṇātipāto saphaloti?",
  "kv17.3:2.6": "Āmantā.",
  "kv17.3:2.7": "Kammavipāko saphaloti?",
  "kv17.3:2.8": "Na hevaṃ vattabbe …pe…",
  "kv17.3:2.9": "kammavipāko aphaloti?",
  "kv17.3:2.10": "Āmantā.",
  "kv17.3:2.11": "Pāṇātipāto aphaloti?",
  "kv17.3:2.12": "Na hevaṃ vattabbe …pe….",
  "kv17.3:3.1": "☑ Kammavipākena adinnaṃ ādiyeyya …pe…",
  "kv17.3:3.2": "musā bhaṇeyya …",
  "kv17.3:3.3": "pisuṇaṃ bhaṇeyya …",
  "kv17.3:3.4": "pharusaṃ bhaṇeyya …",
  "kv17.3:3.5": "samphaṃ palapeyya …",
  "kv17.3:3.6": "sandhiṃ chindeyya …",
  "kv17.3:3.7": "nillopaṃ hareyya …",
  "kv17.3:3.8": "ekāgārikaṃ kareyya …",
  "kv17.3:3.9": "paripanthe tiṭṭheyya …",
  "kv17.3:3.10": "paradāraṃ gaccheyya …",
  "kv17.3:3.11": "gāmaghātakaṃ kareyya …",
  "kv17.3:3.12": "nigamaghātakaṃ kareyya …",
  "kv17.3:3.13": "kammavipākena dānaṃ dadeyya …",
  "kv17.3:3.14": "cīvaraṃ dadeyya …",
  "kv17.3:3.15": "piṇḍapātaṃ dadeyya …",
  "kv17.3:3.16": "senāsanaṃ dadeyya …",
  "kv17.3:3.17": "gilānapaccayabhesajjaparikkhāraṃ dadeyyāti?",
  "kv17.3:3.18": "Āmantā.",
  "kv17.3:3.19": "Gilānapaccayabhesajjaparikkhāro saphaloti?",
  "kv17.3:3.20": "Āmantā.",
  "kv17.3:3.21": "Kammavipāko saphaloti?",
  "kv17.3:3.22": "Na hevaṃ vattabbe …pe…",
  "kv17.3:3.23": "kammavipāko aphaloti?",
  "kv17.3:3.24": "Āmantā.",
  "kv17.3:3.25": "Gilānapaccayabhesajjaparikkhāro aphaloti?",
  "kv17.3:3.26": "Na hevaṃ vattabbe …pe….",
  "kv17.3:4.1": "☒ Na vattabbaṃ—",
  "kv17.3:4.2": "“sabbamidaṃ kammato”ti?",
  "kv17.3:4.3": "Āmantā.",
  "kv17.3:4.4": "Nanu vuttaṃ bhagavatā—",
  "kv17.3:5.1": "“Kammunā vattatī loko,",
  "kv17.3:5.2": "kammunā vattatī pajā;",
  "kv17.3:5.3": "Kammanibandhanā sattā,",
  "kv17.3:5.4": "rathassāṇīva yāyato.",
  "kv17.3:6.1": "Kammena kittiṃ labhate pasaṃsaṃ,",
  "kv17.3:6.2": "Kammena jāniñca vadhañca bandhaṃ;",
  "kv17.3:6.3": "Taṃ kammaṃ nānākaraṇaṃ viditvā,",
  "kv17.3:6.4": "Kasmā vade natthi kammanti loke”ti.",
  "kv17.3:7.1": "Attheva suttantoti?",
  "kv17.3:7.2": "Āmantā.",
  "kv17.3:7.3": "Tena hi “sabbamidaṃ kammato”ti.",
  "kv17.3:7.4": "Sabbamidaṃ kammatotikathā niṭṭhitā."
}