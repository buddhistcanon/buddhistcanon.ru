{
  "kv18.9:0.1": "Kathāvatthu",
  "kv18.9:0.2": "Mahāpaṇṇāsaka",
  "kv18.9:0.3": "Aṭṭhārasamavagga",
  "kv18.9:0.4": "Cakkhunārūpaṃpassatītikathā",
  "kv18.9:1.1": "☑ Cakkhunā rūpaṃ passatīti?",
  "kv18.9:1.2": "Āmantā.",
  "kv18.9:1.3": "Rūpena rūpaṃ passatīti?",
  "kv18.9:1.4": "Na hevaṃ vattabbe …pe…",
  "kv18.9:1.5": "rūpena rūpaṃ passatīti?",
  "kv18.9:1.6": "Āmantā.",
  "kv18.9:1.7": "Rūpena rūpaṃ paṭivijānātīti?",
  "kv18.9:1.8": "Na hevaṃ vattabbe …pe…",
  "kv18.9:1.9": "rūpena rūpaṃ paṭivijānātīti?",
  "kv18.9:1.10": "Āmantā.",
  "kv18.9:1.11": "Rūpaṃ manoviññāṇanti?",
  "kv18.9:1.12": "Na hevaṃ vattabbe …pe…",
  "kv18.9:1.13": "cakkhunā rūpaṃ passatīti?",
  "kv18.9:1.14": "Āmantā.",
  "kv18.9:1.15": "Atthi cakkhussa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:1.16": "Na hevaṃ vattabbe …pe…",
  "kv18.9:1.17": "nanu natthi cakkhussa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:1.18": "Āmantā.",
  "kv18.9:1.19": "Hañci natthi cakkhussa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv18.9:1.20": "“cakkhunā rūpaṃ passatī”ti.",
  "kv18.9:2.1": "☑ Sotena saddaṃ suṇātīti …pe…",
  "kv18.9:2.2": "ghānena gandhaṃ ghāyatīti …pe…",
  "kv18.9:2.3": "jivhāya rasaṃ sāyatīti …pe…",
  "kv18.9:2.4": "kāyena phoṭṭhabbaṃ phusatīti?",
  "kv18.9:2.5": "Āmantā.",
  "kv18.9:2.6": "Rūpena rūpaṃ phusatīti?",
  "kv18.9:2.7": "Na hevaṃ vattabbe …pe….",
  "kv18.9:3.1": "☑ Rūpena rūpaṃ phusatīti?",
  "kv18.9:3.2": "Āmantā.",
  "kv18.9:3.3": "Rūpena rūpaṃ paṭivijānātīti?",
  "kv18.9:3.4": "Na hevaṃ vattabbe …pe…",
  "kv18.9:3.5": "rūpena rūpaṃ paṭivijānātīti?",
  "kv18.9:3.6": "Āmantā.",
  "kv18.9:3.7": "Rūpaṃ manoviññāṇanti?",
  "kv18.9:3.8": "Na hevaṃ vattabbe …pe…",
  "kv18.9:3.9": "kāyena phoṭṭhabbaṃ phusatīti?",
  "kv18.9:3.10": "Āmantā.",
  "kv18.9:3.11": "Atthi kāyassa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:3.12": "Na hevaṃ vattabbe …pe…",
  "kv18.9:3.13": "nanu natthi kāyassa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:3.14": "Āmantā.",
  "kv18.9:3.15": "Hañci natthi kāyassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv18.9:3.16": "“kāyena phoṭṭhabbaṃ phusatī”ti …pe….",
  "kv18.9:4.1": "☒ Na vattabbaṃ—",
  "kv18.9:4.2": "“cakkhunā rūpaṃ passatī”ti …pe…",
  "kv18.9:4.3": "“kāyena phoṭṭhabbaṃ phusatī”ti?",
  "kv18.9:4.4": "Āmantā.",
  "kv18.9:4.5": "Nanu vuttaṃ bhagavatā—",
  "kv18.9:4.6": "“idha, bhikkhave, bhikkhu cakkhunā rūpaṃ passati …pe…",
  "kv18.9:4.7": "kāyena phoṭṭhabbaṃ phusatī”ti.",
  "kv18.9:4.8": "Attheva suttantoti?",
  "kv18.9:4.9": "Āmantā.",
  "kv18.9:4.10": "Tena hi cakkhunā rūpaṃ passati …pe…",
  "kv18.9:4.11": "kāyena phoṭṭhabbaṃ phusatīti.",
  "kv18.9:4.12": "Cakkhunā rūpaṃ passatītikathā niṭṭhitā.",
  "kv18.9:4.13": "Aṭṭhārasamo vaggo.",
  "kv18.9:4.14": "Tassuddānaṃ",
  "kv18.9:5.1": "Buddho bhagavā manussaloke aṭṭhāsi,",
  "kv18.9:5.2": "buddhena bhagavatā dhammo desito,",
  "kv18.9:5.3": "natthi buddhassa bhagavato karuṇā,",
  "kv18.9:5.4": "buddhassa bhagavato uccārapassāvo ativiya aññe gandhajāte adhiggaṇhāti,",
  "kv18.9:5.5": "ekena ariyamaggena cattāri sāmaññaphalāni sacchikaroti,",
  "kv18.9:5.6": "jhānā jhānaṃ saṅkamati,",
  "kv18.9:5.7": "atthi jhānantarikā,",
  "kv18.9:5.8": "samāpanno saddaṃ suṇāti,",
  "kv18.9:5.9": "cakkhunā rūpaṃ passati …pe…",
  "kv18.9:5.10": "kāyena phoṭṭhabbaṃ phusati."
}