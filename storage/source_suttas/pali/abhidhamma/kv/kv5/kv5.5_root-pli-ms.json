{
  "kv5.5:0.1": "Kathāvatthu",
  "kv5.5:0.2": "Mahāpaṇṇāsaka",
  "kv5.5:0.3": "Pañcamavagga",
  "kv5.5:0.4": "Paṭisambhidākathā",
  "kv5.5:1.1": "☑ Sabbaṃ ñāṇaṃ paṭisambhidāti?",
  "kv5.5:1.2": "Āmantā.",
  "kv5.5:1.3": "Sammutiñāṇaṃ paṭisambhidāti?",
  "kv5.5:1.4": "Na hevaṃ vattabbe …pe…",
  "kv5.5:1.5": "sammutiñāṇaṃ paṭisambhidāti?",
  "kv5.5:1.6": "Āmantā.",
  "kv5.5:1.7": "Ye keci sammutiṃ jānanti, sabbe te paṭisambhidāpattāti?",
  "kv5.5:1.8": "Na hevaṃ vattabbe …pe…",
  "kv5.5:1.9": "sabbaṃ ñāṇaṃ paṭisambhidāti?",
  "kv5.5:1.10": "Āmantā.",
  "kv5.5:1.11": "Cetopariyāye ñāṇaṃ paṭisambhidāti?",
  "kv5.5:1.12": "Na hevaṃ vattabbe …pe…",
  "kv5.5:1.13": "cetopariyāye ñāṇaṃ paṭisambhidāti?",
  "kv5.5:1.14": "Āmantā.",
  "kv5.5:1.15": "Ye keci paracittaṃ jānanti, sabbe te paṭisambhidāpattāti?",
  "kv5.5:1.16": "Na hevaṃ vattabbe …pe….",
  "kv5.5:2.1": "☑ Sabbaṃ ñāṇaṃ paṭisambhidāti?",
  "kv5.5:2.2": "Āmantā.",
  "kv5.5:2.3": "Sabbā paññā paṭisambhidāti?",
  "kv5.5:2.4": "Na hevaṃ vattabbe …pe…",
  "kv5.5:2.5": "sabbā paññā paṭisambhidāti?",
  "kv5.5:2.6": "Āmantā.",
  "kv5.5:2.7": "Pathavīkasiṇaṃ samāpattiṃ samāpannassa atthi paññā, sā paññā paṭisambhidāti?",
  "kv5.5:2.8": "Na hevaṃ vattabbe …pe…",
  "kv5.5:2.9": "āpokasiṇaṃ …pe…",
  "kv5.5:2.10": "tejokasiṇaṃ …pe…",
  "kv5.5:2.11": "vāyokasiṇaṃ …pe…",
  "kv5.5:2.12": "nīlakasiṇaṃ …pe…",
  "kv5.5:2.13": "pītakasiṇaṃ …pe…",
  "kv5.5:2.14": "lohitakasiṇaṃ …pe…",
  "kv5.5:2.15": "odātakasiṇaṃ …pe…",
  "kv5.5:2.16": "ākāsānañcāyatanaṃ …pe…",
  "kv5.5:2.17": "viññāṇañcāyatanaṃ …pe…",
  "kv5.5:2.18": "ākiñcaññāyatanaṃ …pe…",
  "kv5.5:2.19": "nevasaññānāsaññāyatanaṃ samāpannassa …pe…",
  "kv5.5:2.20": "dānaṃ dadantassa …pe…",
  "kv5.5:2.21": "cīvaraṃ dadantassa …pe…",
  "kv5.5:2.22": "piṇḍapātaṃ dadantassa …pe…",
  "kv5.5:2.23": "senāsanaṃ dadantassa …pe…",
  "kv5.5:2.24": "gilānapaccayabhesajjaparikkhāraṃ dadantassa atthi paññā, sā paññā paṭisambhidāti?",
  "kv5.5:2.25": "Na hevaṃ vattabbe …pe….",
  "kv5.5:3.1": "☒ Na vattabbaṃ—",
  "kv5.5:3.2": "“sabbaṃ ñāṇaṃ paṭisambhidā”ti?",
  "kv5.5:3.3": "Āmantā.",
  "kv5.5:3.4": "Atthi lokuttarā paññā, sā paññā na paṭisambhidāti?",
  "kv5.5:3.5": "Na hevaṃ vattabbe …pe…",
  "kv5.5:3.6": "tena hi sabbaṃ ñāṇaṃ paṭisambhidāti.",
  "kv5.5:3.7": "Paṭisambhidākathā niṭṭhitā."
}