{
  "kv3.12:0.1": "Kathāvatthu",
  "kv3.12:0.2": "Mahāpaṇṇāsaka",
  "kv3.12:0.3": "Tatiyavagga",
  "kv3.12:0.4": "Nevasaññānāsaññāyatanakathā",
  "kv3.12:1.1": "☑ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:1.2": "“saññā atthī”ti?",
  "kv3.12:1.3": "Āmantā.",
  "kv3.12:1.4": "Asaññabhavo asaññagati asaññasattāvāso asaññasaṃsāro asaññayoni asaññattabhāvapaṭilābhoti?",
  "kv3.12:1.5": "Na hevaṃ vattabbe …pe….",
  "kv3.12:2.1": "Nanu saññābhavo saññāgati saññāsattāvāso saññāsaṃsāro saññāyoni saññattabhāvapaṭilābhoti?",
  "kv3.12:2.2": "Āmantā.",
  "kv3.12:2.3": "Hañci saññābhavo saññāgati …pe…",
  "kv3.12:2.4": "saññattabhāvapaṭilābho, no ca vata re vattabbe—",
  "kv3.12:2.5": "“nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:2.6": "‘saññā atthī’”ti.",
  "kv3.12:3.1": "☑ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:3.2": "“saññā atthī”ti?",
  "kv3.12:3.3": "Āmantā.",
  "kv3.12:3.4": "Ekavokārabhavo gati …pe…",
  "kv3.12:3.5": "attabhāvapaṭilābhoti?",
  "kv3.12:3.6": "Na hevaṃ vattabbe …pe….",
  "kv3.12:4.1": "Nanu catuvokārabhavo gati …pe…",
  "kv3.12:4.2": "attabhāvapaṭilābhoti?",
  "kv3.12:4.3": "Āmantā.",
  "kv3.12:4.4": "Hañci catuvokārabhavo gati …pe…",
  "kv3.12:4.5": "attabhāvapaṭilābho, no ca vata re vattabbe—",
  "kv3.12:4.6": "“nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:4.7": "‘saññā atthī’”ti.",
  "kv3.12:5.1": "☑ Asaññasattesu na vattabbaṃ—",
  "kv3.12:5.2": "“saññā atthi”, so ca asaññabhavo asaññagati asaññasattāvāso asaññasaṃsāro asaññayoni asaññattabhāvapaṭilābhoti?",
  "kv3.12:5.3": "Āmantā.",
  "kv3.12:5.4": "Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:5.5": "“saññā atthi”, so ca asaññabhavo asaññagati asaññasattāvāso asaññasaṃsāro asaññayoni asaññattabhāvapaṭilābhoti?",
  "kv3.12:5.6": "Na hevaṃ vattabbe …pe….",
  "kv3.12:6.1": "☑ Asaññasattesu na vattabbaṃ—",
  "kv3.12:6.2": "“saññā atthi”, so ca ekavokārabhavo gati …pe…",
  "kv3.12:6.3": "attabhāvapaṭilābhoti?",
  "kv3.12:6.4": "Āmantā.",
  "kv3.12:6.5": "Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:6.6": "“saññā atthi”, so ca ekavokārabhavo gati sattāvāso saṃsāro yoni attabhāvapaṭilābhoti?",
  "kv3.12:6.7": "Na hevaṃ vattabbe …pe….",
  "kv3.12:7.1": "☑ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:7.2": "“saññā atthi”, so ca saññābhavo saññāgati …pe…",
  "kv3.12:7.3": "saññattabhāvapaṭilābhoti?",
  "kv3.12:7.4": "Āmantā.",
  "kv3.12:7.5": "Asaññasattesu na vattabbaṃ—",
  "kv3.12:7.6": "“saññā atthi”, so ca saññābhavo saññāgati …pe…",
  "kv3.12:7.7": "saññattabhāvapaṭilābhoti?",
  "kv3.12:7.8": "Na hevaṃ vattabbe …pe…",
  "kv3.12:8.1": "☑ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:8.2": "“saññā atthi”, so ca catuvokārabhavo gati …pe…",
  "kv3.12:8.3": "attabhāvapaṭilābhoti?",
  "kv3.12:8.4": "Āmantā.",
  "kv3.12:8.5": "Asaññasattesu na vattabbaṃ—",
  "kv3.12:8.6": "“saññā atthi”, so ca catuvokārabhavo gati …pe…",
  "kv3.12:8.7": "attabhāvapaṭilābhoti?",
  "kv3.12:8.8": "Na hevaṃ vattabbe …pe….",
  "kv3.12:9.1": "☑ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:9.2": "“saññā atthī”ti?",
  "kv3.12:9.3": "Āmantā.",
  "kv3.12:9.4": "Nanu nevasaññānāsaññāyatanaṃ catuvokārabhavoti?",
  "kv3.12:9.5": "Āmantā.",
  "kv3.12:9.6": "Hañci nevasaññānāsaññāyatanaṃ catuvokārabhavo, no ca vata re vattabbe—",
  "kv3.12:9.7": "“nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:9.8": "‘saññā atthī’”ti.",
  "kv3.12:10.1": "☑ Nevasaññānāsaññāyatanaṃ catuvokārabhavo nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:10.2": "“saññā atthī”ti?",
  "kv3.12:10.3": "Āmantā.",
  "kv3.12:10.4": "Ākāsānañcāyatanaṃ catuvokārabhavo ākāsānañcāyatane na vattabbaṃ—",
  "kv3.12:10.5": "“saññā atthī”ti?",
  "kv3.12:10.6": "Na hevaṃ vattabbe …pe….",
  "kv3.12:11.1": "☑ Nevasaññānāsaññāyatanaṃ catuvokārabhavo, nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:11.2": "“saññā atthī”ti?",
  "kv3.12:11.3": "Āmantā.",
  "kv3.12:11.4": "Viññāṇañcāyatanaṃ …pe…",
  "kv3.12:11.5": "ākiñcaññāyatanaṃ catuvokārabhavo, ākiñcaññāyatane na vattabbaṃ—",
  "kv3.12:11.6": "“saññā atthī”ti?",
  "kv3.12:11.7": "Na hevaṃ vattabbe …pe….",
  "kv3.12:12.1": "☑ Ākāsānañcāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:12.2": "Āmantā.",
  "kv3.12:12.3": "Nevasaññānāsaññāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:12.4": "Na hevaṃ vattabbe …pe….",
  "kv3.12:13.1": "☑ Viññāṇañcāyatanaṃ …pe…",
  "kv3.12:13.2": "ākiñcaññāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:13.3": "Āmantā.",
  "kv3.12:13.4": "Nevasaññānāsaññāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:13.5": "Na hevaṃ vattabbe …pe….",
  "kv3.12:14.1": "☑ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:14.2": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:14.3": "Āmantā.",
  "kv3.12:14.4": "Nanu nevasaññānāsaññāyatanaṃ catuvokārabhavoti?",
  "kv3.12:14.5": "Āmantā.",
  "kv3.12:14.6": "Hañci nevasaññānāsaññāyatanaṃ catuvokārabhavo, no ca vata re vattabbe—",
  "kv3.12:14.7": "“nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:14.8": "‘saññā atthī’ti vā ‘natthī’ti vā”ti.",
  "kv3.12:15.1": "☑ Nevasaññānāsaññāyatanaṃ catuvokārabhavo, nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:15.2": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:15.3": "Āmantā.",
  "kv3.12:15.4": "Ākāsānañcāyatanaṃ …pe…",
  "kv3.12:15.5": "viññāṇañcāyatanaṃ …pe…",
  "kv3.12:15.6": "ākiñcaññāyatanaṃ catuvokārabhavo, ākiñcaññāyatane na vattabbaṃ—",
  "kv3.12:15.7": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:15.8": "Na hevaṃ vattabbe …pe….",
  "kv3.12:16.1": "☑ Ākāsānañcāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:16.2": "Āmantā.",
  "kv3.12:16.3": "Nevasaññānāsaññāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:16.4": "Na hevaṃ vattabbe …pe….",
  "kv3.12:17.1": "☑ Viññāṇañcāyatanaṃ …pe…",
  "kv3.12:17.2": "ākiñcaññāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:17.3": "Āmantā.",
  "kv3.12:17.4": "Nevasaññānāsaññāyatanaṃ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:17.5": "Na hevaṃ vattabbe …pe….",
  "kv3.12:18.1": "☒ Nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:18.2": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:18.3": "Āmantā.",
  "kv3.12:18.4": "Nanu nevasaññānāsaññāyatananti?",
  "kv3.12:18.5": "Āmantā.",
  "kv3.12:18.6": "Hañci nevasaññānāsaññāyatanaṃ, tena vata re vattabbe—",
  "kv3.12:18.7": "“nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:18.8": "‘saññā atthī’ti vā ‘natthī’ti vā”ti.",
  "kv3.12:19.1": "☑ Nevasaññānāsaññāyatananti katvā nevasaññānāsaññāyatane na vattabbaṃ—",
  "kv3.12:19.2": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:19.3": "Āmantā.",
  "kv3.12:19.4": "Adukkhamasukhā vedanāti katvā adukkhamasukhāya vedanāya na vattabbaṃ—",
  "kv3.12:19.5": "“vedanā”ti vā “avedanā”ti vāti?",
  "kv3.12:19.6": "Na hevaṃ vattabbe …pe….",
  "kv3.12:19.7": "Nevasaññānāsaññāyatanakathā niṭṭhitā.",
  "kv3.12:19.8": "Tatiyo vaggo.",
  "kv3.12:19.9": "Tassuddānaṃ",
  "kv3.12:20.1": "Balaṃ sādhāraṇaṃ ariyaṃ,",
  "kv3.12:20.2": "sarāgaṃ cittaṃ vimuccati;",
  "kv3.12:20.3": "Vimuttaṃ vimuccamānaṃ,",
  "kv3.12:20.4": "atthi cittaṃ vimuccamānaṃ.",
  "kv3.12:21.1": "Aṭṭhamakassa puggalassa,",
  "kv3.12:21.2": "Diṭṭhipariyuṭṭhānaṃ pahīnaṃ;",
  "kv3.12:21.3": "Aṭṭhamakassa puggalassa,",
  "kv3.12:21.4": "Natthi pañcindriyāni cakkhuṃ.",
  "kv3.12:22.1": "Sotaṃ dhammupatthaddhaṃ,",
  "kv3.12:22.2": "Yathākammūpagataṃ ñāṇaṃ;",
  "kv3.12:22.3": "Devesu saṃvaro asañña-",
  "kv3.12:22.4": "Sattesu saññā evameva bhavagganti."
}