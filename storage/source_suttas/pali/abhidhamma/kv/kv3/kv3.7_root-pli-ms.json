{
  "kv3.7:0.1": "Kathāvatthu",
  "kv3.7:0.2": "Mahāpaṇṇāsaka",
  "kv3.7:0.3": "Tatiyavagga",
  "kv3.7:0.4": "Dibbacakkhukathā",
  "kv3.7:1.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:1.2": "Āmantā.",
  "kv3.7:1.3": "Maṃsacakkhuṃ dibbacakkhuṃ, dibbacakkhuṃ maṃsacakkhunti?",
  "kv3.7:1.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:2.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:2.2": "Āmantā.",
  "kv3.7:2.3": "Yādisaṃ maṃsacakkhuṃ tādisaṃ dibbacakkhuṃ, yādisaṃ dibbacakkhuṃ tādisaṃ maṃsacakkhunti?",
  "kv3.7:2.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:3.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:3.2": "Āmantā.",
  "kv3.7:3.3": "Taññeva maṃsacakkhuṃ taṃ dibbacakkhuṃ, taṃ dibbacakkhuṃ taṃ maṃsacakkhunti?",
  "kv3.7:3.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:4.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:4.2": "Āmantā.",
  "kv3.7:4.3": "Yādiso maṃsacakkhussa visayo ānubhāvo gocaro tādiso dibbassa cakkhussa visayo ānubhāvo gocaroti?",
  "kv3.7:4.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:5.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:5.2": "Āmantā.",
  "kv3.7:5.3": "Upādinnaṃ hutvā anupādinnaṃ hotīti?",
  "kv3.7:5.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:6.1": "☑ Upādinnaṃ hutvā anupādinnaṃ hotīti?",
  "kv3.7:6.2": "Āmantā.",
  "kv3.7:6.3": "Kāmāvacaraṃ hutvā rūpāvacaraṃ hotīti?",
  "kv3.7:6.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:7.1": "☑ Kāmāvacaraṃ hutvā rūpāvacaraṃ hotīti?",
  "kv3.7:7.2": "Āmantā.",
  "kv3.7:7.3": "Rūpāvacaraṃ hutvā arūpāvacaraṃ hotīti?",
  "kv3.7:7.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:8.1": "☑ Rūpāvacaraṃ hutvā arūpāvacaraṃ hotīti?",
  "kv3.7:8.2": "Āmantā.",
  "kv3.7:8.3": "Pariyāpannaṃ hutvā apariyāpannaṃ hotīti?",
  "kv3.7:8.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:9.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:9.2": "Āmantā.",
  "kv3.7:9.3": "Dibbacakkhuṃ dhammupatthaddhaṃ maṃsacakkhuṃ hotīti?",
  "kv3.7:9.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:10.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:10.2": "Āmantā.",
  "kv3.7:10.3": "Dibbacakkhuṃ dhammupatthaddhaṃ paññācakkhuṃ hotīti?",
  "kv3.7:10.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:11.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:11.2": "Āmantā.",
  "kv3.7:11.3": "Dibbacakkhuṃ dhammupatthaddhaṃ maṃsacakkhuṃ hotīti?",
  "kv3.7:11.4": "Na hevaṃ vattabbe …pe….",
  "kv3.7:12.1": "☑ Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
  "kv3.7:12.2": "Āmantā.",
  "kv3.7:12.3": "Dveva cakkhūnīti?",
  "kv3.7:12.4": "Na hevaṃ vattabbe …pe…",
  "kv3.7:12.5": "dveva cakkhūnīti?",
  "kv3.7:12.6": "Āmantā.",
  "kv3.7:12.7": "Nanu tīṇi cakkhūni vuttāni bhagavatā—",
  "kv3.7:12.8": "maṃsacakkhuṃ, dibbacakkhuṃ, paññācakkhunti?",
  "kv3.7:12.9": "Āmantā.",
  "kv3.7:12.10": "Hañci tīṇi cakkhūni vuttāni bhagavatā—",
  "kv3.7:12.11": "maṃsacakkhuṃ, dibbacakkhuṃ, paññācakkhuṃ, no ca vata re vattabbe—",
  "kv3.7:12.12": "“dveva cakkhūnī”ti.",
  "kv3.7:13.1": "☑ Dveva cakkhūnīti?",
  "kv3.7:13.2": "Āmantā.",
  "kv3.7:13.3": "Nanu vuttaṃ bhagavatā—",
  "kv3.7:13.4": "“tīṇimāni, bhikkhave, cakkhūni.",
  "kv3.7:13.5": "Katamāni tīṇi?",
  "kv3.7:13.6": "Maṃsacakkhuṃ, dibbacakkhuṃ, paññācakkhunti—",
  "kv3.7:13.7": "imāni kho, bhikkhave, tīṇi cakkhūnī”ti.",
  "kv3.7:14.1": "“Maṃsacakkhuṃ dibbacakkhuṃ,",
  "kv3.7:14.2": "Paññācakkhuṃ anuttaraṃ;",
  "kv3.7:14.3": "Etāni tīṇi cakkhūni,",
  "kv3.7:14.4": "Akkhāsi purisuttamo.",
  "kv3.7:15.1": "Maṃsacakkhussa uppādo,",
  "kv3.7:15.2": "maggo dibbassa cakkhuno;",
  "kv3.7:15.3": "Yadā ca ñāṇaṃ udapādi,",
  "kv3.7:15.4": "paññācakkhuṃ anuttaraṃ;",
  "kv3.7:15.5": "Tassa cakkhussa paṭilābhā,",
  "kv3.7:15.6": "sabbadukkhā pamuccatī”ti.",
  "kv3.7:16.1": "Attheva suttantoti?",
  "kv3.7:16.2": "Āmantā.",
  "kv3.7:16.3": "Tena hi na vattabbaṃ—",
  "kv3.7:16.4": "“dveva cakkhūnī”ti.",
  "kv3.7:16.5": "Dibbacakkhukathā niṭṭhitā."
}